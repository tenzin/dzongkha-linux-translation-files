#: errorCreateNewDirMessage
msgid "Directory %S could not be created"
msgstr "སྣོད་ཐོ་ %S དེ་གསར་བསྐྲུན་འབད་མ་ཚུགས།"

#: xmlFilter
msgid "*.xml"
msgstr "*.ཨེགསི་ཨེམ་ཨེལ"

#: openWithoutPermissionMessage_file
msgid "File %S is not readable"
msgstr "ཡིག་སྣོད་%S དེ་ལྷག་ཚུགསཔ་མིན་འདུག"

#: appsTitle
msgid "Applications"
msgstr "འཇུག་སྤྱོད།"

#: noButtonLabel
msgid "No"
msgstr "མེན།"

#: errorCreateNewDirIsFileMessage
msgid "Directory cannot be created, %S is a file"
msgstr "སྣོད་ཐོ་འདི་གསར་བསྐྲུན་འབད་མི་བཏུབ། %S དེ་ཡིག་སྣོད་གཅིག་ཨིན།"

#: errorOpeningFileTitle
msgid ""
"_: errorOpeningFileTitle\n"
"Error opening %S"
msgstr "%S ཁ་ཕྱེ་ནིའི་ནང་འཛོལ་བ།"

#: allFilter
msgid "*"
msgstr "*"

#: confirmTitle
msgid "Confirm"
msgstr "ངེས་གཏན་བཟོ།"

#: imageFilter
msgid "*.png; *.gif; *.jpeg; *.jpg"
msgstr "*.png; *.gif; *.jpeg; *.jpg"

#: confirmFileReplacing
msgid ""
"%S already exists.\n"
"Do you want to replace it?"
msgstr "ཁྱོད་ཀྱིས་འདི་ཚབ་བཙུགས་ནི་ཨིན་ན?"

#: errorDirNotReadableMessage
msgid "Directory %S is not readable or does not exist."
msgstr "སྣོད་ཐོ་%S ལྷག་ཚུགསཔ་མིན་པས་ཡང་ན་ཡོད་རང་མིན་འདུག"

#: dirTextInputLabel
msgid ""
"_: dirTextInputLabel\n"
"Directory name\\:"
msgstr "སྣོད་ཐོའི་མིང་\\:"

#: saveButtonLabel
msgid "Save"
msgstr "སྲུང་བཞང་འབད།"

#: errorNewDirDoesExistTitle
msgid ""
"_: errorNewDirDoesExistTitle\n"
"Error creating %S"
msgstr "%S གསར་བསྐྲུན་འབད་ནིའི་ནང་འཛོལ་བ།"

#: saveWithoutPermissionMessage_dir
msgid "Cannot create file. Directory %S is not writable."
msgstr "ཡིག་སྣོད་གསར་བསྐྲུན་འབད་མི་ཚུགས། སྣོད་ཐོ་%S དེ་འབྲི་བཏུབ་མེན།"

#: errorDirDoesntExistMessage
msgid "Directory %S doesn't exist"
msgstr "སྣོད་ཐོ་%S ཡོད་རང་མེད།"

#: errorSavingFileTitle
msgid "Error saving %S"
msgstr "%S སྲུང་བཞག་འབད་ནིའི་ནང་འཛོལ་བ།"

#: imageTitle
msgid "Image Files"
msgstr "གཟུགས་བརྙན་གྱི་ཡིག་སྣོད་ཚུ།"

#: errorNewDirDoesExistMessage
msgid "A file named %S already exists, directory cannot be created."
msgstr "ཡིག་སྣོད་མིང་%Sསྦེ་མི་ཧེ་མ་ལས་རང་ཡོད་ སྣོད་ཐོ་གསར་བསྐྲུན་འབད་མི་ཚུགས།"

#: errorOpenFileDoesntExistMessage
msgid "File %S doesn't exist"
msgstr "ཡིག་སྣོད་%S ཡོད་རང་མེད།"

#: saveParentDoesntExistMessage
msgid "Path %S doesn't exist, can't save %S"
msgstr "འགྲུལ་ལམ་%S ཡོད་རང་མེད་ %S སྲུང་བཞག་འབད་མི་ཚུགས།"

#: errorCreateNewDirPermissionMessage
msgid "Directory cannot be created, %S not writable"
msgstr "སྣོད་ཐོ་གསར་བསྐྲུན་འབད་མི་ཚུགས་ %S འདི་འབྲི་མི་བཏུབ།"

#: xulFilter
msgid "*.xul"
msgstr "*.xul"

#: saveWithoutPermissionMessage_file
msgid "File %S is not writable."
msgstr "ཡིག་སྣོད་%S དེ་འབྲི་མི་བཏུབ།"

#: errorCreateNewDirTitle
msgid ""
"_: errorCreateNewDirTitle\n"
"Error creating %S"
msgstr "%S གསར་བསྐྲུན་འབད་ནིའི་ནང་འཛོལ་བ།"

#: promptNewDirTitle
msgid "Create new directory"
msgstr "སྣོད་ཐོ་གསརཔ་གསར་བསྐྲུན་འབད།"

#: xmlTitle
msgid "XML Files"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ ཡིག་སྣོད་ཚུ།"

#: errorDirDoesntExistTitle
msgid "Error accessing %S"
msgstr "%S འཛུལ་སྤྱོད་འབད་ནིའི་ནང་འཛོལ་བ།"

#: htmlTitle
msgid "HTML Files"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ ཡིག་སྣོད་ཚུ།"

#: errorOpenFileDoesntExistTitle
msgid ""
"_: errorOpenFileDoesntExistTitle\n"
"Error opening %S"
msgstr "%S ཁ་ཕྱེ་ནིའི་ནང་འཛོལ་བ།"

#: xulTitle
msgid "XUL Files"
msgstr "ཨེགསི་ཡུ་ཨེལ་ ཡིག་སྣོད་ཚུ།"

#: textTitle
msgid "Text Files"
msgstr "ཚིག་ཡིག་གི་ཡིག་སྣོད་ཚུ།"

#: selectFolderButtonLabel
msgid "Select"
msgstr "སེལ་འཐུ་འབད།"

#: openButtonLabel
msgid "Open"
msgstr "ཁ་ཕྱེ།"

#: promptNewDirMessage
msgid ""
"_: promptNewDirMessage\n"
"Directory name\\:"
msgstr "སྣོད་ཐོའི་མིང་\\:"

#: errorPathProblemTitle
msgid "Unknown Error"
msgstr "མ་ཤེས་པའི་འཛོལ་བ།"

#: textFilter
msgid "*.txt; *.text"
msgstr "*.txt; *.text"

#: saveParentIsFileMessage
msgid "%S is a file, can't save %S"
msgstr "%S འདི་ཡིག་སྣོད་གཅིག་ཨིན་ %S སྲུང་བཞག་འབད་མི་ཚུགས།"

#: htmlFilter
msgid "*.html; *.htm; *.shtml; *.xhtml"
msgstr "*.html; *.htm; *.shtml; *.xhtml"

#: errorPathProblemMessage
msgid "An unknown error occured (path %S)"
msgstr "མ་ཤེས་པའི་འཛོལ་བ་གཅིག་བྱུང་ནུག(འགྲུལ་ལམ་%S)"

#: allTitle
msgid "All Files"
msgstr "ཡིག་སྣོད་ཚུ་ག་ར།"

