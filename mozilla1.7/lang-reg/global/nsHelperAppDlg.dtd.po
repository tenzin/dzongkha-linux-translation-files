#: alwaysHandle.label
#: alwaysHandle.accesskey
msgid "&Always perform this action when handling files of this type"
msgstr "འ་ནི་དབྱེ་བཟུམ་མའི་ཡིག་སྣོད་ཚུ་ལེགས་སྐྱོང་འཐབ་པའི་སྐབས་ ཨ་རྟག་རང་འ་ནི་བྱ་བ་འདི་འབད།(&A)"

#: badApp
msgid "The application you specified (&quot;#1&quot;) could not be found.  Check the file name or select another application."
msgstr "འཇུག་སྤྱོད་ཁྱོད་ཀྱིས་གསལ་བཀོད་འབད་མི་(&quot;#1&quot;)འདི་འཚོལ་མ་འཐོབ། ཡིག་སྣོད་མིང་ཞིབ་དཔྱད་འབད་ ཡང་ན་འཇུག་སྤྱོད་གཞན་མི་སེལ་འཐུ་འབད།"

#: badApp.title
msgid "Application not found"
msgstr "འཇུག་སྤྱོད་འཚོལ་མ་འཐོབ།"

#: caption.label
msgid "Opening #1"
msgstr "#1 ཁ་ཕྱེ་དོ"

#: chooseApp.label
#: chooseApp.accesskey
msgid "&Choose..."
msgstr "གདམ་ཁ་རྐྱབས...(&C)"

#: chooseAppFilePickerTitle
msgid "Choose Helper Application"
msgstr "ལས་རོགས་པའི་འཇུག་སྤྱོད་གདམ་ཁ་རྐྱབས།"

#: intro.attachment.label
msgid "The site has suggested that &quot;#3&quot; be handled as an attachment. It is of type #2 (#1) and located at:"
msgstr "ས་ཁོངས་ཀྱིས་&quot;#3&quot; དེ་མཉམ་སྦྲགས་སྦེ་ལེགས་སྐྱོང་འཐབ་དགོ་པའི་བསམ་ཆར་བཀོདཔ་ཨིན་པས། འ་ནི་འདི་ #2 (#1) གི་དབྱེ་བ་དང་འདི་ནང་གནས་ཏེ་ཡོད:"

#: intro.attachment.noDesc.label
msgid "The site has suggested that &quot;#3&quot; be handled as an attachment. It is of type #2 and located at:"
msgstr "ས་ཁོངས་ཀྱིས་&quot;#3&quot;དེ་མཉམ་སྦྲགས་སྦེ་ལེགས་སྐྱོང་འཐབ་དགོ་པའི་བསམ་ཆར་བཀོདཔ་ཨིན་པས། འ་ནི་འདི་#2 གི་དབྱེ་བ་དང་འདི་ནང་གནས་ཏེ་ཡོད:"

#: intro.label
msgid "The file &quot;#3&quot; is of type #2 (#1), and #4 does not know how to handle this file type. This file is located at:"
msgstr "ཡིག་སྣོད་ &quot;#3&quot; དེ་ #2(#1) གི་དབྱེ་བ་དང་ #4 གིས་འ་ནི་ཡིག་སྣོད་ཀྱི་དབྱེ་བ་ལེགས་སྐྱོང་ག་དེ་སྦེ་འཐབ་ནི་ཨིན་ན་མི་ཤེས་པས། འ་ནི་ཡིག་སྣོད་འདི་དེ་ནང་ལུ་གནས་ཏེ་ཡོད:"

#: intro.noDesc.label
msgid "The file &quot;#3&quot; is of type #2, and #4 does not know how to handle this file type. This file is located at:"
msgstr "ཡིག་སྣོད་ &quot;#3&quot;དེ་ #2 གི་དབྱེ་བ་དང་ #4 གིས་འ་ནི་ཡིག་སྣོད་ཀྱི་དབྱེ་བ་ལེགས་སྐྱོང་ག་དེ་སྦེ་འཐབ་ནི་ཨིན་ན་མི་ཤེས་པས། འ་ནི་ཡིག་སྣོད་འདི་དེ་ནང་ལུ་གནས་ཏེ་ཡོད:"

#: openUsing.label
#: openUsing.accesskey
msgid "Open it &with"
msgstr "དེ་དང་གཅིག་ཁར་ཁ་ཕྱེ(&w)"

#: prompt.label
msgid "What should #1 do with this file?"
msgstr "#1 འ་ནི་ཡིག་སྣོད་དང་གཅིག་ཁར་ག་ཅི་འབད་ནི?"

#: saveToDisk.label
#: saveToDisk.accesskey
msgid "&Save it to disk"
msgstr "ཌིགསི་ནང་ལུ་སྲུང་བཞག་འབད།(&S)"

#: useSystemDefault.label
#: useSystemDefault.accesskey
msgid "&Open it with the default application (#1)"
msgstr "སྔོན་སྒྲིག་གི་འཇུག་སྤྱོད་དང་གཅིག་ཁར་ཁ་ཕྱེ(#1)(&O)"

#: useSystemDefault.noDesc.label
msgid "Open it with the default application"
msgstr "སྔོན་སྒྲིག་གི་འཇུག་སྤྱོད་དང་གཅིག་ཁར་ཁ་ཕྱེ།"

