#: bookmarkGroup.label
#: bookmarkGroup.accesskey
msgid "&Bookmark This Group of Tabs"
msgstr "འ་ནི་མཆོང་ལྡེ་གི་སྡེ་ཚན་འདི་ཚུ་དེབ་རྟགས་བཀོད།(&B)"

#: closeOtherTabs.label
#: closeOtherTabs.accesskey
msgid "Close &Other Tabs"
msgstr "མཆོང་ལྡེ་གཞན་མི་ཚུ་ཁ་བསྡམ།(&O)"

#: closeTab.label
#: closeTab.accesskey
msgid "&Close Tab"
msgstr "མཆོང་ལྡེ་ཁ་བསྡམ།(&C)"

#: newTab.label
#: newTab.accesskey
msgid "&New Tab"
msgstr "མཆོང་ལྡེ་གསརཔ།(&N)"

#: newTabButton.tooltip
msgid "Open a new tab"
msgstr "མཆོང་ལྡེ་གསརཔ་གཅིག་ཁ་ཕྱེ།"

#: reloadAllTabs.label
#: reloadAllTabs.accesskey
msgid "Reload &All Tabs"
msgstr "མཆོང་ལྡེ་ཚུ་ག་ར་ཡང་བསྐྱར་མངོན་གསལ་འབད།(&A)"

#: reloadTab.label
#: reloadTab.accesskey
msgid "&Reload Tab"
msgstr "མཆོང་ལྡེ་ཡང་བསྐྱར་མངོན་གསལ་འབད།(&R)"

#: untitledTab
msgid "(Untitled)"
msgstr "(མགོ་མིང་མ་བཀོད་པ)"

