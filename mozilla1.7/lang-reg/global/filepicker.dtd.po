#: filterMenuList.label
#: filterMenuList.accesskey
msgid "Files of &type:"
msgstr "ཡིག་སྣོད་ཚུའི་དབྱེ་བ:(&t)"

#: folderHome.tooltiptext
msgid "Go to home"
msgstr "ཁྱིམ་ནང་འགྱོ།"

#: folderNew.tooltiptext
msgid "Create new directory"
msgstr "སྣོད་ཐོ་གསརཔ་གསར་བསྐྲུན་འབད།"

#: folderUp.tooltiptext
msgid "Go up a level"
msgstr "གནས་རིམ་གཅིག་ཡར་འགྱོ།"

#: lastModified.label
msgid "Last Modified"
msgstr "མཇུག་གི་ལེགས་བཅོས་གྲུབ་ཡོདཔ།"

#: lookInMenuList.label
#: lookInMenuList.accesskey
msgid "&Look in:"
msgstr "ནང་བལྟ:(&L)"

#: name.label
msgid "Name"
msgstr "མིང་།"

#: noPermissionError.label
msgid "You do not have the permissions necessary to view this directory."
msgstr "ཁྱོད་ལུ་འ་ནི་སྣོད་ཐོ་སྟོན་ནིའི་གནང་བ་ཉེར་མཁོ་མེད།"

#: permissions.label
msgid "Permissions"
msgstr "གནང་བ་ཚུ"

#: showHiddenFiles.label
#: showHiddenFiles.accesskey
msgid "&Show hidden files and directories"
msgstr "སྦ་བཞག་ཡོད་མི་ཡིག་སྣོད་ཚུ་དང་སྣོད་ཐོ་ཚུ་སྟོན།(&S)"

#: size.label
msgid "Size"
msgstr "ཚད།"

#: textInput.label
#: textInput.accesskey
msgid "File &name:"
msgstr "ཡིག་སྣོད་མིང་:(&n)"

