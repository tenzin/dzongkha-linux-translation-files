#: cancel
msgid "Cancel"
msgstr "ཆ་མེད་གཏང་།"

#: close
msgid "Close"
msgstr "ཁ་བསྡམ།"

#: completeMsg
msgid "Finished, #2 KB downloaded (elapsed time was #1)"
msgstr "མཇུག་བསྡུ་ཡོད། #2 ཀེ་བི་ཕབ་ལེན་འབད་ནི་(སོང་བའི་ཆུ་ཚོད་དེ་ #1)"

#: defaultTitle
msgid "Saving"
msgstr "སྲུང་བཞག་འབད་དོ།"

#: keep
msgid "Keep this window open after the download is complete."
msgstr "ཕབ་ལེན་འདི་མཇུག་བསྡུ་བའི་ཤུལ་ལས་འ་ནི་སྒོ་སྒྲིག་འདི་ཁ་ཕྱེ་བཞག"

#: launch
msgid "Launch File"
msgstr "ཡིག་སྣོད་གསར་བཙུགས་འབད།"

#: longTimeFormat
msgid "#1:#2:#3"
msgstr "#1:#2:#3"

#: openingSource
msgid "Opening From:"
msgstr "ལས་ཁ་ཕྱེ་དོ:"

#: openingTarget
msgid "With:"
msgstr "གཅིག་ཁར:"

#: openingTitle
msgid "#2&#037; of #1 Completed"
msgstr "#2% #1 ལས་མཇུག་བསྡུ་ཡོད།"

#: pause
msgid "Pause"
msgstr "ཐེམ།"

#: pausedMsg
msgid "Download Paused"
msgstr "ཕབ་ལེན་འབད་ནི་ཐེམ་བཞག་ཡོད།"

#: percentMsg
msgid "#1&#037;"
msgstr "#1&#037;"

#: progress
msgid "Progress:"
msgstr "ཡར་འཕེལ:"

#: progressMsg
msgid "#1KB of #2KB (at #3KB/sec)"
msgstr "#1KB of #2KB (at #3KB/sec)"

#: resume
msgid "Resume"
msgstr "བསྐྱར་ལོག།"

#: reveal
msgid "Show File Location"
msgstr "ཡིག་སྣོད་ཀྱི་གནས་ཁོངས་སྟོན།"

#: savingSource
msgid "Saving From:"
msgstr "ལས་སྲུང་བཞག་འབད་དོ:"

#: savingTarget
msgid "To:"
msgstr "ལུ:"

#: savingTitle
msgid "#2&#037; of #1 Saved"
msgstr "#2% #1 ལས་སྲུང་བཞག་འབད་ཡོད།"

#: shortTimeFormat
msgid "#2:#3"
msgstr "#2:#3"

#: status
msgid "Status:"
msgstr "གནས་ཚད:"

#: timeElapsed
msgid "Time Elapsed:"
msgstr "ཆུ་ཚོད་སོང་བ:"

#: timeLeft
msgid "Time Left:"
msgstr "ཆུ་ཚོད་ལྷག་ལུས:"

#: unknownOpeningTitle
msgid "Opening #1"
msgstr "#1 ཁ་ཕྱེ་དོ"

#: unknownSavingTitle
msgid "Saving #1"
msgstr "#1 སྲུང་བཞག་འབད་དོ"

#: unknownTime
msgid "Unknown"
msgstr "མ་ཤེསཔ།"

