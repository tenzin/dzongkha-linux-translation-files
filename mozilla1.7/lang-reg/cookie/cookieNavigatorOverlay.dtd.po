#: cookieAboutPopupBlocking.label
#: cookieAboutPopupBlocking.accesskey
msgid "About Popup &Blocking"
msgstr "པོཔ་ཨཔ་འགགས་ནིའི་སྐོར(&B)"

#: cookieAllowCookiesCmd.label
#: cookieAllowCookiesCmd.accesskey
msgid "&Allow Cookies from this Site"
msgstr "ཀུ་ཀི་ཚུ་འ་ནི་ས་ཁོངས་ལས་འབད་བཅུག(&A)"

#: cookieAllowCookiesMsg.label
msgid "Cookies from this site will always be allowed."
msgstr "འ་ནི་ས་ཁོངས་ལས་ཀྱི་ཀུ་ཀི་ཚུ་ཨ་རྟག་རང་འབད་ཆོགཔ་ཨིན།"

#: cookieAllowImagesCmd.label
#: cookieAllowImagesCmd.accesskey
msgid "&Allow Images from this Site"
msgstr "འ་ནི་ས་ཁོངས་ཀྱི་གཟུགས་བརྙན་ཚུ་འབད་བཅུག།(&A)"

#: cookieAllowImagesMsg.label
msgid "Images from this site will always be downloaded."
msgstr "འ་ནི་ས་ཁོངས་ལས་ཀྱི་གཟུགས་བརྙན་ཚུ་ཨ་རྟག་རང་ཕབ་ལེན་འབད་ཡོདཔ་ཨིན།"

#: cookieAllowPopupsCmd.label
#: cookieAllowPopupsCmd.accesskey
msgid "&Allow Popups From This Site"
msgstr "པོཔ་ཨཔ་ཚུ་འ་ནི་ས་ཁོངས་ལས་འབད་བཅུག།(&A)"

#: cookieAllowSessionCookiesCmd.label
#: cookieAllowSessionCookiesCmd.accesskey
msgid "Allow &Session Cookies from this Site"
msgstr "ལཱ་ཡུན་གྱི་ཀུ་ཀི་ཚུ་འ་ནི་ས་ཁོངས་ལས་འབད་བཅུག།(&S)"

#: cookieAllowSessionCookiesMsg.label
msgid "This site will be able to set cookies for the current session only."
msgstr "ད་ལྟོའི་ལཱ་ཡུན་གྱི་དོན་ལུ་རྐྱངམ་ཅིག་འ་ནི་ས་ཁོངས་ཀྱིས་ཀུ་ཀི་ཚུ་གཞི་སྒྲིག་འབད་ཚུགས་ནི་ཨིན་པས།"

#: cookieBlockCookiesCmd.label
#: cookieBlockCookiesCmd.accesskey
msgid "&Block Cookies from this Site"
msgstr "ཀུ་ཀི་ཚུ་འ་ནི་ས་ཁོངས་ལས་བཀག་བཞག།(&B)"

#: cookieBlockCookiesMsg.label
msgid "Cookies from this site will always be rejected."
msgstr "འ་ནི་ས་ཁོངས་ལས་ཀྱི་ཀུ་ཀི་ཚུ་ཨ་རྟག་རང་དང་ལེན་མི་འབད།"

#: cookieBlockImagesCmd.label
#: cookieBlockImagesCmd.accesskey
msgid "&Block Images from this Site"
msgstr "གཟུགས་བརྙན་ཚུ་འ་ནི་ས་ཁོངས་ལས་བཀག་བཞག།(&B)"

#: cookieBlockImagesMsg.label
msgid "Images from this site will never be downloaded."
msgstr "འ་ནི་ས་ཁོངས་ལས་ཀྱི་གཟུགས་བརྙན་ཚུ་ནམ་ཡང་ཕབ་ལེན་མི་འབད།"

#: cookieCookieManager.label
#: cookieCookieManager.accesskey
msgid "&Cookie Manager"
msgstr "ཀུ་ཀི་འཛིན་སྐྱོང་པ།(&C)"

#: cookieCookiesDefaultCmd.label
#: cookieCookiesDefaultCmd.accesskey
msgid "&Use Default Cookie Permissions"
msgstr "སྔོན་སྒྲིག་ཀུ་ཀིའི་གནང་བ་ཚུ་ལག་ལེན་འཐབ།(&U)"

#: cookieCookiesDefaultMsg.label
msgid "Cookies from this site will be accepted or rejected based on default settings."
msgstr "འ་ནི་ས་ཁོངས་ལས་ཀྱི་ཀུ་ཀི་ཚུ་སྔོན་སྒྲིག་གི་སྒྲིག་སྟངས་ཚུ་དང་འཁྲིལ་ཏེ་དང་ལེན་ཡང་ན་དང་ལེན་མི་འབད།"

#: cookieDisplayCookiesCmd.label
#: cookieDisplayCookiesCmd.accesskey
msgid "&Manage Stored Cookies"
msgstr "སྲོག་འཇོག་འབད་ཡོད་པའི་ཀུ་ཀི་ཚུ་འཛིན་སྐྱོང་འབད།(&M)"

#: cookieDisplayImagesCmd.label
#: cookieDisplayImagesCmd.accesskey
msgid "&Manage Image Permissions"
msgstr "གཟུགས་བརྙན་གྱི་གནང་བ་ཚུ་འཛིན་སྐྱོང་འབད།(&M)"

#: cookieImageManager.label
#: cookieImageManager.accesskey
msgid "&Image Manager"
msgstr "གཟུགས་བརྙན་འཛིན་སྐྱོང་པ།(&I)"

#: cookieImageMessageTitle.label
msgid "Image Permissions Changed"
msgstr "གཟུགས་བརྙན་གྱི་གནང་བ་ཚུ་བསྒྱུར་བཅོས་འབད་ཡོད།"

#: cookieImagesDefaultCmd.label
#: cookieImagesDefaultCmd.accesskey
msgid "&Use Default Image Permissions"
msgstr "སྔོན་སྒྲིག་གཟུགས་བརྙན་གྱི་གནང་བ་ཚུ་ལག་ལེན་འཐབ།(&U)"

#: cookieImagesDefaultMsg.label
msgid "Images from this site will be downloaded based on default settings."
msgstr "སྔོན་སྒྲིག་གི་སྒྲིག་སྟངས་ཚུ་དང་འཁྲིལ་ཏེ་ འ་ནི་ས་ཁོངས་ལས་ཀྱི་གཟུགས་བརྙན་ཚུ་ཕབ་ལེན་འབདཝ་ཨིན།"

#: cookieManagePopups.label
#: cookieManagePopups.accesskey
msgid "&Manage Popups"
msgstr "པོཔ་ཨཔ་ཚུ་འཛིན་སྐྱོང་འབད།(&M)"

#: cookieMessageTitle.label
msgid "Cookie Permissions Changed"
msgstr "ཀུ་ཀིའི་གནང་བ་ཚུ་བསྒྱུར་བཅོས་འབད་ཡོདཔ།"

#: cookiePopupManager.label
#: cookiePopupManager.accesskey
msgid "P&opup Manager"
msgstr "པོཔ་ཨཔ་འཛིན་སྐྱོང་པ།(&o)"

#: cookieTutorialCmd.label
#: cookieTutorialCmd.accesskey
msgid "&Understanding Privacy"
msgstr "སྒེར་གཙང་ཧ་གོ་ནི།(&U)"

