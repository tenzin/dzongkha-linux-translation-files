#: atEndOfSession
msgid "at end of session"
msgstr "ལཱ་ཡུན་གྱི་མཇུག་ལུ"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "ས་ཁོངས་%Sགིས་ཀུ་ཀི་གཅིག་གཞི་སྒྲིག་འབད་ནི་ཨིན་པས།"

#: forAnyConnection
msgid "Any type of connection"
msgstr "མཐུད་ལམ་གྱི་དབྱེ་བ་ག་ཅི་འབད་རུང་"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: domainColon
msgid "Domain\\:"
msgstr "ཌོ་མེན\\:"

#: hostColon
msgid "Host\\:"
msgstr "ཧོསཊི\\:"

#: rememberThisDecision
msgid "Remember this decision"
msgstr "གྲོས་ཐག་གཅད་མི་དེ་སེམས་ཁར་བཞག"

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "ས་ཁོངས་%S གིས་ཀུ་ཀི་ཡོད་མི་ཅིག་ལེགས་བཅོས་འབད་ནི་ཨིན་པས།"

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "ས་ཁོངས་%Sགིས་ཀུ་ཀི་གཉིས་པ་ཅིག་གཞི་སྒྲིག་འབད་ནི་ཨིན་པས།"

#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"ས་ཁོངས་%S གིས་ཀུ་ཀི་གཞན་མི་ཅིག་གཞི་སྒྲིག་འབད་ནི་ཨིན་པས།\n"
"ཁྱོད་ལུ་ཧེ་མ་ལས་རང་ %S ཀུ་ཀི་ཚུ་ ས་ཁོངས་འདི་ལས་ཡོད།"

#: hideDetails
msgid "Hide Details"
msgstr "རྒྱས་བཤད་ཚུ་སྦ་བཞག"

#: permissionToAcceptImage
msgid "The site %S wants to load an image."
msgstr " %S གི་ས་ཁོངས་ཀྱིས་གཟུགས་བརྙན་ཅིག་མངོན་གསལ་འབད་ནི་ཨིན་པས།"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "གསང་བཟོའི་མཐུད་ལམ་ཚུ་རྐྱངམ་ཅིག"

#: showDetails
msgid "Show Details"
msgstr "རྒྱས་བཤད་ཚུ་སྟོན"

