#: accAllCookiesRadio.label
#: accAllCookiesRadio.accesskey
msgid "Allow all &cookies"
msgstr "ཀུ་ཀི་ཚུ་ཆ་མཉམ་འབད་བཅུག(&c)"

#: accOrgCookiesRadio.label
#: accOrgCookiesRadio.accesskey
msgid "All&ow cookies for the originating web site only"
msgstr "ཀུ་ཀི་ཚུ་འགོ་ཐོག་བྱུང་སའི་ཝེབ་ཀྱི་ས་ཁོངས་ཚུའི་དོན་ལུ་རྐྱངམ་ཅིག་འབད་བཅུག(&o)"

#: accP3PCookiesRadio.label
#: accP3PCookiesRadio.accesskey
msgid "Allow cookies based on &privacy settings"
msgstr "ཀུ་ཀི་ཚུ་སྒེར་གཙང་གི་སྒྲིག་སྟངས་ཚུ་ལུ་གཞི་བཞག་ཐོག་འབད་བཅུག(&p)"

#: acceptForSession.label
#: acceptForSession.accesskey
msgid "Accept for current &session only"
msgstr "ད་ལྟོའི་ལཱ་ཡུན་གྱི་དོན་ལུ་རྐྱངམ་ཅིག་དང་ལེན་འབད།(&s)"

#: acceptNormally.label
#: acceptNormally.accesskey
msgid "Accept cookies &normally"
msgstr "ཀུ་ཀི་ཚུ་སྤྱིར་གཏང་གི་དང་ལེན་འབད།(&n)"

#: acceptforNDays.label
#: acceptforNDays.accesskey
msgid "Accept cookies &for"
msgstr "དོན་ལུ་ཀུ་ཀི་ཚུ་དང་ལེན་འབད།(&f)"

#: cookieLifetimePolicy.label
msgid "Cookie Lifetime Policy"
msgstr "ཀུ་ཀིའི་ཚེ་ཡུན་གྱི་སྲིད་བྱུས"

#: cookiePolicy.label
msgid "Cookie Acceptance Policy"
msgstr "ཀུ་ཀི་དང་ལེན་འབད་ནིའི་སྲིད་བྱུས"

#: days.label
msgid "days"
msgstr "གཟའ་ཚུ"

#: disableCookies.label
#: disableCookies.accesskey
msgid "&Block cookies"
msgstr "ཀུ་ཀི་ཚུ་བཀག་བཞག(&B)"

#: forCurrentSession.label
#: forCurrentSession.accesskey
msgid "&except for session cookies"
msgstr "ལཱ་ཡུན་ཀུ་ཀི་ཚུའི་དོན་ལུ་མ་གཏོགས(&e)"

#: lHeader
msgid "Cookies"
msgstr "ཀུ་ཀི་ཚུ"

#: manageCookies.label
msgid "Manage Cookies and Sites"
msgstr "ཀུ་ཀི་ཚུ་དང་ས་ཁོངས་ཚུ་འཛིན་སྐྱོང་འཐབ།"

#: manageCookiesDescription.label
msgid "Allows you to view and manage stored cookies and per-site settings for accepting and rejecting cookies. Per-site settings will override the settings above."
msgstr "ཁྱོད་ལུ་དང་ལེན་འབད་ནི་དང་མི་འབད་ནིའི་ཀུ་ཀི་ཚུའི་དོན་ལུ་སྲུང་བཞག་འབད་ཡོད་པའི་ཀུ་ཀི་ཚུ་དང་ས་ཁོངས་རེའི་སྒྲིག་སྟངས་ཚ་སྟོན་ནི་དང་འཛིན་སྐྱོང་འཐབ་ནི་ཚུ་འབད་བཅུགཔ་ཨིན། ས་ཁོངས་རེའི་སྒྲིག་སྟངས་ཚུ་གིས་གོང་གི་སྒྲིག་སྟངས་ཚུ་ཟུར་བཞག་འབདཝ་ཨིན།"

#: viewCookies.label
#: viewCookies.accesskey
msgid "Cookie &Manager"
msgstr "ཀུ་ཀི་འཛིན་སྐྱོང་པ(&M)"

#: viewP3P.label
#: viewP3P.accesskey
msgid "&View"
msgstr "མཐོང་སྣང་(&V)"

#: warnAboutCookies.label
#: warnAboutCookies.accesskey
msgid "&Ask for each cookie"
msgstr "ཀུ་ཀི་རེ་རེ་བཞིན་གྱི་དོན་ལུ་འདྲི(&A)"

