#: accAllImagesRadio.label
#: accAllImagesRadio.accesskey
msgid "&Accept all images"
msgstr "གཟུགས་བརྙན་ཚུ་ཆ་མཉམ་དང་ལེན་འབད།(&A)"

#: accOrgImagesRadio.label
#: accOrgImagesRadio.accesskey
msgid "Accept images that come f&rom the originating server only"
msgstr "གཟུགས་བརྙན་ཚུ་འགོ་ཐོག་བྱུང་ས་སར་བར་ལས་འོང་མི་རྐྱངམ་ཅིག་དང་ལེན་འབད།(&r)"

#: animLoopAsSpecified.label
#: animLoopAsSpecified.accesskey
msgid "As &many times as the image specifies"
msgstr "གཟུགས་བརྙན་གསལ་བཀོད་འབད་མི་དེ་ལས་ལྷག་སྟེ(&m)"

#: animLoopNever.label
#: animLoopNever.accesskey
msgid "Ne&ver"
msgstr "ནམ་ཡང་(&v)"

#: animLoopOnce.label
#: animLoopOnce.accesskey
msgid "&Once"
msgstr "ཚར་གཅིག(&O)"

#: animLoopingTitle.label
msgid "Animated images should loop"
msgstr "བསྒུལ་ཅན་གྱི་གཟུགས་བརྙན་དེ་ཚུ་འཕྲལ་བཀོད་འབད་དགོ།"

#: disableImages.label
#: disableImages.accesskey
msgid "Do &not load any images"
msgstr "གཟུགས་བརྙན་གཅིག་ཡང་མངོན་གསལ་མ་འབད།(&n)"

#: imageBlocking.label
msgid "Image Acceptance Policy"
msgstr "གཟུགས་བརྙན་དང་ལེན་འབད་ནིའི་སྲིད་བྱུས།"

#: imageDetails
msgid "Specify how &brandShortName; handles images."
msgstr "&brandShortName;གིས་གཟུགས་བརྙན་ལེགས་སྐྱོང་ག་དེ་སྦེ་འཐབ་ཨིན་ན་གསལ་བཀོད་འབད།"

#: lHeader
msgid "Images"
msgstr "གཟུགས་བརྙན་ཚུ།"

#: viewImages.label
#: viewImages.accesskey
msgid "Manage Image &Permissions"
msgstr "གཟུགས་བརྙན་གྱི་གནང་བ་ཚུ་འཛིན་སྐྱོང་འཐབ།(&P)"

#: viewImagesExplanation.label
msgid "View list of sites about which image acceptance information is stored"
msgstr "ས་ཁོངས་ཚུའི་ཐོ་ཡིག་ཅིག་གཟུགས་བརྙན་དང་ལེན་གྱི་བརྡ་དོན་དེ་སྲུང་བཞག་འབད་འབད་བའི་སྐོར་སྟོན།"

#: warnAboutImages.label
#: warnAboutImages.accesskey
msgid "As&k me before downloading an image"
msgstr "གཟུགས་བརྙན་ཕབ་ལེན་མ་འབད་བའི་ཧེ་མ་ང་ལུ་འདྲི།(&k)"

