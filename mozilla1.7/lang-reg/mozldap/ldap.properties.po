#: 89
msgid "Bad parameter to an LDAP routine"
msgstr "ཨེལ་ཌི་ཨེ་པི་གི་རྒྱུན་ལས་ཅིག་ལུ་ཚད་བཟུང་བྱང་ཉེས།"

#: 88
msgid "User cancelled operation"
msgstr "ལག་ལེན་པ་ཆ་མེད་གཏང་གཏང་པའི་བཀོལ་སྤྱོད།"

#: 87
msgid "Invalid search filter"
msgstr "ནུས་མེད་འཚོལ་ཞིབ་ཀྱི་ཚགས་མ།"

#: 86
msgid "Unknown authentication method"
msgstr "མ་ཤེས་པའི་བདེན་བཤད་ཀྱི་ཐབས་ལམ།"

#: 54
msgid "Loop detected"
msgstr "འཕྲལ་བཀོལ་སྐྱོན་འཛིན་བྱུང་ཡོདཔ།"

#: 85
msgid "The LDAP server timed out"
msgstr "ཨེལ་ཌི་ཨེ་པི་གི་སར་བར་ངལ་མཚམས་ལུས་ཡོད།"

#: 21
msgid "Invalid syntax"
msgstr "ནུས་མེད་ཚིག་སྦྱོར།"

#: 53
msgid "LDAP server is unwilling to perform"
msgstr "ཨེལ་ཌི་ཨེ་པི་གི་སར་བར་དེ་ལས་འགན་གྲུབ་ནི་དང་བླང་མིན་འདུག"

#: 84
msgid "Decoding error"
msgstr "ཌི་ཀོ་ཌིང་གི་འཛོལ་བ།"

#: 20
msgid "Type or value exists"
msgstr "དབྱེ་བ་ཡང་ན་གནས་གོང་ཡོད།"

#: 52
msgid "LDAP server is unavailable"
msgstr "ཨེལ་ཌི་ཨེ་པི་གི་སར་བར་འདི་འཐོབ་ཚུགསཔ་མེད།"

#: 83
msgid "Encoding error"
msgstr "ཨིན་ཀོ་ཌིང་གི་འཛོལ་བ།"

#: 51
msgid "The LDAP server is busy"
msgstr "ཨེལ་ཌི་ཨེ་པི་གི་སར་བར་འདི་ཁོམ་མེད།"

#: 82
msgid "Local error"
msgstr "ཉེ་གནས་ཀྱི་འཛོལ་བ།"

#: 50
msgid "Insufficient access"
msgstr "འཛུལ་སྤྱོད་ལངམ་མིན་འདུག།"

#: 81
msgid "Can't contact the LDAP server"
msgstr "ཨེལ་ཌི་ཨེ་པི་གི་སར་བར་འདི་འབྲེལ་བ་འཐབ་མ་ཚུགས།ས།"

#: 80
msgid "Unknown error"
msgstr "མ་ཤེས་པའི་འཛོལ་བ།"

#: authPromptText
msgid "Please enter your password for %1$S."
msgstr "%1$S གི་དོན་ལུ་ཁྱོད་རའི་ཆོག་ཡིག་ཐོ་བཀོད་འབད་གནང་།"

#: 9
msgid "Partial results and referral received"
msgstr "གྲུབ་འབྲས་ཚུའི་ཆ་ཤས་དང་འབྲེལ་སྟུན་འཐོབ་ཡོདཔ།"

#: 8
msgid "Strong authentication required"
msgstr "བདེན་བཤད་སྒྲིང་སྒྲིང་དགོས་མཁོ་ཡོད།"

#: 19
msgid "Constraint violation"
msgstr "བར་ཆད་འགལ་བ།"

#: 7
msgid "Authentication method not supported"
msgstr "བདེན་བཤད་ཀྱི་ཐབས་ལམ་རྒྱབ་སྐྱོར་མ་འབད།"

#: 18
msgid "Inappropriate matching"
msgstr "འོས་ལྡན་མིན་པའི་མཐུན་སྒྲིག"

#: 6
msgid "Compare true"
msgstr "བདེན་པ་ག་བསྡུར་རྐྱབས།"

#: 17
msgid "Undefined attribute type"
msgstr "ངེས་འཛིན་མ་འབད་བའི་ཁྱད་ཆོས་ཀྱི་དབྱེ་བ།"

#: 49
msgid "Invalid credentials"
msgstr "ནུས་མེད་ངོ་བསྟོད་ཚུ།"

#: 5
msgid "Compare false"
msgstr "རྫུན་མ་ག་བསྡུར་རྐྱབས།"

#: 16
msgid "No such attribute"
msgstr "ཁྱད་ཆོས་དེ་བཟུམ་མེད།"

#: 48
msgid "Inappropriate authentication"
msgstr "འོས་ལྡན་མིན་པའི་བདེན་བཤད།"

#: 4
msgid "Sizelimit exceeded"
msgstr "ཚད་ཀྱི་བཅད་མཚམས་ལྷག་ཡོདཔ།"

#: 3
msgid "Timelimit exceeded"
msgstr "ཆུ་ཚོད་ཀྱི་ཚད་ལྷག་ཡོདཔ།"

#: 14
msgid "SASL bind in progress"
msgstr "ཨེསི་ཨེ་ཨེསི་ཨེལ་ཡར་འཕེལ་ནང་བསྡམ་ཡོད།"

#: 2
msgid "Protocol error"
msgstr "གནད་སྤེལ་ལམ་ལུགས་ཀྱི་འཛོལ་བ།"

#: 13
msgid "Confidentiality required"
msgstr "གསང་བ་འབད་ནི་དགོས་མཁོ་ཡོདཔ།"

#: 1
msgid "Operations error"
msgstr "བཀོལ་སྤྱོད་ཀྱི་འཛོལ་བ།"

#: 12
msgid "Unavailable critical extension"
msgstr "ཚབས་ཆེན་རྒྱ་བསྐྱེད་འཐོབ་ཚུགསཔ་མེད།"

#: 11
msgid "Administrative limit exceeded"
msgstr "བདག་སྐྱོང་གི་ཚད་ལྷག་ཡོདཔ།"

#: 10
msgid "Referral received"
msgstr "འབྲེལ་སྟུན་འཐོབ་ཡོདཔ།"

#: 71
msgid "Affects multiple servers"
msgstr "སྣ་མང་སར་བར་ཚུ་གནོདཔ་ཨིན།"

#: 70
msgid "Results too large"
msgstr "གྲུབ་འབྲས་གནམ་མེད་ས་མེད་ཆེ་བ་ཨིན་པས།བ་"

#: 69
msgid "Cannot modify object class"
msgstr "དངོས་པོའི་དབྱེ་རིགས་ལེགས་བཅོས་འབད་མི་ཚུགས།"

#: 68
msgid "Already exists"
msgstr "ཧེ་མ་ལས་རང་ཡོད།"

#: 36
msgid "Alias dereferencing problem"
msgstr "གཞི་སྟུན་མི་འབད་ནིའི་དཀའ་ངལ་གྱི་མིང་གཞན།"

#: 35
msgid "Object is a leaf"
msgstr "དངོས་པོ་དེ་ལོ་འདབ་ཅིག་ཨིན།"

#: 67
msgid "Operation not allowed on RDN"
msgstr "ཨར་ཌི་ཨེན་གུ་ལུ་བཀོལ་སྤྱོད་འབད་མི་ཆོག་"

#: 34
msgid "Invalid DN syntax"
msgstr "ཌི་ཨེན་གྱི་ཚིག་སྦྱོར་ནུས་མེད།"

#: 66
msgid "Operation not allowed on nonleaf"
msgstr "ལོ་འདབ་མེན་མི་གུ་ལུ་བཀོལ་སྤྱོད་འབད་མི་ཆོག"

#: 97
msgid "Referral hop limit exceeded"
msgstr "འབྲེལ་སྟུན་ཧོཔ་གི་ཚད་ལྷག་ཡོདཔ།"

#: 33
msgid "Alias problem"
msgstr "དཀའ་ངལ་གྱི་མིང་གཞན།"

#: 65
msgid "Object class violation"
msgstr "དངོས་པོའི་དབྱེ་རིགས་འགལ་བ།"

#: 96
msgid "Client detected loop"
msgstr "ཞབས་ཏོག་སྤྱོད་མི་གིས་སྐྱོན་འཛིན་བྱུང་ཡོད་པའི་འཕྲལ་བཀོལ།"

#: 32
msgid "No such object"
msgstr "དེ་བཟུམ་མའི་དངོས་པོ་མེད།"

#: 64
msgid "Naming violation"
msgstr "མིང་བཏགས་ནི་འགལ་བ།"

#: 95
msgid "More results to return"
msgstr "སླར་ལོག་ནིའི་དོན་ལུ་གྲུབ་འབྲས་ཧེང་བཀལ།"

#: 94
msgid "No results returned"
msgstr "གྲུབ་འབྲས་ཚུ་སླར་ལོག་མ་འབད་བས།"

#: 93
msgid "Requested LDAP control not found"
msgstr "ཞུ་བ་འབད་མི་ཨེལ་ཌི་ཨེ་པི་གི་ཚད་འཛིན་འཚོལ་མ་འཐོབ།"

#: 61
msgid "Search results exceed the range specified by the offsets"
msgstr "འཚོལ་ཞིབ་གྲུབ་འབྲས་ཚུ་པར་ལེན་གྱིས་གསལ་བཀོད་འབད་མི་ཁྱབ་ཚད་ལས་ལྷག་ཡོད།"

#: 92
msgid "Not supported by this version of the LDAP protocol"
msgstr "ཨེལ་ཌི་ཨེ་པི་གནད་སྤེལ་ལམ་ལུགས་ཀྱི་ཐོན་རིམ་གྱིས་རྒྱབ་སྐྱོར་མ་འབད།"

#: 60
msgid "Sort Control is missing"
msgstr "དབྱེ་སེལ་གྱི་ཚད་འཛིན་དེ་བརླག་སྟོར་ཞུགས་ཡོདཔ།"

#: 91
msgid "Can't connect to the LDAP server"
msgstr "ཨེལ་ཌི་ཨེ་པི་སར་བར་ནང་ལུ་མཐུད་མ་ཚུགས།"

#: 90
msgid "Out of memory"
msgstr "དྲན་ཚད་ཀྱིས་བདའ་མ་ཟུན།"

#: authPromptTitle
msgid "LDAP Server Password Required"
msgstr "ཨེལ་ཌི་ཨེ་པི་སར་བར་གྱི་ཆོག་ཡིག་དགོ"

