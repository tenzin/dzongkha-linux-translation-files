#: changepassword.label
#: changepassword.accesskey
msgid "&Change Master Password"
msgstr "ཆོག་ཡིག་ཨམ་བསྒྱུར་བཅོས་འབད...(&C)"

#: changepassword.button
msgid "Change Password..."
msgstr "ཆོག་ཡིག་བསྒྱུར་བཅོས་འབད..."

#: changepassword.text
msgid "Your master password protects sensitive information such as web passwords and certificates."
msgstr "ཁྱོད་ཀྱི་ཆོག་ཡིག་ཨམ་གིས་ཚོར་ཅན་གྱི་བརྡ་དོན་ དཔེར་ན་ཝེབ་ཀྱི་ཆོག་ཡིག་ཚུ་དང་ལག་ཁྱེར་ཚུ་བཟུམ་ ཉེན་སྐྱོབ་འབདཝ་ཨིན།"

#: lheader
msgid "Master Passwords"
msgstr "ཆོག་ཡིག་ཨམ་ཚུ།"

#: managepassword.askeverytime
msgid "Every time it is needed"
msgstr "དུས་ཨ་རྟག་རང་དགོཔ་ཨིན།"

#: managepassword.askfirsttime
msgid "The first time it is needed"
msgstr "འགོ་དང་པ་རང་དགོཔ་ཨིན།"

#: managepassword.asktimeout
msgid "If it has not been used for "
msgstr "དོན་ལུ་དེ་ལག་ལེན་མ་འཐབ་ཨིན་པ་ཅིན།"

#: managepassword.label
msgid "Master Password Timeout"
msgstr "ཆོག་ཡིག་ཨམ་ངལ་མཚམས་ལུས་ཡོདཔ།"

#: managepassword.text
msgid "&brandShortName; will ask for your master password:"
msgstr "&brandShortName; གིས་ཁྱོད་ཀྱི་ཆོག་ཡིག་ཨམ་གྱི་སྐོར་འདྲི་འོང་།"

#: managepassword.timeout.unit
msgid "minutes or longer"
msgstr "སྐར་མ་ཚུ་ཡང་ན་སྦོམ་མི་ཚུ།"

#: resetpassword.label
#: resetpassword.accesskey
msgid "&Reset Master Password"
msgstr "ཆོག་ཡིག་ཨམ་སླར་སྒྲིག་འབད།(&R)"

#: resetpassword.button
msgid "Reset Password"
msgstr "ཆོག་ཡིག་སླར་སྒྲིག་འབད།"

#: resetpassword.text
msgid "If you reset your master password, all of your stored Web and e-mail passwords, form data, personal certificates, and private keys will be lost."
msgstr "ཁྱོད་ཀྱིས་ཁྱོད་རའི་ཆོག་ཡིག་ཨམ་སླར་སྒྲིག་འབདཝ་ཨིན་པ་ཅིན་ ཁྱོད་ཀྱི་གསོག་འཇོག་འབད་ཡོད་པའི་ཝེབ་དང་གློག་འཕྲིན་གྱི་ཆོག་ཡིག་ཚུ་ འབྲི་ཤོག་གནད་སྡུད་ རང་དོན་གྱི་ལག་ཁྱེར་ཚུ་ དང་སྒེར་དོན་གྱི་ལྡེ་མིག་ཚུ་བརླག་སྟོར་ཞུགས་འོང་།"

