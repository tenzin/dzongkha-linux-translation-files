#: warn.description
msgid "&brandShortName; can alert you to the security status of the web page you are viewing. Set &brandShortName; to show a warning and ask permission before:"
msgstr "&brandShortName; གིས་ཝེབ་ཤོག་ལེབ་ཁྱོད་ཀྱི་སྟོན་མི་དེ་གི་སྲུང་སྐྱོབ་གནས་ཚད་ནང་ཁྱོད་ལུ་དྲན་བརྡ་འབདཝ་ཨིན། ཧེ་མ་གནང་བ་འདྲི་ནི་དང་ཉེན་བརྡ་སྟོན་ནིའི་དོན་ལུ་&brandShortName; གཞི་སྒྲིག་འབད།"

#: warn.enteringsecure
msgid "Loading a page that supports encryption"
msgstr "གསང་བཟོ་རྒྱབ་སྐྱོར་འབད་མ་ཤོག་ལེབ་ཅིག་མངོན་གསལ་འབད་དོ།"

#: warn.enteringweak
msgid "Loading a page that uses low-grade encryption"
msgstr "དམའ་-རིམ་གི་གསང་བཟོ་ལག་ལེན་འཐབ་མི་ཤོག་ལེབ་ཅིག་མངོན་གསལ་འབད་དོ།"

#: warn.insecurepost
msgid "Sending form data from an unencrypted page to an unencrypted page"
msgstr "གསང་བཟོ་མ་འབད་བའི་ཤོག་ཅིག་ལས་གསང་བཟོ་མ་འབད་བའི་ཤོག་ཅིག་ལུ་འབྲི་ཤོག་གནད་སྡུད་གཏང་དོ།"

#: warn.leavingsecure
msgid "Leaving a page that supports encryption"
msgstr "གསང་བཟོ་རྒྱབ་སྐྱོར་འབད་མི་ཤོག་ལེབ་ཅིག་བཞག་དོ།"

#: warn.secureredirect
msgid "Redirecting from one encrypted page to another"
msgstr "གསང་བཟོ་འབད་འབད་བའི་ཤོག་གཅིག་ལས་གཞན་མི་ཅིག་ལུ་བསྐྱང་ལོག་གཏང་དོ།"

#: warn.secureredirecttoinsecure
msgid "Redirecting from an encrypted page to an unencrypted page"
msgstr "གསང་བཟོ་འབད་འབད་བའི་ཤོག་ལེབ་གཅིག་ལས་གསང་བཟོ་འབད་འབད་བའི་ཤོག་ལེབ་ཅིག་ལུ་བསྐྱར་ལོག་གཏང་དོ།"

#: warn.viewmixed
msgid "Viewing a page with an encrypted/unencrypted mix"
msgstr "གསང་བཟོ་འབད་འབདཝ་/གསང་བཟོ་མ་འབད་བའི་སྦྱོར་ནི་དང་ཅིག་ཁར་ཤོག་ལེབ་ཅིག་སྟོན་དོ།"

