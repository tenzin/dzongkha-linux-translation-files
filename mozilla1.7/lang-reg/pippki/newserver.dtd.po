#: newserver.examine
msgid "Examine Certificate..."
msgstr "ལག་ཁྱེར་བརྟག་དཔྱད་འབད་་་་་་"

#: newserver.notify
msgid "Please notify the site's webmaster about this problem."
msgstr "འ་ནི་དཀའ་ངལ་གྱི་སྐོར་ལས་ས་ཁོངས་ཀྱི་ཝེབ་འགོ་འཛིན་པ་ལུ་བརྡ་བསྐུལ་འབད་གནང་།"

#: newserver.reason1
msgid "- Your browser does not recognize the Certificate Authority that issued the site's certificate."
msgstr "ས་ཁོངས་ཀྱི་ལག་ཁྱེར་སྤྲོད་མི་ -ལག་ཁྱེར་དབང་འཛིན་དེ་ ཁྱོད་ཀྱི་བརའུ་་ཟར་གྱིས་ངོ་མི་ཤེས་པས།"

#: newserver.reason2
msgid "- The site's certificate is incomplete due to a server misconfiguration."
msgstr "སར་བར་རིམ་སྒྲིག་ཕྱི་འགྱུར་སྦེ་འབད་འབདཝ་ཅིག་གིས་སྦེ་ ས་ཁོངས་ཀྱི་ལག་ཁྱེར་དེ་མཇུག་མ་བསྡུ།"

#: newserver.reasons
msgid "Possible reasons for this error:"
msgstr "འ་ནི་འཛོལ་བ་འདི་གི་དོན་ལུ་སྲིད་པའི་རྒྱུ་མཚན་ཚུ:"

#: newserver.refuse
msgid "Do not accept this certificate and do not connect to this Web site"
msgstr "འ་ནི་ལག་ཁྱེར་འདི་དང་ལེན་མ་འབད་ དེ་ལས་འ་ནི་ཝེབ་ཀྱི་ས་ཁོངས་ལུ་མ་མཐུད།"

#: newserver.remember
msgid "Accept this certificate permanently"
msgstr "འ་ནི་ལག་ཁྱེར་འདི་རྟག་བརྟན་སྦེ་དང་ལེན་འབད།"

#: newserver.session
msgid "Accept this certificate temporarily for this session"
msgstr "འ་ནི་ལཱ་ཡུན་གྱི་དོན་ལུ་འ་ནི་ལག་ཁྱེར་འདི་གནས་སྐབས་ཀྱི་དོན་ལས་དང་ལེན་འབད།"

#: newserver.title
msgid "Website Certified by an Unknown Authority"
msgstr "ཝེབ་ཀྱི་ས་ཁོངས་འདི་མ་ཤེས་པའི་དབང་འཛིན་ཅིག་གིས་ངོ་སྦྱོར་འབད་ཡོད།"

