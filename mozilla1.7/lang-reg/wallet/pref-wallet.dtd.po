#: lHeader
msgid "Forms"
msgstr "འབརི་ཤོག་ཚུ།"

#: rHeader
msgid "Manage personal information"
msgstr "རང་དོན་གྱི་བརྡ་དོན་འཛིན་སྐྱོང་འཐབ།"

#: viewWallet.label
#: viewWallet.accesskey
msgid "&Manage Stored Form Data"
msgstr "གསོག་འཇོག་འབད་ཡོད་པའི་འབྲི་ཤོག་གནད་སྡུད་འཛིན་སྐྱོང་འཐབ།(&M)"

#: viewWalletSites.label
#: viewWalletSites.accesskey
msgid "Manage &Sites"
msgstr "ས་ཁོངས་ཚུ་འཛིན་སྐྱོང་འཐབ།(&S)"

#: walletDescription.label
msgid "Form Manager can remember information about you so that filling out forms on web pages is faster."
msgstr "འབྲི་ཤོག་འཛིན་སྐྱོང་པ་གིས་ཁྱོད་ཀྱི་སྐོར་ལས་བརྡ་དོན་སེམས་ཁར་བཞག་ཚུགས། དེ་འབདཝ་ལས་ཝེབ་ཤོག་ལེབ་གུ་ལུ་འབྲི་ཤོག་བཀང་ནི་དེ་མགྱོགས་སུ་འབད་ཚུགས།"

#: walletEnabled.label
#: walletEnabled.accesskey
msgid "Save &form data from web pages when completing forms."
msgstr "འབྲི་ཤོག་ཚུ་མཇུག་བསྡུ་ཚརཝ་ད་ཝེབ་ཤོག་ལེབ་ཚུ་ལས་འབྲི་ཤོག་གནད་སྡུད་སྲུང་བཞག་འབད།(&f)"

#: walletHeader.label
msgid "Form Manager"
msgstr "འབྲི་ཤོག་འཛིན་སྐྱོང་པ།"

#: walletServer.label
#: walletServer.accesskey
msgid "&Server for Form Manager tables:"
msgstr "འབྲི་ཤོག་འཛིན་སྐྱོང་པའི་ཐིག་ཁྲམ་ཚུའི་དོན་ལུ་སར་བར:(&S)"

