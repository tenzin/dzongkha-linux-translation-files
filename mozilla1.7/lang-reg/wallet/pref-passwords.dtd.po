#: encryptDescription.label
msgid "Sensitive data that is stored on your hard disk can be encrypted to prevent the data from being read by an intruder.  A password is used to access the data."
msgstr "ཁྱོད་ཀྱི་ཧརཌི་ཌིཀིསི་ནང་ལུ་གསོག་འཇོག་འབད་ཡོད་པའི་ཚོར་ཅན་གནད་སྡུད་དེ་ གནད་སྡུད་ལྷག་མི་གཞན་མི་ལས་སྔོན་བཀག་འབད་ནིའི་དོན་ལུ་གསང་བཟོ་འབད་ཚུགས། གནད་སྡུད་འཛུལ་སྤྱོད་འབད་ནིའི་དོན་ལས་ཆོག་ཡིག་ཅིག་ལག་ལེན་འཐབ་ཡོད།"

#: encryptEnabled.label
#: encryptEnabled.accesskey
msgid "Use e&ncryption when storing sensitive data."
msgstr "ཚོར་ཅན་གནད་སྡུད་གསོག་འཇོག་འབད་བའི་སྐབས་གསང་བཟོ་འབད་ནི་ལག་ལེན་འཐབ།(&n)"

#: encryptExpire.label
#: encryptExpire.accesskey
msgid "E&xpire master password after each use."
msgstr "ལག་ལེན་རེའི་ཤུལ་ལས་ཆོག་ཡིག་ཨམ་དེ་དུས་ཡོལ་གཏང་།(&x)"

#: encryptHeader.label
msgid "Encrypting versus Obscuring"
msgstr "གསང་བ་བཟོ་ནི་དང་མི་མངོན་པའི་བར་གྱི་འགྲན་བསྡུར།"

#: lHeader
msgid "Passwords"
msgstr "ཆོག་ཡིག་ཚུ།"

#: rHeader
msgid "Manage passwords"
msgstr "ཆོག་ཡིག་ཚུ་འཛིན་སྐྱོང་འཐབ།"

#: signonDescription.label
msgid "Password Manager stores your login information for password-protected web sites, mail servers, and news servers, and enters the information automatically when needed."
msgstr "ཆོག་ཡིག་འཛིན་སྐྱོང་པ་གིས་ ཆོག་ཡིག་གིས་ཉེན་སྐྱོབ་འབད་འབདཝ་ཝེབ་ས་ཁོངས་ཚུ་ ཡིག་འཕྲིན་སར་བར་ཚུ་ དང་གནས་ཚུལ་སར་བར་ཚུའི་དོན་ལུ་ཁྱོད་རའི་ནང་བསྐྱོད་བརྡ་དོན་གསོག་འཇོག་འབདཝ་ཨིན། དེ་ལས་བརྡ་དོན་དེ་དགོ་པའི་སྐབས་རང་བཞིན་གྱིས་ཐོ་བཀོད་འབདཝ་ཨིན།"

#: signonEnabled.label
#: signonEnabled.accesskey
msgid "&Remember passwords"
msgstr "ཆོག་ཡིག་ཚུ་སེམས་ཁར་བཞག(&R)"

#: signonHeader.label
msgid "Password Manager"
msgstr "ཆོག་ཡིག་འཛིན་སྐྱོང་པ།"

#: viewSignons.label
#: viewSignons.accesskey
msgid "Manag&e Stored Passwords"
msgstr "གསོག་འཇོག་འབད་ཡོད་པའི་ཆོག་ཡིག་ཚུ་འཛིན་སྐྱོང་འཐབ།(&e)"

