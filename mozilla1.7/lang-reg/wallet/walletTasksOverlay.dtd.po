#: PasswordExpired
msgid "You are now logged out."
msgstr "ཁྱོད་ད་ཕྱིར་བསྐྱོད་འབད་ཡོད།"

#: PasswordNotChanged
msgid "Unable to change your master password."
msgstr "ཁྱོད་ཀྱི་ཆོག་ཡིག་ཨམ་བསྒྱུར་བཅོས་འབད་མ་ཚུགས།"

#: PasswordNotExpired
msgid "Unable to log out."
msgstr "ཕྱིར་བསྐྱོད་འབད་མ་ཚུགས།"

#: walletChangePasswordCmd.label
#: walletChangePasswordCmd.accesskey
msgid "&Change Master Password..."
msgstr "ཆོག་ཡིག་ཨམ་བསྐྱུར་བཅོས་འབད...(&C)"

#: walletClearCmd.label
#: walletClearCmd.accesskey
msgid "&Reset Master Password"
msgstr "ཆོག་ཡིག་ཨམ་སླར་སྒྲིག་འབད།(&R)"

#: walletDisplaySignonsCmd.label
#: walletDisplaySignonsCmd.accesskey
msgid "&Manage Stored Passwords"
msgstr "གསོག་འཇོག་འབད་ཡོད་པའི་ཆོག་ཡིག་ཚུ་འཛིན་སྐྱོང་འཐབ།(&M)"

#: walletEncryptCmd.label
#: walletEncryptCmd.accesskey
msgid "&Encrypt Sensitive Information"
msgstr "ཚོར་ཅན་གྱི་བརྡ་དོན་གསང་བཟོ་འབད།(&E)"

#: walletExpirePasswordCmd.label
#: walletExpirePasswordCmd.accesskey
msgid "&Log Out"
msgstr "ཕྱིར་བསྐྱོད།(&L)"

#: walletInterviewCmd.label
#: walletInterviewCmd.accesskey
msgid "&Interview"
msgstr "དྲི་བ་དྲི་ལན།(&I)"

#: walletObscureCmd.label
#: walletObscureCmd.accesskey
msgid "&Obscure Sensitive Information"
msgstr "ཚོར་ཅན་བརྡ་དོན་མི་མངོན་པ་བཟོ།(&O)"

#: walletPasswordManager.label
#: walletPasswordManager.accesskey
msgid "&Password Manager"
msgstr "ཆོག་ཡིག་འཛིན་སྐྱོང་པ།(&P)"

#: walletQuickFillCmd.label
#: walletQuickFillCmd.accesskey
msgid "Prefill Form &Quickly"
msgstr "འཕྲལ་མགྱོགས་སྦེ་འབྲི་ཤོག་སྔོན་བཀང་འབད།(&Q)"

#: walletRequestToCaptureCmd.label
#: walletRequestToCaptureCmd.accesskey
msgid "&Save Form Data"
msgstr "འབྲི་ཤོག་གནད་སྡུད་སྲུང་བཞག་འབད།(&S)"

#: walletSafeFillCmd.label
#: walletSafeFillCmd.accesskey
msgid "Prefill &Form"
msgstr "འབྲི་ཤོག་སྔོན་བཀང་འབད།(&F)"

#: walletSamplesCmd.label
#: walletSamplesCmd.accesskey
msgid "&Demonstration"
msgstr "འགྲེམ་སྟོན།(&D)"

