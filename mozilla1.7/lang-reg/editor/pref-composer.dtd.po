msgid "Retain original source formatting"
msgstr "རྩ་སྒྲིག་བཟོ་ནིའི་འབྱུང་ཁུངས་ངོ་མ་ནང་བཞག"

#: preserveExistingFormatting.accesskey
msgid "o"
msgstr "o"

#: preserveExistingFormatting.tooltip
msgid "Preserves line breaks and page's original formatting"
msgstr "གྲལ་ཐིག་གི་རྒྱུན་ཆད་དང་ཤོག་ལེབ་ཀྱི་རྩ་སྒྲིག་བཟོ་ནི་ངོ་མ་ཉམས་སྲུང་འབདཝ་ཨིན།"

#: rHeader
msgid "Settings for authoring of new Web pages"
msgstr "རྩོམ་པ་པོའི་ཝེབ་ཀྱི་ཤོག་ལེབ་ཚུའི་དོན་ལས་སྒྲིག་སྟངས"

#: recentFiles.title
msgid "Recent Pages Menu"
msgstr "འཕྲལ་གྱི་ཤོག་ལེབ་ཚུའི་དཀར་ཆག"

#: reformat.label
#: reformat.accesskey
msgid "&Reformat HTML source"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ གྱི་འབྱུང་ཁུངས་ལོག་རྩ་སྒྲིག་བཟོ།(&R)"

#: reformat.tooltip
msgid "Reformat HTML tags using line breaks and indentation"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ གྱི་ངོ་རྟགས་ཚུ་གྲལ་མཐུད་ཀྱི་རྒྱུན་ཆད་ཚུ་དང་ངོས་འཛིན་འབད་ཡོད་མི་ཚུ་ལག་ལེན་འཐབ་ཐོག་རྩ་སྒྲིག་ལོག་བཟོ།"

#: saveAssociatedFiles.label
msgid "Save images and other associated files when saving pages"
msgstr "གཟུགས་བརྙན་དང་གཞན་མི་ཡིག་སྣོད་ཚུ་དང་འབྲེལ་བ་ཡོད་མི་ཚུ་ཤོག་ལེབ་ཚུ་སྲུང་བཞག་འབད་བའི་སྐབས་སྲུང་བཞག་འབད།"

#: saving
