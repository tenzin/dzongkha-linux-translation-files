#: browseUrl.label
msgid "HTTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr ""
"ཁྱོད་རའི་ཁྱིམ་ཤོག་ལེབ་ཀྱི་ཨེཆ་ཊི་ཊི་པི་ཁ་བྱང་ (དཔེར་ན: "
"'http:\\www.myisp.com/myusername'):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr "ཁྱོད་རའི་ཁྱིམ་གྱི་སྣོད་ཐོའི་ཨེཆ་ཊི་ཊི་པི་:\\ ཁ་བྱང་(ཡིག་སྣོད་མིང་གྲངས་སུ་མ་བཙུགས)"

#: chooseDirButton.label
msgid "Choose..."
msgstr "གདམ་ཁ་རྐྱབས..."

#: docDirList.label
msgid "Site subdirectory for this page:"
msgstr "ཤོག་ལེབ་འདིའི་དོན་ལུ་ས་ཁོངས་ཀྱི་ཡན་ལག་ཁ་བྱང་ཐོ་དེབ:"

#: filename.label
msgid "Filename:"
msgstr "ཡིག་སྣོད་མིང་:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr "ཝེབ་ཤོག་ལེབ་ཀྱི་དོན་ལུ་ '.html' གྲངས་སུ་བཙུགས་ཏེ་ ཡིག་སྣོད་འདིའི་དོན་ལུ་མིང་གཅིག་ཐོ་བཀོད་འབད།"

#: filenameExample.label
msgid "e.g,: &quot;mypage.html&quot;"
msgstr "དཔེར་ན: &quot;mypage.html&quot;"

#: loginInfo.label
msgid "Login Information"
msgstr "བརྡ་དོན་ནང་བསྐྱོད་འབད།"

#: newSiteButton.label
msgid "New Site"
msgstr "ས་ཁོངས་གསརཔ"

#: pageTitle.label
msgid "Page Title:"
msgstr "ཤོག་ལེབ་ཀྱི་མགོ་མིང་:"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr "དེབ་རྟགས་ཚུ་ནང་དང་སྒོ་སྒྲིག་ནང་གི་ཤོག་ལེབ་ངོས་འཛིན་འབད་ནིའི་དོན་ལས་མགོ་མིང་གཅིག་ཐོ་བཀོད་འབད།"

#: pageTitleExample.label
msgid "e.g.: &quot;My Web Page&quot;"
msgstr "དཔེར་ན: &quot;My Web Page&quot;"

#: password.label
msgid "Password:"
msgstr "ཆོག་ཡིག:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "ཁྱོད་རའི་ལག་ལེན་པའི་མིང་དང་འབྲེལ་བའི་ཆོག་ཡིག"

#: publishImgCheckbox.label
msgid "Include images and other files"
msgstr "གཟུགས་བརྙན་ཚུ་དང་གཞན་མི་ཡིག་སྣོད་ཚུ་གྲངས་སུ་བཙུགས།"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr "ཤོག་ལེབ་འདི་གིས་གཞི་བསྟུན་འབད་མི་གཟུགས་བརྙན་ཚུ་དང་གཞན་མི་ཡིག་སྣོད་ཚུ་དཔེ་བསྐྲུན་འབད།"

#: publishSites.label
msgid "Publishing Sites"
msgstr "དེབ་བསྐྲུན་འབད་ནིའི་ས་ཁོངས་"

#: publishTab.label
msgid "Publish"
msgstr "དཔེ་བསྐྲུན་འབད"

#: relatedDocs.label
msgid "Other files to publish"
msgstr "གཞན་མི་ཡིག་སྣོད་ཚུ་དཔེ་བསྐྲུན་འབད་ནིའི་དོན་ལས"

#: removeButton.label
msgid "Remove Site"
msgstr "ས་ཁོངས་རྩ་བསྐྲད་གཏང་"

#: sameLocationRadio.label
msgid "Use same location as page"
msgstr "ཤོག་ལེབ་བཟུམ་སྦེ་གཅིག་པའི་གནས་ཁོངས་ལག་ལེན་འཐབ།"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "ཡིག་སྣོད་ཚུ་ཤོག་ལེབ་བཟུམ་སྦེ་གཅིག་པའི་གནས་ཁོངས་ལུ་དཔེ་བསྐྲུན་འབད།"

#: savePassword.label
msgid "Save Password"
msgstr "ཆོག་ཡིག་སྲུང་བཞག་འབད།"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr "ཁྱོད་རའི་ཆོག་ཡིག་སྲུང་བཞག་འབད་ནིའི་དོན་ལས་ཆོག་ཡིག་འཛིན་སྐྱོང་པ་མཐའ་བཙན་སྦེ་ལག་ལེན་འཐབ་ཐོག་ འ་ནི་སེལ་འཐུ་འབད།"

#: serverInfo.label
msgid "Server Information"
msgstr "སར་བར་གྱི་བརྡ་དོན"

#: setDefSiteAndDir.label
msgid "Set as Default Site and Directory"
msgstr "སྔོན་སྒྲིག་ས་ཁོངས་དང་སྣོད་ཐོ་སྦེ་གཞི་སྒྲིག་འབད།"

#: setDefaultButton.label
msgid "Set as Default"
msgstr "སྔོན་སྒྲིག་སྦེ་གཞི་སྒྲིག་འབད།"

#: settingsTab.label
msgid "Settings"
msgstr "སྒྲིག་སྟངས"

#: siteDirList.label
msgid "Select or type a directory name in your publishing site:"
msgstr "ཁྱོད་རའི་དེབ་བསྐྲུན་གྱི་ས་ཁོངས་ནང་སྣོད་ཐོའི་མིང་གཅིག་སེལ་འཐུ་ཡང་ན་ཡིག་དཔར་རྐྱབས:"

#: siteDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory to publish to"
msgstr "ཐག་རིང་གི་ཡན་ལག་ཁ་བྱང་ཐོ་དེབ་ཀྱི་མིང་ འདི་ལུ་དཔེ་བསྐྲུན་འབད་ནིའི་དོན་ལུ་ གདམ་ཁ་ཡང་ན་ཐོ་བཀོད་འབད།"

#: siteList.label
msgid ""
"_: siteList.label\n"
"Site Name:"
msgstr "ས་ཁོངས་མིང་:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "ས་ཁོངས་ཁྱོད་ཀྱི་དཔེ་བསྐྲུན་འབད་དགོ་མནོ་མི་དེ་གདམ་ཁ་རྐྱབས།"

#: siteName.label
msgid ""
"_: siteName.label\n"
"Site Name:"
msgstr "ས་ཁོངས་མིང་:"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr "དཔེ་བསྐྲུན་འབད་ནིའི་ས་ཁོངས་ངོས་འཛིན་འབད་མི་སྐྱོན་མིང་(དཔེར་ན: 'MySite')"

#: siteUrl.label
msgid "Publishing address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr "དཔེ་བསྐྲུན་འབད་ནིའི་ཁ་བྱང་(དཔེར་ན: 'ftp://ftp.myisp.com/myusername'):"

#: siteUrl.tooltip
msgid "The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr "ཨེཕ་ཊི་པི:// ཡང་ན་ ཨེཆ་ཊི་ཊི་པི་:// གི་ཁ་བྱང་ཁྱོད་རའི་ ཨའི་ཨེསི་པི་ ཡང་ན་ཝེབ་ཧོསི་ཊིང་ ཞབས་ཏོག་གིས་བྱིན་ཏེ་ཡོད།"

#: useSubdir.tooltip
msgid "Choose or enter name of remote subdirectory where files will be published"
msgstr "ཐག་རིང་གི་ཡན་ལག་ཁ་བྱང་ཐོ་དེབ་ཀྱི་མིང་ ཡིག་སྣོད་ཚུ་ག་སྟེ་དཔེ་བསྐྲུན་འབད་ནི་ཨིན་ན་ གདམ་ཁ་ཡང་ན་ཐོ་བཀོད་འབད།"

#: useSubdirRadio.label
msgid "Use this site subdirectory:"
msgstr "འ་ནི་ས་ཁོངས་ཀྱི་ཡན་ལག་ཁ་བྱང་ཐོ་དེབ་ལག་ལེན་འཐབ:"

#: username.label
msgid "User name:"
msgstr "ལག་ལེན་པའི་མིང་:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr "ལག་ལེན་པའི་མིང་ཁྱོད་ཀྱིས་ཁྱོད་རའི་ ཨའི་ཨེསི་པི་ ཡང་ན་ཝེབ་ཧོསི་ཊིང་ཞབས་ཏོག་ནང་ ལོག་འབད་མི།"

#: windowTitle.label
msgid "Publish Page"
msgstr "ཤོག་ལེབ་དཔེ་བསྐྲུན་འབད།"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "སྒྲིག་སྟངས་དཔེ་བསྐྲུན་འབད།"

