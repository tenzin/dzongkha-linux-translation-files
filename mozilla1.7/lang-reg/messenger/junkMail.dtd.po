#: account.label
#: account.accesskey
msgid "&Account:"
msgstr "རྩིས་ཐོ:(&A)"

#: info1.label
msgid "Junk mail controls evaluate your incoming messages and identify those that are most likely to be junk mail, or unsolicited mail.  A junk icon is displayed if the message is identified as junk mail."
msgstr "མཁོ་མེད་ཚད་འཛིན་གྱིས་ཁྱོད་ཀྱི་ནང་འབྱོར་གྱི་འཕྲིན་དོན་ཚུ་བརྟག་ཞིབ་འབདཝ་ཨིན་ དེ་ལས་མཁོ་མེད་ཡིག་འཕྲིན་ཨིན་ནི་སྦེ་མི་ ཡང་ན་ཞུ་བསྐུལ་མ་འབད་བའི་འཕྲིན་དོན་དེ་ཚུ་ངོས་འཛིན་འབདཝ་ཨིན། ག་དེམ་ཅི་སྦེ་ འཕྲིན་དོན་དེ་མཁོ་མེད་ཡིག་འཕྲིན་སྦེ་ངོས་འཛིན་འབད་ཡོདཔ་ཨིན་པ་ཅིན་ མཁོ་མེད་ངོས་དཔར་དེ་བཀྲམ་སྟོན་འབདཝ་ཨིན།"

#: info2.label
msgid "Junk mail controls can be fine-tuned by using the Junk Mail toolbar button to mark junk messages appropriately."
msgstr "མཁོ་མེད་ཡིག་འཕྲིན་ཚད་འཛིན་དེ་ མཁོ་མེད་འཕྲིན་དོན་ཚུ་འོས་ལྡན་སྦེ་རྟགས་བཀལ་ནིའི་དོན་ལུ་མཁོ་མེད་ཡིག་འཕྲིན་གྱི་ལག་ཆས་ཕྲ་རིང་གི་གཡེབ་རྟ་ལག་ལེན་འཐབ་ཐོག་ སྒྲ་གདངས་ལེགས་ཤོམ་བཟོ་ཚུགས།"

#: junkFolderOn.label
msgid "&quot;Junk&quot; folder on:"
msgstr "གུ་&quot;junk&quot; སྣོད་འཛིན:"

#: level.label
#: level.accesskey
msgid "&Enable junk mail controls"
msgstr "མཁོ་མེད་ཡིག་འཕྲིན་གྱི་ཚད་འཛིན་ཚུ་ལྕོགས་ཅན་བཟོ།(&E)"

#: manualMark.label
msgid "When I manually mark messages as Junk:"
msgstr "ང་གིས་ལག་ཐོག་ལས་འཕྲིན་དོན་ཚུ་མཁོ་མེད་སྦེ་རྟགས་བཀལ་བའི་སྐབས:"

#: manualMarkModeDelete.label
msgid "Delete them"
msgstr "དེ་ཚུ་བཏོན་གཏང་།"

#: manualMarkModeMove.label
msgid "Move them to the &quot;Junk&quot; folder"
msgstr "དེ་ཚུ་&quot;junk&quot; སྣོད་འཛིན་ནང་ལུ་བཤུད།"

#: move.label
msgid "Move incoming messages determined to be junk mail to:"
msgstr "ལུ་ ནང་འབྱོར་གྱི་འཕྲིན་དོན་ཚུ་མཁོ་མེད་ཡིག་འཕྲིན་སྦེ་གཏན་འབེབས་བཟོ་ཡོད་མི་ཚུ་ བཤུད:"

#: otherFolder.label
msgid "Other:"
msgstr "གཞན་མི:"

#: purge1.label
#: purge1.accesskey
msgid "A&utomatically delete junk messages older than"
msgstr "ལས་རྒས་མི་མཁོ་མེད་ཡིག་འཕྲིན་ཚུ་རང་བཞིན་གྱིས་བཏོན་གཏང་།(&u)"

#: purge2.label
msgid "days from this folder"
msgstr "འ་ནི་སྣོད་འཛིན་ལས་ཉིན་གྲངས"

#: viewLog.label
#: viewLog.accesskey
msgid "&Junk Mail Log"
msgstr "མཁོ་མེད་ཡིག་འཕྲིན་གྱི་དྲན་དེབ།(&J)"

#: whitelist.label
#: whitelist.accesskey
msgid "&Do not mark messages as junk mail if the sender is in my address book:"
msgstr "གཏང་མི་འདི་ངེས་ཀྱི་ཁ་བྱང་ཀི་དེབ་ནང་ལུ་ཡོདཔ་ཨིན་པ་ཅིན་ འཕྲིན་དོན་དེ་མཁོ་མེད་ཡིག་འཕྲིན་སྦེ་རྟགས་མ་བཀལ:(&D)"

#: window.title
msgid "Junk Mail Controls"
msgstr "མཁོ་མེད་ཡིག་འཕྲིན་གྱི་ཚད་འཛིན་ཚུ།"

