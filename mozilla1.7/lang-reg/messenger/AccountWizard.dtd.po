#: usernameDesc.label
msgid "Enter the incoming user name given to you by your email provider "
msgstr "ཁྱོད་ལུ་ཁྱོད་རའི་གློག་འཕྲིན་བྱིན་མཁན་གྱིས་བྱིན་མི་ནང་འབྱོར་ལག་ལེན་པའི་མིང་དེ་ཐོ་བཀོད་འབད།"

#: usernameExample.label
msgid "(for example, &quot;jsmith&quot;)."
msgstr "(དཔེར་ན་ &quot;jsmith&quot;)."

#: usernameLabel.label
#: usernameLabel.accesskey
msgid "&Incoming User Name:"
msgstr "ནང་འབྱོར་གྱི་ལག་ལེན་པའི་མིང་:(&I)"

#: usernameSmtpDesc.label
msgid "Enter the outgoing user name given to you by your email provider (this is typically the same as your incoming user name)."
msgstr "ཁྱོད་ལུ་ཁྱོད་རའི་གློག་འཕྲིན་བྱིན་མཁན་གྱིས་བྱིན་མི་ཕྱིར་གཏང་ལག་ལེན་པའི་མིང་དེ་ཐོ་བཀོད་འབད།(འ་ནི་དེ་འཁྲུལ་བ་མེད་པར་ཁྱོད་རའི་ནང་འབྱོར་ལག་ལེན་པའི་མིང་བཟུམ་སྦེ་ཨིན།)"

#: usernameSmtpLabel.label
#: usernameSmtpLabel.accesskey
msgid "&Outgoing User Name:"
msgstr "ཕྱིར་གཏང་ལག་ལེན་པའི་མིང་:(&O)"

