#: accountName.label
#: accountName.accesskey
msgid "Account &Name:"
msgstr "རྩིས་ཐོའི་མིང་:(&N)"

#: accountNameDesc.label
msgid "The account name is used to identify each account"
msgstr "རྩིས་ཐོའི་མིང་དེ་རྩིས་ཐོ་རེ་རེ་བཞིན་ངོས་འཛིནའབད་ནི་ལུ་ལག་ལེན་འཐབ་ཨིན།"

#: accountSettingsDesc.label
msgid "The following information is needed before you can send messages. If you do not know the information requested, please contact your system administrator or Internet Service Provider."
msgstr "འོག་གི་བརྡ་དོན་དེ་ཁྱོད་ཀྱིས་འཕྲིན་དོན་ཚུ་མ་གཏང་པའི་ཧེ་མ་དགོཔ་ཨིན། ཁྱོད་ཀྱིས་བརྡ་དོན་ཞུ་བ་འབད་ཡོད་མི་མ་ཤེསཔ་ཨིན་པ་ཅིན་ ཁྱོད་རའི་རིམ་ལུགས་བདག་སྐྱོང་པ་ཡང་ན་ཨིན་ཊར་ནེཊི་གི་ཞབས་ཏོག་སྤེལ་མི་དང་འབྲེལ་བ་འཐབ་གནང་།"

#: accountTitle.label
msgid "Account Settings"
msgstr "རྩིས་ཐོའི་སྒྲིག་སྟངས"

#: attachVCard.label
#: attachVCard.accesskey
msgid "Attach my &vCard to messages"
msgstr "ངེས་ཀྱི་ཝི་ཤོག་བྱང་འཕྲིན་དོན་ཚུ་ལུ་མཉམ་སྦྲགས་འབད།(&v)"

#: choose.label
#: choose.accesskey
msgid "&Choose..."
msgstr "གདམ་ཁ་རྐྱབས...(&C)"

#: edit.label
msgid "Edit..."
msgstr "ཞུན་དག་འབད..."

#: editVCard.label
#: editVCard.accesskey
msgid "E&dit Card..."
msgstr "ཤོག་བྱང་ཞུན་དག་འབད...(&d)"

#: email.label
#: email.accesskey
msgid "&Email Address:"
msgstr "གློག་འཕྲིན་ཁ་བྱང་:(&E)"

#: identityDesc.label
msgid "Each account has an identity, which is the information that other people see when they read your messages."
msgstr "རྩིས་ཐོ་རེ་ལུ་ གཞན་མི་མི་མང་གིས་ཁྱོད་ཀྱི་འཕྲིན་དོན་ཚུ་ལྷག་པའི་སྐབས་མཐོང་བའི་བརྡ་དོན་གྱི་ངོ་རྟགས་ཅིག་ཡོད།"

#: identityTitle.label
msgid "Identity"
msgstr "ངོ་རྟགས།"

#: name.label
#: name.accesskey
msgid "&Your Name:"
msgstr "ཁྱོད་རའི་མིང་:(&Y)"

#: organization.label
#: organization.accesskey
msgid "&Organization:"
msgstr "ཚོགས་སྡེ:(&O)"

#: replyTo.label
#: replyTo.accesskey
msgid "Reply-to Addre&ss:"
msgstr "ཁ་བྱང་ལུ་-ལན་གསལ་འབད:(&s)"

#: signature.label
#: signature.accesskey
msgid "A&ttach this signature:"
msgstr "འ་ནི་མིང་རྟགས་འདི་མཉམ་སྦྲགས་འབད:(&t)"

