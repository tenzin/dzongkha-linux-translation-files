msgstr "r"

#: encryption.certificate_clear.button
msgid ""
"_: encryption.certificate_clear.button\n"
"Clear"
msgstr "བསལ།"

#: encryptionCert.message
msgid "Use this certificate to encrypt &amp; decrypt messages sent to you:"
msgstr "འ་ནི་ལག་འཁྱེར་འདི་ཁྱོད་ལུ་གཏང་བའི་འཕྲིན་དོན་ཚུ་གསང་བཟོ་འབད་ནི་དང་ &amp; གསང་བཤོལ་འབད་ནིའི་དོན་ལུ་ལག་ལེན་འཐབ:"

#: encryptionCert.notselected
msgid ""
"_: encryptionCert.notselected\n"
"No certificate set"
msgstr "ལག་འཁྱེར་གྱི་ཆ་ཚན་མེད།"

#: encryptionChoiceLabel.label
msgid "Default encryption setting when sending messages:"
msgstr "འཕྲིན་དོན་ཚུ་གཏང་བའི་སྐབས་ སྔོན་སྒྲིག་གསང་བཟོའི་སྒྲིག་སྟངས:"

#: encryptionGroupTitle.label
msgid "Encryption"
msgstr "གསང་བཟོ།"

#: neverEncrypt.label
#: neverEncrypt.accesskey
msgid "&Never (do not use encryption)"
msgstr "ནམ་ཡང་(གསང་བཟོ་ལག་ལེན་མ་འཐབ)(&N)"

#: securityHeading.label
msgid "To send and receive signed or encrypted messages, you should specify both a digital signing certificate and an encryption certificate."
msgstr "མིང་རྟགས་བཀོད་བཀོདཔ་ཡང་ན་གསང་བཟོ་འབད་འབད་བའི་འཕྲིན་དོན་ཚུ་གཏང་ནི་དང་ལེན་འབད་ནིའི་དོན་ལུ་ ཁྱོད་ཀྱིས་ཌི་ཇི་ཊཱལ་མིང་རྟགས་བཀོད་པའི་ལག་འཁྱེར་དང་གསང་བཟོའི་ལག་འཁྱེར་གཉིས་ཆ་ར་གསལ་བཀོད་འབད་དགོ"

#: securityTitle.label
msgid "Security"
msgstr "མཐའ་བཙན།"

#: signMessage.label
#: signMessage.accesskey
msgid "&Digitally sign messages (by default)"
msgstr "ཌི་ཇི་ཊཱལ་གྱི་ཐོག་ལས་འཕྲིན་དོན་ཚུ་མིང་རྟགས་བཀོད།(སྔོན་སྒྲིག་གི་སྦེ)(&D)"

#: signingCert.message
msgid "Use this certificate to digitally sign messages you send:"
msgstr "འ་ནི་ལག་འཁྱེར་འདི་ཌི་ཇི་ཊཱལ་གྱི་ཐོག་ལས་ཁྱོད་ཀྱིས་གཏང་མི་འཕྲིན་དོན་ཚུ་མིང་རྟགས་བཀོད་ནིའི་དོན་ལུ་ལག་ལེན་འཐབ:"

#: signingCert.notselected
msgid ""
"_: signingCert.notselected\n"
"No certificate set"
msgstr "ལག་འཁྱེར་གྱི་ཚན་མེད།"

#: signingGroupTitle.label
msgid "Digital Signing"
msgstr "ཌི་ཇི་ཊཱལ་མིང་རྟགས་བཀོད་དོ།"

