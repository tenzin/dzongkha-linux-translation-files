#: 5054
msgid "Connection to server %S timed out."
msgstr "སར་བར་%S ལུའི་མཐུད་ལམ་དེ་ངལ་མཚམས་ལུས་སོང་ནུག"

#: 5053
msgid "Could not connect to mail server %S; the connection was refused."
msgstr "ཡིག་འཕྲིན་གྱི་སར་བར་%S ལུ་མཐུད་མ་ཚུགས། མཐུད་ལམ་དེ་ ངོས་ལེན་འབད་མ་བཏུབ།"

#: 5052
msgid "Failed to connect to server %S."
msgstr "སར་བར་%S ལུ་མཐུད་ནི་དེ་འཐུས་ཤོར་བྱུང་ཡོད།"

#: 5051
msgid "Mail Server Password Required"
msgstr "ཡིག་འཕྲིན་སར་བར་གྱི་ཆོག་ཡིག་དགོ"

#: 5049
msgid "The current command did not succeed.  The mail server responded\\:"
msgstr "ད་ལྟོའི་བརྡ་བཀོད་དེ་མཐར་འཁྱོལ་མ་འབྱུང་། ཡིག་འཕྲིན་སར་བར་གྱིས་ལན་བཀོད་འབད་ཡོད\\:"

#: 5048
msgid "Mail server %S is not an IMAP4 mail server."
msgstr "ཡིག་འཕྲིན་སར་བར་%S འདི་ ཨའི་ཨེམ་ཨེ་པི་༤ ཡིག་འཕྲིན་སར་བར་མེན།"

#: 5047
msgid "Enter your password for %S\\:"
msgstr "%S གི་དོན་ལུ་ཁྱོད་རའི་ཆོག་ཡིག་ཐོ་བཀོད་འབད\\:"

#: 5046
msgid "Found folder\\: %S"
msgstr "ཐོབ་པའི་སྣོད་འཛིན\\: %S"

#: 5045
msgid "%S - Receiving\\: message %lu of %lu"
msgstr "%S - ཐོབ་དོ\\: འཕྲིན་དོན་ %lu %lu ལས།"

#: 5044
msgid "Opening folder %S..."
msgstr "སྣོད་འཛིན་ %S ཁ་ཕྱེ་དོ..."

#: 5043
msgid "Copying message to %S..."
msgstr "%S ནང་ལུ་འཕྲིན་དོན་འདྲ་བཤུས་རྐྱབས་དོ..."

#: 5042
msgid "Copying messages to %S..."
msgstr "%S ནང་ལུ་འཕྲིན་དོན་ཚུ་འདྲ་བཤུས་རྐྱབས་དོ..."

#: 5041
msgid "Moving message to %S..."
msgstr "%S ནང་ལུ་འཕྲིན་དོན་བཤུད་དོ..."

#: 5040
msgid "Moving messages to %S..."
msgstr "%S ནང་ལུ་འཕྲིན་དོན་ཚུ་བཤུད་དོ..."

#: 5039
msgid "Deleting message..."
msgstr "འཕྲིན་དོན་བཏོན་གཏང་དོ..."

#: 5038
msgid "Deleting messages..."
msgstr "འཕྲིན་དོན་ཚུ་བཏོན་གཏང་དོ..."

#: 5037
msgid "%S Receiving\\: Message flags %lu of %lu"
msgstr "%S ཐོབ་དོ\\: འབྲིན་དོན་གྱི་ཟུར་རྟགས་ཚུ་ %lu %lu ལས།"

#: 5036
msgid "%S Receiving\\: Message headers %lu of %lu"
msgstr "%S ཐོབ་དོ\\: འཕྲིན་དོན་གྱི་མགོ་ཡིག་ཚུ་ %lu %lu ལས།"

#: 5035
msgid "Login to server %S failed."
msgstr "སར་བར་%S ནང་ལུ་ནང་བསྐྱོད་འབད་ནི་འཐུས་ཤོར་འབྱུང་ཡོད།"

#: 5034
msgid "IMAP Error\\: The online folder information could not be retrieved."
msgstr "ཨའི་ཨེམ་ཨེ་པི་གི་འཛོལ་བ\\: གློག་ཐོག་སྣོད་འཛིན་གྱི་བརྡ་དོན་དེ་སླར་འདྲེན་འབད་མི་ཚུགས།"

#: 5033
msgid "IMAP Error\\: The message could not be saved due to an error."
msgstr "ཨའི་ཨེམ་ཨེ་པི\\: འཕྲིན་དོན་དེ་འཛོལ་བ་གཅིག་གིས་སྦེ་སྲུང་བཞག་འབད་མ་ཚུགས།"

#: 5097
msgid "There are no storage quotas on this folder."
msgstr "འ་ནི་སྣོད་འཛིན་གུ་ལུ་བསག་མཛོད་ཀྱི་ཐོབ་སྐལ་ཚུ་མེད།"

#: 5032
msgid "This body part will be downloaded on demand."
msgstr "འ་ནི་མ་གཟུགས་ཀྱི་ཡན་ལག་དེ་མཁོ་འདོད་བཀོད་མི་དང་འཁྲིལ་ཏེ་ཕབ་ལེན་འབད་འོང་།"

#: 5096
msgid "This server does not support quotas."
msgstr "འ་ནི་སར་བར་འདི་གིས་ཐོབ་སྐལ་ཚུ་ལུ་རྒྱབ་སྐྱོར་མི་འབད།"

#: 5031
msgid "Getting Mailbox Configuration Info..."
msgstr "ཡིག་འཕྲིན་རིམ་སྒྲིག་གི་བརྡ་དོན་ལེན་དོ..."

#: 5095
msgid "Quota information is not available because the folder is not open."
msgstr "ཐོབ་སྐལ་གྱི་བརྡ་དོན་དེ་འཐོབ་ཚུགསཔ་མེད་ ག་ཅི་སྦེ་ཟེར་བ་ཅིན་ སྣོད་འཛིན་དེ་ཁ་མ་ཕྱེ་བས།"

#: 5030
msgid "Getting Server Configuration Info..."
msgstr "སར་བར་རིམ་སྒྲིག་གི་བརྡ་དོན་ལེན་དོ..."

#: 5093
msgid "Unable to connect to your IMAP server. You may have exceeded the maximum number of connections to this server. If so, use the Advanced IMAP Server Settings dialog to reduce the number of cached connections."
msgstr "ཁྱོད་ཀྱི་ཨའི་ཨེམ་ཨེ་པི་སར་བར་ལུ་མཐུད་མ་ཚུགས། ཁྱོད་ཀྱིས་འ་ནི་སར་བར་ལུའི་མཐུད་ལམ་གྱི་གྲངས་ཀྱི་མང་མཐའ་ལས་ལྷག་སོངམ་འོང་། དེ་སྦེ་ཨིན་པ་ཅིན་ འདྲ་མཛོད་མཐུད་ལམ་ཚུའི་གྲངས་མར་ཕབ་འབད་ནིའི་དོན་ལུ་མཐོ་རིམ་ཨའི་ཨེམ་ཨེ་པི་སར་བར་སྒྲིག་སྟངས་ཚུའི་ཌའི་ལོག་དེ་ལག་ལེན་འཐབ།"

#: 5092
msgid "Would you like to subscribe to %1$S?"
msgstr "%1$S ལུ་ཁྱོད་ཀྱིས་མཁོ་མངགས་འབད་ནི་ཨིན་ན?"

#: 5091
msgid "Login failed."
msgstr "ནང་བསྐྱོད་འབད་ནི་འཐུས་ཤོར་འབྱུང་ཡོད།"

#: 5090
msgid "Server %S has disconnected. The server may have gone down or there may be a network problem."
msgstr "སར་བར་%S གིས་མཐུད་ལམ་བཏོག་ཡོད། སར་བར་འདི་མར་ཉམས་ཡར་སོངམ་འོང་ ཡང་ན་ཡོངས་འབྲེལ་གྱི་དཀའ་ངལ་ཅིག་ཡོདཔ་འོང་།"

#: 5029
msgid "Getting folder ACL..."
msgstr "སྣོད་འཛིན་ཨེ་སི་ཨེལ་ལེན་དོ..."

#: 5028
msgid "The upgrade was successful."
msgstr "ཡར་བསྐྱེད་འབད་ནི་དེ་མཐར་འཁྱོལ་ཅན་བྱུང་ཡོད།"

#: 5026
msgid "Please choose which folders you want subscribed in the Subscribe window..."
msgstr "མཁོ་མངགས་སྒོ་སྒྲིག་ནང་ལུ་ཁྱོད་ཀྱིས་སྣོད་འཛིན་ག་འདི་མཁོ་མངགས་འབད་ནི་ཨིན་ན་གདམ་ཁ་རྐྱབས་གནང་..."

#: 5024
msgid "Any folders which are left unsubscribed will not appear in your folder lists, but can be subscribed to later by choosing File / Subscribe."
msgstr "སྣོད་འཛིན་ག་ཅི་རང་འབད་རུང་ མཁོ་མངགས་མ་འབད་བར་བཞག་མི་དེ་ཁྱོད་ཀྱི་སྣོད་འཛིན་གྱི་ཐོ་ཡིག་ནང་ལུ་མི་འབྱུང་ དེ་འབདཝ་ད་ ཡིག་སྣོད་/མཁོ་མངགས་གདམ་ཁ་རྐྱབས་ཐོག་ཤུལ་ལས་མཁོ་མངགས་འབད་བཏུབ།"

#: 5021
msgid "\"The mail server indicates that you may not have any personal mail folders. Please verify your subscriptions."
msgstr "\"ཡིག་འཕྲིན་གྱི་སར་བར་གྱིས་ཁྱོད་ལུ་རང་དོན་ཡིག་འཕྲིན་གྱི་སྣོད་འཛིན་ཚུ་མེདཔ་འོང་ཟེར་བརྡ་སྟོན་འབདཝ་ཨིན། ཁྱོད་ཀྱིས་མཁོ་མངགས་འབད་མི་ཚུ་བདེན་སྦྱོར་འབད་གནང་།"

#: 5020
msgid "Checking IMAP Namespace..."
msgstr "ཨའི་ཨེམ་ཨེ་པི་མིང་གི་བར་སྟོང་ཞིབ་དཔྱད་འབད་དོ..."

#: 5084
msgid "This server does not support shared folders."
msgstr "འ་ནི་སར་བར་འདི་གིས་རུབ་སྤྱོད་འབད་འབད་བའི་སྣོད་འཛིན་ཚུ་རྒྱབ་སྐྱོར་མི་འབད།"

#: 5083
msgid "Unknown"
msgstr "མ་ཤེསཔ།"

#: 5082
msgid "Administer Folder"
msgstr "སྣོད་འཛིན་བདག་སྐྱོང་འཐབ།"

#: 5081
msgid "Delete Messages"
msgstr "འཕྲིན་དོན་ཚུ་བཏོན་གཏང་།"

#: 5080
msgid "Create Subfolder"
msgstr "སྣོད་འཛིན་ཡན་ལག་གསར་བསྐྲུན་འབད།"

#: 5015
msgid "Downloading message..."
msgstr "འཕྲིན་དོན་ཕབ་ལེན་འབད་དོ..."

#: 5014
msgid "Sending authenticate login information..."
msgstr "བདེན་བཤད་ནང་བསྐྱོད་ཀྱི་བརྡ་དོན་གཏང་དོ..."

#: 5079
msgid "Post"
msgstr "གནས་བསྐྱོད"

#: 5013
msgid "Sending login information..."
msgstr "ནང་བསྐྱོད་ཀྱི་བརྡ་དོན་གཏང་དོ..."

#: 5078
msgid "Insert (Copy Into)"
msgstr "ནང་ན་བཙུགས(ནང་ལུ་འདྲ་བཤུས)"

#: 5012
msgid "Checking mail server capabilities..."
msgstr "ཡིག་འཕྲིན་སར་བར་གྱི་ལྕོགས་གྲུབ་ཞིབ་དཔྱད་འབད་དོ..."

#: 5077
msgid "Write"
msgstr "འབྲི།"

#: 5011
msgid "Logging out..."
msgstr "ཕྱིར་བསྐྱོད་འབད་དོ..."

#: 5076
msgid "Set Read/Unread State"
msgstr "ལྷག་/མ་ལྷག་པའི་གནས་ལུགས་གཞི་སྒྲིག་འབད།"

#: 5010
msgid "Compacting folder..."
msgstr "སྣོད་འཛིན་སྲབ་ཆུང་བཟོ་དོ..."

#: 5075
msgid "Read"
msgstr "ལྷག"

#: 5074
msgid "Lookup"
msgstr "བལྟ་བ།"

#: 5073
msgid "Full Control"
msgstr "ཚད་འཛིན་ཧྲིལ་བུ།"

#: 5072
msgid "This is a mail folder shared by the user '%S'."
msgstr "འ་ནི་འདི་ཡིག་འཕྲིན་གྱི་སྣོད་འཛིན་ ལག་ལེན་པ་ %S གིས་རུབ་སྤྱོད་འབད་མི་ཅིག་ཨིན།"

#: 5071
msgid "This is a public folder."
msgstr "འ་ནི་འདི་མི་མང་གི་སྣོད་འཛིན་ཅིག་ཨིན།"

#: 5070
msgid "This is a personal mail folder.  It has been shared."
msgstr "འ་ནི་འདི་རང་དོན་ཡིག་འཕྲིན་གྱི་སྣོད་འཛིན་ཅིག་ཨིན། དེ་རུབ་སྤྱོད་འབད་ཡོད།"

#: 5009
msgid "Closing folder..."
msgstr "སྣོད་འཛིན་ཁ་བསྡམ་དོ..."

#: 5102
msgid ""
"You cannot log in to %S because you have enabled secure authentication and this server does not support it.\n"
"\n"
"To log in, turn off secure authentication for this account."
msgstr ""
