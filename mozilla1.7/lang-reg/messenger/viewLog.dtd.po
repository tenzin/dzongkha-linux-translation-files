#: clearLog.label
#: clearLog.accesskey
msgid "&Clear Log"
msgstr "དྲན་དེབ་བསལ།(&C)"

#: enableLog.label
#: enableLog.accesskey
msgid "&Enable the Filter Log"
msgstr "ཚགས་མའི་དྲན་དེབ་ལྕོགས་ཅན་བཟོ།(&E)"

#: viewLog.title
msgid "Filter Log"
msgstr "ཚགས་མའི་དྲན་དེབ།"

#: viewLogInfo.text
msgid "The Filter Log is a file that documents what filters have been run. Use the check box below to enable logging."
msgstr "ཚགས་མའི་དྲན་དེབ་དེ་ཚགས་མ་ག་དེ་ཚུ་གཡོག་ག་དེ་སྦེ་བཀོལ་ཡི་ག་ ཡིག་ཆ་བཀོད་མི་གི་ཡིག་སྣོད་ཅིག་ཨིན། འོག་གི་དཔྱད་སྒྲོམ་དེ་ ནང་བསྐྱོད་འབད་ནི་ལྕོགས་ཅན་བཟོ་ནིའི་དོན་ལུ་ལག་ལེན་འཐབ།"

