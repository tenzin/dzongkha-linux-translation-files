#: dualUseFolders.label
#: dualUseFolders.accesskey
msgid "Server supports &folders that contain sub-folders and messages"
msgstr "སྣོད་འཛིན་ནང་ན་སྣོད་འཛིན་ཡན་ལག་དང་འཕྲིན་དོན་ཡོད་མི་ཚུ་ལུ་སར་བར་གྱིས་རྒྱབ་སྐྱོར་འབདཝ་ཨིན།(&f)"

#: imapAdvanced.label
msgid "Advanced IMAP Server Settings"
msgstr "ཨའི་ཨེམ་ཨེ་པི་ སར་བར་སྒྲིག་སྟངས་ཚུའི་མཐོ་རིམ།"

#: maximumConnectionsNumber.label
#: maximumConnectionsNumber.accesskey
msgid "&Maximum number of server connections to cache"
msgstr "འདྲ་མཛོད་ནང་ལུ་སར་བར་མཐུད་ལམ་གྱི་གྲངས་མང་མཐའ།(&M)"

#: namespaceDesc.label
msgid "These preferences specify the namespaces on your IMAP server"
msgstr "འ་ནི་དགའ་གདམ་ཚུ་གིས་ཁྱོད་རའི་ཨའི་ཨེམ་ཨེ་པི་སར་བར་གུའི་མིང་གི་བར་སྟོང་ཚུ་གསལ་བཀོད་འབདཝ་ཨིན།"

#: otherUsersNamespace.label
#: otherUsersNamespace.accesskey
msgid "&Other Users:"
msgstr "ལག་ལེན་པ་གཞན་མི་ཚུ:(&O)"

#: overrideNamespaces.label
#: overrideNamespaces.accesskey
msgid "&Allow server to override these namespaces"
msgstr "སར་བར་གྱིས་འ་ནི་མིང་གི་བར་སྟོང་ཚུ་ཟུར་བཞག་འབད་བཅུག(&A)"

#: personalNamespace.label
#: personalNamespace.accesskey
msgid "&Personal namespace:"
msgstr "རང་དོན་མིང་གི་བར་སྟོང་:(&P)"

#: publicNamespace.label
#: publicNamespace.accesskey
msgid "P&ublic (shared):"
msgstr "མི་མང་གི(རུབ་སྤྱོད་འབད་ཡོདཔ།):"

#: serverAdvanced.label
msgid "Advanced Account Settings"
msgstr "མཐོ་རིམ་རྩིས་ཐོའི་སྒྲིག་སྟངས།"

#: serverDirectory.label
#: serverDirectory.accesskey
msgid "IMAP server &directory:"
msgstr "ཨའི་ཨེམ་ཨེ་པི་སར་བར་གྱི་སྣོད་ཐོ:(&d)"

#: smtpDefaultServer.label
msgid "Always use default server"
msgstr "ཨ་རྟག་རང་ངེས་ཀྱི་སྔོན་སྒྲིག་གི་སར་བར་ལག་ལེན་འཐབ།"

#: smtpDesc.label
msgid "When sending messages from this identity, always use the following outgoing server (SMTP):"
msgstr "འ་ནི་ངོ་རྟགས་ལས་འཕྲིན་དོན་ཚུ་གཏང་བའི་སྐབས་ ཨ་རྟག་རང་འོག་གི་ཕྱིར་གཏང་སར་བར་(ཨེསི་ཨེམ་ཊི་པི)ལག་ལེན་འཐབ:"

#: smtpName.label
#: smtpName.accesskey
msgid "&Server:"
msgstr "སར་བར:(&S)"

#: useIdle.label
#: useIdle.accesskey
msgid "Use &IDLE command if the server supports it"
msgstr "སར་བར་གྱིས་འདི་ལུ་རྒྱབ་སྐྱོར་འབདཝ་ཨིན་པ་ཅིན་ ལས་མེད་ཀྱི་བརྡ་བཀོད་ལག་ལེན་འཐབ།(&I)"

#: usingSubscription.label
#: usingSubscription.accesskey
msgid "Sho&w only subscribed folders"
msgstr "མཁོ་མངགས་འབད་ཡོད་པའི་སྣོད་འཛིན་ཚུ་རྐྱངམ་ཅིག་སྟོན།(&w)"

