#: alwaysSend.label
msgid "Always send"
msgstr "ཨ་རྟག་རང་གཏང་།"

#: askMe.label
msgid "Ask me"
msgstr "ང་ལུ་འདྲི།"

#: leaveIt.label
#: leaveIt.accesskey
msgid "Leave it in my &Inbox"
msgstr "ངེས་ཀྱི་ཨིན་བོགསི་ནང་ལུ་བཞག(&I)"

#: moveToSent.label
#: moveToSent.accesskey
msgid "&Move it to my &quot;Sent&quot; folder"
msgstr "ངེས་ཀྱི་&quot;Sent&quot; སྣོད་འཛིན་ནང་ལུ་བཤུད།(&M)"

#: never.label
#: never.accesskey
msgid "&Never send a return receipt"
msgstr "འབྱོར་རྟགས་སླར་ལོག་མི་གཅིག་ནམ་ཡང་མ་གཏང་།(&N)"

#: neverSend.label
msgid "Never send"
msgstr "ནམ་ཡང་མ་གཏང་།"

#: notInToCc.label
#: notInToCc.accesskey
msgid "If I'm not in the &To or Cc of the message:"
msgstr "ང་འཕྲིན་དོན་ངོ་མ་ཡང་ན་འདྲ་བཤུས་འཐོབ་མི་མེན་པ་ཅིན:(&T)"

#: otherCases.label
#: otherCases.accesskey
msgid "In all &other cases:"
msgstr "གནད་དོན་གཞན་མི་ཆ་མཉམ་གྱི་ནང་ལུ:(&o)"

#: outsideDomain.label
#: outsideDomain.accesskey
msgid "If the &sender is outside my domain:"
msgstr "གཏང་མི་འདི་ངེས་ཀྱི་ཌོ་མེན་གྱི་ཕྱི་ཁར་ཨིན་པ་ཅིན:(&s)"

#: pane.title
msgid "Return Receipts"
msgstr "འབྱོར་རྟགས་ཚུ་སླར་ལོག་འབད།"

#: receiptArrive.label
msgid "When a receipt arrives:"
msgstr "འབྱོར་རྟགས་གཅིག་འབྱོར་བའི་སྐབས:"

#: requestMDN.label
msgid "When I receive a request for a return receipt:"
msgstr "ང་གིས་འབྱོར་རྟགས་སླར་ལོག་མིའི་དོན་ལུ་ཞུ་བ་གཅིག་འཐོབ་པའི་སྐབས:"

#: requestReceipt.label
#: requestReceipt.accesskey
msgid "&When sending messages, always request a return receipt"
msgstr "འཕྲིན་དོན་ཚུ་གཏང་བའི་སྐབས་ ཨ་རྟག་རང་འབྱོར་རྟགས་སླར་ལོག་མི་གཅིག་ཞུ་བ་འབད།(&W)"

#: returnSome.label
#: returnSome.accesskey
msgid "Allow r&eturn receipts for some messages"
msgstr "འཕྲིན་དོན་ལ་ལུའི་དོན་ལུ་འབྱོར་རྟགས་ཚུ་སླར་ལོག་གཏང་བཅུག(&e)"

#: useCustomPrefs.label
#: useCustomPrefs.accesskey
msgid "&Customize return receipts for this account"
msgstr "འ་ནི་རྩིས་ཐོའི་དོན་ལུ་འབྱོར་རྟགས་སླར་ལོག་མིཚུ་སྲོལ་སྒྲིག་འབད།(&C)"

#: useGlobalPrefs.label
#: useGlobalPrefs.accesskey
msgid "&Use my global return receipt preferences for this account"
msgstr "ངེས་ཀྱི་སྤྱི་ཁྱབ་འབྱོར་རྟགས་སླར་ལོག་གི་དགའ་གདམ་ཚུ་འ་ནི་རྩིས་ཐོའི་དོན་ལུ་ལག་ལེན་འཐབ།(&U)"

