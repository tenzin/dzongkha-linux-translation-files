#: autounzip.label
#: autounzip.accesskey
msgid "&Automatically expand zip files when saving attachments"
msgstr "མཉམ་སྦྲགས་ཚུ་སྲུང་བཞག་འབད་བའི་སྐབས གནས་ཡུལ་གྱི་ཡིག་སྣོད་ཚུ་རང་བཞིན་གྱིས་རྒྱ་འཕརཝ་ཨིན།(&AS)"

#: confirmMove.label
#: confirmMove.accesskey
msgid "&Confirm when moving folders to the Trash"
msgstr "སྣོད་འཛིན་ཚི་ཕྱགས་ཧོད་ནང་ལུ་བཤུད་པའི་སྐབས་ངེས་དཔྱད་འབད།(&C)"

#: enableStartPage.label
#: enableStartPage.accesskey
msgid "&When Mail launches, show the Start Page in the message area"
msgstr "ཡིག་འཕྲིན་གསར་བཙུགས་འབད་བའི་སྐབས་ འཕྲིན་དོན་གྱི་མངའ་ཁོངས་ནང་ལུ་འགོ་བཙུགས་ཀྱི་ཤོག་ལེབ་སྟོན།(&W)"

#: fileButton.label
#: fileButton.accesskey
msgid "&File"
msgstr "ཡིག་སྣོད།(&F)"

#: generalSettings.label
msgid "General Settings"
msgstr "ཡོངས་ཁྱབ་སྒྲིག་སྟངས་ཚུ།"

#: junkMailButton.label
#: junkMailButton.accesskey
msgid "&Junk"
msgstr "མཁོ་མེད།(&J)"

#: location.label
#: location.accesskey
msgid "&Location:"
msgstr "གནས་ཁོངས:(&L)"

#: messengerStartPage.label
msgid "Mail Start Page"
msgstr "ཡིག་འཕྲིན་དགོ་བཙུགས་ཀྱི་ཤོག་ལེབ།"

#: nextButton.label
#: nextButton.accesskey
msgid "&Next"
msgstr "ཤུལ་མམ།(&N)"

#: pane.title
msgid "Mail &amp; Newsgroups"
msgstr "ཡིག་འཕྲིན་ &amp; གནས་ཚུལ་སྡེ་ཚན་ཚུ།"

#: printButton.label
#: printButton.accesskey
msgid "&Print"
msgstr "དཔར་བསྐྲུན་འབད།(&P)"

#: rememberLastMsg.label
#: rememberLastMsg.accesskey
msgid "R&emember the last selected message"
msgstr "མཐའ་མཇུག་ལུ་སེལ་འཐུ་འབད་མི་འཕྲིན་དོན་དེ་སེམས་ཁར་བཞག(&e)"

#: stopButton.label
#: stopButton.accesskey
msgid "&Stop"
msgstr "བཀག(&S)"

#: toolbarIntro.label
msgid "Select the buttons you want to see in the toolbar"
msgstr "ཁྱོད་ཀྱིས་ལག་ཆས་ཕྲ་རིང་ནང་ལུ་བལྟ་དགོ་མནོ་མི་གཡེབ་རྟ་ཚུ་སེལ་འཐུ་འབད།"

#: useDefault.label
#: useDefault.accesskey
msgid "&Restore Default"
msgstr "སྔོན་སྒྲིག་ཚུ་སོར་ཆུད་འབད།(&R)"

#: useMessenger.label
#: useMessenger.accesskey
msgid "&Use Mail from MAPI-based applications"
msgstr "ཨེམ་ཨེ་པི་ཨའི་-གཞི་བཞག་གི་འཇུག་སྤྱོད་ཚུ་ནང་ལས་ཡིག་འཕྲིན་ལག་ལེན་འཐབ།(&U)"

