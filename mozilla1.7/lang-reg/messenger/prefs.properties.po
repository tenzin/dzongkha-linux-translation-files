#: accountName
msgid "%1$S - %2$S"
msgstr "%1$S - %2$S"

#: customizedEmailText
msgid "Enter your %1$S %2$S (for example, if your %1$S email address is \"%3$S\", your %2$S is \"%4$S\")."
msgstr "ཁྱོད་རའི་%1$S %2$S ཐོ་བཀོད་འབད། (དཔེར་ན་ ཁྱོད་ཀྱི་%1$S གློག་འཕྲིན་ཁ་བྱང་འདི་ \"%3$S\" ཨིན་པ་ཅིན་ ཁྱོད་ཀྱི་ %2$S འདི་ \"%4$S\" ཨིན།)"

#: enterValidEmail
msgid ""
"_: enterValidEmail\n"
"Please enter a valid email address."
msgstr "ནུས་ཅན་གློག་འཕྲིན་གྱི་ཁ་བྱང་ཅིག་ཐོ་བཀོད་འབད་གནང་།"

#: prefPanel-server
msgid "Server Settings"
msgstr "སར་བར་གྱི་སྒྲིག་སྟངས།"

#: prefPanel-offline-and-diskspace
msgid "Offline & Disk Space"
msgstr "ཨོཕ་ལའིན་ & ཌིགསི་གི་བར་སྟོང་།"

#: modifiedAccountExists
msgid "An account with that user name and server name already exists. Please enter a different user name and/or server name."
msgstr "རྩིས་ཐོ་ ལག་ལེན་པའི་མིང་དང་སར་བར་གྱིན་མིང་དང་ཅིག་ཁར་འབད་མི་ཧེ་མ་ལས་རང་ཡོད། ལག་ལེན་པའི་མིང་དང་/ཡང་ན་སར་བར་གྱི་མིང་སོ་སོ་ཅིག་ཐོ་བཀོད་འབད་གནང་།"

#: enterAccountName
msgid "Please enter a name for this account."
msgstr "འ་ནི་རྩིས་ཐོའི་དོན་ལུ་མིང་ཅིག་ཐོ་བཀོད་འབད་གནང་།"

#: WizardContinue
msgid "Cancel"
msgstr "ཆ་མེད་གཏང་།"

#: prefPanel-addressing
msgid "Composition & Addressing"
msgstr "རྩོམ་འབྲི་ & ཁ་བྱང་བཏགས་ནི།"

#: failedDuplicateAccount
msgid "Failed to duplicate this account."
msgstr "འ་ནི་རྩིས་ཐོ་དེ་རྫུན་མ་བཟོ་ནི་ལུ་འཐུས་ཤོར་འབྱུང་ཡོད།"

#: prefPanel-copies
msgid "Copies & Folders"
msgstr "འདྲ་བཤུས་ཚུ་ & སྣོད་འཛིན་ཚུ།"

#: defaultEmailText
msgid "Enter your email address. This is the address others will use to send email to you (for example, \"%1$S@%2$S\")."
msgstr "ཁྱོད་རའི་གློག་འཕྲིན་ཁ་བྱང་ཐོ་བཀོད་འབད། འ་ནི་འདི་གཞན་མི་གིས་ཁྱོད་ལུ་གློག་འཕྲིན་གཏང་ནིའི་དོན་ལུ་ལག་ལེན་འཐབ་མི་ཁ་བྱང་འདི་ཨིན།(དཔེར་ན་ \"1$S@%2$S\")"

#: prefPanel-smtp
msgid "Outgoing Server (SMTP)"
msgstr "ཕྱིར་གཏང་གི་སར་བར(ཨེསི་ཨེམ་ཊི་པི)"

#: WizardExit
msgid "Exit"
msgstr "ཕྱིར་ཐོན།"

#: serverNameChanged
msgid "The server name setting has changed. Please verify that any folders used by filters exist on the new server."
msgstr "སར་བར་མིང་གི་སྒྲིག་སྟངས་དེ་བསྒྱུར་བཅོས་འབད་ཡོད། ཚགས་མ་ཚུ་གིས་ལག་ལེན་འཐབ་མི་སྣོད་འཛིན་ག་སྦེ་རུང་སར་བར་གསརཔ་གུ་ལུ་ཡོདཔ་སྦེ་བདེན་སྦྱོར་འབད་གནང་།"

#: emailFieldText
msgid "Email Address\\:"
msgstr "གློག་འཕྲིན་ཁ་བྱང་\\:"

#: enterName
msgid "Please enter your name."
msgstr "ཁྱོད་རའི་མིང་ཐོ་བཀོད་འབད་གནང་།"

#: confirmRemoveAccountTitle
msgid "Remove Account"
msgstr "རྩིས་ཐོ་རྩ་བསྐྲད་གཏང་།"

#: enterUserName
msgid "Please enter a username."
msgstr "ལག་ལེན་པའི་མིང་ཅིག་ཐོ་བཀོད་འབད་གནང་།"

#: accountExists
msgid "A mail or newsgroup account with the same user name and server name already exists. Click Back and enter a different server name, or click Cancel."
msgstr "ཡིག་འཕྲིན་ཡང་ན་གནས་ཚུལ་སྡེ་ཚན་གྱི་རྩིས་ཐོ་ ལག་ལེན་པའི་མིང་དང་སར་བར་གྱི་མིང་གཅིག་པ་སྦེ་མི་དང་ཅིག་ཁར་ཧེ་མ་ལས་རང་ཡོད། ལོག་ནི་གུ་ཨེབ་གཏང་སྟེ་སར་བར་གྱི་མིང་སོ་སོ་ཅིག་ཐོ་བཀོད་འབད་ ཡང་ན་ཆ་མེད་གཏང་ནི་གུ་ཨེབ་གཏང་།"

#: prefPanel-advanced
msgid "Advanced"
msgstr "མཐོ་རིམ།"

#: failedRemoveAccount
msgid "Failed to remove this account."
msgstr "འ་ནི་རྩིས་ཐོ་དེ་རྩ་བསྐྲད་གཏང་ནི་འཐུས་ཤོར་འབྱུང་ཡོད།"

#: userNameChanged
msgid "Your User Name has been updated. You may also need to update your Email Address and/or User Name associated with this account."
msgstr "ཁྱོད་ཀྱི་ལག་ལེན་པའི་མིང་དེ་དུས་མཐུན་བཟོ་ཡོད། ཁྱོད་ཀྱིས་ཁྱོད་རའི་གློག་འཕྲིན་ཁ་བྱང་དང་/ཡང་ན་ལག་ལེན་པའི་མིང་ རྩིས་ཐོ་དེ་དང་ཅིག་ཁར་འབྲེལ་བ་ཡོད་མི་ཡང་དུས་མཐུན་བཟོ་དགོཔ་འོང་།"

#: cancelWizard
msgid ""
"Are you sure you want to exit the Account Wizard?\n"
"\n"
"If you exit, any information you have entered will be lost and the account will not be created."
msgstr ""
"ཁྱོད་ངེས་ཏིག་རང་རྩིས་ཐོའི་ཝི་ཛརཌི་ཕྱིར་ཐོན་འབད་ནི་ཨིན་ན?\n"
"\n"
"ཁྱོད་ཕྱིར་ཐོན་འབདཝ་ཨིན་པ་ཅིན་ བརྡ་དོན་ག་ཅི་རང་སྦེ་རུང་ཁྱོད་ཀྱིས་ཐོ་བཀོད་འབད་མི་ཚུ་བརླག་སྟོར་ཞུགས་ཏེ་རྩིས་ཐོ་དེ་གསར་བསྐྲུན་འབད་མི་ཚུགས།"

#: enterValidEmailPrefix
msgid ""
"_: enterValidEmailPrefix\n"
"Please enter a valid email address."
msgstr "ནུས་ཅན་གློག་འཕྲིན་གྱི་ཁ་བྱང་ཅིག་ཐོ་བཀོད་འབད་གནང་།"

#: prefPanel-diskspace
msgid "Disk Space"
msgstr "ཌིགསི་གི་བར་སྟོང་།"

#: exampleEmailDomain
msgid "example.net"
msgstr "དཔེར་བརྗོད་ ནེཊི"

#: accountWizard
msgid "Account Wizard"
msgstr "རྩིས་ཐོའི་ཝི་ཛརཌི།"

#: enterValidHostname
msgid "Please enter a valid hostname."
msgstr "ནུས་ཅན་ཧོསཊི་གི་མིང་ཅིག་ཐོ་བཀོད་འབད་གནང་།"

#: confirmRemoveAccount
msgid "Are you sure you want to remove the account \"%S\"?"
msgstr "ཁྱོད་ངེས་ཏིག་རང་རྩིས་ཐོ་\"%S\" དེ་རྩ་བསྐྲད་གཏང་ནི་ཨིན་ན?"

#: exampleEmailUserName
msgid "user"
msgstr "ལག་ལེན་པ།"

