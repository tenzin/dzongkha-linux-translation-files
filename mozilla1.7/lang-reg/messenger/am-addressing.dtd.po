#: aboveQuote.label
msgid "start my reply above the quote"
msgstr "ངེས་ཀྱི་ལན་གསལ་འདྲེན་ཚིག་གི་ལྟག་ལུ་འགོ་བཙུགས།"

#: aboveText.label
msgid "below my reply (above the quote)"
msgstr "གེས་ཀྱི་ལན་གསལ་གྱི་འོག་ལུ་(འདྲེན་ཚིག་གི་ལྟག་ལུ།)"

#: addressing.label
msgid "Composition &amp; Addressing"
msgstr "རྩོམ་བྲིས་དང་ཁ་བྱང་བཏགས་ནི།"

#: addressingGroupTitle.label
msgid "Addressing"
msgstr "ཁ་བྱང་བཏགས་ནི།"

#: addressingText.label
msgid "When looking up addresses:"
msgstr "ཁ་བྱང་ཚུ་བལྟ་བའི་སྐབས:"

#: autoQuote.label
#: autoQuote.accesskey
msgid "Automatically &quote the original message when replying"
msgstr "ལན་གསལ་བཀོད་པའི་སྐབས འཕྲིན་དོན་ངོ་མ་དེ་རང་བཞིན་གྱིས་འདྲེན་ཚིག་བཀོདཔ་ཨིན།(&q)"

#: belowQuote.label
msgid "start my reply below the quote"
msgstr "ངེས་ཀྱི་ལན་གསལ་འདི་འདྲེན་ཚིག་གི་འོག་ལུ་འགོ་བཙུགས།"

#: belowText.label
msgid "below the quote (recommended)"
msgstr "འདྲེན་ཚིག་གི་འོག་ལུ་(འོས་སྦྱོར་འབད་ཡོདཔ།)"

#: compositionGroupTitle.label
msgid "Composition"
msgstr "རྩོམ་བྲིས།"

#: directories.label
#: directories.accesskey
msgid "Use a different L&DAP server:"
msgstr "ཨེལ་ཌི་ཨེ་པི་སར་བར་སོ་སོ་ཅིག་ལག་ལེན་འཐབ:"

#: editDirectories.label
#: editDirectories.accesskey
msgid "&Edit Directories..."
msgstr "སྣོད་ཐོ་ཚུ་ཞུན་དག་འབད...(&E)"

#: place.label
#: place.accesskey
msgid "and &place my signature"
msgstr "དང་ངེས་ཀྱི་མིང་རྟགས་བཞག(&p)"

#: selectAndQuote.label
msgid "select the quote"
msgstr "འདྲེན་ཚིག་སེལ་འཐུ་འབད།"

#: then.label
#: then.accesskey
msgid "&Then,"
msgstr "དེ་ལས (&T)"

#: useGlobal.label
#: useGlobal.accesskey
msgid "&Use my global LDAP server preferences for this account"
msgstr "འ་ནི་རྩིས་ཐོའི་དོན་ལུ་ངེས་ཀྱི་སྤྱི་ཁྱབ་ཨེལ་ཌི་ཨེ་པི་སར་བར་གྱི་དགའ་གདམ་ལག་ལེན་འཐབ།(&U)"

#: useHtml.label
#: useHtml.accesskey
msgid "&Compose messages in HTML format"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་རྩ་སྒྲིག་ནང་ལུ་འཕྲིན་དོན་ཚུ་བརྩམ།(&C)"

