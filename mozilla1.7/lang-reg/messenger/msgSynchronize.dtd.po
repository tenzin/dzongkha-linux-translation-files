#: MsgSelect.label
msgid "Items for Offline Use"
msgstr "ཨོཕ་ལའིན་ལག་ལེན་གྱི་དོན་ལུ་རྣམ་གྲངས་ཚུ།"

#: MsgSelectDesc.label
msgid "Choose mail folders and newsgroups for offline use."
msgstr "ཨོཕ་ལའིན་ལག་ལེན་གྱི་དོན་ལུ་ཡིག་འཕྲིན་གྱི་སྣོད་འཛིན་ཚུ་དང་གནས་ཚུལ་སྡེ་ཚན་ཚུ་གདམ་ཁ་རྐྱབས།"

#: MsgSelectInd.label
msgid "Download"
msgstr "ཕབ་ལེན་འབད།"

#: MsgSelectItems.label
msgid "Folders and Newsgroups"
msgstr "སྣོད་འཛིན་ཚུ་དང་གནས་ཚུལ་སྡེ་ཚན་ཚུ།"

#: MsgSyncDesc.label
msgid "If you have already selected mail folders or newsgroups for offline use, you can download and/or sync them now. Otherwise, use the &quot;Select&quot; button to choose mail folders and newsgroups for offline use."
msgstr "ཁྱོད་ཀྱིས་ཨོཕ་ལའིན་ལག་ལེན་གྱི་དོན་ལུ་ཡིག་འཕྲིན་སྣོད་འཛིན་ཚུ་ཡང་ན་གནས་ཚུལ་སྡེ་ཚན་ཚུ་ཧེ་མ་ལས་རང་སེལ་འཐུ་འབད་ཡོདཔ་ཨིན་པ་ཅིན་ ཁྱོད་ཀྱིས་དེ་ཚུ་ད་ལྟོ་ཕབ་ལེན་དང་/ཡང་ན་མཉམ་འབྱུང་འབད། དེ་མེན་པ་ཅིན ཨོཕ་ལའིན་ལག་ལེན་གྱི་དོན་ལུ་ཡིག་འཕྲིན་སྣོད་འཛིན་ཚུ་དང་གནས་ཚུལ་སྡེ་ཚན་ཚུ་གདམ་ཁ་རྐྱབས་ནིའི་དོན་ལས་ &quot;Select&quot; གཡེབ་རྟ་ལག་ལེན་འཐབ།"

#: MsgSyncDirections.label
msgid "Download and/or sync the following:"
msgstr "འོག་གི་ཚུ་ཕབ་ལེན་དང་/ཡང་ན་མཉམ་འབྱུང་འབད:"

#: MsgSynchronize.label
msgid "Download and Sync Messages"
msgstr "འཕྲིན་དོན་ཚུ་ཕབ་ལེན་དང་མཉམ་འབྱུང་འབད།"

#: selectButton.label
#: selectButton.accesskey
msgid "S&elect..."
msgstr "སེལ་འཐུ་འབད...(&e)"

#: sendMessage.label
#: sendMessage.accesskey
msgid "&Send Unsent messages"
msgstr "མ་གཏང་པའི་འཕྲིན་དོན་ཚུ་གཏང་།(&S)"

#: syncTypeMail.label
#: syncTypeMail.accesskey
msgid "&Mail messages"
msgstr "འཕྲིན་དོན་ཚུ་གཏང་།(&M)"

#: syncTypeNews.label
#: syncTypeNews.accesskey
msgid "&Newsgroup messages"
msgstr "གནས་ཚུལ་སྡེ་ཚན་གྱི་འཕྲིན་དོན་ཚུ།(&N)"

#: workOffline.label
#: workOffline.accesskey
msgid "&Work offline once download and/or sync is complete"
msgstr "ཕབ་ལེན་དང་/ཡང་ན་མཉམ་འབྱུང་དེ་ཚར་ཅིག་མཇུག་བསྡུ་ཚརཝ་ད་ཨོཕ་ལའིན་ལཱ་འབད།(&W)"

