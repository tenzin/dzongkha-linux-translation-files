#: alwaysSend.label
msgid "Always send"
msgstr "ཨ་རྟག་རང་གཏང་།"

#: askMe.label
msgid "Ask me"
msgstr "ང་ལུ་འདྲི།"

#: leaveIt.label
#: leaveIt.accesskey
msgid "Leave it in my &Inbox"
msgstr "ངེས་ཀྱི་ཨིན་བོགསི་ནང་ལུ་བཞག(&I)"

#: moveToSent.label
#: moveToSent.accesskey
msgid "&Move it to my &quot;Sent&quot; folder"
msgstr "ངེས་ཀྱི་&quot;Sent&quot; སྣོད་འཛིན་ནང་ལུ་བཤུད།(&M)"

#: never.label
#: never.accesskey
msgid "&Never send a return receipt"
msgstr "སླར་ལོག་འབྱོར་རྟགས་ཅིག་ནམ་ཡང་མ་གཏང་།(&N)"

#: neverSend.label
msgid "Never send"
msgstr "ནམ་ཡང་མ་གཏང་།"

#: notInToCc.label
#: notInToCc.accesskey
msgid "If I'm not in the &To or Cc of the message:"
msgstr "ང་འཕྲིན་དོན་ངོ་མ་ཡང་ན་འདྲ་བཤུས་འཐོབ་མི་མེན་པ་ཅིན:(&T)"

#: otherCases.label
#: otherCases.accesskey
msgid "In all &other cases:"
msgstr "གནད་དོན་གཞན་ཆ་མཉམ་ནང་ལུ:(&o)"

#: outsideDomain.label
#: outsideDomain.accesskey
msgid "If the &sender is outside my domain:"
msgstr "གཏང་མི་འདི་ངེས་ཀྱི་ཌོ་མེན་གྱི་ཕྱི་ཁར་ཨིན་པ་ཅིན:(&s)"

#: pane.description
msgid "Settings for requesting or returning receipts"
msgstr "ཞུ་བ་འབད་མི་ཡང་ན་སླར་ལོག་མི་འབྱོར་རྟགས་ཚུའི་དོན་ལུ་སྒྲིག་སྟངས་ཚུ།"

#: pane.title
msgid "Return Receipts"
msgstr "སླར་ལོག་འབྱོར་རྟགས་ཚུ།"

#: receiptArrive.label
msgid "When a receipt arrives:"
msgstr "འབྱོར་རྟགས་ཅིག་འབྱོར་བའི་སྐབས:"

#: requestMDN.label
msgid "When I receive a request for a return receipt:"
msgstr "ང་གིས་སླར་ལོག་འབྱོར་རྟགས་ཀྱི་དོན་ལུ་ཞུ་བ་ཅིག་ལེན་པའི་སྐབས:"

#: requestReceipt.label
#: requestReceipt.accesskey
msgid "&When sending messages, always request a return receipt"
msgstr "འཕྲིན་དོན་ཚུ་གཏང་བའི་སྐབས་ ཨ་རྟག་རང་སླར་ལོག་འབྱོར་རྟགས་ཅིག་ཞུ་བ་འབད།"

#: returnSome.label
#: returnSome.accesskey
msgid "Allow r&eturn receipts for some messages"
msgstr "འཕྲིན་དོན་ལ་ལུའི་དོན་ལུ་སླར་ལོག་འབྱོར་རྟགས་ཚུ་འོང་བཅུག"

