#: accountsSectionHdr.label
msgid "Accounts"
msgstr "རྩིས་ཐོ་ཚུ།"

#: advFeaturesSectionHdr.label
msgid "Advanced Features"
msgstr "མཐོ་རིམ་གྱི་ཁྱད་རྣམ་ཚུ།"

#: composeMsgLink.label
msgid "Compose a new message"
msgstr "འཕྲིན་དོན་གསརཔ་ཅིག་བརྩམ།"

#: emailSectionHdr.label
msgid "Email"
msgstr "གློག་འཕྲིན།"

#: filtersLink.label
msgid "Manage message filters"
msgstr "འཕྲིན་དོན་གྱི་ཚགས་མ་ཚུ་འཛིན་སྐྱོང་འཐབ།"

#: junkSettings.label
msgid "Junk mail settings"
msgstr "མཁོ་མེད་ཡིག་འཕྲིན་གྱི་སྒྲིག་སྟངས་ཚུ།"

#: newAcctLink.label
msgid "Create a new account"
msgstr "རྩིས་ཐོ་གསརཔ་ཅིག་གསར་བསྐྲུན་འབད།"

