#: description.label
#: description.accesskey
msgid "&Create as a subfolder of:"
msgstr "གི་སྣོད་འཛིན་ཡན་ལག་སྦེ་གསར་བསྐྲུན་འབད:(&C)"

#: folderRestriction1.label
msgid "This server restricts folders to two special kinds."
msgstr "འ་ནི་སར་བར་གྱིས་སྣོད་འཛིན་ཚུ་དམིགས་བསལ་གྱི་དབྱེ་ཁག་གཉིས་ལུ་བཀག་དམ་འབདཝ་ཨིན།"

#: folderRestriction2.label
msgid "Allow your new folder to contain:"
msgstr "ཁྱོད་ཀྱི་སྣོད་འཛིན་གསརཔ་ལུ་ནང་ན་ ་ཡོད་བཅུག:"

#: foldersOnly.label
msgid "Folders Only"
msgstr "སྣོད་འཛིན་ཚུ་རྐྱངམ་ཅིག"

#: messagesOnly.label
msgid "Messages Only"
msgstr "འཕྲིན་དོན་ཚུ་རྐྱངམ་ཅིག"

#: name.label
#: name.accesskey
msgid "&Name:"
msgstr "མིང་:(&N)"

