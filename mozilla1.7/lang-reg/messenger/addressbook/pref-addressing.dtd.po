#: addressingEnable.label
#: addressingEnable.accesskey
msgid "&Local Address Books"
msgstr "ཉེ་གནས་ཀྱི་ཁ་བྱང་ཀི་དེབ་ཚུ།(&L)"

#: addressingTitle.label
msgid "Address Autocompletion"
msgstr "ཁ་བྱང་གི་རང་བཞིན་མཇུག་སྡུད།"

#: autocompleteText.label
msgid "When addressing messages, look for matching entries in:"
msgstr "འཕྲིན་དོན་ཚུ་ཁ་བྱང་བཏགས་པའི་སྐབས་ ནང་ལུ་མཐུན་པའི་ཐོ་བཀོད་ཚུ་ལུ་བལྟ:"

#: directories.label
#: directories.accesskey
msgid "&Directory Server:"
msgstr "སྣོད་ཐོའི་སར་བར:(&D)"

#: editDirectories.label
#: editDirectories.accesskey
msgid "&Edit Directories..."
msgstr "སྣོད་ཐོ་ཚུ་ཞུན་དག་འབད...(&E)"

#: emailCollectionPicker.label
#: emailCollectionPicker.accesskey
msgid "&Add email addresses to my:"
msgstr "ངེས་ཀྱི་དེ་ནང་ གློག་འཕྲིན་ཁ་བྱང་ཁ་སྐོང་རྐྱབས:(&A)"

#: emailCollectiontext.label
msgid "Email addresses from outgoing messages can be automatically added to a local address book."
msgstr "ཕྱིར་གཏང་བའི་འཕྲིན་དོན་ཚུ་ལས་གློག་འཕྲིན་ཁ་བྱང་ཚུ་རང་བཞིན་གྱིས་ཉེ་གནས་ཁ་བྱང་ཀི་དེབ་ནང་ལུ་ཁ་སྐོང་རྐྱབས་ཨིན།"

#: emailCollectiontitle.label
msgid "Email Address Collection"
msgstr "གློག་འཕྲིན་ཁ་བྱང་གི་བསྡུ་གསོག"

#: matchText.label
msgid "If there is a match in your local address books:"
msgstr "ཁྱོད་ཀྱི་ཉེ་གནས་ཁ་བྱང་ཀི་དེབ་ཚུ་ནང་ལུ་མཐུན་པ་ཡོདཔ་ཨིན་པ་ཅིན:"

#: pane.title
msgid "Addressing"
msgstr "ཁ་བྱང་བཏགས་ནི།"

#: skipDirectory.label
msgid "Do not search in the directory"
msgstr "སྣོད་ཐོ་ནང་ལུ་འཚོལ་ཞིབ་མ་འབད།"

