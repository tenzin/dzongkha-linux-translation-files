#: chooseExp.label
msgid "Note: For this setting to take effect, you will need to exit and then restart Mail."
msgstr "དྲན་འཛིན: འ་ནི་སྒྲིག་སྟངས་འདི་ལུ་ཕན་གནོད་འབྱུང་ནིའི་དོན་ལས་ ཁྱོད་ཀྱིས་ཕྱིར་ཐོན་འབད་ཞིནམ་ལས་དེ་ལས་ཡིག་འཕྲིན་ལོག་འགོ་བཙུགས་དགོ"

#: pane.title
msgid ""
"_: pane.title\n"
"Windows"
msgstr "སྒོ་སྒྲིག་ཚུ།"

#: reuseExp.label
msgid "When opening messages, display them in:"
msgstr "འཕྲིན་དོན་ཚུ་ཁ་ཕྱེ་བའི་སྐབས་ དེ་ཚུ་དེ་ནང་བཀྲམ་སྟོན་འབད:"

#: reuseExpRadio0.label
#: reuseExpRadio0.accesskey
msgid "A &new message window"
msgstr "འཕྲིན་དོན་གྱི་སྒོ་སྒྲིག་གསརཔ་ཅིག(&n)"

#: reuseExpRadio1.label
#: reuseExpRadio1.accesskey
msgid "An &existing message window"
msgstr "ཧེ་མ་ལས་ཡོད་པའི་འཕྲིན་དོན་གྱི་སྒོ་སྒྲིག་ཅིག(&e)"

#: reuseExpRadio2.label
#: reuseExpRadio2.accesskey
msgid "A &tab in an existing message window"
msgstr "ཧེ་མ་ལས་ཡོད་པའི་འཕྲིན་དོན་གྱི་སྒོ་སྒྲིག་ནང་ལུ་མཆོང་ལྡེ་ཅིག(&t)"

#: selectWindowLayout.label
#: selectWindowLayout.accesskey
msgid "Se&lect the window layout you prefer for Mail:"
msgstr "ཡིག་འཕྲིན་གྱི་དོན་ལུ་ཁྱོད་ཀྱིས་དགའ་མི་སྒོ་སྒྲིག་གི་སྒྲིག་བཀོད་སེལ་འཐུ་འབད:(&l)"

#: warnOnSendAccelKey.label
#: warnOnSendAccelKey.accesskey
msgid "C&onfirm when using keyboard shortcut to send message"
msgstr "འཕྲིན་དོན་གཏང་ནིའི་དོན་ལུ་ལྡེ་སྒྲོམ་གྱི་མགྱོགས་ཐབས་ལག་ལེན་འཐབ་པའི་སྐབས་ངེས་དཔྱད་འབད།(&o)"

