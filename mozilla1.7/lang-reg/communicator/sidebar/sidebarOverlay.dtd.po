#: sidebar.close.tooltip
msgid "Close Sidebar"
msgstr "ཟུར་ཁའི་ཕྲ་རིང་ཁ་བསྡམ།"

#: sidebar.customize.label
#: sidebar.customize.accesskey
msgid "C&ustomize Sidebar..."
msgstr "ཟུར་ཁའི་ཕྲ་རིང་སྲོལ་སྒྲིག་འབད...(&u)"

#: sidebar.hide.label
#: sidebar.hide.accesskey
msgid "&Hide Tab"
msgstr "མཆོང་ལྡེ་སྦ་བཞག(&H)"

#: sidebar.loading.label
msgid "Loading..."
msgstr "མངོན་གསལ་འབད་དོ..."

#: sidebar.loading.stop.label
#: sidebar.loading.stop.accesskey
msgid "&Stop"
msgstr "བཀག(&S)"

#: sidebar.loadstopped.label
msgid "Load stopped"
msgstr "བཀག་བཞག་བཞགཔ་ཚུ་མངོན་གསལ་འབད།"

#: sidebar.no-panels.add
msgid "You may add tabs by clicking on the \"Tabs\" button above."
msgstr "གོང་གི་\"Tabs\"གི་གཡེབ་རྟ་གུ་ཨེབ་གཏང་ཐོག་ཁྱོད་ཀྱིས་མཆོང་ལྡེ་ཚུ་ཁ་སྐོངས་རྐྱབས་ཚུགས།"

#: sidebar.no-panels.hide
msgid "If you would like to completely hide the Sidebar, click on the \"View\" menu above, and select \"Sidebar.\""
msgstr "ཁྱོད་ཟུར་ཁའི་ཕྲ་རིང་དེ་རྩ་བ་ལས་སྦ་བཞག་ནི་ཨིན་པ་ཅིན་ གོང་གི་ \"View\" གི་དཀར་ཆག་གུ་ཨེབ་སྟེ་ \"Sidebar\" སེལ་འཐུ་འབད།"

#: sidebar.no-panels.state
msgid "The sidebar is currently empty."
msgstr "ཟུར་ཁའི་ཕྲ་རིང་དེ་ད་ལྟོ་སྟོངམ་ཨིན།"

#: sidebar.open.tooltip
msgid "Open Sidebar"
msgstr "ཟུར་ཁའི་ཕྲ་རིང་ཁ་ཕྱེ།"

#: sidebar.pagenotfound.label
msgid "This tab is not available right now."
msgstr "འ་ནི་མཆོང་ལྡེ་ད་ལྟོ་འཐོབ་མི་ཚུགས།"

#: sidebar.panels.label
msgid "Sidebar"
msgstr "ཟུར་ཁའི་ཕྲ་རིང་།"

#: sidebar.picker.label
msgid "Tabs"
msgstr "མཆོང་ལྡེ་ཚུ།"

#: sidebar.reload.label
#: sidebar.reload.accesskey
msgid "&Reload"
msgstr "ཡང་བསྐྱར་མངོན་གསལ་འབད།(&R)"

#: sidebar.sbDirectory.label
msgid "Sidebar Directory..."
msgstr "ཟུར་ཁའི་ཕྲ་རིང་གི་སྣོད་ཐོ..."

#: sidebar.switch.label
#: sidebar.switch.accesskey
msgid "Switch to &Tab"
msgstr "མཆོང་ལྡེ་ལུ་སོར་བསྒྱུར་འབད།(&T)"

#: sidebarCmd.label
#: sidebarCmd.accesskey
msgid "Side&bar"
msgstr "ཟུར་ཁའི་ཕྲ་རིང་།(&b)"

