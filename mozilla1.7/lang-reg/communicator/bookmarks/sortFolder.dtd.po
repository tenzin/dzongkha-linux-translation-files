#: description.label
msgid "&brandShortName; can sort individual folders. Use these options to customize the sorting for this Folder."
msgstr ""
"&brandShortName; གིས་ངོ་རྐྱང་གི་སྣོད་འཛིན་ཚུ་དབྱེ་སེལ་འབད་ཚུགས།"
"འ་ནི་གདམ་ཁ་ཚུ་སྣོད་འཛིན་གྱི་དོན་ལུ་སྲོལ་སྒྲིག་གི་དབྱེ་སེལ་འབད་ནིའི་དོན་ལས་ལག་ལེན་འཐབ།"

#: sortBy.bookmarkAddDate.label
msgid "Added"
msgstr "ཁ་སྐོངས་རྐྱབས་ཡོད།"

#: sortBy.description.label
msgid "Description"
msgstr "འགྲེལ་བཤད།"

#: sortBy.label
msgid "Sort by:"
msgstr "དབྱེ་སེལ་འབད་ཐངས:"

#: sortBy.lastModifiedDate.label
msgid "Last Modified"
msgstr "མཐའ་མཇུག་ལུ་ལེགས་བཅོས་གྲུབ་ཡོདཔ།"

#: sortBy.lastVisitDate.label
msgid "Last Visited"
msgstr "མཐའ་མཇུག་ལུ་བལྟ་སྤྱོད་འབད་ཡོདཔ།"

#: sortBy.name.label
msgid "Name"
msgstr "མིང་།"

#: sortBy.shortcutUrl.label
msgid "Keyword"
msgstr "གཙོ་ཚིག"

#: sortBy.url.label
msgid "Location"
msgstr "གནས་ཁོངས།"

#: sortFoldersFirst.label
msgid "Sort folders first"
msgstr "སྣོད་འཛིན་ཚུ་ཧེ་མ་དབྱེ་སེལ་འབད།"

#: sortOptions.label
msgid "Sort options"
msgstr "གདམ་ཁ་ཚུ་དབྱེ་སེལ་འབད།"

#: sortOrder.ascending.label
msgid "A > Z"
msgstr "A > Z"

#: sortOrder.descending.label
msgid "Z > A"
msgstr "Z > A"

#: sortOrder.label
msgid "Sort order:"
msgstr "གོ་རིམ་དབྱེ་སེལ་འབད:"

#: sortRecursively.label
msgid "Sort recursively"
msgstr "སླར་བྱུང་མི་དབྱེ་སེལ་འབད།"

#: window.title
msgid "Sort Folder"
msgstr "སྣོད་འཛིན་དབྱེ་སེལ་འབད།"

