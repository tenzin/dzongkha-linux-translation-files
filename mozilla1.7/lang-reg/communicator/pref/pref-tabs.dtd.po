#: autoHide.label
msgid "Hide the tab bar when only one tab is open"
msgstr "མཆོང་ལྡེ་གཅིག་རྐྱངམ་ཅིག་ཁ་ཕྱེ་སྟེ་ཡོད་པའི་སྐབས་མཆོང་ལྡེའི་ཕྲ་རིང་དེ་སྦ་བཞག།"

#: background.label
msgid "Load links in the background"
msgstr "རྒྱབ་གཞི་ནང་གི་འབྲེལ་ལམ་ཚུ་མངོན་གསལ་འབད།"

#: loadGroup.label
msgid "When opening a bookmark group"
msgstr "དེབ་རྟགས་ཀྱི་སྡེ་ཚན་ཁ་ཕྱེ་བའི་སྐབས།"

#: loadGroupAppend.label
#: loadGroupAppend.accesskey
msgid "&Add tabs"
msgstr "མཆོང་ལྡེ་ཁ་སྐོང་རྐྱབས།(&A)"

#: loadGroupReplace.label
#: loadGroupReplace.accesskey
msgid "&Replace existing tabs"
msgstr "ཡོད་བཞིན་པའི་མཆོང་ལྡེ་ཚུ་ཚབ་བཙུགས།(&R)"

#: openTabs.label
msgid "Open tabs instead of windows for"
msgstr "དོན་ལུ་སྒོ་སྒྲིག་ཚུའི་ཚབ་ལུ་མཆོང་ལྡེ་ཚུ་ཁ་ཕྱེ།"

#: tabDisplay.label
msgid "Tab Display"
msgstr "མཆོང་ལྡེ་བཀྲམ་སྟོན་འབད།"

#: tabHeader.label
msgid "Tabbed Browsing"
msgstr "མཆོང་ལྡེ་འབད་མི་བརྡ་འཚོལ་འབད་དོ།"

