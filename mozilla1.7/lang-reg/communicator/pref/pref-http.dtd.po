#: lHeader
msgid "HTTP Networking"
msgstr "ཨེཆ་ཊི་ཊི་པི་ཡོངས་འབྲེལ་བཟོ་དོ།"

#: prefDirect.label
msgid "Direct Connection Options"
msgstr "ཐད་ཀར་མཐུད་ལམ་གྱི་གདམ་ཁ་ཚུ།"

#: prefEnableHTTP10.label
#: prefEnableHTTP10.accesskey
msgid "&Use HTTP 1.0"
msgstr "ཨེཆ་ཊི་ཊི་པི་༡.༠་ལག་ལེན་འཐབ།(&U)་"

#: prefEnableHTTP10Proxy.accesskey
msgid "S"
msgstr "S"

#: prefEnableHTTP11.label
#: prefEnableHTTP11.accesskey
msgid "Use &HTTP 1.1"
msgstr "ཨེཆ་ཊི་ཊི་པི་༡.༡ལག་ལེན་འཐབ།(&H)"

#: prefEnableHTTP11Proxy.accesskey
msgid "T"
msgstr "T"

#: prefEnableKeepAlive.label
#: prefEnableKeepAlive.accesskey
msgid "Enable &Keep-Alive"
msgstr "གསོན་པོར་བཞག་ནི་ལྕོགས་ཅན་བཟོ།(&K)"

#: prefEnableKeepAliveProxy.accesskey
msgid "L"
msgstr "L"

#: prefEnablePipelining.label
#: prefEnablePipelining.accesskey
msgid "Enable &Pipelining"
msgstr "རྒྱུད་དུང་འབྲེལ་ལམ་ལྕོགས་ཅན་བཟོ།(&P)"

#: prefEnablePipeliningProxy.accesskey
msgid "I"
msgstr "I"

#: prefPara
msgid "HTTP connections may be fine-tuned using these options to enhance either performance or compatibility. Some proxy servers, for example, are known to require HTTP/1.0 (see the release notes for details)."
msgstr "ཨེཆ་ཊི་ཊི་པི་གི་མཐུད་ལམ་ཚུ་འ་ནི་གདམ་ཁ་ཚུའི་ཡང་ཅིན་ལཱ་ཤུགས་དང་ཡང་ན་མཐུན་འགྱུར་གྱི་གོང་སྤེལ་ལག་ལེན་འཐབ་སྟེ་གདངས་ལེགས་ཤོམ་བཟོ་ཚུགསཔ་འོང་།དཔེ་འབད་བ་ཅིན་པོརོགསིའི་སར་བར་ལ་ལུ་ཅིག་ལུ་ཨེཆ་ཊི་ཊི་པི/ ༡.༠ དགོས་མཁོ་ཡོདཔ་ཤེས་དོ་བཟུམ།(རྒྱས་བཤད་ཀྱི་དོན་ལུ་དྲན་འཛིན་གསར་བཏོན་འབད་མི་ཚུ་བལྟ)"

#: prefPipeWarning
msgid "WARNING: pipelining is an experimental feature, designed to improve page-load performance, that is unfortunately not well supported by some web servers and proxies."
msgstr "ཉེན་བརྡ: རྒྱུད་དུང་གི་འབྲེལ་ལམ་བཟོ་ནི་དེ་མོ་བཏབ་ཀྱི་ཁྱད་རྣམ་ཅིག་ཨིན་ དེ་ཝེབ་ཀྱི་སར་བར་དང་པོ་རོགསི་ལ་ལུ་གིས་ཁ་རྗེ་མེད་པར་རྒྱབ་སྐྱོར་མ་འབད་མི་ ཤོག་ལེབ་ཀྱི་མངོན་གསལ་གྱི་ལཱ་ཤུགས་ཡར་དྲག་བཟོ་ནི་ལུ་བཀོད་བསྒྲིག་འབད་འབདཝ་ཚུ་ཨིན།"

#: prefProxy.label
msgid "Proxy Connection Options"
msgstr "པོ་རོགསིའི་མཐུད་ལམ་གྱི་གདམ་ཁ་ཚུ།"

#: rHeader
msgid "HTTP Network Preferences"
msgstr "ཨེཆ་ཊི་ཊི་པི་ཡོངས་འབྲེལ་གྱི་དགའ་གདམ་ཚུ།"

