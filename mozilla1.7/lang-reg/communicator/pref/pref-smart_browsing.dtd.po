#: autoCompleteAdvanced.label
#: autoCompleteAdvanced.accesskey
msgid "&Advanced..."
msgstr "མཐོ་རིམ...(&A)"

#: autoCompleteAdvanced.title
msgid "Autocomplete Preferences"
msgstr "རང་བཞིན་གྱིས་མཇུག་བསྡུ་བའི་དགའ་གདམ་ཚུ།"

#: autoCompleteAutoFill.label
msgid "Autocomplete best match as you type"
msgstr "ཁྱོད་ཀྱིས་ཡིག་དཔར་རྐྱབས་ད་ཅིག་ཁར་དྲག་ཤོས་ཀྱི་མཐུན་སྒྲིག་རང་བཞིན་གྱིས་མཇུག་བསྡུ།"

#: autoCompleteEnabled.label
#: autoCompleteEnabled.accesskey
msgid "Automatically complete text typed into &Location bar."
msgstr "ཚིག་ཡིག་གནས་ཁོངས་ཀྱི་ཕྲ་རིང་ནང་ལུ་ཡིག་དཔར་རྐྱབས་མི་ཚུ་རང་བཞིན་གྱིས་མཇུག་བསྡུ།(&L)"

#: autoCompleteHeader.label
msgid "Location Bar Autocomplete"
msgstr "གནས་ཁོངས་ཀྱི་ཕྲ་རིང་རང་བཞིན་གྱིས་མཇུག་བསྡུ།"

#: autoCompleteMatchOnlyTyped.label
msgid "Match only websites you've typed previously"
msgstr "ཁྱོད་ཀྱིས་ཧེ་མ་ཡིག་དཔར་རྐྱབ་རྐྱབས་ཝེབ་ཀྱི་ས་ཁོངས་ཚུ་རྐྱངམ་ཅིག་མཐུན་སྒྲིག་འབད།"

#: autoCompleteShowPopup.label
msgid "Show list of matching results"
msgstr "མཐུན་སྒྲིག་འབད་མི་གྲུབ་འབྲས་ཚུའི་ཐོ་ཡིག་སྟོན།"

#: autoCompleteShowSearch.label
msgid "Show internet search engine"
msgstr "ཨིན་ཊར་ནེཊི་འཚོལ་ཞིབ་ཀྱི་མ་འཕྲུལ་སྟོན།"

#: autocompletePreview.label
msgid "Preview"
msgstr "སྔོན་ལྟ།"

#: domainGuessingEnabled.label
msgid "Automatically add &quot;www.&quot; and &quot;.com&quot; to the location if a web page isn't found."
msgstr "ཝེབ་ཀྱི་ཤོག་ལེབ་དེ་འཚོལ་མ་འཐོབ་ཨིན་པ་ཅིན་ རང་བཞིན་གྱིས་&quot;www.&quot;དང་&quot;.com&quot;གནས་ཁོངས་ལུ་ཁ་སྐོང་རྐྱབས།"

#: domainGuessingHeader.label
msgid "Domain Guessing"
msgstr "ཌོ་མེན་གྱིས་ཕོ་ཚོད་དཔག་དོ།"

#: internetKeywordsDescription.label
msgid "&brandShortName; Internet Keywords enables fast access to services such as stock quotes, search and other information from the Location bar."
msgstr "&brandShortName;ཨིན་ཊར་ནེཊི་གི་གཙོ་ཚིག་ཚུ་གིས་གནས་ཁོངས་ཀྱི་ཕྲ་རིང་ནང་གི་ཞབས་ཏོག་ ཅ་མཛོད་ཀྱི་འདྲེན་ཚིག་བཀོད་ནི་ཚུ་ འཚོལ་ཞིབ་འབད་ནི་དང་ བརྡ་དོན་གཞན་མི་བཟུམ་ཚུ་མགྱོགས་པར་འཛུལ་སྤྱོད་འབད་ནིའི་དོན་ལུ་ལྕོགས་ཅན་བཟོཝ་ཨིན།"

#: internetKeywordsHeader.label
msgid "Internet Keywords"
msgstr "ཨིན་ཊར་ནེཊི་གི་གཙོ་ཚིག་ཚུ།"

#: keywordsEnabled.label
#: keywordsEnabled.accesskey
msgid "Enable Internet &Keywords"
msgstr "ཨིན་ཊར་ནེཊི་གི་གཙོ་ཚིག་ཚུ་ལྕོགས་ཅན་བཟོ།(&K)"

#: lHeader
msgid "Smart Browsing"
msgstr "བརྡ་འཚོལ་འབད་ནི་སྤུར་བསྒྱིར་དྲགས།"

#: moreInformation.label
#: moreInformation.accesskey
msgid "&More Information..."
msgstr "བརྡ་དོན་ཧེང་བཀལ...(&M)"

