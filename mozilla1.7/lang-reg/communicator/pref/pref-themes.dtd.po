#: author.label
msgid "Author"
msgstr "རྩོམ་པ་པོ།"

#: general.label
msgid "General"
msgstr "ཡོངས་ཁྱབ།"

#: getNewThemes.label
msgid "Get New Themes"
msgstr "བརྗོད་དོན་གསརཔ་ཚུ་ལེན།"

#: installedThemes.label
msgid "Theme to use:"
msgstr "བརྗོད་དོན་ལག་ལེན་འཐབ་ནི་དེ:"

#: lHeader
msgid "Themes"
msgstr "བརྗོད་དོན་ཚུ།"

#: preview.label
msgid "Preview Image"
msgstr "གཟུགས་བརྙན་སྔོན་ལྟ་འབད།"

#: selectSkin.label
#: selectSkin.accesskey
msgid "&Apply Theme"
msgstr "བརྗོད་དོན་འཇུག་སྤྱོད་འབད།(&A)"

#: skinIntro.label
msgid "Selecting a new theme changes the look of buttons, dialog boxes, menus, toolbars, and other items. It does not change the way these items function."
msgstr "བརྗོད་དོན་གསརཔ་སེལ་འཐུ་འབད་ནི་དེ་གཡེབ་རྟའི་བཟོ་རྣམ་ཚུ་ ཌའི་ལོག་གི་སྒྲོམ་ཚུ་ ལག་ཆས་ཕྲ་རིང་ཚུ་ དང་རྣམ་གྲངས་གཞན་མི་ཚུ་ བསྒྱུར་བཅོས་གཏངམ་ཨིན། འ་ནི་དེ་རྣམ་གྲངས་ཚུ་ལཱ་འབད་དོ་བཟུམ་སྦེ་བསྒྱུར་བཅོས་མི་འགྱོ།"

#: uninstallSkin.label
#: uninstallSkin.accesskey
msgid "&Uninstall Theme"
msgstr "བརྗོད་དོན་གཞི་བཙུགས་མ་འབད།(&U)"

