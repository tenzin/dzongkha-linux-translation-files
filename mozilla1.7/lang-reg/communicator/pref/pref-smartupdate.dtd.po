#: enableNotification.label
#: enableNotification.accesskey
msgid "&Notify me when new and updated software is available"
msgstr "གསརཔ་དང་དུས་མཐུན་བཟོ་ཡོད་པའི་མཉེན་ཆས་དེ་འཐོབ་ཚུགསཔ་ཡོདཔ་ད་ ང་ལུ་བརྡ་བསྐུལ་འབད།"

#: enableSU.label
#: enableSU.accesskey
msgid "&Enable software installation. (You will be prompted for each update.)"
msgstr "མཉེན་ཆས་གཞི་བཙུགས་འབད་ནི་འདི་ཚུ་ལྕོགས་ཅན་བཟོ(དུས་མཐུན་རེའི་དོན་ལུ་ཁྱོད་ནུས་སྤེལ་འབད་འོང་)།(&E)"

#: enableUN.label
#: enableUN.accesskey
msgid "&Check for updates:"
msgstr "དུས་མཐུན་གྱི་དོན་ལུ་ཞིབ་དཔྱད་འབད:(&C)"

#: lHeader
msgid "Software Installation"
msgstr "མཉེན་ཆས་གཞི་བཙུགས་འབད་ནི།"

#: monthly.label
#: monthly.accesskey
msgid "&monthly"
msgstr "ཟླ་རིམ་བཞིན།(&m)"

#: requireConfirmation.label
msgid "Require manual confirmation of each install"
msgstr "གཞི་བཙུགས་རེའི་ལག་དེབ་ངེས་གཏན་བཟོ་ནི་དགོས་མཁོ་ཡོདཔ་ཨིན།"

#: selectUninstall.label
msgid "To uninstall, select from the following list and click the uninstall button"
msgstr "གཞི་བཙུགས་མི་འབད་ནིའི་དོན་ལས་ འོག་གི་ཐོ་ཡིག་ནང་ལས་སེལ་འཐུ་འབད་དེ་ གཞི་བཙུགས་མི་འབད་ནིའི་གཡེབ་རྟ་གུ་ཨེབ་གཏང་།"

#: smartTitle.label
msgid "Manage Software Installations and Updates"
msgstr "མཉེན་ཆས་ཀྱི་གཞི་བཙུགས་འབད་ནི་ཚུ་དང་དུས་མཐུན་ཚུ་འཛིན་སྐྱོངས་འབད།"

#: uninstallButton.label
#: uninstallButton.accesskey
msgid "&Uninstall"
msgstr "གཞི་བཙུགས་མ་འབད།(&U)"

#: updateNotifications.desc
msgid "Check for new versions of this software.  No information about you or your computer will be revealed during this operation."
msgstr "འ་ནི་མཉེད་ཆས་ཀྱི་ཐོན་རིམ་གསར་པའི་དོན་ལུ་ཞིབ་དཔྱད་འབད། ཁྱོད་དང་ཁྱོད་རའི་གློག་རིག་གི་སྐོར་ལས་བཀོལ་སྤྱོད་འབད་བའི་སྐབས་བརྡ་དོན་ཚུ་ཕྱིར་གསལ་མི་འབད།"

#: updateNotifications.label
msgid "Update Notifications"
msgstr "དུས་མཐུན་ཡོད་པའི་བརྡ་བསྐུལ།"

#: weekly.label
#: weekly.accesskey
msgid "&weekly"
msgstr "བདུན་ཕྲག་ཨ་རྟག་རང་།(&w)"

