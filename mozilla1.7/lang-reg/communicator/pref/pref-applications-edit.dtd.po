#: application.label
#: application.accesskey
msgid "Open it &with:"
msgstr "དང་ཅིག་ཁར་ཁ་ཕྱེ:(&w)"

#: askBeforeOpen.label
#: askBeforeOpen.accesskey
msgid "Always as&k me before handling files of this type"
msgstr "འ་ནི་བཟུམ་མའི་ཡིག་སྣོད་ཀྱི་དབྱེ་བ་ལེགས་སྐྱོངས་མ་འཐབ་པའི་ཧེ་མ་ང་ལུ་འདྲི།(&k)"

#: browse.label
#: browse.accesskey
msgid "&Choose..."
msgstr "གདམ...(&C)"

#: description.label
#: description.accesskey
msgid "&Description:"
msgstr "འགྲེལ་བཤད...(&D)"

#: editType.label
msgid "Edit Type"
msgstr "ཞུན་དག་གི་དབྱེ།"

#: extension.label
#: extension.accesskey
msgid "&Extension:"
msgstr "རྒྱ་བསྐྱེད:(&E)"

#: handling.label
msgid "When a file of this type is encountered"
msgstr "འདི་བཟུམ་མའི་ཡིག་སྣོད་ཀྱི་དབྱེ་བ་དང་གདོང་ཐུག་བྱུང་ཡོདཔ་ད།"

#: mimetype.label
#: mimetype.accesskey
msgid "&MIME Type:"
msgstr "མ་ཡིམ་དབྱེ་བ:(&M)"

#: newType.label
msgid "New Type"
msgstr "དབྱེ་བ་གསརཔ།"

#: saveToDisk.label
#: saveToDisk.accesskey
msgid "&Save it to Disk"
msgstr "ཌིགསི་ནང་ལུ་སྲུང་བཞག་འབད།(&S)"

#: useDefault.label
#: useDefault.accesskey
msgid "&Open it using the default application"
msgstr "སྔོན་སྒྲིག་གི་འཇུག་སྤྱོད་ལག་ལེན་འཐབ་ཐོག་ཁ་ཕྱེ།(&O)"

