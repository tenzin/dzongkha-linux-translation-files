#: history.label
#: history.accesskey
msgid "Move back and forward in t&he browsing history"
msgstr "བརྡ་འཚོལ་འབད་ནིའི་སྔོན་རབས་ནང་ལུ་རྒྱབ་དང་གདོང་བཤུད་འབད།(&h)"

#: mouseWheelPanel.label
#: mouseWheelPanel.accesskey
msgid "Specify mouse &wheel behavior when used with this modifier key: "
msgstr "ལེགས་བཅོས་འཐབ་མི་ལྡེ་མིག་དང་གཅིག་ཁར་ལག་ལེན་འཐབ་པའི་སྐབས་ མའུསི་གི་འཁོར་ལོའི་སྤྱོད་ལམ་གསལ་བཀོད་འབད།"

#: mouseWheelTitle.label
msgid "Mouse wheel configuration"
msgstr "མའུསི་གི་འཁོར་ལོའི་རིམ་སྒྲིག།"

#: scroll.label
#: scroll.accesskey
msgid "&Scroll the document by "
msgstr "་གི་སྦེ་ཡིག་ཆ་བཤུད་སྒྲིལ་འབད།(&S)"

#: scrollLines.label
msgid " lines. "
msgstr "གྲལ་ཐིག་ཚུ།"

#: scrollPgUpPgDn.label
#: scrollPgUpPgDn.accesskey
msgid "Scroll a &page up or a page down"
msgstr "ཤོག་ལེབ་ཡར་ཡང་ན་ཤོག་ལེབ་མར་བཤུད་སྒྲིལ་འབད།(&p)"

#: textsize.label
#: textsize.accesskey
msgid "Make &the text larger or smaller"
msgstr "ཚིག་ཡིག་ཚུ་སྦོམ་མི་ཡང་ན་ཆུང་མི་བཟོ།(&t)"

#: title.label
msgid "Mouse Wheel"
msgstr "མའུསི་གི་འཁོར་ལོ།"

#: useSystemDefault.label
#: useSystemDefault.accesskey
msgid "Use s&ystem default"
msgstr "རིམ་ལུགས་ཀྱི་སྔོན་སྒྲིག་ལག་ལེན་འཐབ།(&y)"

#: usingJustTheWheel.label
msgid "No modifier key"
msgstr "ལེགས་བཅོས་འབད་མི་ལྡེ་མིག་མེད།"

#: usingWheelAndAlt.label
msgid "&altKey.label;"
msgstr "&altKey.label;"

#: usingWheelAndCtrl.label
msgid "&ctrlKey.label;"
msgstr "&ctrlKey.label;"

#: usingWheelAndShft.label
msgid "&shiftKey.label;"
msgstr "&shiftKey.label;"

