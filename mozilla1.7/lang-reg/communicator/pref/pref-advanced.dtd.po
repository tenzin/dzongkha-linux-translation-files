#: advancedTitle.label
msgid "Features that help interpret web pages"
msgstr "ཝེབ་ཀྱི་ཤོག་ལེབ་ཚུ་ཁ་བསྒྱུར་འབད་ནི་གྲོགས་རམ་འབད་མི་ཁྱད་རྣམ་ཚུ།"

#: autoLoadImgCheck.label
#: autoLoadImgCheck.accesskey
msgid "Automat&ically load images"
msgstr "གཟུགས་བརྙན་ཚུ་རང་བཞིན་གྱིས་མངོན་གསལ་འབདཝ་ཨིན།(&i)"

#: enableTurbo.label
msgid "Keep &brandShortName; in memory to improve startup performance"
msgstr "&brandShortName;དེ་ ལཱ་ཤུགས་འགོ་བཙུགས་ནི་ཡར་དྲག་བཟོ་ནིའི་དོན་ལུ་ དྲན་ཚད་ནང་བཞག།"

#: enableTurboCheck.accesskey
msgid "t"
msgstr "t"

#: enbJavaCheck.label
#: enbJavaCheck.accesskey
msgid "Enable &Java"
msgstr "ཇ་བ་ལྕོགས་ཅན་བཟོ།(&J)"

#: imageBlocking.label
#: imageBlocking.accesskey
msgid "Image &Blocking..."
msgstr "གཟུགས་བརྙན་བཀག་བཞག་དོ...(&B)"

#: jvm.configuration
msgid "Select Java version (change requires restart)"
msgstr "ཇ་བ་གི་ཐོན་རིམ་སེལ་འཐུ་འབད(བསྒྱུར་བཅོས་འབད་མི་ལུ་ལོག་འགོ་བཙུགས་ནི་དགོས་མཁོ་ཡོད)"

#: jvm.home
msgid "Path"
msgstr "འགྲུལ་ལམ།"

#: jvm.name
msgid "Version"
msgstr "ཐོན་རིམ།"

#: lHeader
msgid "Advanced"
msgstr "མཐོ་རིམ"

#: perfTitle.label
msgid "Quick Launch"
msgstr "འཕྲལ་མགྱོགས་གསར་བཙུགས།"

#: perfdescription.label
msgid "If you check this item, part of &brandShortName; will stay in memory when not in use, allowing it to start up faster."
msgstr "ཁྱོད་འ་ནི་རྣམ་གྲངས་དེ་ཞིབ་དཔྱད་འབདཝ་ཨིན་པ་ཅིན་ &brandShortName;གི་ཡན་ལག་དེ་ལག་ལེན་མ་འཐབ་པའི་སྐབས་མགྱོགས་སུ་སྦེ་འགོ་བཙུགས་བཅུག་སྟེ་དྲན་ཚད་ནང་ལུ་སྡོད་འོང་།"

#: sendAddFtpCheck.label
#: sendAddFtpCheck.accesskey
msgid "S&end this email address as anonymous FTP password:"
msgstr "འ་ནི་གློག་འཕྲིན་ཁ་བྱང་དེ་མིང་མེདཔ་ཨེཕ་ཊི་པི་ཆོག་ཡིག་སྦེ་གཏང་:"

#: systemPrefCaption.label
msgid "System Preferences"
msgstr "རིམ་ལུགས་དགའ་གདམ་ཚུ།"

#: systemPrefCheck.label
#: systemPrefCheck.accesskey
msgid "Use Preferences from &System"
msgstr "རིམ་ལུགས་ལས་དགའ་གདམ་ཚུ་ལག་ལེན་འཐབ།(&S)"

#: systemPrefDescription.label
msgid "Check this item to inherit preferences from the system. The system settings will override the &brandShortName; preferences"
msgstr "འ་ནི་རྣམ་གྲངས་དེ་རིམ་ལུགས་ལས་དགའ་གདམ་ཚུ་བརྒྱུད་དེ་ཐོབ་ནིའི་དོན་ལུ་ཞིབ་དཔྱད་འབད། རིམ་ལུགས་སྒྲིག་སྟངས་དེ་གིས་&brandShortName;གི་དགའ་གདམ་ཚུ་ཟུར་བཞག་འབད་འོང་།"

