#: autoRadio.label
#: autoRadio.accesskey
msgid "&When the page is out of date"
msgstr "ཤོག་ལེབ་དེ་ཚེས་གྲངས་ལས་འགལ་བ་ཨིན་པའི་སྐབས(&W)"

#: cachePara
msgid "The cache keeps copies of frequently visited web pages on your hard disk. (Clicking Reload always shows you the latest version of a page.)"
msgstr "འདྲ་མཛོད་ཀྱིས་ཁྱོད་རའི་ཧརཌི་ཌིཀསི་གུ་འཕྲལ་འཕྲལ་རང་བལྟ་སྤྱོད་འབད་འབད་བའི་ཝེབ་ཀྱི་ཤོག་ལེབ་ཚུའི་འདྲ་བཤུས་ཚུ་བཞགཔ་ཨིན། (ཡང་བསྐྱར་བཀལ་བཙུགས་ལུ་ཨེབ་གཏངམ་ད་ཨ་རྟག་རང་ཤོག་ལེབ་ཅིག་གི་མཐའ་མཇུག་ཐོན་རིམ་སྟོནམ་ཨིན།)"

#: cacheTitle.label
msgid "Set Cache Options"
msgstr "འདྲ་མཛོད་ཀྱི་གདམ་ཁ་ཚུ་གཞི་སྒྲིག་འབད།"

#: chooseDiskCacheFolder.label
#: chooseDiskCacheFolder.accesskey
msgid "C&hoose Folder..."
msgstr "སྣོད་འཛིན་གདམ་ཁ་རྐྱབས...(&h)"

#: chooseFolder.label
#: chooseFolder.accesskey
msgid "Choose &Folder..."
msgstr "སྣོད་འཛིན་གདམ་ཁ་རྐྱབས...(&F)"

#: clearDiskCache.label
#: clearDiskCache.accesskey
msgid "C&lear Cache"
msgstr "འདྲ་མཛོད་བསལ་བ(&l)"

#: diskCache.label
#: diskCache.accesskey
msgid "&Cache:"
msgstr "འདྲ་མཛོད:(&C)"

#: diskCacheFolder.label
msgid "Cache Folder:"
msgstr "སྣོད་འཛིན་གྱི་འདྲ་མཛོད:"

#: diskCacheFolderExplanation
msgid "Cache files will be stored in a subfolder named &quot;Cache&quot; of the directory you specify. Restart &brandShortName; for changes to take effect."
msgstr ""
"འདྲ་མཛོད་ཀྱི་ཡིག་སྣོད་དེ་ཚུ་ཁྱོད་ཀྱི་གསལ་བཀོད་འབད་མི་སྣོད་ཐོའི་སྣོད་འཛིན་ཡན་ལག་མིང་&quot;Cache&quot;འབད་མི་ནང་ལུ་སྲུང་བཞག་འབད་བཞགཔ་ཨིན། "
"བསྒྱུར་བཅོས་ཚུ་འབྱུང་ནིའི་དོན་ལུ་ &brandShortName;ལོག་འགོ་བཙུགས།"

#: docCache
msgid "Compare the page in the cache to the page on the network:"
msgstr "འདྲ་མཛོད་ནང་གི་ཤོག་ལེབ་དེ་ཡོངས་འབྲེལ་གུའི་ཤོག་ལེབ་དང་ག་བསྡུར་བརྐྱབ:"

#: enablePrefetch.label
#: enablePrefetch.accesskey
msgid "&Prefetch web pages when idle, so that links in web pages designed for prefetching can load faster."
msgstr "ཝེབ་ཤོག་ལེབ་ཚུ་ལས་མེད་སྐབས་སྔོན་ལེན་འབད། འ་ནི་དེ་ཝེབ་ཤོག་ལེབ་ནང་གི་འབྲེལ་མཐུད་སྔོན་ལེན་འབད་ནིའི་དོན་ལུ་བཀོད་བསྒྲིག་འབད་མི་ཚུ་མངོན་གསལ་མགྱོགས་སུ་སྦེ་འབད་ཚུགས།"

#: everyTimeRadio.label
#: everyTimeRadio.accesskey
msgid "&Every time I view the page"
msgstr "ཨ་རྟག་རང་ང་གིས་ཤོག་ལེབ་སྟོནམ་ཨིན།(&E)"

#: lHeader
msgid "Cache"
msgstr "འདྲ་མཛོད།"

#: mbytes
msgid "MB"
msgstr "ཨེམ་བི།"

#: neverRadio.label
#: neverRadio.accesskey
msgid "&Never"
msgstr "ནམ་ཡང་།(&N)"

#: oncePsessionRadio.label
#: oncePsessionRadio.accesskey
msgid "&Once per session"
msgstr "ལཱ་ཡུན་རེ་ལུ་ཚར་རེ(&O)"

#: prefetchTitle.label
msgid "Link Prefetching"
msgstr "འབྲེལ་མཐུད་སྔོན་ལེན་འབད་དོ།"

