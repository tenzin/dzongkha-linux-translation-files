#: activeLinkColor.label
#: activeLinkColor.accesskey
msgid "A&ctive Links:"
msgstr "ཤུགས་ལྡན་གྱི་འབྲེལ་མཐུད་ཚུ:(&c)"

#: alwaysUseDocumentColors.label
#: alwaysUseDocumentColors.accesskey
msgid "&Always use the colors and background specified by the web page"
msgstr "ཝེབ་ཤོག་ལེབ་ཀྱིས་གསལ་བཀོད་འབད་མི་ཚོས་གཞི་ཚུ་དང་རྒྱབ་གཞི་ཨ་རྟག་རང་ལག་ལེན་འཐབ།(&A)"

#: backgroundColor.label
#: backgroundColor.accesskey
msgid "&Background:"
msgstr "རྒྱབ་གཞི:(&B)"

#: color
msgid "Text and Background"
msgstr "ཚིག་ཡིག་དང་རྒྱབ་གཞི།"

#: lHeader
msgid "Colors"
msgstr "ཚོས་གཞི་ཚུ"

#: linkColor.label
#: linkColor.accesskey
msgid "Unvisited &Links:"
msgstr "བལྟ་སྤྱོད་མ་འབད་བའི་འབྲེལ་ལམ་ཚུ་:(&L)"

#: links
msgid "Link Colors"
msgstr "འབྲེལ་མཐུད་ཀྱི་ཚོས་གཞི་ཚུ།"

#: someProvColors
msgid "When a web page provides its own colors and background"
msgstr "ཝེབ་ཤོག་ལེབ་ཀྱིས་བདག་དབང་གི་ཚོས་གཞི་ཚུ་དང་རྒྱབ་གཞི་བྱིནམ་ད།"

#: textColor.label
#: textColor.accesskey
msgid "&Text:"
msgstr "ཚིག་ཡིག:(&T)"

#: underlineLinks.label
#: underlineLinks.accesskey
msgid "&Underline links"
msgstr "འབྲེལ་ལམ་ཚུ་འོག་ཐིག་རྐྱབས།(&U)"

#: useMyColors.label
#: useMyColors.accesskey
msgid "Use my chosen colors, &ignoring the colors and background image specified"
msgstr "ཚོས་གཞི་ཚུ་དང་རྒྱབ་གཞིའི་གཟུགས་བརྙན་གསལ་བཀོད་འབད་མི་ཚུ་སྣང་མེད་བཞག་ཞིནམ་ལས་ང་གིས་གདམ་ཁ་རྐྱབས་ཏེ་ཡོད་པའི་ཚོས་གཞི་ཚུ་ལག་ལེན་འཐབ།(&i)"

#: useSystemColors.label
#: useSystemColors.accesskey
msgid "U&se system colors"
msgstr "རིམ་ལུགས་ཀྱི་ཚོས་གཞི་ཚུ་ལག་ལེན་འཐབ།(&s)"

#: visitedLinkColor.label
#: visitedLinkColor.accesskey
msgid "&Visited Links:"
msgstr "བལྟ་སྤྱོད་འབད་འབད་བའི་འབྲེལ་ལམ་ཚུ:(&V)"

