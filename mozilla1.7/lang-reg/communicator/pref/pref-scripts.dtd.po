#: allowContextmenuDisable.label
msgid "Disable or replace context menus"
msgstr "སྐབས་འབབ་ཀྱི་དཀར་ཆག་ཚུ་ལྕོགས་མིན་བཟོ་ནི་དང་ཡང་ན་ཚབ་བཙུགས།"

#: allowHideStatusBar.label
msgid "Hide the status bar"
msgstr "གནས་ཚད་ཀྱི་ཕྲ་རིང་དེ་སྦ་བཞག།"

#: allowScripts.label
msgid "Allow scripts to:"
msgstr "ཡིག་ཚུགས་ཚུ་ འབད་བཅུག:"

#: allowWindowFlip.label
msgid "Raise or lower windows"
msgstr "སྒོ་སྒྲིག་ཚུ་ཆེར་བསྐྱེད་ཡང་ན་མར་ཕབ་འབད།"

#: allowWindowImageSrcChange.label
msgid "Change images"
msgstr "གཟུགས་བརྙན་ཚུ་བསྒྱུར་བཅོས་འབད།"

#: allowWindowMoveResize.label
msgid "Move or resize existing windows"
msgstr "སྒོ་སྒྲིག་ཧེ་མ་ལས་ཡོད་མི་ཚུ་བཤུད་ནི་དང་ཡང་ན་ཚད་བསྐྱར་བཟོ་འབད།"

#: allowWindowOpen.label
msgid "Open unrequested windows"
msgstr "ཞུ་བ་མ་འབད་བའི་སྒོ་སྒྲིག་ཚུ་ཁ་ཕྱེ།"

#: allowWindowStatusChange.label
msgid "Change status bar text"
msgstr "གནས་ཚད་ཕྲ་རིང་གི་ཚིག་ཡིག་ཚུ་སོར།"

#: enableJavaScript.label
msgid "Enable JavaScript for"
msgstr "གི་དོན་ལུ་ཇ་བ་ཡིག་ཚུགས་ལྕོགས་ཅན་བཟོ།"

#: enablePlugin.label
msgid "Enable Plug-ins for"
msgstr "གི་དོན་ལུ་ པ་ལག་ཨིནསི་ ཚུ་ལྕོགས་ཅན་བཟོ།"

#: lHeader
msgid "Scripts &amp; Plug-ins"
msgstr "ཚིག་ཡིག་ཚུའི་ &amp; པ་ལག་ཨིནསི།"

#: navigator.label
#: navigator.accesskey
msgid "Navigato&r"
msgstr "འགྲུལ་བསྐྱོད་པ།(&r)"

