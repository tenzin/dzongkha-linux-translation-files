#: charset.customize.add.label
msgid "Add"
msgstr "ཁ་སྐོང་རྐྱབས།"

#: charset.customize.additional.label
msgid "Available Character Encodings:"
msgstr "འཐོབ་ཚུགས་པའི་ཡིག་འབྲུའི་ཨིན་ཀོ་ཌིང་ཚུ:"

#: charset.customize.cancel.label
msgid "Cancel"
msgstr "ཆ་མེད་གཏང་།"

#: charset.customize.current.label
msgid "Active Character Encodings:"
msgstr "ཤུགས་ལྡན་ཡིག་འབྲུའི་ཨིན་ཀོ་ཌིང་ཚུ:"

#: charset.customize.left.header
msgid "Character Encoding"
msgstr "ཡིག་འབྲུའི་ཨིན་ཀོ་ཌིང་།"

#: charset.customize.moveDown.label
#: charset.customize.moveDown.accesskey
msgid "Move &Down"
msgstr "མར་བཤུད།(&D)"

#: charset.customize.moveUp.label
#: charset.customize.moveUp.accesskey
msgid "Move &Up"
msgstr "ཡར་བཤུད(&U)"

#: charset.customize.remove.label
msgid "Remove"
msgstr "རྩ་བསྐྲད་གཏང་།"

#: charset.customize.reorder.label
msgid "Reorder"
msgstr "གོ་རིམ་བསྐྱར་སྒྲིག་འབད།"

#: charset.customize.right.header
msgid "Select Supported Character Encodings"
msgstr "རྒྱབ་སྐྱོར་ཡོད་པའི་ཡིག་འབྲུའི་ཨིན་ཀོ་ཌིང་ཚུ་སེལ་འཐུ་འབད།"

#: charset.customize.save.label
msgid "Save"
msgstr "སྲུང་།"

#: charset.customize.save.popup
msgid "Please restart the browser for changes to take effect..."
msgstr "བསྒྱུར་བཅོས་ཚུ་ལུ་ཕན་གནོད་འབྱུང་ནིའི་དོན་ལུ་བརའུ་ཟར་དེ་ལོག་འགོ་བཙུགས་གནང་..."

#: charset.customize.title.label
msgid "Customize Character Encoding"
msgstr "ཡིག་འབྲུའི་ཨིན་ཀོ་ཌིང་སྲོལ་སྒྲིག་འབད།"

