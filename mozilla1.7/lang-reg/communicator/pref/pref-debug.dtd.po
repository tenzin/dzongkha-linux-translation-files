#: acceleratorKey.label
#: acceleratorKey.accesskey
msgid "&Accelerator key:"
msgstr "མགྱོགས་འཕྲུལ་་ལྡེ་མིག:(&A)"

#: accelerators.label
msgid "Keyboard accelerators"
msgstr "ལྡེ་སྒྲོམ་གྱི་མགྱོགས་འཕྲུལ་ཚུ།"

#: acceleratorsBlurb.label
msgid "A value of zero disables a modifier key."
msgstr "ཀླད་ཀོར་གྱི་གནས་གོང་གིས་ལེགས་བཅོས་འབད་མི་ལྡེ་མིག་དེ་ལྕོགས་མིན་བཟོཝ་ཨིན།"

#: debugConsoleJavascript.label
msgid "Show chrome JavaScript errors and warnings"
msgstr "ཀོ་རོམ་ཇ་བ་ཡིག་ཚུགས་ཀྱི་འཛོལ་བ་ཚུ་དང་ཉེན་བརྡ་ཚུ་སྟོན།"

#: debugEnableDump.label
msgid "Enable JavaScript dump() output"
msgstr "ཇ་བ་ཡིག་ཚུགས་བཀོག་ནིའི་()ཨའུཊི་པུཊི་ལྕོགས་ཅན་བཟོ།"

#: debugMiscellaneous.label
msgid "Miscellaneous"
msgstr "སྣ་ཚོགས"

#: debugStrictJavascript.label
msgid "Show strict JavaScript warnings"
msgstr "ཇ་བ་ཡིག་ཚུགས་ཀྱི་ཉེན་བརྡ་བཙན་པོ་ཚུ་སྟོན།"

#: debugUseXBLForms.label
msgid "Use XBL-based form controls (takes effect after restart). These are still under construction."
msgstr "ཨེགསི་བི་ཨེལ་-གཞི་རྟེན་གྱི་འབྲི་ཤོག་ཚད་འཛིན་ཚུ་(ལོག་འགོ་བཙུགས་པའི་ཤུལ་ལས་ཕན་གནོད་བྱུངམ་ཨིན)ལག་ལེན་འཐབ། འ་ནི་ཚུ་བཟོ་བསྐྲུན་འབད་བའི་བསྒང་ཨིན།"

#: debugXULBox.label
msgid "debug XUL boxes"
msgstr "ཨེགསི་ཡུ་ཨེལ་སྒྲོམ་ཚུའི་རྐྱེན་སེལ་འབད།"

#: lHeader
msgid "Debug"
msgstr "རྐྱེན་སེལ།"

#: menuAccessKey.label
#: menuAccessKey.accesskey
msgid "&Menu access key:"
msgstr "འཛུལ་སྤྱོད་ལྡེ་མིག་གི་དཀར་ཆག:(&M)"

#: printUseGlobalPrintSettings.label
msgid "Use Global Print Settings"
msgstr "སྤྱི་ཁྱབ་དཔར་བསྐྲུན་འབད་ནིའི་སྒྲིགས་སྟངས་ཚུ་ལག་ལེན་འཐབ།"

#: rHeader
msgid "General debugging preferences"
msgstr "ཡོངས་ཁྱབ་རྐྱེན་སེལ་འབད་ནིའི་དགའ་གདམ་ཚུ།"

#: useNativePrintDialog.label
msgid "Use Native Print Dialog (where supported)"
msgstr "རང་ཡུལ་གྱི་དཔར་བསྐྲུན་ཌའི་ལོག་(ག་སྟེ་རྒྱབ་སྐྱོར་འབད་ས)ལག་ལེན་འཐབ།"

#: widgetRendering.label
msgid "Rendering"
msgstr "ལྷག་སྟོན།"

