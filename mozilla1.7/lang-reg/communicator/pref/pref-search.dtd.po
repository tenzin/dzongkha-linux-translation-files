#: defaultSearchEngine.label
#: defaultSearchEngine.accesskey
msgid "Search &using:"
msgstr "ལག་ལེན་འཐབ་ཐོག་ འཚོལ་ཞིབ་འབད:(&u)"

#: lHeader
msgid "Internet Search"
msgstr "ཨིན་ཊར་ནེཊི་གི་འཚོལ་ཞིབ།"

#: legendHeader
msgid "Default Search Engine"
msgstr "སྔོན་སྒྲིག་འཚོལ་ཞིབ་འབད་ནིའི་མ་འཕྲུལ།"

#: openSidebarSearchPanel.label
#: openSidebarSearchPanel.accesskey
msgid "&Open the Search tab in the Sidebar when search results are available"
msgstr "འཚོལ་ཞིབ་ཀྱི་གྲུབ་འབྲས་ཚུ་འཐོབ་ཚུགསཔ་སྦེ་ཡོདཔ་ད་ཟུར་ཁའི་ཕྲ་རིང་ནང་གི་འཚོལ་ཞིབ་ཀྱི་མཆོང་ལྡེ་དེ་ཁ་ཕྱེ།(&O)"

#: searchModeAdvanced.label
#: searchModeAdvanced.accesskey
msgid "&Advanced"
msgstr "མཐོ་རིམ(&A)"

#: searchModeBasic.label
#: searchModeBasic.accesskey
msgid "&Basic"
msgstr "གཞི་རྩ།(&B)"

#: searchResults.label
msgid "Search Results"
msgstr "འཚོལ་ཞིབ་ཀྱི་འགྲུབ་འབྲས་ཚུ།"

#: sidebarSearchTabPref.label
msgid "Sidebar Search Tab Preference"
msgstr "ཟུར་ཁའི་ཕྲ་རིང་འཚོལ་ཞིབ་ཀྱི་མཆོང་ལྡེའི་དགའ་གདམ།"

