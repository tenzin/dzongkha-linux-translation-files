#: bmp.label
#: bmp.accesskey
msgid "&BMP images"
msgstr "བི་ཨེམ་པི་ གཟུགས་བརྙན་ཚུ།(&B)"

#: chrome.label
#: chrome.accesskey
msgid "chrome: (&Chrome Protocol)"
msgstr "ཀོ་རོམ:(ཀོ་རོམ་ གནས་སྤེལ་ལམ་ལུགས)(&C)"

#: files.label
msgid "File Types"
msgstr "ཡིག་སྣོད་ཀྱི་དབྱེ་བ།"

#: files.text
msgid "Windows should use &brandShortName; to open these file types:"
msgstr "སྒོ་སྒྲིག་ཚུ་གིས་&brandShortName;འ་ནི་ཡིག་སྣོད་ཀྱི་དབྱེ་བ་ཚུ་ཁ་ཕྱེ་ནིའི་དོན་ལས་ལག་ལེན་འཐབ་དགོ:"

#: ftp.label
#: ftp.accesskey
msgid "ftp: (&File Transfer Protocol)"
msgstr "ཨེཕ་ཊི་པི:(ཡིག་སྣོད་གནས་སོར་གྱི་གནས་སྤེལ་ལམ་ལུགས)(&F)"

#: gif.label
#: gif.accesskey
msgid "&GIF images"
msgstr "ཇི་ཨའི་ཨེཕ་ གཟུགས་བརྙན་ཚུ།(&G)"

#: gopher.label
#: gopher.accesskey
msgid "gopher: (&Gopher Protocol)"
msgstr "གོ་ཕར:(གོ་ཕར་གནས་སྤེལ་ལམ་ལུགས)(&G)"

#: html.label
#: html.accesskey
msgid "&HTML documents"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ ཡིག་ཆ་ཚུ།(&H)"

#: http.label
#: http.accesskey
msgid "http: (&Hypertext Transfer Protocol)"
msgstr "ཨེཆ་ཊི་ཊི་པི་:(ཧའི་པར་ཚིག་ཡིག་གནས་སོར་གྱི་གནས་སྤེལ་ལམ་ལུགས)(&H)"

#: https.label
#: https.accesskey
msgid "https: (&Secure HTTP)"
msgstr "ཨེཆ་ཊི་ཊི་པི་ཨེསི:(མཐའ་བཙན་ཨེཆ་ཊི་ཊི་པི)(&S)"

#: ico.label
#: ico.accesskey
msgid "&ICO images"
msgstr "ཨའི་སི་ཨོ་ གཟུགས་བརྙན་ཚུ།(&I)"

#: jpeg.label
#: jpeg.accesskey
msgid "&JPEG images"
msgstr "ཇེ་པི་ཨི་ཇི་ གཟུགས་བརྙན་ཚུ།(&J)"

#: mng.label
#: mng.accesskey
msgid "&MNG images"
msgstr "ཨེམ་ཨེན་ཇི་ གཟུགས་བརྙན་ཚུ།(&M)"

#: png.label
#: png.accesskey
msgid "&PNG images"
msgstr "པི་ཨེན་ཇི་ གཟུགས་བརྙན་ཚུ།(&P)"

#: shortcuts.label
msgid "Internet Shortcuts"
msgstr "ཨིན་ཊར་ནེཊི་གི་མགྱོགས་ཐབས་ཚུ།"

#: shortcuts.text
msgid "Windows should use &brandShortName; to handle these protocols:"
msgstr "སྒོ་སྒྲིག་ཚུ་གིས་འ་ནི་གནས་སྤེལ་ལམ་ལུགས་དེ་ལེགས་སྐྱོངས་འཐབ་ནིའི་དོན་ལས་&brandShortName;ལག་ལེན་འཐབ་དགོ:"

#: showDialog.label
#: showDialog.accesskey
msgid "&Alert me if other applications change these settings"
msgstr "གློག་རིམ་གཞན་མི་ཚུ་གིས་འ་ནི་སྒྲིག་སྟངས་ཚུ་བསྒྱུར་བཅོས་འབདཝ་ཨིན་པ་ཅིན་ ང་ལུ་དྲན་བརྡ་འབད།(&A)"

#: title.label
msgid "System"
msgstr "རིམ་ལུགས།"

#: xbm.label
#: xbm.accesskey
msgid "XBM im&ages"
msgstr "ཨེགསི་བི་ཨེམ་ གཟུགས་བརྙན་ཚུ།(&a)"

#: xhtml.label
#: xhtml.accesskey
msgid "XH&TML documents"
msgstr "ཨེགསི་ཨེཆ་ཊི་ཨེམ་ཨེལ་ ཡིག་ཆ་ཚུ།(&T)"

#: xml.label
#: xml.accesskey
msgid "&XML documents"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ ཡིག་ཆ་ཚུ།(&X)"

#: xul.label
#: xul.accesskey
msgid "X&UL documents"
