#: findAsYouTypeAutoLinks.label
#: findAsYouTypeAutoLinks.accesskey
msgid "Links &only"
msgstr "འབྲེལ་ལམ་རྐྱངམ་ཅིག(&o)"

#: findAsYouTypeAutoText.label
#: findAsYouTypeAutoText.accesskey
msgid "&Any text in the page"
msgstr "ཤོག་ལེབ་ནང་གི་ཚིག་ཡིག་ག་འབད་རུང་(&A)"

#: findAsYouTypeBehavior.label
msgid "Find As You Type"
msgstr "ཡིག་དཔར་རྐྱབས་ད་ཅིག་ཁར་འཚོལ་ཞིབ་འབད།"

#: findAsYouTypeEnableAuto.label
#: findAsYouTypeEnableAuto.accesskey
msgid "Find &automatically when typing within a web page:"
msgstr "ཝེབ་ཀྱི་ཤོག་ལེབ་ནང་འཁོད་ཡིག་དཔར་རྐྱབས་ད་ཅིག་ཁར་རང་བཞིན་གྱིས་འཚོལ་ཞིབ་འབད།:(&a)"

#: findAsYouTypeSound.label
#: findAsYouTypeSound.accesskey
msgid "&Play a sound when typed text isn't found"
msgstr "ཡིག་དཔར་རྐྱབ་རྐྱབས་ཀྱི་ཚིག་ཡིག་དེ་འཚོལ་མ་ཐོབ་ད་སྒྲ་སྐད་གཅིག་གཏང་།(&P)"

#: findAsYouTypeTimeout.label
#: findAsYouTypeTimeout.accesskey
msgid "&Clear the current search after a few seconds of inactivity"
msgstr "ལཱ་མེད་པའི་སྐར་ཆ་དག་པ་ཅིག་གི་ཤུལ་ལས་ད་ལྟོའི་འཚོལ་ཞིབ་འབད་མི་ཚུ་བསལ་གཏང་།(&C)"

#: findAsYouTypeTip.label
msgid "Tip: To manually start Find As You Type, type / to find text or ' to find links, followed by the text you want to find."
msgstr "ཕན་བསླབ: ཡིག་དཔར་རྐྱབས་ད་ཅིག་ཁར་འཚོལ་ ཟེར་མི་དེ་ལག་ཐོག་ལས་འགོ་བཙུགས་ནིའི་དོན་ལས་ ཚིག་ཡིག་འཚོལ་ནི་ཡང་ན་འབྲེལ་ལམ་འཚོལ་ནི་ཚུ་གིས་རྗེས་སུ་འབྲང་སྟེ་ཁྱོད་ཀྱི་འཚོལ་ནི་ཨིན་མི་ཚིག་ཡིག་ཚུ་/ཡིག་དཔར་རྐྱབས།"

#: lHeader
msgid "Keyboard Navigation"
msgstr "ལྡེ་སྒྲོམ་འགྲུལ་བསྐྱོད།"

#: tabNavigationBehavior.label
msgid "Tab Key Navigation"
msgstr "མཆོང་ལྡེ་ལྡེ་མིག་གི་འགྲུལ་བསྐྱོད།"

#: tabNavigationDesc.label
msgid "When Tab or Shift+Tab is pressed, move between:"
msgstr "མཆོང་ལྡེ་ཡང་ན་སོར་ལྡེ+མཆོང་ལྡེ་དེ་གཡེབ་པའི་སྐབས་ བར་ན་ལུ་བཤུད:"

#: tabNavigationForms.label
#: tabNavigationForms.accesskey
msgid "&Buttons, radio buttons, checkboxes, and lists"
msgstr "གཡེབ་རྟ་ཚུ་ རེ་ཌིའོ་གཡེབ་རྟ་ཚུ་ དཔྱད་སྒྲོམ་ཚུ་ དང་ཐོ་ཡིག་ཚུ།(&B)"

#: tabNavigationLinks.label
#: tabNavigationLinks.accesskey
msgid "&Links"
msgstr "འབྲེལ་ལམ་ཚུ།(&L)"

#: tabNavigationTextboxes.label
msgid "Note: text boxes are always part of the tabbing order."
msgstr "དྲན་འཛིན: ཚིག་ཡིག་གི་སྒྲོམ་འདི་ཚུ་ཨ་རྟ་རང་མཆོང་ལྡེའི་གོ་རིམ་གྱི་ཡན་ལག་ཨིན།"

