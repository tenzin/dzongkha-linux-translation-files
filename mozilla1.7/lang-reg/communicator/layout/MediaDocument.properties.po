#: ImageTitleWithNeitherDimensionsNorFile
msgid "(%S Image)"
msgstr "(%Sགཟུགས་བརྙན)"

#: MediaTitleWithFile
msgid "%S (%S Object)"
msgstr "%S(%Sདངོས་པོ)"

#: ImageTitleWithoutDimensions
msgid "%S (%S Image)"
msgstr "%S(%Sགཟུགས་བརྙན)"

#: TitleWithStatus
msgid "%S - %S"
