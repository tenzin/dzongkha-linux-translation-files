#: LoadingError
msgid "Error loading stylesheet\\: %S"
msgstr "སི་ཊ་ཡེལ་ཤིཊི་མངོན་གསལ་འབད་ནིའི་འཛོལ་བ་\\:%S"

#: TransformError
msgid "Error during XSLT transformation\\: %S"
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་བཟོ་བསྒྱུར་འབད་བའི་སྐབས་འཛོལ་བ་\\:%S"

#: 19
msgid "XPath parse failure\\: invalid variable name\\:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འབད་ནི་འཐུས་ཤོར\\:ནུས་མེད་འགྱུར་ཅན་གྱི་མིང་\\:"

#: 18
msgid "XPath parse failure\\: ']' expected\\:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འབད་ནི་འཐུས་ཤོར\\:']'རེ་བ་བསྐྱེད་བསྐྱེདཔ\\:"

#: 17
msgid "XPath parse failure\\: Name or Nodetype test expected\\:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འབད་ནི་འཐུས་ཤོར\\:མིང་ཡང་ན་མཐུད་མཚམས་ཀྱི་དབྱེ་བའི་བརྟག་ཞིབ་རེ་བ་བསྐྱེད་བསྐྱེདཔ\\:"

#: 16
msgid "XPath parse failure\\: invalid axis\\:"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་མིང་དཔྱད་འབད་ནི་འཐུས་ཤོར\\:ནུས་མེད་ཚད་ཐིག\\:"

#: 15
msgid "XPath parse failure\\: ')' expected\\:"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་མིང་དཔྱད་འབད་ནི་འཐུས་ཤོར\\:')'རེ་བ་བསྐྱེད་བསྐྱེདཔ\\:"

#: 14
msgid "An unknown XPath extension function was called."
msgstr "མ་ཤེས་པའི་ཨེགསི་འགྲུལ་ལམ་རྒྱ་བསྐྱེད་ཀྱི་ལས་འགན་ཅིག་བོད་བརྡ་འབད་ནུག།"

#: 13
msgid "An XPath function was called with the wrong number of arguments."
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་ལས་འགན་གཅིག་གྲངས་ཕྱི་འགྱུར་འབད་མི་སྒྲུབ་རྟགས་དང་གཅིག་ཁར་བོད་བརྡ་འབད་ནུག"

#: 12
msgid "An XSLT stylesheet directly or indirectly imports or includes itself\\:"
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་སི་ཊཡེལ་ཤིཊི་གིས་ཁོ་རང་ཐད་ཀར་དུ་ཡང་ན་ཐད་ཀར་མིན་པར་ནང་འདྲེན་ཡང་ན་གྲངས་སུ་བཙུགསཔ་ཨིན།\\:"

#: 11
msgid "An XSLT stylesheet does not have an XML mimetype\\:"
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ སི་ཊཡེལ་ཤིཊི་ ལུ་ཨེགསི་ཨེམ་ཨེལ་ མ་ཡིམ་དབྱེ་བ་མེད།\\:"

#: 10
msgid "A network error occured loading an XSLT stylesheet\\:"
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ སི་ཊཡེལ་ཤིཊི་ མངོན་གསལ་འབད་བའི་སྐབས་ཡོངས་འགྲེལ་ལུ་འཛོལ་བ་བྱུང་ནུག\\:"

#: 9
msgid "XSLT transformation was terminated by <xsl\\:message>."
msgstr "<xsl\\:message>གྱིས་ཨེགསི་ཨེསི་ཨེལ་ཊི་ བཟོ་བསྒྱུར་དེ་རྩ་གྲོལ་གཏང་ནུག།"

#: 8
msgid "An XPath expression was expected to return a NodeSet."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ གསལ་བརྗོད་དེ མཐུད་མཚམས་གཞི་སྒྲིག་སླར་ལོག་ནི་ལུ་རེ་བ་བསྐྱེད་ནུག།"

#: 7
msgid "Attribute value illegal in XSLT 1.0."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་༡.༠་ནང་ཁྱད་ཆོས་ཀྱི་གནས་གོང་ཁྲིམས་འགལ།"

#: 6
msgid "XSLT Stylesheet (possibly) contains a recursion."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ སི་ཊཡེལ་ཤིཊི་(སྲིད)ནང་ན་སླར་བྱུང་མི་གཅིག་ཡོད།"

#: 27
msgid "An XSLT stylesheet load was blocked for security reasons."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ སི་ཊཡེལ་ཤིཊི་མངོན་གསལ་འབད་མི་དེ་མཐའ་བཙན་རྒྱུ་མཚན་ཚུ་གྱི་དོན་ལུ་བཀག་བཞག་ནུག།"

#: 5
msgid "Invalid XSLT/XPath function."
msgstr "ནུས་མེད་ཨེགསི་ཨེསི་ཨེལ་ཊི་/ཨེགསི་འགྲུལ་ལམ་གྱི་ལས་འགན།"

#: 26
msgid "XPath parse failure\\: binary operator expected\\:"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་མིང་དཔྱད་འབད་ནི་འཐུས་ཤོར་\\:ཟུང་ལྡན་གྱི་བཀོལ་སྤྱོད་པ་རེ་བ་བསྐྱེད་ཡོདཔ\\:"

#: 4
msgid "XSLT transformation failed."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་བཟོ་བསྒྱུར་འཐུས་ཤོར་བྱུང་ཡོདཔ།"

#: 25
msgid "XPath parse failure\\: illegal character found\\:"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་མིང་དཔྱད་འཐུས་ཤོར་\\:ཁྲིམས་འགལ་གྱི་ཡིག་འབྲུ་ཐོབ་ཡོདཔ\\:"

#: 24
msgid "XPath parse failure\\: '\\!' unexpected, negation is not()\\:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འཐུས་ཤོར་\\:'\\!'རེ་བ་མེད་ དགག་ཆ་དེ་()\\:"

#: 2
msgid "Parsing an XPath expression failed."
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་གསལ་བརྗོད་མིང་དཔྱད་འབད་ནི་འཐུས་བྱུང་ཡོདཔ།"

#: 23
msgid "XPath parse failure\\: '\\:' unexpected\\:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འབད་ནི་འཐུས་ཤོར་\\:'\\:'རེ་བ་མེདཔ\\:"

#: 1
msgid "Parsing an XSLT stylesheet failed."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ སི་ཊཡེལ་ཤིཊི་མིང་དཔྱད་འབད་ནི་འཐུས་ཤོར་བྱུང་ཡོདཔ།"

#: 22
msgid "XPath parse failure\\: unclosed literal\\:"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་མིང་དཔྱད་འཐུས་ཤོར་\\:ཁ་མ་བསྡམ་པའི་ཐད་དོན\\:"

#: 21
msgid "XPath parse failure\\: operator expected\\:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འཐུས་ཤོར\\:བཀོལ་སྤྱོད་པ་རེ་བ་བསྐྱེད་བསྐྱེདཔ\\:"

#: 20
msgid "XPath parse failure\\: unexpected end of expression\\:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འཐུས་ཤོར\\:རེ་བ་མེད་པའི་གསལ་བརྗོད་ཀྱི་མཇུག\\:"

