#: thisEngine
msgid "this"
msgstr "འདི"

#: changeEngineTitle
msgid "Change default search engine?"
msgstr "སྔོན་སྒྲིག་འཚོལ་ཞིབ་ཀྱི་མ་འཕྲུལ་བསྒྱུར་བཅོས་འབད?"

#: excludeurl
msgid "Exclude this URL from future searches"
msgstr "ཡུ་ཨར་ཨེལ་འདི་མ་འོངས་འཚོལ་ཞིབ་ཀྱི་གྲངས་སུ་མ་བཙུགས།"

#: excludedomain
msgid "Exclude this domain from future searches"
msgstr "ཌོ་མིན་འདི་མ་འོངས་འཚོལ་ཞིབ་ཚུའི་གྲངས་སུ་མ་བཙུགས།"

#: disableAdvanced
msgid "Disable advanced search options"
msgstr "མཐོ་རིམ་གྱི་འཚོལ་ཞིབ་གདམ་ཁ་ཚུ་ལྕོགས་མིན་བཟོ།"

#: addtobookmarks
msgid "Add to bookmarks"
msgstr "དེབ་རྟགས་ཚུ་ལུ་ཁ་སྐོང་རྐྱབས།"

#: searchButtonText
msgid "Search"
msgstr "འཚོལ་ཞིབ་འབད།"

#: enableAdvanced
msgid "Enable advanced search options"
msgstr "མཐོ་རིམ་གྱི་འཚོལ་ཞིབ་གདམ་ཁ་ཚུ་ལྕོགས་ཅན་བཟོ།"

#: clearfilters
msgid "Clear all search filters"
msgstr "འཚོལ་ཞིབ་ཀྱི་ཚགས་མ་ཚུ་ག་ར་བསལ་་གཏང་།"

#: changeEngineMsg
msgid "Would you like to make %S your default search engine?"
msgstr "ཁྱོད་%S ཁྱོད་རའི་སྔོན་སྒྲིག་གི་འཚོལ་ཞིབ་མ་འཕྲུལ་བཟོ་ནི་ཨིན་ན?"

#: stopButtonText
msgid "Stop"
msgstr "བཀག"

#: enterstringandlocation
msgid "Enter some text to search for and select at least one location to search."
msgstr "འཚོལ་ཞིབ་ཀྱི་དོན་ལུ་ཚིག་ཡིག་ལ་ལུ་གཅིག་ཐོ་བཀོད་འབད་ནི་དང་ཉུང་མཐའ་གནས་ཁོངས་གཅིག་འཚོལ་ཞིབ་ཀྱི་དོན་ལུ་སེལ་འཐུ་འབད།"

#: searchTitle
msgid "Search\\: '%S'"
msgstr "འཚོལ་ཞིབ་འབད\\: '%S'"

#: dontAskAgainMsg
msgid "Change default engine without asking in the future"
msgstr "སྔོན་སྒྲིག་གི་མ་འཕྲུལ་མ་འོངས་པར་མ་འདྲི་བར་བསྒྱུར་བཅོས་འབད།"

#: addquerytobookmarks
msgid "Add search query to bookmarks"
msgstr "དེབ་རྟགས་ཚུ་ལུ་འཚོལ་ཞིབ་ཀྱི་འདྲི་དཔྱད་ཁ་སྐོང་རྐྱབས།"

