#: allengines.label
msgid "All Engines"
msgstr "མ་འཕྲུལ་ཚུ་ཆ་མཉམ།"

#: checkbox.column.label
msgid "Use"
msgstr "ལག་ལེན་འཐབ།"

#: choose.label
msgid "Choose search engines, then click Search"
msgstr "འཚོལ་ཞིབ་ཀྱི་མ་འཕྲུལ་ཚུ་གདམ་ཁ་རྐྱབས་ཞིནམ་ལས་འཚོལ་ཞིབ་གུ་ཨེབ་གཏང་།"

#: customize.menuitem.label
msgid "Edit Categories ..."
msgstr "དབྱེ་རིམ་ཚུ་ཞུན་དག་འབད..."

#: engine.column.label
msgid "Search Engines"
msgstr "མ་འཕྲུལ་ཚུ་འཚོལ་ཞིབ་འབད།"

#: next.button.label
msgid "Next"
msgstr "ཤུལ་མམ།"

#: next.button.tooltip
msgid "Show next search results"
msgstr "འཚོལ་ཞིབ་ཀྱི་གྲུབ་འབྲས་ཤུལ་མམ་ཚུ་སྟོན།"

#: previous.button.label
msgid "Previous"
msgstr "ཧེ་མ།"

#: previous.button.tooltip
msgid "Show previous search results"
msgstr "འཚོལ་ཞིབ་ཀྱི་གྲུབ་འབྲས་ཧེ་མམ་དེ་སྟོན།"

#: results.label
msgid "Search Results"
msgstr "གྲུབ་འབྲས་ཚུ་འཚོལ་ཞིབ་འབད།"

#: savesearch.button.label
msgid "Bookmark this Search"
msgstr "འཚོལ་ཞིབ་འབད་མི་དེ་དེབ་རྟགས་བཀལ།"

#: search.advanced.tab
msgid "Settings for "
msgstr "དོན་ལུ་སྒྲིག་སྟངས་ཚུ།"

#: search.button.label
msgid "Search"
msgstr "འཚོལ་ཞིབ་འབད།"

#: search.results.tab
msgid "Apply"
msgstr "འཇུག་སྤྱོད་འབད།"

#: stop.button.label
msgid "Stop"
msgstr "བཀག"

#: using.label
msgid "using"
msgstr "ལག་ལེན་འཐབ་དོ།"

#: within.label
msgid "within"
msgstr "ནང་འཁོད།"

