#: accepted
msgid "accepted"
msgstr "དང་ལེན་འབད་ཡོདཔ།"

#: popupTitle
msgid "Pop-up Manager"
msgstr "པོཔ་ཨཔ་ འཛིན་སྐྱོངས་པ།"

#: flagged
msgid "flagged"
msgstr "ཟུར་རྟགས་བཀལ་ཡོདཔ།"

#: downgraded
msgid "session"
msgstr "ལཱ་ཡུན།"

#: deleteAllCookiesTitle
msgid "Remove All Cookies"
msgstr "ཀུ་ཀི་ཚུ་ག་ར་རྩ་བསྐྲད་གཏང་།"

#: textBannedPopups
msgid "Sites from which pop-up windows are or are not allowed."
msgstr "པོཔ་ཨཔ་ སྒོ་སྒྲིག་ཚུ་ལས་ས་ཁོངས་ཚུ་འབད་བཅུག་ནི་དང་ཡང་ན་འབད་མི་བཅུག"

#: deleteAllImagesSites
msgid "Are you sure you want to delete all of the image sites?"
msgstr "ཁྱོད་ངེས་ཏིག་རང་གཟུགས་བརྙན་གྱི་ས་ཁོངས་ཚུ་ག་ར་བཏོན་གཏང་ནི་ཨིན་ན?"

#: canPopups
msgid "site can show popups"
msgstr "ས་ཁོངས་ཀྱིས་པོཔ་ཨཔ་ཚུ་སྟོནམ་ཨིན།"

#: textBannedImages
msgid "Sites from which images are or are not loaded."
msgstr "གཟུགས་བརྙན་ཚུ་ལས་ས་ཁོངས་ཚུ་མངོན་གསལ་ཡང་ན་མངོན་གསལ་མི་འབད།"

#: cannotPopups
msgid "site cannot show popups"
msgstr "ས་ཁོངས་ཚུ་གིས་པོཔ་ཨཔ་ཚུ་སྟོན་མི་ཚུགས།"

#: canImages
msgid "site can load images"
msgstr "ས་ཁོངས་ཀྱིས་གཟུགས་བརྙན་ཚུ་མངོན་གསལ་འབདཝ་ཨིན།"

#: can
msgid "site can set cookies"
msgstr "ས་ཁོངས་ཀྱིས་ཀུ་ཀི་ཚུ་གཞི་སྒྲིག་འབདཝ་ཨིན།"

#: cannot
msgid "site cannot set cookies"
msgstr "ས་ཁོངས་ཀྱིས་ཀུ་ཀི་ཚུ་གཞི་སྒྲིག་འབད་མི་ཚུགས།"

#: cannotImages
msgid "site cannot load images"
msgstr "ས་ཁོངས་ཀྱིས་གཟུགས་བརྙན་ཚུ་མངོན་གསལ་འབད་མི་ཚུགས།"

#: host
msgid "Server which set the cookie\\:"
msgstr "ཀུ་ཀི་གཞི་སྒྲིག་འབད་མི་སར་བར་\\:"

#: close
msgid "Close"
msgstr "ཁ་བསྡམ།"

#: policyImplicitConsent
msgid "stores identifiable information unless user opts out"
msgstr "ལག་ལེན་པ་གིས་བཏོན་གཏང་ནིའི་གདམ་ཁ་རྐྱབསན་མ་ཤེས་ དེ་མེན་ངོས་འཛིན་ཡོད་པའི་བརྡ་དོན་དེ་གསོག་འཇོག་འབདཝ་ཨིན།"

#: deleteAllCookies
msgid "Are you sure you want to delete all the cookies?"
msgstr "ཁྱོད་ཀུ་ཀི་ཚུ་ག་ར་བཏོན་གཏང་ནི་ངོས་ཏིག་ཨིན་ན?"

#: policyNoConsent
msgid "stores identifiable information without any user consent"
msgstr "ལག་ལེན་པའི་ཁས་ལེན་ག་ནི་ཡང་མེད་པར་ངོས་འཛིན་ཡོད་པའི་བརྡ་དོན་གསོག་འཇོག་འབདཝ་ཨིན།"

#: canSession
msgid "site can set session cookies"
msgstr "ས་ཁོངས་ཀྱིས་ལཱ་ཡུན་གྱི་ཀུ་ཀི་ཚུ་གཞི་སྒྲིག་འབདཝ་ཨིན།"

#: policyUnstated
msgid "no policy about storing identifiable information"
msgstr "ངོས་འཛིན་ཡོད་པའི་བརྡ་དོན་གསོག་འཇོག་འབད་ནིའི་དོན་ལས་སྲིད་བྱུས་མེད།"

#: tabBannedPopups
msgid "Pop-up Sites"
msgstr "པོཔ་ཨཔ་གི་ས་ཁོངས་ཚུ།"

#: imageTitle
msgid "Image Manager"
msgstr "གཟུགས་བརྙན་འཛིན་སྐྱོངས་པ།"

#: forAnyConnection
msgid "Any type of connection"
msgstr "མཐུད་ལམ་གྱི་དབྱེ་བ་ག་ཅི་རང་འབད་རུང་།"

#: AtEndOfSession
msgid "at end of session"
msgstr "ལཱ་ཡུན་གྱི་མཇུག་ལུ།"

#: deleteAllCookiesSites
msgid "Are you sure you want to delete all of the cookie sites?"
msgstr "ཁྱོད་ཀུ་ཀིའི་ས་ཁོངས་ཚུ་ག་ར་བཏོན་གཏང་ནི་ངོས་ཏིག་ཨིན་ན?"

#: domainColon
msgid "Domain\\:"
msgstr "ཌོ་མིན \\:"

#: hostColon
msgid "Host\\:"
msgstr "ཧོསཊི\\:"

#: tabBannedImages
msgid "Image Sites"
msgstr "གཟུགས་བརྙན་གྱི་ས་ཁོངས་ཚུ།"

#: policyExplicitConsent
msgid "stores identifiable information if user opts in"
msgstr "ལག་ལེན་པ་གིས་ཁས་ལེན་འབདཝ་ཨིན་པ་ཅིན་ ངོས་འཛིན་ཡོད་པའི་བརྡ་དོན་གསོག་འཇོག་འབདཝ་ཨིན།"

#: policyNoIICollected
msgid "does not store identifiable information"
msgstr "ངོས་འཛིན་ཡོད་པའི་བརྡ་དོན་གསོག་འཇོག་མི་འབད།"

#: deleteAllSitesTitle
msgid "Remove All Sites"
msgstr "ས་ཁོངས་ཚུ་ག་ར་རྩ་བསྐྲད་གཏང་།"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "གསང་བཟོས་འབད་འབད་བའི་མཐུད་ལམ་ཚུ་རྐྱངམ་ཅིག་ཨིན།"

#: domain
msgid "Domain for which this cookie applies\\:"
msgstr "འ་ནི་ཀུ་ཀིའི་དོན་ལས་ཌོ་མིན་འཇུག་སྤྱོད་འབད་མི་ཚུ \\:"

