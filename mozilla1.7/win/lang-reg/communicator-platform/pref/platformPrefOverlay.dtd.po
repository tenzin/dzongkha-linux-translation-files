#: alreadyDefaultText
msgid "&brandShortName; is already your default browser."
msgstr "&brandShortName; ཧེ་མ་ལས་རང་ཁྱོད་ཀྱི་སྔོན་སྒྲིག་བརའུ་ཟར་ཨིན།"

#: defaultBrowserButton.label
msgid "Set Default Browser"
msgstr "སྔོན་སྒྲིག་བརའུ་ཟར་གཞི་སྒྲིག་འབད།"

#: defaultBrowserGroup.label
msgid "Default Browser"
msgstr "སྔོན་སྒྲིག་བརའུ་ཟར།"

#: defaultPendingText
msgid "&brandShortName; will be set as your default browser when you click OK."
msgstr "ཁྱོད་ཀྱིས་བཏུབ་ཨེབ་གཏང་པའི་སྐབས་ &brandShortName; དེ་ཁྱོད་ཀྱི་སྔོན་སྒྲིག་བརའུ་ཟར་སྦེ་གཞི་སྒྲིག་འབད་འོང་།"

#: makeDefaultText
msgid "Set &brandShortName; as your default browser."
msgstr "&brandShortName; དེ་ཁྱོད་རའི་སྔཽན་སྒྲིག་བརའུ་ཟར་སྦེ་གཞི་སྒྲིག་འབད།"

#: middleClick.label
msgid "Middle-click, Ctrl+click or Ctrl+Enter on links in a Web page"
msgstr "ཝེབ་ཤོག་ལེབ་ནང་གི་འབྲེལ་ལམ་གུ་ བར་མ་-ཨེབ་གཏང་ ཚད་འཛིན་+ཨེབ་གཏང་ ཡང་ན་ཚད་འཛིན་+ཐོ་བཀོད།"

#: prefWindow.size
msgid ""
"_: Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger.\n"
"width: 52em; height: 41em;"
msgstr "རྒྱ་ཚད:ཨི་ཨེམ་༥༢། མཐོ་ཚད: ཨི་ཨེམ་༤༡།"

#: urlbar.label
msgid "Ctrl+Enter in the Location bar"
msgstr "གནས་ཁོངས་ཕྲ་རིང་ནང་ལུ་ཚད་འཛིན་+ཏོ་བཀོད།"

#: winhooks.label
msgid "System"
msgstr "རིམ་ལུགས།"

