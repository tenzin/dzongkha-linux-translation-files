#: engineManager.title
msgid "Manage Search Engine List"
msgstr "འཚོལ་ཞིབ་མ་འཕྲུལ་ཐོ་ཡིག་འཛིན་སྐྱོང་འཐབ་"

#: engineManager.style
msgid "min-width: 35em;"
msgstr "རྒྱ་ཚད་ཉུང་མཐའ་:ཨི་ཨེམ་༣༥"

#: engineManager.intro
msgid "You have the following search engines installed:"
msgstr "ཁྱོད་ལུ་འོག་གི་འཚོལ་ཞིབ་མ་འཕྲུལ་ཚུ་ གཞི་བཙུགས་འབད་དེ་ཡོད་:"

# Buttons
#: up.label up.accesskey
msgid "Move &Up"
msgstr "ཡར་སྤོ་(&U)"

#: dn.label dn.accesskey
msgid "Move &Down"
msgstr "མར་སྤོ་ (&D)"

#: remove.label remove.accesskey
msgid "&Remove"
msgstr "རྩ་བསྐྲད་གཏང་(&R)"

#: addEngine.label addEngine.accesskey
msgid "Get more se&arch engines..."
msgstr "འཚོལ་ཞིབ་མ་འཕྲུལ་མངམ་ལེན་་་(&a)"

#: enableSuggest.label enableSuggest.accesskey
msgid "&Show search suggestions"
msgstr "འཚོལ་ཞིབ་བསམ་འཆར་སྟོན་(&S)"

#: restoreDefaults.label restoreDefaults.accesskey
msgid "R&estore Defaults"
msgstr "སྔོན་སྒྲིག་སླར་གསོ་འབད་(&e)"

