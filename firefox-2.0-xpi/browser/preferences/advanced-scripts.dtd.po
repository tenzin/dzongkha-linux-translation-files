#: advancedJSDialog.title
msgid "Advanced JavaScript Settings"
msgstr "མཐོ་རིམ་ཅན་གྱི་ ཇ་ཝ་སིཀིརིཔཊི་སྒྲིག་སྟངས།"

#: window.width
msgid ""
"_: Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger.\n"
"37em"
msgstr "ཨི་ཨེམ་༣༧"

#: allowScripts.label
msgid "Allow scripts to:"
msgstr "ཡིག་ཚུགས་གིས་:"

#: moveResizeWindows.label
#: moveResizeWindows.accesskey
msgid "&Move or resize existing windows"
msgstr "ད་ལྟོའི་སྒོ་སྒྲིག་ཚུ་ སྤོ་བཤུད་ ཡང་ན་ སླར་ཚད་སྒྲིག་འབད་བཅུག(&M)"

#: raiseLowerWindows.label
#: raiseLowerWindows.accesskey
msgid "&Raise or lower windows"
msgstr "སྒོ་སྒྲིག་ཡར་སེང་དང་མར་ཕབ་འབད་བཅུག(&R)"

#: disableContextMenus.label
#: disableContextMenus.accesskey
msgid "&Disable or replace context menus"
msgstr "སྐབས་འབབ་དཀར་ཆག་ཚུ་ ལྕོགས་མིན་བཟོ་བཅུག་ ཡང་ན་ ཚབ་བཙུགས་བཅུག(&D)"

#: hideStatusBar.label
#: hideStatusBar.accesskey
msgid "&Hide the status bar"
msgstr "གནས་ཚད་ཕྲ་རིང་སྦ་བཅུག(&H)"

#: changeStatusBar.label
#: changeStatusBar.accesskey
msgid "&Change status bar text"
msgstr "གནས་ཚད་ཕྲ་རིང་གི་ཚིག་ཡིག་སོར་བཅུག(&C)"

