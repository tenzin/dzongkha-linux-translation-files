#: changeAction.title
msgid "Change Action"
msgstr "བྱ་བ་སོར་"

#: window.width
msgid ""
"_: Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger.\n"
"36em"
msgstr "ཨི་ཨེམ་༣༦"

#: whenDownloading.label
msgid "When downloading files like this:"
msgstr "ཡིག་སྣོད་འདི་བཟུམ་ཚུ་ ཕབ་ལེན་འབད་བའི་སྐབས་:"

#: openDefault.label
#: openDefault.accesskey
msgid "&Open them with the default application:"
msgstr "དེ་ཚུ་སྔོན་སྒྲིག་གློག་རིམ་ཐོག་ལས་ཁ་ཕྱེ་:(&O)"

#: openApplication.label
#: openApplication.accesskey
msgid "Op&en them with this application:"
msgstr "གློག་རིམ་འདི་གི་ཐོག་ལས་ཁ་ཕྱེ་:(&e)"

#: changeApp.label
#: changeApp.accesskey
msgid "&Browse..."
msgstr "བརྡ་འཚོལ་་་་(&B)"

#: saveToDisk.label
#: saveToDisk.accesskey
msgid "&Save them on my computer"
msgstr "དེ་ཚུ་ཁྱོད་རའི་གློག་རིག་གུ་བསྲུང་བཞག(&S)"

#: saveToDefaultFolder.label
#: saveToDefaultFolder.accesskey
msgid "in the &default download folder"
msgstr "སྔོན་སྒྲི་ཕབ་ལེན་སྣོད་འཛིན་ནང་"

#: saveToThisFolder.label
#: saveToThisFolder.accesskey
msgid "in t&his folder:"
msgstr "སྣོད་འཛིན་འདི་ནང་:(&h)"

#: chooseFolder.label
#: chooseFolder.accesskey
msgid "Bro&wse..."
msgstr "བརྡ་འཚོལ་་་་(&w)"

#: saveToAskMe.label
#: saveToAskMe.accesskey
msgid "&ask me where to save the file"
msgstr "ཡིག་སྣོད་དེ་ག་ཏེ་བསྲུང་ནི་ཨིན་ན་ ང་ལུ་དྲིས་(&a)"

#: usePlugin.label
#: usePlugin.accesskey
msgid "Use this &Plugin:"
msgstr "པ་ལག་ཨིན་འདི་ལག་ལེན་འཐབ་(&P)"

