#: sanitizeDialog.title
msgid "Clear Private Data"
msgstr "སྒེར་གྱི་གནད་སྡུད་བསལ་གཏང་།"

#: sanitizeItems.label
msgid "Clear the following items now:"
msgstr "འོག་གི་རྣམ་གྲངས་ཚུ་ ད་ལྟོ་རང་བསལ་གཏང་།"

#: itemHistory.label itemHistory.accesskey
msgid "&Browsing History"
msgstr "ལོ་རྒྱུས་བརྡ་འཚོལ་འབད་དོ།(&B)"

#: itemFormSearchHistory.label itemFormSearchHistory.accesskey
msgid "Saved &Form and Search History"
msgstr "བསྲུངས་ཡོད་པའི་འབྲི་ཤོག་དང་འཚོལ་ཞིབ་ལོ་རྒྱུས་"

#: itemPasswords.label itemPasswords.accesskey
msgid "Saved &Passwords"
msgstr "བསྲུངས་ཡོད་པའི་ཆོག་ཡིག།(&P)"

#: itemCookies.label itemCookies.accesskey
msgid "&Cookies"
msgstr "ཀུ་ཀིསི།(&C)"

#: itemCache.label itemCache.accesskey
msgid "C&ache"
msgstr "འདྲ་མཛོད།(&a)"

#: itemDownloads.label itemDownloads.accesskey
msgid "&Download History"
msgstr "ལོ་རྒྱུས་ཕབལེན་འབད།(&D)"

#: itemSessions.label itemSessions.accesskey
msgid "Authenticated &Sessions"
msgstr "བཤེད་བཤད་འབད་ཡོད་པའི་ལཱ་ཡུན།"

#~ msgid "Saved &Form Information"
#~ msgstr "བསྲུངས་ཡོད་པའི་འབྲི་ཤོག་བརྡ་དོན།(&F)"
#~ msgid "As&k me before clearing private data"
#~ msgstr "སྒེར་གྱི་གནད་སྡུད་བསལ་མ་གཏངམ་ལས་ང་ལུ་དྲིས།"

