# extracted from content/openLocation.xul
#: enter.label
msgid "Enter the web location (URL), or specify the local file you would like to open:"
msgstr "ཝེབ་ཀྱི་གནས་ཁོངས་ (ཡུ་ཨར་ཨེལ) བཙུགས་ ཡང་ན་ ཁྱོད་ཀྱིས་ཁ་ཕྱེ་དགོ་མནོ་མི་ ཉེ་གནས་ཀྱི་ཡིག་སྣོད་དེ་གསལ་བཀོད་འབད༔"

#: chooseFile.label
msgid "Choose File..."
msgstr "ཡིག་སྣོད་གདམ་ཁ་རྐྱབས་..."

#: newWindow.label
msgid "New Window"
msgstr "སྒོ་སྒྲིག་གསརཔ།"

#: newTab.label
msgid "New Tab"
msgstr "ཨེབ་ལྡེ་གསརཔ།"

#: topWindow.label
msgid "Current Window"
msgstr "ད་ལྟོའི་སྒོ་སྒྲིག།"

#: caption.label
msgid "Open Web Location"
msgstr "ཝེབ་ཀྱི་གནས་ཁོངས་ཁ་ཕྱེ།"

#: openWhere.label
msgid "Open in:"
msgstr "ནང་ན་ཁ་ཕྱེ་:"

#: example.label
msgid "Example: &vendorURL;"
msgstr "དཔེར་བརྗོད་: &vendorURL;"

#: openBtn.label
msgid "Open"
msgstr "ཁ་ཕྱེ།"

