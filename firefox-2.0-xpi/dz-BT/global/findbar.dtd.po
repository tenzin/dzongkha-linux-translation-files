# entities split out from browser.dtd
#: next.label next.accesskey
msgid "&Next"
msgstr "ཤུལ་མམ་(&N)"

#: next.tooltip
msgid "Find the next occurrence of the phrase"
msgstr "ཚིག་ཚན་གྱི་ཤུལ་མའི་འབྱུང་བ་འཚོལ་"

#: previous.label previous.accesskey
msgid "&Previous"
msgstr "ཧེ་མམ་(&P)"

#: previous.tooltip
msgid "Find the previous occurrence of the phrase"
msgstr "ཚིག་ཚན་གྱི་ཧེ་མའི་འབྱུང་བ་འཚོལ་"

#: findCloseButton.tooltip
msgid "Close Find bar"
msgstr "འཚོལ་ནིའི་ཕྲ་རིང་ཁ་བསྡམས།"

#: highlight.label highlight.accesskey
msgid "Highlight &all"
msgstr "ཆ་མཉམ་ལུ་གཙོ་དམིགས་འབད་(&al)"

#: highlight.tooltiptext
msgid "Highlight all occurrences of the phrase"
msgstr "ཚིག་ཚོགས་ཀྱི་བྱུང་ལས་ཆ་མཉམ་གཙོ་དམིགས་འབད་"

#: caseSensitiveCheckbox.label caseSensitiveCheckbox.accesskey
msgid "Mat&ch case"
msgstr "ཡི་གུའི་མཐུན་སྒྲིག(&c)"

