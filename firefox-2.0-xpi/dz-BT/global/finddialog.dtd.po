# extracted from finddialog.xul
#: findDialog.title
msgid "Find in This Page"
msgstr "ཤོག་ལེབ་འདི་ནང་འཚོལ་"

#: findDialogFrame.title
msgid "Find in This Frame"
msgstr "གཞི་ཁྲམ་འདི་ནང་འཚོལ་"

#: findField.label
#: findField.accesskey
msgid "Fi&nd what:"
msgstr "ག་ཅི་འཚོན་ནི་:"

#: caseSensitiveCheckbox.label
#: caseSensitiveCheckbox.accesskey
msgid "Mat&ch case"
msgstr "ཡི་གུ་མཐུན་སྒྲིག་"

#: wrapCheckbox.label
#: wrapCheckbox.accesskey
msgid "&Wrap"
msgstr "ལོག་མཚམས་བཟོ་(&W)"

#: backwardsCheckbox.label
#: backwardsCheckbox.accesskey
msgid "Search &backwards"
msgstr "རྒྱབ་ཕྱོགས་ལུ་འཚོལ་ཞིབ་འབད་(&b)"

#: findField.tooltip
msgid "Type one or more words to search for"
msgstr "འཚོལ་ནིའི་དོན་ལུ་ མིང་ཚིག་གཅིག་ ཡང་ན་ དེ་མངམ་ཡིག་དཔར་རྐྱབས།"

#: findButton.label
#: findButton.accesskey
msgid "&Find Next"
msgstr "ཤུལ་མམ་འཚོལ་(&F)"

#: cancelButton.label
msgid "Cancel"
msgstr "ཆ་མེད་"

#: closeButton.label
msgid "Close"
msgstr "ཁ་བསྡམས་"

#: up.label
#: up.accesskey
msgid "&Up"
msgstr "ཡར་(&U)"

#: down.label
#: down.accesskey
msgid "&Down"
msgstr "མར་(&D)"

#: direction.label
msgid "Direction"
msgstr "ཁ་ཕྱོགས་"

