#: hostColon
msgid "Host:"
msgstr "ཧོསཊི:"

#: domainColon
msgid "Domain:"
msgstr "ཌོ་མེན:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "གསང་བཟོས་མཐུད་ལམ་ཚུ་རྐྱངམ་ཅིག"

#: forAnyConnection
msgid "Any type of connection"
msgstr "མཐུད་ལམ་གྱི་དབྱེ་བ་གང་རུང་།"

#: atEndOfSession
msgid "at end of session"
msgstr "ལཱ་ཡུན་གྱི་མཇུག་ལུ།"

#: showDetails
msgid "Show Details"
msgstr "རྒྱས་བཤད་ཚུ་སྟོན།"

#: hideDetails
msgid "Hide Details"
msgstr "རྒྱས་་ཚུ་གསང་།"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "ས་ཁོངས་%ཨེསི་དེ་ཀུ་ཀི་གཞི་སྒྲིག་འབད་ནི་ཨིན་མས།"

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "ས་ཁོངས་%ཨེསི་དེ་ཀུ་ཀི་གཉིས་པམ་དེ་གཞི་སྒྲིག་འབད་ནི་ཨིན་མས།"

#  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"ས་ཁོངས %ཨེསི་དེ་ཀུ་ཀི་གཞན་གཞི་སྒྲིག་འབད་ནི་ཨིན་མས\n"
"ཁྱོད་ལུ་%ཨེསི་ཀུ་ཀི་ཚུ་ཧེ་མ་ལས་ས་ཁོངས་དེ་ལས་ཡོདཔ་ཨིན།"

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "ས་ཁོངས་ %ཨེསི་དེ་ཀུ་ཀི་ཡོད་མི་ཅིག་ལེགས་བཅོས་འབད་ནི་ཨིན་མས།"

#: permissionToAcceptImage
msgid "The site %S wants to load an image."
msgstr "ས་ཁོངས་%ཨེསི་དེ་གཟུགས་བརྙན་ཅིག་མངོན་གསལ་འབད་ནི་ཨིན་མས།"

#: rememberThisDecision
msgid "Remember this decision"
msgstr "གྲོད་ཆོད་དེ་སེམས་ཁར་བཞག"

