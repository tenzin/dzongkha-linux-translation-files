#: updateWizard.title
msgid "Software Update"
msgstr "མཉེན་ཆས་དུས་མཐུན་བཟོ་ནི།"

#: window.width
msgid ""
"_: Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger.\n"
"36em"
msgstr "༣༦ ཨི་ཨེམ།"

#: window.macWidth
msgid "39em"
msgstr "༣༩ ཨི་ཨེམ།"

#: checking.title
msgid "Checking for Updates"
msgstr "དུས་མཐུན་བཟོ་ནི་ཚུའི་དོན་ལུ་ཞིབ་དཔྱད་འབད་དོ།"

#: checking.label
msgid "&brandShortName; is now checking for updates..."
msgstr "&brandShortName དེ་ད་ལྟོ་དུས་མཐུན་བཟོ་ནི་ཚུའི་དོན་ལུ་ཞིབ་དཔྱད་འབད་དོ ...(&b)"

#: cancel.label cancel.accesskey
msgid "&Cancel"
msgstr "ཆ་མེད་གཏང་།(&C)"

#: noupdatesfound.title
msgid "No Updates Found"
msgstr "དུས་མཐུན་བཟོ་ནི་ཚུ་འཚོལ་མ་ཐོབ།"

#: noupdatesfound.intro
msgid "There are no new updates available. &brandShortName; may check periodically for new updates."
msgstr "དུས་མཐུན་བཟོ་ནི་གསརཔ་ཚུ་ཐོབ་མ་ཚུགསཔ་ད་&brandShortName གིས་དུས་མཐུན་བཟོ་ནི་གསརཔ་ཚུའི་དོན་ལས་འཕྲལ་འཕྲལ་སྦེ་ཞིབ་དཔྱད་འབད་འོང་།"

#: updatesfound.title
msgid "Update Available"
msgstr "དུས་མཐུན་བཟོ་ནི་ཐོབ་ཚུགསཔ།"

#: moreDetails.label moreDetails.accesskey
msgid "&More Details &#0187;"
msgstr "#༠༡༨༧ རྒྱས་བཤད་ཚུ་ཧེང་ཀེལ།(&M)"

#: clickHere.label
msgid "View more information about this update"
msgstr "དུས་མཐུན་བཟོ་ནི་འདིའི་སྐོར་ལས་བརྡ་དོན་ཧེང་ཀེལ་སྔོན་ལྟ་འབད།"

#: incompatible.warning
msgid "This update will cause some of your extensions and/or themes to stop working until they are updated."
msgstr "དུས་མཐུན་བཟོ་ནི་འདི་གིས་ཁྱོད་ཀྱི་རྒྱ་བསྐྱེད་ཚུ་ལ་ལོ་ཅིག་གི་རྒྱུ་རྐྱེན་དང་ཡང་ན་བརྗོད་དོན་ཚུ་དུས་མཐུན་མ་བཟོ་ཚུན་ཚོད་བཀག་འོང་།"

#: listIncompatible.label listIncompatible.accesskey
msgid "Show &List"
msgstr "ཐོ་ཡིག་སྟོན།(&L)"

#: upgrade.evangelism
msgid "It is strongly recommended that you upgrade &brandShortName; as soon as possible."
msgstr "ཁྱོད་ལུ་&brandShortName ་ག་དེ་མགྱོགས་ཚུགས་སྦེ་ཡར་བསྐྱེད་ནི་ལུ་འོས་སྦྱོར་སྒྲིང་སྒྲིང་འབད་ཡོདཔ་ཨིན།"

#: license.title
msgid "License Agreement"
msgstr "ཆོག་ཐམ་གན་འཛིན།"

#: license.intro
msgid "To install this update you must agree to this license agreement. Please read it carefully:"
msgstr "དུས་མཐུན་འདི་གཞི་བཙུགས་འབད་ནི་ལུ་ཁྱོད་ཀྱིས་ཆོག་ཐམ་གན་འཛིན་དེ་ལུ་ཁ་འཆམ་དགོཔ་ལས་དྲན་ཤེས་ཀྱི་སྒོ་ལས་ལྷག་གནང་:"

#: license.instructions
msgid "If you agree to the terms of this agreement, click I Agree below to continue installing this update."
msgstr "ཁྱོད་ཀྱིས་གན་འཛིན་དེ་གི་ཐ་སྙད་དེ་ལུ་ཁ་འཆམ་པ་ཅིན་ དུས་མཐུན་བཟོ་ནི་དེ་འཕྲོ་གཞི་བཙུགས་འབད་ནི་ལུ་འོག་གི་ང་ཁ་འཆམ་གུ་ཨེབ་གཏང་འབད།"

#: license.titleText
msgid "Software License Agreement"
msgstr "མཉེན་ཆས་ཆོག་ཐམ་གྱི་གན་ཡིག་"

#: license.introText
msgid "Terms and conditions for using this software."
msgstr "མཉེན་ཆས་འདི་ལག་ལེན་འཐབ་ནིའི་གནས་ཚིག་"

#: license.instructionText
msgid "Please read the following license agreement. Use the scroll bar to view the rest of this agreement."
msgstr "འོག་གི་ཆོག་ཐམ་གན་ཡིག་དེ་ལྷག་གནང་། གན་ཡིག་འདི་ཆ་ཚང་བལྟ་ནི་ལུ་ བཤུད་ཕྲ་ལག་ལེན་འཐབ།"

#: license.accept
msgid "I accept the terms of the License Agreement"
msgstr "ང་གིས་གན་ཡིག་འདི་གི་གནས་ཚིག་ཚུ་ལུ་ ཁས་ལེན་འབདཝ་ཨིན།"

#: license.accept.accesskey
msgid "c"
msgstr "སི་"

#: license.decline
msgid "I do NOT accept the terms of the License Agreement"
msgstr "ང་གིས་གན་ཡིག་འདི་གནས་ཚིག་ཚུ་ལུ་ ཁས་ལེན་མི་འབད་"

#: license.decline.accesskey
msgid "T"
msgstr "ཊི་"

#: downloading.title
msgid "Downloading Update"
msgstr "དུས་མཐུན་བཟོ་ནི་ཕབ་ལེན་འབད་དོ།"

#: downloading.intro
msgid "Downloading the update..."
msgstr "དུས་མཐུན་བཟོ་ནི་དེ་ཕབ་ལེན་འབད་དོ..."

#: connecting.label
msgid "Connecting to the update server..."
msgstr "དུས་མཐུན་བཟོ་ནི་སར་བར་དེ་ལུ་མཐུད་དོ..."

#: verificationFailedText.label
msgid "&brandShortName; was unable to verify the integrity of the incremental update it downloaded, so it is now downloading the complete update package."
msgstr "&brandShortName དེ་དུས་མཐུན་བཟོ་ནི་ཡར་འཕར་ཕབ་ལེན་ཡོད་མི་གི་ཆིག་སྒྲིལ་དེ་བལྟ་སྤྱོད་འབད་མི་བཏུབ་ལས་ད་ལྟོ་ཡོངས་སྒྲུབ་དུས་མཐུན་བཟོ་ནི་ཐུམ་སྒྲིལ་དེ་ཕབ་ལེན་འབད་དོ།"

#: details.link update.details.label
msgid "Details"
msgstr "རྒྱས་བཤད་ཚུ།"

#: pause.label pause.accesskey
msgid "Paus&e"
msgstr "ཐེམ།(&e)"

#: close.label close.accesskey
msgid "&Close"
msgstr "ཁ་བསྡམས།(&C)"

#: error.label
msgid "There were problems checking for, downloading or installing this update. &brandShortName; could not be updated because:"
msgstr "དུས་མཐུན་བཟོ་ནི་འདི་གཞི་བཙུགས་འབད་ནི་ཡང་ན་ཕབ་ལེན་འབད་ནི་དོན་ལུ་དཀའ་ངལ་ཚུ་ཞིབ་དཔྱད་འབད་དོ།  &brandShortName དེ་དུས་མཐུན་བཟོ་མ་བཏུབ་ག་ཅི་སྨོ་ཟེར་བ་ཅིན:"

#: errorManual.label
msgid "You can update &brandShortName; manually by visiting this link and downloading the latest version:"
msgstr "ཁྱོད་ཀྱིས་འབྲེལ་ལམ་དེ་བལྟ་སྤྱོད་འབད་ནི་དང་ཐོན་རིམ་མཐའ་མཇུག་དེ་ལག་ཐོག་ལས་ཕབ་ལེན་འབད་དེ་གིས་&brandShortName དུས་མཐུན་བཟོ་བཏུབ་ཨིན།"

#: errorpatching.title
msgid "Update Failed"
msgstr "དུས་མཐུན་བཟོ་ནི་འཐུས་ཤོར་ཡོདཔ།"

#: errorpatching.intro
msgid "The partial Update could not be applied. &brandShortName; will try again by downloading a complete Update."
msgstr "དུས་མཐུན་བཟོ་ནི་ཤ་ཆས་དེ་འཇུག་སྤྱོད་འབད་མ་བཏུབ་ལས་&brandShortName གིས་དུས་མཐུན་བཟོ་ནི་ཡོངས་བསྒྲུབས་ཅིག་ཕབ་ལེན་འབད་དེ་གིས་ལོག་འབད་རྩོལ་འབད་འོང་།"

#: finished.title
msgid "Update Downloaded"
msgstr "དུས་མཐུན་བཟོ་ནི་ཕབ་ལེན་འབད་ཡོདཔ།"

#: finished.text
msgid "The update was successfully downloaded and verified, and it will be installed the next time &brandShortName; starts."
msgstr "དུས་མཐུན་ཚུ་ མཐར་འཁྱོལ་སྦེ་ ཕབ་ལེན་འབད་དེ་ བདེན་སྦྱོར་འབད་ཡོདཔ་ལས་ ཤུལ་ལས་&brandShortName; འགོ་བཙུགསཔ་ད་ གཞི་བཙུགས་འབད་འོང་།"

#: finishedBackground.title
msgid "Update Ready to Install"
msgstr "དུས་མཐུན་བཟོ་ནི་གཞི་བཙུགས་འབད་ནི་ལུ་གྲ་སྒྲིག"

#: finishedBackground.text
msgid "&brandShortName; has downloaded and verified an important update. Click the link below for more details."
msgstr "&brandShortName; གིས་ དུས་མཐུན་གལ་ཅན་ཅིག་ ཕབ་ལེན་འབད་དེ་ བདེན་སྦྱོར་འབད་ཡི། ཁ་གསལ་གྱི་དོན་ལུ་ འོག་གི་འབྲེལ་ལམ་ལུ་ ཨེབ་གཏང་འབད།"

#: finishedBackground.name
msgid "Update:"
msgstr "དུས་མཐུན་བཟོ་ནི:"

#: finishedBackground.more1
msgid "Click Restart &brandShortName; Now to install the update now."
msgstr "དུས་མཐུན་དེ་ ད་ལྟོ་ར་གཞི་བཙུགས་འབད་ནི་ལུ་ &brandShortName; ད་ལྟོ་ར་ ལོག་འགོ་བཙུགས་ཟེར་མི་ལུ་ ཨེབ་གཏང་འབད།"

#: finishedBackground.more2
msgid "Click Later to close this dialog and continue working. &brandShortName; will install the update the next time it starts."
msgstr "ཌའི་ལོག་འདི་ཁ་བསྡམས་ཏེ་ ལཱ་འཕྲོ་མཐུད་དེ་འབད་ནི་ལུ་ ཤུལ་ལས་ཟེར་མི་ལུ་ ཨེབ་གཏང་འབད། &brandShortName; གིས་ ཤུལ་ལས་འགོ་བཙུགསཔ་ད་ དུས་མཐུན་དེ་ གཞི་བཙུགས་འབད་འོང་།"

#: installed.title
msgid "Update Installed"
msgstr "དུས་མཐུན་བཟོ་ནི་གཞི་བཙུགས་འབད་ཡོདཔ།"

#: installed.intro
msgid "The update was successfully installed."
msgstr "དུས་མཐུན་བཟོ་ནི་དེ་མཐར་འཁྱོལ་སྦེ་གཞི་བཙུགས་འབད་ཡོདཔ།"

#: whatsnew.label
msgid "Read more about what's new..."
msgstr "ག་ཅི་གསརཔ་ཨིན་མི་གི་སྐོར་ལས་ཧེང་ཀེལ་ལྷག..."

#: update.installedOn.label
msgid "Installed on:"
msgstr "གཞི་བཙུགས་ཡོདཔ་གུ:"

#: update.status.label
msgid "Status:"
msgstr "གནས་ཚད:"

