#: aboutWindowTitle aboutAddon
msgid "About %S"
msgstr "%Sགི་སྐོར་ལས་"

#: aboutWindowCloseButton
msgid "Close"
msgstr "ཁ་བསྡམས་"

#: aboutWindowVersionString
msgid "version %S"
msgstr "ཐོན་རིམ་%S"

#: updatingMsg
msgid "Looking for updates..."
msgstr "དུས་མཐུན་གྱི་དོན་ལུ་བལྟ་དོ་་་་"

#: updateCompatibilityMsg
msgid "A compatibility update has been applied."
msgstr "མཐུན་འགྱུར་ཡོདཔའི་ དུས་མཐུན་ཅིག་ལག་ལེན་འཐབ་ཅི།"

#: updateNoUpdateMsg noUpdatesMsg
msgid "No updates were found."
msgstr "དུས་མཐུན་བཟོ་དགོཔ་མིན་འདུག།"

#: updateErrorMessage
msgid "An error occurred while trying to find updates for %S."
msgstr "%Sགི་དོན་ལུ་ དུས་མཐུན་འཚོལ་བའི་སྐབས་ འཛོལ་བ་ཅིག་བྱུང་ཡི།"

#: updateDisabledMessage
msgid "Updates are disabled for %S."
msgstr "%Sགི་དོན་ལུ་ དུས་མཐུན་ཚུ་ ལྕོགས་མིན་བཟོ་དགོ།"

#: updateAppManagedMessage
msgid "Updates to %S are performed when %S is updated."
msgstr "%Sདེ་དུས་མཐུན་བཟོཝ་ད་ %Sལུ་དུས་མཐུན་བཟོཝ་ཨིན།"

#: updateReadOnlyMessage
msgid "Update not supported (install location is read only)."
msgstr "དུས་མཐུན་བཟོ་ནི་ལུ་རྒྱབ་སྐྱོར་མིན་འདུག།(གསར་་བཙུགས་གནས་ཁོངས་དེ་ལྷག་ནི་ཙམ་ཨིན་མས)"

#: updateNotManagedMessage
msgid "Update not supported (install location is not managed by %S)."
msgstr "དུས་མཐུན་བཟོ་ནི་ལུ་རྒྱབ་སྐྱོར་མིན་འདུག།(གསར་་བཙུགས་གནས་ཁོངས་དེ་%Sགིས་འཛིན་སྐྱོང་མ་འཐབ་པས)"

#: restartBeforeEnableTitle
msgid "Enable Extension"
msgstr "རྒྱ་བསྐྱེད་ལྕོགས་ཅན་བཟོ།"

#: restartBeforeDisableTitle
msgid "Disable Extension"
msgstr "རྒྱ་བསྐྱེད་ལྕོགས་མིན་བཟོ།"

#: incompatibleUpdateMessage
msgid "%S is checking for a compatibility update to %S."
msgstr "%Sགིས་ %Sལུ་མཐུན་འགྱུར་གྱི་དུས་མཐུན་ཅིག་ཡོད་མེད་ ཞིབ་དཔྱད་འབད་དོ།"

#: installSuccess
msgid "Install completed successfully"
msgstr "གཞི་བཙུགས་མཐར་འཁྱོལ་སྦེ་ མཇུག་བསྡུ་ཡི།"

#: installWaiting
msgid "Waiting..."
msgstr "བསྒུག་སྡོད་དོ་་་་་"

#: installInstalling
msgid "Installing..."
msgstr "གཞི་བཙུགས་འབད་དོ་་་་"

#: droppedInWarning
msgid "The following items were found in your Extensions folder. Do you want to install them?"
msgstr "ཁྱོད་ཀྱི་རྒྱ་བསྐྱེད་སྣོད་འཛིན་ནང་ འོག་གི་རྣམ་གྲངས་ཚུ་འདུག། ཁྱོད་ཀྱིས་དེ་ཚུ་གཞི་བཙུགས་འབད་ནི་ཨིན་ན?"

#: uninstallButton
msgid "Uninstall"
msgstr "གཞི་བཙུགས་བཤོལ།"

#: disableButton
msgid "Disable"
msgstr "ལྕོགས་མིན་བཟོ་"

#: cancelButton
msgid "Cancel"
msgstr "ཆ་མེད་གཏང་"

#: restartButton
msgid "Restart %S"
msgstr "%Sལོག་སྟེ་འགོ་བཙུགས་"

#: laterButton updatesAvailableCancel
msgid "Later"
msgstr "ཤུལ་ལས་"

#: moreInfoText
msgid "More information"
msgstr "བརྡ་དོན་མངམ་"

#: uninstallTitle
msgid "Uninstall %S"
msgstr "%Sདེ་གཞི་བཙུགས་བཤོལ།"

#: uninstallWarnDependMsg
msgid "%S is required by one or more add-ons. If you continue, the following items will be disabled:"
msgstr "%Sའདི་ཁ་སྐོང་གཅིག་ ཡང་ན་ དེ་ལས་མངམ་གིས་དགོཔ་འདུག། ཁྱོད་ཀྱིས་འཕྲོ་མཐུད་པ་ཅིན་ འོག་གི་རྣམ་གྲངས་ཚུ་ ལྕོགས་མིན་བཟོ་འོང་།"

#: uninstallQueryMessage
msgid "Do you want to uninstall %S?"
msgstr "ཁྱོད་ཀྱིས་%Sདེ་གཞི་བཙུགས་བཤོལ་ནི་ཨིན་ན?"

#: disableTitle
msgid "Disable %S"
msgstr "%Sལྕོགས་མིན་བཟོ་"

#: disableWarningDependMessage
msgid "If you disable %S, the following items that require this extension will also be disabled:"
msgstr "ཁྱོད་ཀྱིས་%Sལྕོགས་མིན་བཟོ་བ་ཅིན་ འོག་གི་རྣམ་གྲངས་ རྒྱ་སྐྱེད་འདི་དགོ་མི་ཚུ་ཡང་ ལྕོགས་མིན་བཟོ་འོང་་:"

#: disableQueryMessage
msgid "Do you want to disable %S?"
msgstr "ཁྱོད་ཀྱིས་%Sལྕོགས་མིན་བཟོ་ནི་ཨིན་ན?"

