#: window.width
msgid ""
"_: Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger.\n"
"38em"
msgstr "ཨི་ཨེམ་༣༨།"

#: ocspDialog.title
msgid "Verification"
msgstr "བདེན་སྦྱོར་"

#: validation.ocsp.description
msgid "&brandShortName; can use Online Certificate Status Protocol (OCSP) to verify certificates. Set &brandShortName; to use OCSP as follows:"
msgstr "&brandShortName; གིས་ ལག་ཁྱེར་ཚུ་བདེན་སྦྱོར་འབད་ནི་ལུ་ གློག་ཐོག་ལག་ཁྱེར་གནས་ཚད་གནད་སྤེལ་ལམ་ལུགས་(ཨོ་སི་ཨེསི་པི) ལག་ལེན་འཐབ་ཚུགས། ཨོ་སི་ཨེསི་པི་ ལག་ལེན་འཐབ་ནི་ལུ་ &brandShortName; དེ་ གཤམ་གསལ་ལྟར་དུ་་གཞི་སྒྲིག་འབད་:"

#: disableOCSP.label
msgid "Do not use OCSP for certificate validation"
msgstr "ལག་ཁྱེར་ནུས་ལྡན་བཟོ་ནི་ལུ་ ཨོ་སི་ཨེསི་པི་ ལག་ལེན་མ་འཐབ།"

#: certOCSP.label
msgid "Use OCSP to validate only certificates that specify an OCSP service URL"
msgstr "ཨོ་སི་ཨེསི་པི་ ཞབས་ཏོག་གི་ ཡུ་ཨར་ཨེལ་ཅིག་ གསལ་བཀོད་འབད་མི་ ལག་ཁྱེར་ཚུ་རྐྱངམ་གཅིག་ ནུས་ལྡན་བཟོ་ནི་ལུ་་ཨོ་སི་ཨེསི་པི་ ལག་ལེན་འཐབ།"

#: proxyOCSP.label
msgid "Use OCSP to validate all certificates using this URL and signer:"
msgstr "ཡུ་ཨར་ཨེལ་འདི་དང་ མིང་རྟགས་བཀོད་མི་ ལག་ལེན་འཐབ་མི་ ལག་ཁྱེར་ཆ་མཉམ་ ནུས་ལྡན་བཟོ་ནི་ལུ་ ཨོ་སི་ཨེསི་པི་ ལག་ལེན་འཐབ།"

#: serviceURL.label
#: serviceURL.accesskey
msgid "&Service URL:"
msgstr "ཞབས་ཏོག་གི་ཡུ་ཨར་ཨེལ་:(&S)"

#: signingCA.label
#: signingCA.accesskey
msgid "&Response Signer:"
msgstr "ངོས་ལན་ལུ་མིང་རྟགས་བཀོད་མི་:(&R)"

