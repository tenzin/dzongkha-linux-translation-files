#: transferred
msgid "%1SKB of %2SKB"
msgstr "%2SKBགི་%1SKB"

#: downloading
msgid "Downloading"
msgstr "ཕབ་ལེན་འབད་དོ།"

#: notStarted
msgid "Not Started"
msgstr "འགོ་མ་བཙུགས་པས།"

#: failed
msgid "Failed"
msgstr "འཐུས་ཤོར་བྱུང་ནུག"

#: finished
msgid "Finished"
msgstr "མཇུག་བསྡུ་ནུག"

#: canceled
msgid "Canceled"
msgstr "ཆ་མེད་གཏང་ནུག"

#: downloadErrorAlertTitle
msgid "Download Error"
msgstr "འཛོལ་བ་འདི་ཕབ་ལེན་འབད།"

#: quitCancelDownloadsAlertTitle
#: offlineCancelDownloadsAlertTitle
msgid "Cancel All Downloads?"
msgstr "ཕབ་ལེན་ཚུ་ཆ་མཉམ་ཆ་མེད་གཏང་ག?"

#: quitCancelDownloadsAlertMsg
msgid "If you exit now, 1 download will be canceled. Are you sure you want to exit?"
msgstr "ཁྱོད་ཀྱིས་ད་ལྟོ་ཕྱིར་འཐོན་པ་ཅིན་  ཕབ་ལེན་ ༡ ་ཆ་མེད་གཏང་འོང་།  ཁྱོད་ཕྱིར་འཐོན་ནི་འདི་ངོས་ཏིག་ཨིན་ན?"

#: quitCancelDownloadsAlertMsgMultiple
msgid "If you exit now, %S downloads will be canceled. Are you sure you want to exit?"
msgstr "ཁྱོད་ཀྱིས་ད་ལྟོ་ཕྱིར་འཐོན་པ་ཅིན་  ཕབ་ལེན་ %S འདི་ཆ་མེད་གཏང་འོང་། ཁྱོད་ཕྱིར་འཐོན་ནི་འདི་ངེས་ཏིག་ཨིན་ན?"

#: quitCancelDownloadsAlertMsgMac
msgid "If you quit now, 1 download will be canceled. Are you sure you want to quit?"
msgstr "ད་ལྟོ་ཁྱོད་སྤང་ནི་ཨིན་པ་ཅིན་  ཕབ་ལེན་ ༡ ་ཆ་མེད་གཏང་འོང་། ཁྱོད་ངེག་ཏིག་རང་སྤང་ནི་ཨིན་ན?"

#: quitCancelDownloadsAlertMsgMacMultiple
msgid "If you quit now, %S downloads will be canceled. Are you sure you want to quit?"
msgstr "ཁྱོད་ད་ལྟོ་སྤང་པ་ཅིན་  ཕབ་ལེན་ %S འདི་ཆ་མེད་གཏང་འོང་།  ཁྱོད་ངེས་ཏིག་རང་སྤང་ནི་ཨིན་ན?"

#: offlineCancelDownloadsAlertMsgMultiple
msgid "If you go offline now, %S downloads will be canceled. Are you sure you want to go offline?"
msgstr "ཁྱོད་ཀྱིས་ད་ལྟོ་ཨོཕ་ལ་ཡིན་འགྱོ་བ་ཅིན་  ཕབ་ལེན་  %S འདི་ཆ་མེད་གཏང་འོང་།  ཁྱོད་ཨོཕ་ལ་ཡིན་འགྱོ་ནི་འདི་ངེས་ཏིག་ཨིན?"

#: offlineCancelDownloadsAlertMsg
msgid "If you go offline now, 1 download will be canceled. Are you sure you want to go offline?"
msgstr "ཁྱོད་ཨོཕ་ལ་ཡིན་འགྱོ་བ་ཅིན་  ཕབ་ལེན་ ༡ ཆ་མེད་འགྱོ་འོང་།  ཁྱོད་ཨོཕ་ལ་ཡིན་འགྱོ་ནི་འདི་ངེས་ཏིག་ཨིན་ན?"

#: cancelDownloadsOKText
msgid "Cancel 1 Download"
msgstr "ཕབ་ལེན་ ༡ ཆ་མེད་གཏང་།"

#: cancelDownloadsOKTextMultiple
msgid "Cancel %S Downloads"
msgstr "ཕབ་ལེན་ %S ཚུ་ཆ་མེད་གཏང་།"

#: dontQuitButtonWin
msgid "Don't Exit"
msgstr "ཕྱིར་མ་འཐོན།"

#: dontQuitButtonMac
msgid "Don't Quit"
msgstr "མ་སྤང་།"

#: dontGoOfflineButton
msgid "Stay Online"
msgstr "གློག་ཐོག་སྡོད།"

#: downloadsCompleteTitle
msgid "Downloads Complete"
msgstr "ཕབ་ལེན་ཚུ་མཇུག་བསྡུ།"

#: downloadsCompleteMsg
msgid "All files have finished downloading."
msgstr "ཡིག་སྣོད་ཚུ་ཆ་མཉམ་ཕབ་ལེན་འབད་ཚར་ནུག"

#: statusFormatKBKB
msgid "#1 of #2 KB"
msgstr " #2 KB གི་ #1"

#: statusFormatKBMB
msgid "#1 KB of #2 MB"
msgstr " #2 MB གི་ #1 KB"

#: statusFormatMBMB
msgid "#1 of #2 MB"
msgstr " #2 MB གི་ #1"

#: statusFormatUnknownKB
msgid "#1 KB"
msgstr "#1 KB"

#: statusFormatUnknownMB
msgid "#1 MB"
msgstr "#1 MB"

#: remain
msgid "remain"
msgstr "ལྷག་ལུས།"

#: unknownFilesize
msgid "unknown file size"
msgstr "མ་ཤེས་པའི་ཡིག་སྣོད་ཀྱི་ཚད།"

#: statusFormat
msgid "#1 at #2 KB/sec; #3"
msgstr "#2 KB/sec; #3 ལུ་ #1"

#: longTimeFormat
msgid "#1:#2:#3"
msgstr "#1:#2:#3"

#: shortTimeFormat
msgid "#2:#3"
msgstr "#2:#3"

#: fileDoesNotExistOpenTitle
msgid "Cannot Open %S"
msgstr "%S འདི་ཁ་ཕྱེ་མི་ཚུགས།"

#: fileDoesNotExistShowTitle
msgid "Cannot Show %S"
msgstr "%S འདི་སྟོན་མི་ཚུགས།"

#: fileDoesNotExistError
msgid "%S does not exist. It may have been renamed, moved, or deleted since it was downloaded."
msgstr "%S འདི་གནས་དེ་མེད།  ཕབ་ལེན་འབད་ཚར་བའི་བསྒང་ལས་རང་  བསྐྱར་མིང་བཏགས་ནི་  སྤོ་བཤུད་འབད་ནི་  ཡང་ན་  བཏོན་གཏངམ་འོང་།"

#: chooseAppFilePickerTitle
msgid "Open With..."
msgstr "དང་བཅས་ཁ་ཕྱེ..."

#: downloadsTitle
msgid "%S%% of 1 file - Downloads"
msgstr "ཡིག་སྣོད་ ༡ གི་ %S%% - འདི་ཕབ་ལེན་འབདཝ་ཨིན།"

#: downloadsTitleMultiple
msgid "%S%% of %S files - Downloads"
msgstr "%S ཡིག་སྣོད་ཚུ་གི་  %S%% - འདི་ཕབ་ལེན་འབདཝ་ཨིན།"

#: fileExecutableSecurityWarning
msgid "\"%S\" is an executable file. Executable files may contain viruses or other malicious code that could harm your computer. Use caution when opening this file. Are you sure you want to launch \"%S\"?"
msgstr "\"%S\" འདི་ ལག་ལེན་འཐབ་བཏུབ་པའི་ཡིག་སྣོད་ཨིན།  ལག་ལེན་འཐབ་བཏུབ་པའི་ཡིག་སྣོད་དེ་གིས་  ཁྱོད་ཀྱི་གློག་རིག་ལུ་གནོད་ཚུགས་པའི་  གནོད་འབུབ་ཡང་ན་  གནོད་ཅན་གྱི་ཨང་རྟགས་ཚུ་གནས་དེ་འོངམ་ཨིན།  ཡིག་སྣོད་འདི་ཁ་ཕྱེ་བའི་བསྒང་  དྲནམ་བཏོན་འདི་ལག་ལེན་འཐབ།  ཁྱོད་  \"%S\" གསར་བཙུགས་འབད་ནི་འདི་ངེས་ཏིག་ཨིན་ན?"

#: fileExecutableSecurityWarningTitle
msgid "Open Executable File?"
msgstr "ལག་ལེན་འཐབ་བཏུབ་པའི་ཡིག་སྣོད་འདི་ ཁ་ཕྱེ་ག?"

#: fileExecutableSecurityWarningDontAsk
msgid "Don't ask me this again"
msgstr "འདི་ང་ལུ་ལོག་འདི་བ་འདྲི།"

#: displayNameDesktop
msgid "Desktop"
msgstr "ཌེཀསི་ཊོཔ།"

