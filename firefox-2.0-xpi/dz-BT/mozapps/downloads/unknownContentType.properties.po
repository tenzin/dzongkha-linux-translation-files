#: title
msgid "Opening %S"
msgstr "%Sའདི་ཁ་ཕྱེ་དོ།"

#: saveDialogTitle
msgid "Enter name of file to save to..."
msgstr "སྲུང་ནི་ལུ་  ཡིག་སྣོད་ཀྱི་མིང་འདི་ཐོ་བཀོད་འབད..."

#: defaultApp
msgid "%S (default)"
msgstr "%S (སྔོན་སྒྲིག)"

#: chooseAppFilePickerTitle
msgid "Choose Helper Application"
msgstr "གྲོག་རམ་འབད་མི་འཇུག་སྤྱོད་འདི་གདམ་ཁ་བརྐྱབ།"

#: badApp
msgid "The application you chose (\"%S\") could not be found.  Check the file name or choose another application."
msgstr "ཁྱོད་ཀྱིས་གདམ་ཁ་བརྐྱབ་ཡོད་པའི་ (\"%S\") འདི་འཐོབ་མ་ཚུགས།  ཡིག་སྣོད་མིང་འདི་ཞིབ་དཔྱད་འབད་ནི་  ཡང་ན་  འཇུག་སྤྱོད་གཞན་ཅིག་གདམ་ཁ་བརྐྱབ།"

#: badApp.title
msgid "Application not found"
msgstr "འཇུག་སྤྱོད་འདི་འཐོབ་མ་ཚུགས།"

#: selectDownloadDir
msgid "Select Download Folder"
msgstr "ཕབ་ལེན་སྣོད་འཛིན་འདི་སེལ་འཐུ་འབད།"

#: myDownloads
msgid "My Downloads"
msgstr "ངེས་ཀྱི་ཕབ་ལེན་ཚུ།"

#: unknownAccept.label
msgid "Save File"
msgstr "ཡིག་སྣོད་སྲུངས་"

#: unknownCancel.label
msgid "Cancel"
msgstr "ཆ་མེད་གཏང་"

