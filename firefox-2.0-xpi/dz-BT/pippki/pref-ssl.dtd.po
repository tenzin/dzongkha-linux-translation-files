#: SSLProtocolVersions.caption
msgid "SSL Protocol Versions"
msgstr "ཨེསི་ཨེསི་ཨེལ་ གནས་སྤེལ་ལམ་ལུགས་ཐོན་རིམ་ཚུ་"

#: SSLWarnings.caption
msgid "SSL Warnings"
msgstr "ཨེསི་ཨེསི་ཨེལ་གྱི་ཉེན་བརྡ་"

#: SSLClientAuthMethod.caption
msgid "Client Certificate Selection"
msgstr "ཞབས་ཏོག་སྤྱོད་མིའི་ལག་ཁྱེར་གྱི་སེལ་འཐུ་"

#: pref.ssl.title
msgid "Secure Sockets Layer (SSL)"
msgstr "སོ་ཀེཊི་བི་རིམ་མཐའ་བཙན་བཟོ་(ཨེསི་ཨེསི་ཨེལ་)"

#: enable.ssl3
msgid "Enable SSL version 3"
msgstr "ཨེསི་ཨེསི་ཨེལ་ ཐོན་རིམ་༣ པ་ ལྕོགས་ཅན་བཟོ་"

#: enable.ssl3.accesskey
msgid "3"
msgstr "༣"

#: enable.tls
msgid "Enable TLS"
msgstr "ཊི་ཨེལ་ཨེསི་ ལྕོགས་ཅན་བཟོ་"

#: enable.tls.accesskey
msgid "T"
msgstr "ཊི་"

#: warn.description
msgid "&brandShortName; can alert you to the security status of the web page you are viewing. Set &brandShortName; to show a warning and ask permission before:"
msgstr "&brandShortName; གིས་ ཁྱོད་ཀྱིས་བལྟ་བའི་བསྒང་ཡོད་མི་ ཤོག་ལེབ་དེ་གི་ སྲུང་སྐྱོབ་གནས་སྟངས་ཀྱི་ དྲན་བརྡ་འབད་ཚུགས་ཨིན། &brandShortName; གཞི་སྒྲིག་འབད་ཞིནམ་ལས་ ཉེན་བརྡ་སྟོན་ གནང་བ་ལེན་:"

#: warn.enteringsecure
msgid "Loading a page that supports encryption"
msgstr "གསང་བཟོ་ལུ་རྒྱབ་སྐྱོར་འབད་མི་ ཤོག་ལེབ་ཅིག་མངོན་གསལ་མ་འབད་བའི་ཧེ་མར་"

#: warn.enteringsecure.accesskey
msgid "L"
msgstr "ཨེལ་"

#: warn.enteringweak
msgid "Loading a page that uses low-grade encryption"
msgstr "གསང་བཟོའི་སྡེ་རིམ་ཆུང་བའི་ཤོག་ལེབ་ཅིག་ མངོན་གསལ་མ་འབད་བའི་ཧེ་མ་"

#: warn.enteringweak.accesskey
msgid "o"
msgstr "ཨོ་"

#: warn.insecurepost
msgid "Sending form data from an unencrypted page to an unencrypted page"
msgstr "འབྲི་ཤོག་གནས་སྡུད་ གསང་བ་མ་བཟོ་བའི་ཤོག་ལེབ་ཅིག་ གསང་བ་མ་བཟོ་བའི་ ཤོག་ལེབ་ཅིག་ལུ་ མ་བཏང་པའི་ཧེ་མ་"

#: warn.insecurepost.accesskey
msgid "S"
msgstr "ཨེསི་"

#: warn.leavingsecure
msgid "Leaving a page that supports encryption"
msgstr "གསང་བཟོ་ལུ་རྒྱབ་སྐྱོར་འབད་མི་ ཤོག་ལེབ་ཅིག་ བཞག་མ་འགྱོ་བའི་ཧེ་མ་"

#: warn.leavingsecure.accesskey
msgid "a"
msgstr "ཨེ་"

#: warn.secureredirect
msgid "Redirecting from one encrypted page to another"
msgstr "གསང་བཟོས་ཤོག་ལེབ་གཅིག་ལས་ གཞན་མི་གཅིག་ལུ་ སླར་ལོག་མ་བཏང་པའི་ཧེ་མ་"

#: warn.secureredirecttoinsecure
msgid "Redirecting from an encrypted page to an unencrypted page"
msgstr "གསང་བཟོ་ཤོག་ལེབ་ཅིག་ལས་ གསང་བ་མ་བཟོ་བའིང་ཤོག་ལེབ་ཅིག་ལུ་ སླར་ལོག་མ་བཏང་པའི་ཧེ་མ་"

#: warn.viewmixed
msgid "Viewing a page with an encrypted/unencrypted mix"
msgstr "གསང་བཟོས་དང་གསང་བ་མ་བཟོཝ སླ་བསྲེས་འབད་མི་ཤོག་ལེབ་ཅིག་ལུ་ མ་བལྟའི་ཧེ་མ་"

#: warn.viewmixed.accesskey
msgid "V"
msgstr "ཝི་"

#: certselect.description
msgid "Decide how &brandShortName; selects a security certificate to present to web sites that require one:"
msgstr "&brandShortName; གིས་ སྲུང་སྐྱོབ་ལག་ཁྱེར་ཅིག་ སེལ་འཐུ་འབད་དེ་ དེ་དགོ་མི་ཝེབ་ས་ཁོངས་ལུ་ ག་དེ་སྦེ་བྱིནམ་ཨིན་ན་ཐག་གཅད་:"

#: certselect.auto
msgid "Select Automatically"
msgstr "རང་བཞིན་གྱིས་སེལ་འཐུ་འབད་"

#: certselect.auto.accesskey
msgid "A"
msgstr "ཨེ་"

#: certselect.ask
msgid "Ask Every Time"
msgstr "ཨ་རྟག་རང་དྲིས་"

#: certselect.ask.accesskey
msgid "E"
msgstr "E"

