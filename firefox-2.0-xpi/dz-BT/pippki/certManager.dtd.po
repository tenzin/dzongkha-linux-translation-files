msgid "&Close"
msgstr "ཁ་བསྡམས་(&C)"

#: certmgr.view.label certmgr.view.accesskey
msgid "&View"
msgstr "སྟོན་(&V)"

#: certmgr.edit.label certmgr.edit.accesskey
msgid "&Edit"
msgstr "ཞུ་དག་(&E)"

#: certmgr.editca.label certmgr.editca.accesskey
msgid "E&dit CA Trust"
msgstr "ལག་ཁྱེར་དབང་འཛིན་གྱི་བློ་གཏད་ཞུན་དག་རྐྱབས།(&d)"

#: certmgr.add.label certmgr.add.accesskey
msgid "&Add"
msgstr "ཁ་སྐོང་རྐྱབས་(&A)"

#: certmgr.delete.label certmgr.delete.accesskey
msgid "&Delete"
msgstr "བཏོན་གཏང་(&D)"

#: certmgr.backup.label certmgr.backup.accesskey
msgid "&Backup"
msgstr "རྒྱབ་ཐག་(&B)"

#: certmgr.backupall.label certmgr.backupall.accesskey
msgid "Bac&kup All"
msgstr "ཆ་མཉམ་ལུ་རྒྱབ་ཐག་འབད་(&k)"

#: certmgr.restore.label certmgr.restore.accesskey
msgid "I&mport"
msgstr "ནང་འདྲེན་འབད་(&m)"

#: certmgr.details.label certmgr.details.accesskey
msgid "Certificate &Fields"
msgstr "ལག་ཁྱེར་ས་སྒོ་(&F)"

#: certmgr.fields.label certmgr.fields.accesskey
msgid "Field &Value"
msgstr "གནས་གོང་འཚོལ་(&V)"

#: certmgr.hierarchy.label certmgr.hierarchy.accesskey
msgid "&Certificate Hierarchy"
msgstr "ལག་ཁྱེར་གྱི་གནས་རིམ་(&C)"

