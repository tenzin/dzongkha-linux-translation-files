msgid "&Under Color Removal"
msgstr "ཚོས་གཞི་རྩ་བསྐྲད་འབད་བཏུབ་པའི་འོག་ལུ།(&U)"

msgid "&Display CMYK"
msgstr "སི་ཨེམ་ཝའི་ཀེ་ བཀྲམ་སྟོན་འབད།(&D)"

msgid "&C"
msgstr "སི།(&C)"

msgid "&M"
msgstr "ཨེམ།(&M)"

msgid "&Y"
msgstr "ཝའི།(&Y)"

msgid "&K"
msgstr "ཀེ(&K)"

msgid ""
"Provides a more pleasant view of text items in the viewer, at the expense\n"
"of a slight slowdown in previewing. This only affects Type 1 fonts"
msgstr ""
"སྔོན་ལྟ་འབདཝ་ད་ ལྷོད་ལེན་དུམ་གྲ་ཅིག་གི་སྒོ་ལས་\n"
" མཐོང་སྣང་སྟོན་མིའི་ནང་ལུ་ ཚིག་ཡིག་རྣམ་གྲངས་ཚུའི་མཐོང་སྣང་བྱ་མཆིས་ཆི་ལེ་ཤ་ཅིག་བྱིནམ་ཨིན། འདི་གིས་ དབྱེ་བ་ ༡ གི་ཡིག་གཟུགས་ཚུ་ལུ་རྐྱངམ་ཅིག་ ཕན་གནོད་འབྱུངམ་ཨིན།"

msgid ""
"Provides a more pleasant view of TrueType Fonts, OpenType Fonts, EPS, PDF and\n"
"vector graphics in the preview, at the expense of a slight slowdown in previewing"
msgstr ""
"སྔོན་ལྟ་འབདཝ་ད་ ལྷོད་ལེན་དུམ་གྲ་ཅིག་གི་སྒོ་ལས་\n"
" སྔོན་ལྟ་ནང་ལུ་ བདེན་པའི་དབྱེ་བའི་ཡིག་གཟུགས་ཚུ་དང་ཨི་པི་ཨེསི་ དེ་ལས་པི་ཌི་ཨེཕི་དང་མཉམ་ཐིག་ཚད་རིས་ཚུའི་མཐོང་སྣང་བྱ་མཆིས་ཆི་ལེ་ཤ་ཅིག་བྱིནམ་ཨིན། "

msgid "Shows transparency and transparent items in your document. Requires Ghostscript 7.07 or later"
msgstr "ཁྱོད་ཀྱི་ཡིག་ཆ་ནང་ལུ་ དྭངས་གསལ་དང་དྭངས་གསལ་རྣམ་གྲངས་ཚུ་སྟོནམ་ཨིན། གྲིབ་ཡིག་ ༧.༠༧ ཡང་ན་ ཤུལ་ལས་ དགོསཔ་ཨིན།"

msgid "Gives a print preview using simulations of generic CMYK inks, instead of RGB colors"
msgstr "ཨར་ཇི་བི་ཚོས་གཞི་ཚུ་མ་ཡིན་པའི་ སི་ཨེམ་ཝའི་ཀེ་སྣག་རྩི་གི་དབྱེ་རིགས་ཚུའི་མཚུངས་བཟོ་ལག་ལེན་གྱི་ཐོག་ལས་ དཔར་བསྐྲུན་སྔོན་ལྟ་བྱིནམ་ཨིན།"

msgid "Enable/disable the C (Cyan) ink plate"
msgstr "སི་ (སྔོ་ལྗང་)སྣག་རྩིའི་ཧོད་ ལྕོགས་ཅན་/ལྕོགས་མིན་བཟོ།"

msgid "Enable/disable the M (Magenta) ink plate"
msgstr "ཨེམ་ (དམར་སྨུག་)སྣག་རྩིའི་ཧོད་ ལྕོགས་ཅན་/ལྕོགས་མིན་བཟོ།"

msgid "Enable/disable the Y (Yellow) ink plate"
msgstr "ཝའི་ (སེརཔོ་)སྣག་རྩིའི་ཧོད་ ལྕོགས་ཅན་/ལྕོགས་མིན་བཟོ།"

msgid "Enable/disable the K (Black) ink plate"
msgstr "ཀེ (གནགཔོ་)སྣག་རྩིའི་ཧོད་ ལྕོགས་ཅན་/ལྕོགས་མིན་བཟོ།"

msgid "firstPageOrder is bigger than allowed."
msgstr "ཤོག་ལེབ་དང་པའི་གོ་རིམ་འདི་ འབད་ཆོག་མི་འདི་ལས་སྦོམ།"

msgid "Failed to open document"
msgstr "ཡིག་ཆ་ཁ་ཕྱེ་ནི་ འཐུས་ཤོར་བྱུང་ནུག"

msgid "Failed to save document"
msgstr "ཡིག་ཆ་སྲུང་ནི་ འཐུས་ཤོར་བྱུང་ནུག"

msgid "Unit out of range. Use one of the scribus.UNIT_* constants."
msgstr "ཁྱབ་ཚད་ལས་བརྒལ་བའི་ཆ་ཕྲན། སི་ཀིརི་བསི་གཅིག་ལག་ལེན་འཐབ། UNIT * ཆད་མེད།(_*)"

msgid "Passed object is not callable"
msgstr "རྩིས་སྤྲོད་འབད་ཡོད་པའི་དངོས་པོ་འདི་ བོད་བརྡ་འབད་བཏུབ་མེན།"

msgid "Scribus - New Macro"
msgstr "སི་ཀིརི་བསི་-མེ་ཀོརོ་གསརཔ།"

msgid "<qt>This is the Edit Macro / New Macro dialog box. Here you can change the source code to macros. Edit the source code to the macro in the text editing area below the \"Source Code\" label and click OK to save your changes to the macro.</qt>"
msgstr "<qt>འ་ནི་འདི་ ཞུན་དག་མེ་ཀོརོ་ / མེ་ཀོརོ་གསརཔ་གི་ཌའི་ལོག་སྒྲོམ་ཨིན། འ་ན་ལུ་ཁྱོད་ཀྱིས་ འབྱུང་ཁུངས་ཨང་རྟགས་འདི་ མེ་ཀོརོསི་ལུ་ བསྒྱུར་བཅོས་འབད་ཚུགས། འབྱུང་ཁུངས་ཨང་རྟགས་འདི་ \"Source Code\"ཁ་ཡིག་གི་འོག་ལུ་ ཚིག་ཡིག་ཞུན་དག་མངའ་ཁོངས་ནང་ མེ་ཀོརོ་ལུ་ ཞུན་དག་འབད་ཞིནམ་ལས་ ཁྱོད་ཀྱི་བསྒྱུར་བཅོས་ཚུ་མེ་ཀོརོ་ལུ་སྲུང་ནིའི་དོན་ལས་ བཏུབ་ལུ་ ཨེབ་གཏང་འབད།</qt>"

msgid "Source Code:"
msgstr "འབྱུང་ཁུངས་ཨང་རྟགས་:"

msgid "Editing Macro:"
msgstr "ཞུན་དག་མེ་ཀོརོ་:"

msgid "The name of the macro being edited."
msgstr "ཞུན་དག་འབད་ཡོད་པའི་མེ་ཀོརོ་གི་མིང་།"

msgid "</qt>This item displays the name of the macro you are currently editing.<qt>"
msgstr "</qt>རྣམ་གྲངས་འདི་གིས་ ཁྱོད་ཀྱི་ད་ལྟོ་ཞུན་དག་འབད་དོ་ཡོད་པའི་མེ་ཀོརོ་གི་མིང་ བཀྲམ་སྟོན་འབདཝ་ཨིན།<qt>"

msgid "<qt>Discard all changes and exit.</qt>"
msgstr "<qt>བསྒྱུར་བཅོས་ཚུ་ཆ་མཉམ་རང་བཏོན་བཀོག་ཞིནམ་ལས་ཕྱི་ཁར་ཐོན།</qt>"

msgid "<qt>Exit the editing dialog, discarding all changes you have made. If you want to exit without saving the macro but don't want to lose your changes, save your changes with \"Save Source As...\".</qt>"
msgstr "</qt>ཁྱོད་ཀྱིས་བསྒྱ྄ར་བཅོས་འབད་ཡོད་མི་ཚུ་ཆ་མཉམ་རང་བཏོན་བཀོག་སྟེ་ ཞུན་དག་ཌའི་ལོག་ ཕྱི་ཁར་བཏོན། ཁྱོད་ཀྱིས་ མེ་ཀོརོ་སྲུང་བཞག་མ་འབད་བ་ཞིན་ན་ བསྒྱུར་བཅོས་ཚུ་མ་བྱང་བར་ཕྱི་ཁར་འཐོན་ནི་ཨིན་པ་ཅིན་  \"Save Source As...\"གི་ཐོག་ལས་ ཁྱོད་རའི་བསྒྱུར་བཅོས་ཚུ་ སྲུངས།</qt>"

msgid "&Ok"
msgstr "བཏུབ།(&O)"

msgid "<qt>Save changes and exit.</qt>"
msgstr "<qt>བསྒྱུར་བཅོས་ཚུ་སྲུང་ཞིནམ་ལས་ཕྱི་ཁར་འཐོན།<qt>"

msgid "<qt>Save changes to the macro and exit. If there is a problem with the macro, a message will be displayed and the editing dialog will not close.</qt>"
msgstr "<qt>བསྒྱུརབཅོས་ཚུ་མེ་ཀོརོ་ལུ་སྲུངས་ཞིནམ་ལས་ཕྱི་ཁར་འཐོན། མེ་ཀོརོ་ལུ་དཀའ་ངལ་ཡོད་པ་ཅིན་ འཕྲིན་དོན་འདི་བཀྲམ་སྟོན་འབད་ནི་ཨིནམ་དང་ ཞུན་དག་ཌའི་ལོག་འདི་སྒོ་མི་བསྡམ།</qt>"

msgid "<qt>This text area contains the source code of the macro. If you're creating a new macro there won't be anything in it, and if you're editing an existing macro the source code the macro was defined with will be shown here.</qt>"
msgstr "<qt>ཚིག་ཡིག་འདི་ནང་ལུ་ མེ་ཀོརོ་གི་འབྱུང་ཁུངས་ཨང་རྟགས་ཡོདཔ་ཨིན། ཁྱོད་ཀྱིས་ མེ་ཀོརོ་གསརཔ་ཅིག་གསར་བསྐྲུན་འབད་ནི་ཨིན་པ་ཅིན་ དེ་ནང་ག་ཅི་ཡང་མེདཔ་ཨིན་ དེ་ལས་ ཁྱོད་ཀྱིས་ ཡོད་བཞིན་པའི་མེ་ཀོརོ་འདི་ཞུན་དག་འབདཝ་ཨིན་པ་ཅིན་ ངེས་འཛིན་འབད་ཡོད་པའི་མེ་ཀོརོ་དང་བཅས་ འབྱུང་ཁུངས་ཨང་རྟགས་འདི་ ནཱ་ལུ་སྟོན་འོང་།</qt>"

msgid "&Load Source ..."
msgstr "མངོན་གསལ་འབྱུང་ཁུངས་་་་(&L)"

msgid "Alt+L"
msgstr "གདམ་ལྡེ་+ཨེལ།"

msgid "<qt>Replace the current source code with code from a file.</qt>"
msgstr "<qt>ཡིག་སྣོད་ནང་ལས་ ད་ལྟོའི་འབྱུང་ཁུངས་ཨང་རྟགས་འདི་ཨང་རྟགས་དང་བཅས་ ཚབ་བཙུགས།<qt>"

msgid "<qt>Load new source code into the editing area from \"file\". Any source code in the editing area is replaced. The loaded source must be a Scribus macro function. If you load any other script, you'll need to edit it so that it'll work as a scripter macro before saving it.</qt>"
msgstr "<qt>\"file\" ནང་ལས་ འབྱུང་ཁུངས་ཨང་རྟགས་གསརཔ་འདི་ ཞུན་དག་མངའ་ཁོངས་ནང་ལུ་ མངོན་གསལ་འབད། ཞུན་དག་མངའ་ཁོངས་ནང་ འབྱུང་ཁུངས་ཨང་རྟགས་གང་རུང་ཅིག་ ཚབ་བཙུགས་ཅི། མངོན་གསལ་འབད་ཡོད་པའི་འབྱུང་ཁུངས་འདི་ སི་ཀིརི་བསི་ མེ་ཀོརོ་ ལས་འགན་ཅིག་ཨིན་དགོ ཁྱོད་ཀྱིས་ ཡིག་ཚུགས་གང་རུང་གཞན་ཅིག་མངོན་གསལ་འབད་བ་ཅིན་ འདི་ཞུན་དག་འབད་དགོ་ དེ་འབད་ཅིན་ མ་སྲུང་བའི་ཧེ་མ་ ཡིག་ཚུགས་པ་སྦེ་ ལཱ་འབད་འོང་།</qt>"

msgid "&Save Source As..."
msgstr "དེ་བཟུམ་སྦེ་ སྲུངས་་་་(&S)"

msgid "Alt+S"
msgstr "གདམ་ལྡེ་+ཨེསི།"

msgid "<qt>Save the source code being edited to a file.</qt>"
msgstr "<qt>ཡིག་སྣོད་ལུ་ཞུན་དག་འབད་ཡོདཔ་ལས་ འབྱུང་ཁུངས་ཨང་རྟགས་ སྲུངས།<qt>"

msgid "Save the source code - the text - of the macro to a file. You can edit the saved source and load it again with \"Load Source...\"."
msgstr "མེ་ཀོརོ་གི་འབྱུང་ཁུངས་ཨང་རྟགས་-ཚིག་ཡིག་-ཡིག་སྣོད་ལུ་སྲུངས།  ཁྱོད་ཀྱིས་ སྲུངས་ཡོད་པའི་འབྱུང་ཁུངས་འདི་ ཞུན་དག་འབད་ཚུགསཔ་དང་ \"Load Source...\"གི་ཐོག་ལས་ དོ་རུང་མངོན་གསལ་འབད་ཚུགས།"

