#: ../gtk/gsynaptic.cc:70
msgid "Usage: synaptic [options]\n"
msgstr "བེད་སྤྱོད་: སའིན་ཨེཔ་ཊིཀ་ [options]\n"

#: ../gtk/gsynaptic.cc:71
msgid "-h   This help text\n"
msgstr "-h  གྲོགས་རམ་ཚིག་ཡིག་འདི།\n"

#: ../gtk/gsynaptic.cc:72
msgid "-r   Open in the repository screen\n"
msgstr "-r   མཛོད་གནས་གསལ་གཞི་ནང་ཁ་ཕྱེ།\n"

#: ../gtk/gsynaptic.cc:73
msgid "-f=? Give an alternative filter file\n"
msgstr "-f=? ཐབས་གཞན་ཚགས་མའི་ཡིག་སྣོད་ཅིག་བྱིན།\n"

#: ../gtk/gsynaptic.cc:74
msgid "-t   Give an alternative main window title (e.g. hostname with `uname -n`)\n"
msgstr "-t   སྒོ་སྒྲིག་ངོ་མའི་མགོ་མིང་ཐབས་གཞན་ཅིག་བྱིན་ (དཔེར་ན་  `uname -n`དང་བཅས་ཧོསཊི་མིང་)\n"

#: ../gtk/gsynaptic.cc:75
msgid "-i=? Start with the initial Filter with given name\n"
msgstr "-i=? བྱིན་ཡོད་པའི་མིང་དང་ཅིག་ཁར་  འགོ་ཐོག་ཚགས་མ་དང་ཅིག་ཁར་འགོ་བཙུགས།\n"

#: ../gtk/gsynaptic.cc:76
msgid "-o=? Set an arbitary configuration option, eg -o dir::cache=/tmp\n"
msgstr "-o=? ཚུལ་མིན་གྱི་རིམ་སྒྲིག་གདམ་ཁ་འདི་གཞི་སྒྲིག་འབད་, དཔེར་ན་ -o dir::cache=/tmp\n"

#: ../gtk/gsynaptic.cc:77
msgid "--upgrade-mode  Call Upgrade and display changes\n"
msgstr "--ཡར་བསྐྱེད་ནི་-ཐབས་ལམ་  ཡར་བསྐྱེད་བཀུག་ནི་དང་བསྒྱུར་བཅོས་ཚུ་བཀྲམ་སྟོན་འབད།\n"

#: ../gtk/gsynaptic.cc:78
msgid "--dist-upgrade-mode  Call DistUpgrade and display changes\n"
msgstr "--བགོ་བཀྲམ་ནི་-ཡར་བསྐྱེད་-ཐབས་ལམ་  བགོ་བཀྲམ་ཡར་བསྐྱེད་བཀུག་ནི་དང་  བསྒྱུར་བཅོས་ཚུ་བཀྲམ་སྟོན་འབད།\n"

#: ../gtk/gsynaptic.cc:79
msgid "--update-at-startup  Call \"Reload\" on startup\n"
msgstr "--འགོ་བཙུགས་-ལུ་-དུས་མཐུན་བཟོ་  \"Reload\" འགོ་བཙུགས་གུ་བཅར་ད།\n"

#: ../gtk/gsynaptic.cc:80
msgid "--non-interactive Never prompt for user input\n"
msgstr "--མ་ཡིན་པ་-ཕན་ཚུན་འབྲེལ་ལྡན་མ་ཡིན་པ་འདི་  ལག་ལེན་པའི་ཨིན་པུའུཊི་དོན་ལུ་ནུས་སྤེལ་མ་འབད།\n"

#: ../gtk/gsynaptic.cc:81
msgid "--task-window Open with task window\n"
msgstr "--ལས་ཀའི་-སྒོ་སྒྲིག་འདི་  ལས་ཀའི་སྒོ་སྒྲིག་དང་ཅིག་ཁར་ཁ་ཕྱེ།\n"

#: ../gtk/gsynaptic.cc:82
msgid "--add-cdrom Add a cdrom at startup (needs path for cdrom)\n"
msgstr "--ཁ་སྐོང་བརྐྱབ་-སི་ཌི་རཱོམ་  འགོ་བཙུགས་ (སི་ཌི་རཱོམ་གྱི་དོན་ལུ་འགྲུལ་ལམ་དགོ་)གཅིག་ལུ་སི་ཌི་རཱོམ་ཁ་སྐོང་བརྐྱབ།\n"

#: ../gtk/gsynaptic.cc:83
msgid "--ask-cdrom Ask for adding a cdrom and exit\n"
msgstr "--འདྲི་ནི་-སི་ཌི་རཱོམ་  སི་ཌི་རཱོམ་ཁ་སྐོང་བརྐྱབ་ནི་དང་ཕྱིར་འཐོན་ནི་ལུ་འདྲི།\n"

