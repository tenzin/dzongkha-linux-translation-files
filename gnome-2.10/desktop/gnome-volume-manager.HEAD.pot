msgstr "ཌིཀསི་སྟོངམ་ནང་ན་བཙུགསཔ་ད་ བྱ་བ་ལག་ལེན་འཐབ།(_b)"

#: gnome-volume-properties.glade.h:18
msgid "Play _audio CD discs when inserted"
msgstr "ནང་ན་བཙུགསཔ་ད་ རྣར་ཉན་སི་ཌི་གཏང་།(_a)"

#: gnome-volume-properties.glade.h:19
msgid "Play _video DVD disks when inserted"
msgstr "ནང་ན་བཙུགསཔ་ད་ ཝི་ཌིའོ་གི་ ཌི་བི་ཌི་ ཌིཀསི་གཏང་།(_v)"

#: gnome-volume-properties.glade.h:20
msgid "Removable Drives and Media Preferences"
msgstr "རྩ་བསྐྲད་གཏང་བཏུབ་པའི་འདྲེན་འཕྲུལ་དང་ བརྡ་ལམ་གྱི་དགའ་གདམ་ཚུ།"

#: gnome-volume-properties.glade.h:21
msgid "Storage"
msgstr "བསག་མཛོད།"

#: gnome-volume-properties.glade.h:22
msgid "_Auto-run programs on new drives and media"
msgstr "འདྲེན་འཕྲུལ་དང་ བརྡ་ལམགསརཔ་ཚུ་ལུ་ རང་བཞིན་གཡོག་བཀོལ་ལས་རིམ་ཚུ།(_A)"

#: gnome-volume-properties.glade.h:23
msgid "_Mount removable drives when hot-plugged"
msgstr "ཧོཊི་-པ་ལག་འབདཝ་ད་ རྩ་བསྐྲད་གཏང་བཏུབ་པའི་འདྲེན་འཕྲུལ་ཚུ་ སྦྱར་བརྩེགས་འབད།(_M"

#: src/manager.c:229
msgid "Run command from inserted media?"
msgstr "ནང་ན་བཙུགས་ཏེ་ཡོད་པའི་བརྡ་ལམནང་ལས་ བརྡ་བཀོད་གཡོག་བཀོལ།"

#: src/manager.c:232
#, c-format
msgid "The file \"%s\" on the inserted media is an auto-runfile."
msgstr "ནང་ན་བཙུགས་ཏེ་ཡོད་པའི་བརྡ་ལམ་ནང་ ཡིག་སྣོད་\"%s\" དེ་ རང་བཞིན་-གཡོག་བཀོལ་ཡིག་སྣོད་ཅིག་ཨིན་པས།"

#: src/manager.c:237
msgid "_Run Command"
msgstr "བརྡ་བཀོད་གཡོག་བཀོལ།(_R)"

#: src/manager.c:319
msgid "Import photos from device?"
msgstr "ཐབས་འཕྲུལ་ནང་ལས་ པར་ཚུ་ནང་འདྲེན་འབད་ག?"

#: src/manager.c:321
msgid "There are photos on the inserted media. Would you like to import these photos into your photo album?"
msgstr "ནང་ན་བཙུགས་ཏེ་ཡོད་པའི་ བརྡ་ལམ་གུ་པར་འདུག། ཁྱོད་ཀྱིས་པར་དེ་ཚུ་ ཁྱོད་རའི་པར་དེབ་ནང་ ནང་འདྲེན་འབད་ནི་ཨིན་ན?"

#: src/manager.c:327
#: src/manager.c:373
msgid "_Import Photos"
msgstr "པར་ཚུ་ནང་འདྲེན་འབད།(_I)"

#: src/manager.c:366
msgid "Import photos from camera?"
msgstr "པར་ཆས་ནང་ལས་ པར་ཚུ་ནང་འདྲེན་འབད་ག?།"

#: src/manager.c:368
msgid "There are photos on the plugged-in camera. Would you like to import these photos into your photo album?"
msgstr "ནང་མཐུད་འབད་དེ་ཡོད་པའི་ པར་ཆས་དེ་ནང་པར་འདུག། ཁྱོད་ཀྱིས་པར་དེ་ཚུ་ ཁྱོད་རའི་པར་དེབ་ནང་ ནང་འདྲེན་འབད་ནི་ཨིན་ན?"

#: src/manager.c:556
msgid "Browse files or play tracks from disc?"
msgstr "ཡིག་སྣོད་ཚུ་གི་བརྡ་འཚོལ་འབད་ ཡང་ན་ ཌིཀསི་ནང་ལས་ གླུ་རིམ་གཏང་།"

#: src/manager.c:559
msgid "There are both audio tracks and data files on the inserted disc. Choose if you want to play the audio tracks to listen to music or browse the files to manage the stored data."
msgstr "ནང་ན་བཙུགས་ཏེ་ཡོད་མི་ ཌིཀསི་དེ་ནང་ རྣར་ཉན་གླུ་གཞས་དང་ གནད་སྡུད་ཡིག་སྣོད་ཚུ་ གཉིས་ཆ་ར་འདུག། ཉན་ནིའི་དོན་ལུ་ གླུ་གཞས་གཏང་ནི་ཨིན་ན་ བསགས་ཏེ་ཡོད་པའི་གནད་སྡུད་ ལག་ལེན་གྱི་དོན་ལུ་ བརྡ་འཚོལ་འབད་ནི་ཨིན་ན་ གདམ་ཁ་རྐྱབས།"

#: src/manager.c:565
msgid "_Browse Files"
msgstr "ཡིག་སྣོད་ཚུ་ བརྡ་འཚོལ་འབད།(_B)"

#: src/manager.c:567
msgid "_Play Tracks"
msgstr "གླུ་རིམ་གཏང་།(_P)"

#: src/properties.c:187
#, c-format
msgid ""
"Error starting gnome-volume-manager daemon:\n"
"%s"
msgstr ""
"ཇི་ནོམ་-སྐད་ཤུགས་-འཛིན་སྐྱོང་པའི་ཌེ་མཱོན་ འགོ་བཙུགས་ནི་ལུ་འཛོལ་བ། \n"
"%s"

#: src/properties.c:375
msgid "Volume management not supported"
msgstr "སྐད་ཤུགས་འཛིན་སྐྱོང་ལུ་ རྒྱབ་སྐྱོར་མིན་འདུག།"

#: src/properties.c:377
msgid "The \"hald\" service is required but not currently running. Enable the service and rerun this applet, or contact your system administrator."
msgstr "\"hald\" ཟེར་མི་ ཞབས་ཏོག་དེ་དགོཔ་ཡོད། དེ་འབདཝ་ད་ ད་ལྟོ་གཡོག་བཀོལ་ཏེ་མིན་འདུག། ཞབས་ཏོག་དེ་ལྕོགསཅན་བཟོ་སྟེ་ ཨེཔ་ལེཊི་འདི་ ལོག་གཡོག་བཀོལ། ཡང་ན་ ཁྱོད་རའི་རིམ་ལུགས་འཛིན་སྐྱོང་པ་ལུ་ འབྲེལ་བ་འཐབ།"

#: src/properties.c:400
msgid "Could not load the main interface"
msgstr "ངོས་འདྲ་བ་གཙོ་བོ་དེ་ མངོན་གསལ་འབད་མ་ཚུགས།"

#: src/properties.c:403
msgid "Please make sure that the volume manager is properly installed"
msgstr "སྐད་ཤུགས་འཛིན་སྐྱོང་པ་དེ་ ངེས་བདེན་སྦེ་ གཞི་བཙུགས་འབདཡོདཔ་ ངེས་གཏན་བཟོ་གནང་།"

