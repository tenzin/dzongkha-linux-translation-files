#: gnome-volume-manager.schemas.in.h:1
msgid "':' delimited list of paths to check for autorun binaries and scripts."
msgstr "རང་བཞིན་གཡོག་བཀོལ་གྱི་ བའི་ན་རིསི་དང་ ཡིག་ཚུགས་ཞིབ་དཔྱད་བརྐྱབ་ནིའི་ འགྲུལ་ལམ་གྱི་ ':' དབྱེ་རྟགས་ཅན་གྱི་ཐོ་ཡིག།"

#: gnome-volume-manager.schemas.in.h:2
msgid "Automount removable drives"
msgstr "རང་བཞིན་སྦྱར་བརྩེགས་ཀྱིས་ རྩ་བསྐྲད་གཏང་བཏུབ་པའི་འདྲེན་འཕྲུལ།"

#: gnome-volume-manager.schemas.in.h:3
msgid "Automount removable media"
msgstr "རང་བཞིན་སྦྱར་བརྩེགས་ཀྱི་ རྩ་བསྐྲད་གཏང་བཏུབ་པའི་བརྡ་ལམ།"

#: gnome-volume-manager.schemas.in.h:4
msgid "Autorun nautilus"
msgstr "རང་བཞིན་གཡོག་བཀོལ་ ནའུ་ཊི་ལཱསི།"

#: gnome-volume-manager.schemas.in.h:5
msgid "Autorun path"
msgstr "རང་བཞིན་གཡོག་བཀོལ་འགྲུལ་ལམ།"

#: gnome-volume-manager.schemas.in.h:6
msgid "CD burn command"
msgstr "སི་ཌི་ནང་ བཙུགས་སྤྱོད་ཀྱི་བརྡ་བཀོད།"

#: gnome-volume-manager.schemas.in.h:7
msgid "CD play command"
msgstr "སི་ཌི་གཏང་ནིའི་བརྡ་བཀོད།"

#: gnome-volume-manager.schemas.in.h:8
msgid "Command to run when a digital camera is connected or media from a digital camera is inserted."
msgstr "ཌི་ཇི་ཊལ་པར་ཆས་ཅིག་ མཐུད་པའི་སྐབས་ལུའམ་ ཌི་ཇི་ཊལ་པར་ཆས་ཅིག་ནང་ལས་ བརྡ་ལམ་བཙུགས་པའི་སྐབས་ལུ་ གཡོག་བཀོལ་ནིའི་བརྡ་བཀོད།"

#: gnome-volume-manager.schemas.in.h:9
msgid "Command to run when a video DVD is inserted."
msgstr "ཝི་ཌིའོ་གི་ ཌི་བི་ཌི་བཙུགསཔ་ད་ གཡོག་བཀོལ་ནིའི་བརྡ་བཀོད།"

#: gnome-volume-manager.schemas.in.h:10
msgid "Command to run when an audio CD is inserted."
msgstr "རྣར་ཉན་གྱི་ སི་ཌི་བཙུགསཔ་ད་ གཡོག་བཀོལ་ནིའི་བརྡ་བཀོད།"

#: gnome-volume-manager.schemas.in.h:11
msgid "Command to run when an empty CD is inserted."
msgstr "སི་ཌི་སྟོངམ་བཙུགསཔ་ད་ གཡོག་བཀོལ་ནིའི་བརྡ་བཀོད།"

#: gnome-volume-manager.schemas.in.h:12
msgid "Command to run when the eject button on a optical drive is pressed."
msgstr "ཨོཔ་ཊི་ཀཱལ་འདྲེན་འཕྲུལ་གུ་ ཕྱིར་བཏོན་ཨེབ་རྟ་ལུ་ཨེབ་ད་ གཡོག་བཀོལ་ནིའི་བརྡ་བཀོད།"

#: gnome-volume-manager.schemas.in.h:13
msgid "DVD play command"
msgstr "ཌི་བི་ཌི་ གཏང་ནིའི་བརྡ་བཀོད།"

#: gnome-volume-manager.schemas.in.h:14
msgid "Eject command"
msgstr "ཕྱིར་བཏོན་བརྡ་བཀོད།"

#: gnome-volume-manager.schemas.in.h:15
msgid "Open nautilus on removeable media insert."
msgstr "རྩ་བསྐྲད་གཏང་བཏུབ་པའི་ བརྡ་ལམ་བཙུགས་ཏེ་ཡོད་མིའི་གུ་ ནའུ་ཊི་ལཱསི་ཁ་ཕྱེ།"

#: gnome-volume-manager.schemas.in.h:16
msgid "Perform autorun"
msgstr "རང་གཞིན་གཡོག་བཀོལ་ ལག་ལེན་འཐབ།"

#: gnome-volume-manager.schemas.in.h:17
msgid "Photo management command"
msgstr "པར་གྱི་འཛིན་སྐྱོང་བརྡ་བཀོད།"

#: gnome-volume-manager.schemas.in.h:18
msgid "Play inserted audio CD's"
msgstr "བཙུགས་ཏེ་ཡོད་པའི་ རྣར་ཉན་སི་ཌི་གཏང་།"

#: gnome-volume-manager.schemas.in.h:19
msgid "Play inserted video DVDs"
msgstr "བཙུགས་ཏེ་ཡོད་པའི་ ཝི་ཌིའོ་གི་ ཌི་བི་ཌི་གཏང་།"

#: gnome-volume-manager.schemas.in.h:20
msgid "Run CD burn command"
msgstr "སི་ཌི་བཙུགས་སྤྱོད་ཀྱི་བརྡ་བཀོད་ གཡོག་བཀོལ།"

#: gnome-volume-manager.schemas.in.h:21
msgid "Run photo management command"
msgstr "པར་གྱི་འཛིན་སྐྱོང་བརྡ་བཀོད་ གཡོག་བཀོལ།"

#: gnome-volume-manager.schemas.in.h:22
msgid "Whether autorun programs (binaries and scripts located in 'autorun_path') are run on newly mounted media."
msgstr "རང་བཞིན་གཡོག་བཀོལ་ལས་རིམ་ཚུ་(རང་བཞིན་གཡོག་བཀོལ་འགྲུལ་ལམ་ནང་ཡོད་མི་ བའི་ན་རིསི་དང་ ཡིག་ཚུགས་) གསརཔ་སྦྱར་བརྩེགས་འབད་མི་ བརྡ་ལམ་གུ་གཡོག་བཀོལ་མ་བཀོལ།(_p)"

#: gnome-volume-manager.schemas.in.h:23
msgid "Whether gnome-volume-manager should automatically mount removable storage drives when they are inserted."
msgstr "རྩ་བསྐྲད་གཏང་བཏུབ་པའི་ བསག་མཛོད་འདྲེན་འཕྲུལ་ཚུ་ ནང་ན་བཙུགསཔ་ད་ ཇི་ནོམ་-སྐད་ཤུགས་-འཛིན་སྐྱོང་པ་གིས་ དེ་ཚུ་རང་བཞིན་གྱིས་ སྦྱར་བརྩེགས་་འབད་ག་མི་འབད།"

#: gnome-volume-manager.schemas.in.h:24
msgid "Whether gnome-volume-manager should automatically mount removable storage media when they are inserted."
msgstr "རྩ་བསྐྲད་གཏང་བཏུབ་པའི་ བསག་མཛོད་བརྡ་ལམ་ཚུ་ ནང་ན་བཙུགསཔ་ད་ ཇི་ནོམ་-སྐད་ཤུགས་-འཛིན་སྐྱོང་པ་གིས་ དེ་ཚུ་རང་བཞིན་གྱིས་ སྦྱར་བརྩེགས་འབད་དགོཔ་ཨིན་ན་མེན།"

#: gnome-volume-manager.schemas.in.h:25
msgid "Whether, when a blank CD is inserted, gnome-volume-manager should run 'autoburn_cdr_command'."
msgstr "སི་ཌི་སྟོངམ་ཅིག་བཙུགསཔ་ད་ ཇི་ནོམ་-སྐད་ཤུགས་-འཛིན་སྐྱོང་པ་གིས་ 'autoburn_cdr_command' བརྡ་བཀོད་དེ་ གཡོག་བཀོལ་དགོ་ག་མིན་དགོ།"

#: gnome-volume-manager.schemas.in.h:26
msgid "Whether, when a digital camera is connected or media from a digital camera is inserted, gnome-volume-manager should run 'autophoto_command'."
msgstr "ཌི་ཇི་ཊལ་པར་ཆས་ཅིག་ མཐུད་པའི་སྐབས་ལུའམ་ ཌི་ཇི་ཊལ་པར་ཆས་ནང་ལས བརྡ་ལམ་ཅིག་བཙུགས་པའི་སྐབས་ ཇི་ནོམ་-སྐད་ཤུགས་-འཛིན་སྐྱོང་པ་གིས autophoto_command' བརྡ་བཀོད་དེ་ གཡོག་བཀོལ་དགོ་ག་མིན་དགོ།"

#: gnome-volume-manager.schemas.in.h:27
msgid "Whether, when a video DVD is inserted, gnome-volume-manager should run 'autoplay_dvd_command'."
msgstr "ཝི་ཌིའོ་གི་ ཌི་བི་ཌི་ཅིག་བཙུགསཔ་ད་ ཇི་ནོམ་-སྐད་ཤུགས་-འཛིན་སྐྱོང་པ་གིས ར'autoplay_dvd_command བརྡ་བཀོད་དེ་ གཡོག་བཀོལ་དགོ་ག་མིན་དགོ།"

#: gnome-volume-manager.schemas.in.h:28
msgid "Whether, when an audio CD is inserted, gnome-volume-manager should run 'autoplay_cda_command'."
msgstr "རྣར་ཉན་སི་ཌི་ཅིག་བཙུགསཔ་ད་ ཇི་ནོམ་-སྐད་ཤུགས་-འཛིན་སྐྱོང་པ་གིས 'autoplay_cda_command' བརྡ་བཀོད་དེ་ གཡོག་བཀོལ་དགོ་ག་མིན་དགོ།"

#: gnome-volume-properties.desktop.in.h:1
