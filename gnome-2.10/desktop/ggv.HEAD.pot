msgstr "སྔོན་སྒྲིག་ཕྱོགས།"

#: bonobo/ggv-postscript-view.c:2178
msgid "Default size"
msgstr "སྔོན་སྒྲིག་ཚད།"

#: bonobo/ggv-postscript-view.c:2183
msgid "Override document orientation"
msgstr "ཡིག་ཆའི་ཕྱོགས་ཟུར་བཞག་འབད།"

#: bonobo/ggv-postscript-view.c:2187
msgid "Override document size"
msgstr "ཡིག་ཆའི་ཚད་ ཟུར་བཞག་འབད།"

#: bonobo/ggv-postscript-view.c:2191
#: ggv.schemas.in.h:21
msgid "Respect EOF comment"
msgstr "ཨི་ཨོ་ཨེཕ་ བསམ་བཀོད་ལུ་ བརྩི་བཀུར་འབད།"

#: bonobo/ggv-postscript-view.c:2195
msgid "Antialiasing"
msgstr "ཨེན་ཊི་ཨེ་ལའི་སིང་།"

#: bonobo/ggv-sidebar.c:358
msgid "Toggle marked state of all pages"
msgstr "ཤོག་ལེབ་ཆ་མཉམ་གྱི་ རྟགས་བཀོད་གནས་སྟངས་ སོར་སྟོན་འབད།"

#: bonobo/ggv-sidebar.c:359
msgid "Toggle marked state of all pages: previously marked pages will be unmarked and unmarked ones will become marked."
msgstr "ཤོག་ལེབ་ཆ་མཉམ་གྱི་ རྟགས་བཀོད་གནས་སྟངས་ སོར་སྟོན་འབད་: ཧེ་མར་རྟགས་བཀལ་ཏེ་ཡོད་མི་ཚུ་ལུ་ རྟགས་མེདཔ་འགྱོ་ནི་དང་ རྟགས་མེད་མི་ཚུ་ལུ་ རྟགས་བཀལ་འོང་།"

#: bonobo/ggv-sidebar.c:371
msgid "Toggle marked state of odd pages"
msgstr "ཆ་མིན་ཤོག་ལེབ་ཀྱི་ རྟགས་བཀོད་གནས་སྟངས་ སོར་སྟོན་འབད།"

#: bonobo/ggv-sidebar.c:372
msgid "Toggle marked state of odd pages: previously marked odd pages will be unmarked and unmarked ones will become marked."
msgstr "ཆ་མིན་ཤོག་ལེབ་ཀྱི་ རྟགས་བཀོད་གནས་སྟངས་ སོར་སྟོན་འབད་: ཧེ་མར་རྟགས་བཀལ་ཏེ་ཡོད་མི་ ཆ་མིན་ཤོག་ལེབ་ཚུ་ལུ་ རྟགས་མེདཔ་འགྱོ་ནི་དང་ རྟགས་མེད་མི་ཚུ་ལུ་ རྟགས་བཀལ་འོང་།"

#: bonobo/ggv-sidebar.c:386
msgid "Toggle marked state of even pages"
msgstr "ཆ་ཅན་ཤོག་ལེབ་ཀྱི་ རྟགས་བཀོད་གནས་སྟངས་ སོར་སྟོན་འབད།"

#: bonobo/ggv-sidebar.c:387
msgid "Toggle marked state of even pages: previously marked even pages will be unmarked and unmarked ones will become marked."
msgstr "ཆ་ཅན་ཤོག་ལེབ་ཀྱི་ རྟགས་བཀོད་གནས་སྟངས་ སོར་སྟོན་འབད་: ཧེ་མར་རྟགས་བཀལ་ཏེ་ཡོད་མི་ ཆ་ཅན་ཤོག་ལེབ་ཚུ་ལུ་ རྟགས་མེདཔ་འགྱོ་ནི་དང་ རྟགས་མེད་མི་ཚུ་ལུ་ རྟགས་བཀལ་འོང་།"

#: bonobo/ggv-sidebar.c:401
msgid "Clear marked state of all pages"
msgstr "ཤོག་ལེབ་ཆ་མཉམ་གྱི་ རྟགས་བཀོད་གནས་སྟངས་བསལ།"

#: bonobo/ggv-sidebar.c:402
msgid "Clear marked state of all pages: all pages will be unmarked."
msgstr "ཤོག་ལེབ་ཆ་མཉམ་གྱི་ རྟགས་བཀོད་གནས་སྟངས་བསལ་: ཤོག་ལེབ་ཆ་མཉམ་ རྟགས་མེདཔ་འགྱོ་འོང་།"

#: ggv.desktop.in.h:1
msgid "PostScript Viewer"
msgstr "པོསིཊི་སིཀིརིཔཊི་ མཐོང་བྱེད།"

#: ggv.desktop.in.h:2
msgid "View PostScript files"
msgstr "པོསིཊི་སིཀིརིཔཊི་ ཡིག་སྣོད་ཚུ་སྟོན།"

#: ggv.schemas.in.h:1
msgid "Antialiased rendering"
msgstr "ཨེན་ཊི་ཨ་ལི་ཡཱསི་ ལྷན་སྟོན་འབད་དོ་།"

#: ggv.schemas.in.h:2
msgid "Antialiasing arguments"
msgstr "ཨེན་ཊི་ཨ་ལི་ཡ་སིང་གི་ སྒྲུབ་རྟགས།"

#: ggv.schemas.in.h:3
msgid "Autojump"
msgstr "རང་བཞིན་ཕར་མཆོངས།"

#: ggv.schemas.in.h:4
msgid "Command line arguments for antialiasing."
msgstr "ཨེན་ཊི་ཨ་ལི་ཡ་སིང་གི་དོན་ལུ་ བཀོད་ལམ་གྱི་སྒྲུབ་རྟགས།"

#: ggv.schemas.in.h:5
msgid "Command line used to execute the PostScript interpreter."
msgstr "པོསིཊི་སིཀིརིཔཊི་ ཁ་སྒྱུར་མི་དེ་ བཀོལ་སྤྱོད་ཀྱི་དོན་ལུ་ ལག་ལེན་འཐབ་ནིའི་བཀོད་ལམ།"

#: ggv.schemas.in.h:6
msgid "Coordinate units"
msgstr "ཆ་ཕྲན་ཚུ་ མཉམ་འབྲེལ་འབད།"

#: ggv.schemas.in.h:7
msgid "Default document orientation"
msgstr "ཡིག་ཆའི་ཕྱོགས་ སྔོན་སྒྲིག་འབད།"

#: ggv.schemas.in.h:8
msgid "Default page size"
msgstr "ཤོག་ལེབ་ཀྱི་ཚད་ སྔོན་སྒྲིག་འབད།"

#: ggv.schemas.in.h:9
msgid "Default zoom factor"
msgstr "རྒྱས་ཟུམ་ཆ་རྐྱེན་ སྔོན་སྒྲིག་འབད།"

#: ggv.schemas.in.h:10
msgid "If true, the orientation specified by the document will be overriden with the default orientation."
msgstr "ངེས་བདེན་ཨིན་པ་ཅིན་ སྔོན་སྒྲིག་ཕྱོགས་དེ་གིས་ ཡིག་ཆ་གིས་གསལ་བཀོད་འབད་མི་ཕྱོགས་དེ་ ཟུར་བཞག་འབད་འོང་།"

#: ggv.schemas.in.h:11
msgid "Override document specified orientation"
msgstr "ཡིག་ཆ་གིས་གསལ་བཀོད་འབད་མི་ ་ཕྱོགས་དེ་ཟུར་བཞག་འབད།"

#: ggv.schemas.in.h:12
msgid "Override document specified page size"
msgstr "ཡིག་ཆ་གིས་གསལ་བཀོད་འབད་མི་ ཤོག་ལེབ་ཀྱི་ཚད་ ཟུར་བཞག་འབད་འོང་།"

#: ggv.schemas.in.h:13
msgid "PDF to DSC conversion command"
msgstr "པི་ཌི་ཨེཕ་ལས་ ཌི་ཨེསི་སི་ལུ་ གཞི་བསྒྱུར་གྱི་བརྡ་བཀོད།"

#: ggv.schemas.in.h:14
msgid "PDF to PS conversion command"
msgstr "པི་ཌི་ཨེཕ་ལས་ པི་ཨེསི་ལུ་ གཞི་བསྒྱུར་གྱི་བརྡ་བཀོད།"

#: ggv.schemas.in.h:15
msgid "Page flip"
msgstr "ཤོག་ལེབ་མྱུར་ལྷོག"

#: ggv.schemas.in.h:16
msgid "Panel on right-hand side"
msgstr "པེ་ནཱལ་ལགཔ་གཡས་པའི་ཁ་ཐུག་ལུ།"

#: ggv.schemas.in.h:17
msgid "Percentage of the viewable area to move when scrolling."
msgstr "བཤུད་སྒྲིལ་སྐབས་ མཐོང་ཚུགས་པའི་མངའ་ཁོངས་བརྒྱ་ཆ།"

#: ggv.schemas.in.h:18
msgid "Percentage of the viewable area to scroll"
msgstr "མཐོང་ཚུགས་པའི་མངའ་ཁོངས་ བཤུད་སྒྲིལ་འབད་དགོ་པའི་བརྒྱ་ཆ།"

#: ggv.schemas.in.h:19
msgid "PostScript interpreter command"
msgstr "པོསཔཊི་སིཀིརིཔཊི་ ཁ་བསྒྱུར་པའི་བརྡ་བཀོད།"

#: ggv.schemas.in.h:20
msgid "Printing command"
msgstr "དཔར་བསྐྲུན་བརྡ་བཀོད།"

#: ggv.schemas.in.h:22
msgid "Save window geometry"
msgstr "སྒོ་སྒྲིག་གི་འཇལ་རྩིས་སྲུངས།"

#: ggv.schemas.in.h:23
msgid "Show menubar"
msgstr "དཀར་ཆག་ཕྲ་རིང་སྟོན།"

#: ggv.schemas.in.h:24
msgid "Show previous visible area when scrolling"
msgstr "ཧེ་་མའི་བཤུད་བསྒྲིལ་སྐབས་ མཐོང་ཚུགས་པའི་མངའ་ཁོངས་སྟོན།"

#: ggv.schemas.in.h:25
msgid "Show side panel"
msgstr "ཟུར་ཁའི་པེ་ནཱལ་སྟོན།"

#: ggv.schemas.in.h:26
msgid "Show statusbar"
msgstr "གནས་ཚད་ཕྲ་རིང་སྟོན།"

#: ggv.schemas.in.h:27
msgid "Show toolbar"
msgstr "ལག་ཆས་ཕྲ་རིང་སྟོན།"

#: ggv.schemas.in.h:28
msgid "The command line used to convert PDF files to PostScript."
msgstr "པི་ཌི་ཨེཕ་ཡིག་སྣོད་ཚུ་ པོསིཊི་སིཀིརིཔཊི་ལུ་ གཞི་བསྒྱུར་འབད་ནི་ལུ་ ལག་ལེན་འཐབ་ནིའི་བཀོད་ལམ།"

#: ggv.schemas.in.h:29
msgid "The command line used to create PostScript DSC files from PDF files."
msgstr "པི་ཌི་ཨེཕ་ཡིག་སྣོད་ནང་ལས་ པོསིཊི་སིཀིརིཔཏི་ ཌི་ཨེསི་སི་ཡིག་སྣོད་ གསར་བསྐྲུན་འབད་ནི་ལུ་ ལག་ལེན་འཐབ་ནིའི་བཀོད་ལམ། "

#: ggv.schemas.in.h:30
msgid "The command used to unpack files compressed with bzip2."
msgstr "བི་ཛིཔ་༢ ཀྱིས་ ཨེབ་བཙུགས་འབད་བཞག་མི་ ཡིག་སྣོད་ཚུ་བཤུབ་ནི་ལུ་ ལག་ལེན་འཐབ་ནིའི་བརྡ་བཀོད།"

#: ggv.schemas.in.h:31
msgid "The command used to unpack files compressed with gzip."
msgstr "ཇི་ཛིཔ་གིས་ཨེབ་བཙུགས་འབད་བཞག་མི་ ཡིག་སྣོད་ཚུ་བཤུབ་ནི་ལུ་ ལག་ལེན་འཐབ་ནིའི་བརྡ་བཀོད། "

#: ggv.schemas.in.h:32
msgid "The default page size to use when no page size is specified by the document or when the user wishes to override the size specified in the document."
msgstr "ཡིག་ཆ་གིས་ ཤོག་ལེབ་ཀྱི་ཚད་ གསལ་བཀོད་མ་འབད་བའི་སྐབས་སུའམ་ ཡང་ན་ ལག་ལེན་པ་གིས་ ཡིག་ཆ་ནང་ གསལ་བཀོད་འབད་མི་ཚད་དེ་ ཟུར་བཞགའབད་ནིའི་ བློ་འདོད་ཡོད་པའི་སྐབས་ ལག་ལེན་འཐབ་ནིའི་ སྔོན་སྒྲིག་གི་ཤོག་ལེབ་ཚད། "

#: ggv.schemas.in.h:33
msgid "The default zoom factor for the document. The value should belong to the interval [0.05, 30.0]."
msgstr "ཡིག་ཆའི་དོན་ལུ་ སྔོན་སྒྲིག་རྒྱས་ཟུམ་ཆ་རྐྱེན། བེ་ལུ་དེ་ བར་མཚམས་ {༠.༠༥,༣༠.༠} ལུ་འབངས་དགོཔ་ཨིན"

#: ggv.schemas.in.h:34
msgid "The height of the viewer's window."
msgstr "མཐོང་བྱེད་ཀྱི་ སྒོ་སྒྲིག་གི་མཐོ་ཚད།"

#: ggv.schemas.in.h:35
msgid "The orientation to use when the document defines no orientation or when the user wishes to override the document specified orientation."
msgstr "ཡིག་ཆ་གིས་ ཕྱོགས་ངེས་འཛིན་མ་འབད་བའི་སྐབས་ལུ་ ཡང་ཅིན་ ལག་ལེན་པ་དེ་གིས་ ཡིག་ཆ་གིས་གསལ་བཀོད་འབད་མི་ཕྱོགས་དེ་ ཟུར་བཞག་འབད་ནིའི་ བློ་འདོད་ཡོད་པའི་སྐབས་སུ་ ལག་ལེན་འཐབ་ནིའི་ཕྱོགས།"

#: ggv.schemas.in.h:36
msgid "The shell command used to print files."
