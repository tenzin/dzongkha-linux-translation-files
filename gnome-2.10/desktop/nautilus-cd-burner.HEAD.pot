#: nautilus-cd-burner.c:305
msgid "Please make sure another application is not using the drive."
msgstr "གློག་རིམ་གཞན་ཅིག་གིས་ འདྲེན་འཕྲུལ་དེ་ ལག་ལེན་འཐབ་བཞིན་མེདཔ་ ངེས་ཏིག་བཟོ་གནང་།"

#: nautilus-cd-burner.c:306
msgid "Drive is busy"
msgstr "འདྲེན་འཕྲུལ་ཁོམ་མེད་ཨིན་པས།"

#: nautilus-cd-burner.c:308
#, c-format
msgid "Please put a rewritable or blank disc, with at least %d MiB free, into the drive."
msgstr "འདྲེན་འཕྲུལ་ནང་ལུ་ ཊིཀསི་བསྐྱར་འབྲི་འབད་བཏུབ་པའམ་ སྟོངམ་འབད་མི་ བར་སྟོང་ཨེམ་ཨའི་བི་%d ཡོད་མི་ཅིག་ བཙུགས་གནང་།"

#: nautilus-cd-burner.c:310
msgid "Insert rewritable or blank disc"
msgstr "བསྐྱར་འབྲི་འབད་བཏུབ་པའམ་ སྟོངམ་འབད་མི་ ཊིཀསི་ཅིག་བཙུགས་།"

#: nautilus-cd-burner.c:312
#, c-format
msgid "Please put a blank disc, with at least %d MiB free, into the drive."
msgstr "འདྲེན་འཕྲུལ་ནང་ལུ་ ཊིཀསི་སྟོངམ་ བར་སྟོང་ཨེམ་ཨའི་བི་%d ཡོད་མི་ཅིག་ བཙུགས་གནང་།"

#: nautilus-cd-burner.c:314
msgid "Insert blank disc"
msgstr "ཊིཀསི་སྟོངམ་བཙུགས།"

#: nautilus-cd-burner.c:316
#, c-format
msgid "Please replace the disc in the drive with a rewritable or blank disc, with at least %d MiB free."
msgstr "འདྲེན་འཕྲུལ་ནང་ཡོད་མི་ ཊིཀསི་དེ་གི་ཚབ་ལུ་ བསྐྱར་འབྲི་འབད་བཏུབ་པའམ་ སྟོངམ་འབད་མི་ཊིཀསི་ ཉུང་མཐའ་ལུ་ བར་སྟོང་ཨེམ་ཨའི་བི་ %d ཡོད་མི་ཅིག་ བཙུགས་གནང་།"

#: nautilus-cd-burner.c:318
msgid "Reload rewritable or blank disc"
msgstr "བསྐྱར་འབྲི་འབད་བཏུབ་པའམ་ སྟོངམ་འབད་མི་ཊིཀསི་ཅིག་ སླར་མངོན་གསལ་འབད།"

#: nautilus-cd-burner.c:320
#, c-format
msgid "Please replace the disc in the drive with a blank disc, with at least %d MiB free."
msgstr "འདྲེན་འཕྲུལ་ནང་ཡོད་མི་ ཊིཀསི་དེ་གི་ཚབ་ལུ་ སྟོངམ་འབད་མི་ཊིཀསི་ ཉུང་མཐའ་ལུ་ བར་སྟོང་ཨེམ་ཨའི་བི་ %d ཡོད་མི་ཅིག་ བཙུགས་གནང་།"

#: nautilus-cd-burner.c:322
msgid "Reload blank disc"
msgstr "ཊིཀསི་སྟོངམ་ སླར་མངོན་གསལ་འབད།"

#: nautilus-cd-burner.c:330
msgid "Retry disc"
msgstr "ཊིཀསི་ལུ་ སླར་འབད་རྩོལ་བསྐྱེད།"

#: nautilus-cd-burner.c:368
msgid "The inserted disc was already written to."
msgstr "ནང་ན་བཙུགས་མི་ ཊིཀསི་དེ་ལུ་ ཧེ་མ་ལས་རང་བྲིས་ནུག"

#: nautilus-cd-burner.c:369
msgid "Do you want to insert another disc, or erase this one?"
msgstr "ཊིཀསི་གཞན་མི་ཅིག་ བཙུགས་ནི་ཨིན་ན་ ཡང་ཅིན་ འདི་ཀྲེག་གཏང་ནི་ཨིན་ན?"

#: nautilus-cd-burner.c:371
msgid "Insert another disc"
msgstr "ཊིཀསི་གཞན་མི་ཅིག་བཙུགས།"

#: nautilus-cd-burner.c:372
msgid "Erase this disc"
msgstr "ཊིཀསི་འདི་ ཀྲེག་གཏང་།"

#: nautilus-cd-burner.c:394
