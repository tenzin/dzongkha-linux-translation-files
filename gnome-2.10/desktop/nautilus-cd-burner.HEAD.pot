#: ncb.schemas.in.h:4
msgid "Directory used for the temporary ISO images, leave blank to use the default system temporary directory."
msgstr "འཕྲལ་སྐབས་ཀྱི་ ཨའི་ཨེསི་ཨོ་ གཟུགས་བརྙན་ཚུ་གི་དོན་ལུ་ ལག་ལེན་འཐབ་མི་ ཁ་བྱང་ཐོ་ཡིག འཕྲལ་སྐབས་ཀྱི་སྣོད་ཐོ་ སྔོན་སྒྲིག་རིམ་ལུགས་ཀྱི་དོན་ལུ་ ལག་ལེན་འཐབ་ནིའི་ བར་སྟོང་བཞག"

#: ncb.schemas.in.h:5
msgid "Enable Burnproof option"
msgstr "བཙུགས་སྤྱོད་ཉེན་སྲུང་གི་གདམ་ཁ་ ལྕོགས་ཅན་བཟོ།"

#: ncb.schemas.in.h:6
msgid "Enable debug"
msgstr "རྐྱེན་སེལ་ལྕོགས་ཅན་བཟོ།"

#: ncb.schemas.in.h:7
msgid "Enable overburn"
msgstr "ཚད་ལས་བརྒལ་བའི་བཙུགས་སྤྱོད་ ལྕོགས་ཅན་བཟོ།"

#: ncb.schemas.in.h:8
#: ncb.schemas.in.h:9
msgid "Whether to enable the debugging for CD Recording."
msgstr "སི་ཌི་སྒྲ་བཟུང་གི་དོན་ལུ་ རྐྱེན་སེལ་ལྕོགས་ཅན་ བཟོ་ནི་ཨིན་ན་མེན།"

#: ncb.schemas.in.h:10
msgid "Whether to use the Burnproof option available on some recorders."
msgstr "བཟུང་འཕྲུལ་གཞན་ཚུ་ལུ་ཡོད་མི་ བཙུགས་སྤྱོད་ཉེན་སྲུང་ ལག་ལེན་འཐབ་ནི་ཨིན་ན་མེན།"

