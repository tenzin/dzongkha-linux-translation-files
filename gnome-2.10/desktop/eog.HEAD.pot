msgstr " %s ཁ་ཕྱེ་ནིའི་དོན་ལུ་ གནས་སྐབས་ཀྱི་ཡིག་སྣོད་ གསར་བསྐྲུན་འབད་མ་ཚུགས།"

#: libeog/eog-image-jpeg.c:349
#: libeog/eog-image-jpeg.c:590
msgid "Couldn't allocate memory for loading JPEG file"
msgstr "ཇེ་པི་ཨི་ཇི་ཡིག་སྣོད་ མངོན་གསལ་གྱི་་དོན་ལས་ དྲན་ཚད་སྤྲོད་མ་ཚུགས།"

#: libeog/eog-image.c:814
msgid "empty file"
msgstr "ཡིག་སྣོད་སྟོངམ་བཏོན།"

#: libeog/eog-image.c:1247
#: libeog/eog-image.c:1458
msgid "File exists"
msgstr "ཡིག་སྣོད་འདུག"

#: libeog/eog-image.c:1400
#: libeog/eog-image.c:1526
msgid "No image loaded."
msgstr "གཟུགས་བརྙན་ཅིག་ཡང་ མངོན་གསལ་མ་འབད་བས།"

#: libeog/eog-image.c:1409
