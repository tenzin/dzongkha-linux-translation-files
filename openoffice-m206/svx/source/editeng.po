msgid "Move"
msgstr "སྤོ་བཤུད་འབད།"

#: editeng.src#RID_EDITUNDO_INSERT.string.text
msgid "Insert"
msgstr "བཙུགས།"

#: editeng.src#RID_EDITUNDO_REPLACE.string.text
msgid "Replace"
msgstr "ཚབ་བཙུགས།"

#: editeng.src#RID_EDITUNDO_SETATTRIBS.string.text
