#: ../install-menu/functions.cc:92
msgid "Zero-size argument to print function."
msgstr "ལས་འགན་དཔར་བསྐྲུན་འབད་ནིའི་ ཀླད་ཀོར་སྒྲུབ་རྟགས།"

#: ../install-menu/install-menu.cc:202
msgid "install-menu: checking directory %1\n"
msgstr "དཀར་ཆག་གཞི་བཙུག་འབད་: སྣོད་ཐོ་ %1 ཞིབ་དཔྱད་འབད་དོ་\n"

#: ../install-menu/install-menu.cc:215
msgid "install-menu: creating directory %1:\n"
msgstr "དཀར་ཆག་གཞི་བཙུག་འབད་: སྣོད་ཐོ་ %1: གསར་བསྐྲུན་འབད་དོ་\n"

#: ../install-menu/install-menu.cc:217
msgid "Could not create directory(%1): %2"
msgstr "སྣོད་ཐོ་(%1): %2 གསར་བསྐྲུན་འབད་མི་ཚུགས་"

#: ../install-menu/install-menu.cc:219
msgid "Could not change directory(%1): %2"
msgstr "སྣོད་ཐོ་(%1): %2 བསྒྱུར་བཅོས་འབད་མི་ཚུགས་"

#: ../install-menu/install-menu.cc:222
msgid "install-menu: directory %1 already exists\n"
msgstr "དཀར་ཆག་གཞི་བཙུག་འབད་: སྣོད་ཐོ་ %1 འདི་ ཧེ་མ་ལས་ཡོདཔ་ཨིན་\n"

#. Do not translate supported
#: ../install-menu/install-menu.cc:447
msgid "install-menu: [supported]: name=%1\n"
msgstr "དཀར་ཆག་གཞི་བཙུག་འབད་: [རྒྱབ་སྐྱོར་འབད་ཡོདཔ་]: མིང་=%1\n"

#: ../install-menu/install-menu.cc:464
msgid "Menu entry lacks mandatory field \"%1\".\n"
msgstr "དཀར་ཆག་ཐོ་བཀོད་ལུ་ ངེས་མཁོའི་ས་སྒོ་ \"%1\" མེདཔ་ཨིན།\n"

#: ../install-menu/install-menu.cc:470
msgid "Unknown value for field %1=\"%2\".\n"
msgstr "ས་སྒོ་ %1=\"%2\" གི་དོན་ལུ་ མ་ཤེས་པའི་བེ་ལུ།\n"

#. Do not translate quoted text
#: ../install-menu/install-menu.cc:617
msgid "install-menu: \"hotkeycase\" can only be \"sensitive\" or \"insensitive\"\n"
msgstr "དཀར་ཆག་གཞི་བཙུག་འབད་: \"hotkeycase\" འདི་ \"sensitive\" or \"insensitive\" རྐྱངམ་ཅིག་གི་ཁྱད་ཆོདཔ་ཨིན་\n"

#: ../install-menu/install-menu.cc:647
msgid "install-menu: Warning: Unknown identifier `%1' on line %2 in file %3. Ignoring.\n"
msgstr "དཀར་ཆག་གཞི་བཙུག་འབད་: ཉེན་བརྡ་: ཡིག་སྣོད་ %3 ནང་གི་གྲལ་ཐིག་ %2 གུ་ལུ་ མ་ཤེས་པའི་ངོས་འཛིན་པ་ `%1' ། སྣང་མེད་བཞག་ནི།\n"

#: ../install-menu/install-menu.cc:657
msgid "install-menu: %1 must be defined in menu-method %2"
msgstr "དཀར་ཆག་གཞི་བཙུག་འབད་: %1 འདི་ དཀར་ཆག་ཐབས་ལམ་ %2 ནང་ ངེས་འཛིན་འབད་དགོ"

#: ../install-menu/install-menu.cc:824
msgid "Cannot open file %1 (also tried %2).\n"
msgstr "ཡིག་སྣོད་ %1ཁ་ཕྱེ་མི་ཚུགས་ (%2 ལུ་ཡང་འབད་རྩོལ་བསྐྱེད་ཡི་)།\n"

#: ../install-menu/install-menu.cc:832
#: ../install-menu/install-menu.cc:839
#: ../install-menu/install-menu.cc:847
msgid "Cannot open file %1.\n"
msgstr "ཡིག་སྣོད་ %1འདི་ ཁ་ཕྱེ་མི་ཚུགས།\n"

#: ../install-menu/install-menu.cc:849
msgid ""
"In order to be able to create the user config file(s) for the window manager,\n"
"the above file needs to be writeable (and/or the directory needs to exist).\n"
msgstr "སྒོ་སྒྲིག་འཛིན་སྐྱོང་པའི་དོན་ལུ་ ལག་ལེན་པའི་རིམ་སྒྲིག་ཡིག་སྣོད་(ཚུ་) གསར་བསྐྲུན་འབད་དགོ་པ་ཅིན་ གོང་གི་ཡིག་སྣོད་ཚུ་ བྲིས་བཏུབ་(དང་ ཡང་ན་ སྣོད་ཐོ་ཡོད་དགོ་)ཨིན་དགོ \n"

#: ../install-menu/install-menu.cc:871
msgid "Warning: the string %1 did not occur in template file %2\n"
msgstr "ཉེན་བརྡ་: ཡིག་རྒྱུན་ %1འདི་ ཊེམ་པེལེཊི་ ཡིག་སྣོད་ %2 ནང་ལུ་མ་འབྱུང་པས་\n"

#. Don't translate quoted string
#: ../install-menu/install-menu.cc:896
msgid ""
"install-menu [-vh] <menu-method>\n"
"  Read menu entries from stdin in \"update-menus --stdout\" format\n"
"  and generate menu files using the specified menu-method.\n"
"  Options to install-menu:\n"
"     -h --help    : this message\n"
"        --remove  : remove the menu instead of generating it.\n"
"     -v --verbose : be verbose\n"
msgstr ""
"དཀར་ཆག་གཞི་བཙུག་འབད་ [-vh] <menu-method>\n"
"དཀར་ཆག་ཐོ་བཀོད་ཚུ་ stdin in \"update-menus --stdout\" format\n"
" ནང་ལས་ལྷག་ནི་དང་ དཀར་ཆག་ཡིག་སྣོད་ཚུ་ གསལ་བཀོད་འབད་ཡོད་པའི་དཀར་ཆག་ཐབས་ལམ་ ལག་ལེན་འཐབ་ཐོག་ལས་ བཟོ་བཏོན་འབད།\n"
"དཀར་ཆག་གཞི་བཙུག་འབད་ནིིའི་གདམ་ཁ་:\n"
"     -h --help    : འཕྲིན་དོན་\n"
"        --remove  : བཟོ་བཏོན་མ་འབད་བར་ དཀར་ཆག་རྩ་བསྐྲད་གཏང་།\n"
"     -v --verbose : ཚིག་མང་དགོ་\n"

#: ../install-menu/install-menu.cc:943
