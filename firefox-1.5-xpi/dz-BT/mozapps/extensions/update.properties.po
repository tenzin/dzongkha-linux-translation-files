msgstr "དུས་མཐུན་གསརཔ་འཐོབ་ཚུགས་པས།"

#: updatesAvailableText
msgid "Click Here to View"
msgstr "བལྟ་ནིའི་དོན་ལུ་ ནཱ་ལུ་ཨེབ་གཏང་འབད།"

#: checkingPrefix
msgid "Checking for Updates to %S ..."
msgstr "%Sལུ་དུས་མཐུན་བཟོ་དགོཔ་ཡོད་མེད་ ཞིབ་དཔྱད་རྐྱབ་དོ་་་"

#: downloadingPrefix
msgid "Downloading: %S"
msgstr "%S  ཕབ་ལེན་འབད་དོ།"

#: installingPrefix
msgid "Installing: %S"
msgstr "%S གཞི་བཙུགས་འབད་དོ།"

#: closeButton
msgid "Close"
msgstr "ཁ་བསྡམས།"

#: installErrorDescription
msgid "The following components could not be installed due to errors (the file could not be downloaded, was corrupt, or for some other reason)."
msgstr "འཛོལ་བ་ལུ་བརྟེན་ཏེ་ འོག་གི་ཆ་ཤས་ཚུ་ གཞི་བཙུགས་འབད་མ་ཚུགས། (ཡིག་སྣོད་དེ་ཕབ་ལེན་འབད་མ་ཚུགས། ངན་ཅན་ལུ་གྱུར་ཡོད་པའམ་ རྒྱུ་མཚན་གཞན་ཡོདཔ་འོང་ནི་མས)"

#: checkingErrorDescription
msgid "%S could not check for updates to the following components (either the update server(s) did not respond, or the update service(s) were not found)."
msgstr "%Sགིས་འོག་གི་ཆ་ཤས་ཚུ་ལུ་ དུས་མཐུན་བཟོ་དགོཔ་ཡོད་མེད་ ཞིབ་དཔྱད་འབད་མ་ཚུགས། (དུས་མཐུན་བཟོ་ནིའི་སར་བར་དེ་གིས་ ངོས་ལན་འབད་མ་བཏུབ་ ཡང་ན་ དུས་མཐུན་བཟོ་ནིའི་ཞབས་ཏོག་དེ་ ཐོབ་མ་ཚུགསཔ་འོང་ནི་མས)"

#: installErrorItemFormat
msgid "%S (%S)"
msgstr "%S (%S)"

#: versionUpdateComplete
msgid "Version Compatibility Update Complete"
msgstr "ཐོན་རིམ་མཐུན་འགྱུར་ དུས་མཐུན་བཟོ་ནི་མཇུག་བསྡུ་ཡོད།"

#: updatesAvailableTooltip-0
#: updatesAvailableTooltip-1
msgid "Update(s) Available"
msgstr "དུས་མཐུན་བཟོ་ནི་ཡོདཔ།"

#: updatesAvailableTooltip-2
msgid "Critical Update(s) Available"
msgstr "གལ་གནད་ཆེའི་དུས་མཐུན་བཟོ་དགོཔ་ཡོདཔ།"

#: updatesCheckForUpdatesTooltip
msgid "Double-click here to check for updates"
msgstr "དུས་མཐུན་བཟོ་དགོཔ་ཡོད་མེད་ ཞིབ་དཔྱད་ཀྱི་དོན་ལུ་ ནཱ་ལུ་ཨེབ་གཏང་ཐེངས་གཉིས་འབད།"

#: installTextNoFurtherActions
#: foundInstructions
msgid "Choose the ones you want to install and click Install Now to install them."
msgstr "ཁྱོད་ཀྱིས་གཞི་བཙུགས་འབད་དགོ་མནོ་མི་ཚུ་ གདམ་ཁ་བརྐྱབས་ཞིནམ་ལས་ དེ་ཚུ་གཞི་བཙུགས་འབད་ནི་ལུ་ ད་ལྟོ་ར་གཞི་བཙུགས་འབད་ གུ་ཨེབགཏང་འབད།"

#: installTextFurtherCations
#: foundInstructionsAppComps
msgid "Choose the ones you want to install and click Next to continue."
msgstr "ཁྱོད་ཀྱིས་གཞི་བཙུགས་འབད་དགོ་མནོ་མི་ཚུ་ གདམ་ཁ་བརྐྱབས་ཞིནམ་ལས་ འཕྲོ་མཐུད་ནི་ལུ་ ཤུལ་མམ་ གུ་ཨེབ་གཏང་འབད།"

#: appNameAndVersionFormat
msgid "%S %S"
msgstr "%S %S"

#: updateTypePatches
msgid "Critical Updates (%S)"
msgstr "གལ་གནད་ཆེའི་དུས་མཐུན་(%S)"

#: updateTypeComponents
msgid "Optional Components (%S)"
msgstr "གདམ་ཁ་ཅན་གྱི་ཆ་ཤས་(%S)"

#: updateTypeExtensions
msgid "Extensions and Themes (%S)"
msgstr "རྒྱ་བསྐྱེད་དང་བརྗོད་དོན་(%S)"

#: updateTypeLangPacks
msgid "Language Packs (%S)"
msgstr "སྐད་ཡིག་སྦུང་ཚན་(%S)"

#: foundAppLabel
msgid "%S %S is available. We strongly recommend that you install this upgrade as soon as possible."
msgstr "%S %Sའཐོབ་ཚུགས་པས། ཁྱོད་ཀྱིས་ཡར་བསྐྱེད་འདི་ ག་དེ་མགྱོགས་མགྱོགས་ གཞི་བཙུགས་འབད་བ་ཅིན་དྲག་འོང་།"

#: foundAppFeatures
msgid "%S %S features:"
msgstr "%S %Sགི་ཁྱད་རྣམ་ཚུ་:"

#: foundAppInfoLink
msgid "More information about %S %S ..."
msgstr "%S %Sགི་སྐོར་ལས་བརྡ་དོན་ཧེང་བཀལ་་་་"

#: appupdateperformedtitle
msgid "Restart Required"
msgstr "ལོག་འགོ་བཙུགས་དགོ་པས།"

#: appupdateperformedmessage
msgid "%S has been updated this session. Please restart %S before performing any more updates."
msgstr "%Sདེ་ལཱ་ཡུན་འདི་ནང་ དུས་མཐུན་བཟོ་ཡི། ཧེང་བཀལ་དུས་མཐུན་མ་བཟོ་བའི་ཧེ་མ་ %Sདེ་ལོག་འགོ་བཙུགས་གནང་།"

#: updatesdisabledtitle
msgid "Update Disabled"
msgstr "དུས་མཐུན་བཟོ་ནི་ལྕོགས་མིན་བཟོ་ནུག།"

#: updatesdisabledmessage
msgid "Update has been disabled by your Administrator."
msgstr "Update has been disabled by your Administrator."

