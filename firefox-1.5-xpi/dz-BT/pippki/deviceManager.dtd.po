msgstr "ཐབས་འཕྲུལ་འཛིན་སྐྱོང་པ་"

#: devmgr.devlist.label
msgid "Security Modules and Devices"
msgstr "སྲུང་སྐྱོབ་ཀྱི་མོ་ཌུལ་དང་ཐབས་འཕྲུལ་ཚུ་"

#: devmgr.details.title
msgid "Details"
msgstr "རྒྱས་བཤད་"

#: devmgr.details.title2
msgid "Value"
msgstr "བེ་ལུ་"

#: devmgr.status.label
msgid "Status"
msgstr "གནས་ཚད་"

#: devmgr.version.label
msgid "Version"
msgstr "ཐོན་རིམ་"

#: devmgr.button.changeslotname.label
msgid "Change Slot Name"
msgstr "ས་སྒོའི་མིང་སོར་"

#: devmgr.button.login.label
msgid "Log In"
msgstr "ནང་བསྐྱོད་"

#: devmgr.button.logout.label
msgid "Log Out"
msgstr "ཕྱིར་བསྐྱོད་"

#: devmgr.button.changepw.label
msgid "Change Password"
msgstr "ཆོག་ཡིག་སོར་"

