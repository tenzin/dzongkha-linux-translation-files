msgid "Module Name:"
msgstr "མོ་ཌུལ་མིང་:"

#: loaddevice.modname.default
msgid "New PKCS#11 Module"
msgstr "པི་ཀེ་སི་ཨེསི་གསརཔ་ཨང་༡༡ འབད་མི་མོ་ཌུལ།"

#: loaddevice.filename
msgid "Module filename:"
msgstr "མོ་ཌུལ་གྱི་མིང་:"

