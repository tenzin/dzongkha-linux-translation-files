#: SSLProtocolVersions
msgid "SSL Protocol Versions"
msgstr "ཨེསི་ཨེསི་ཨེལ་ གནས་སྤེལ་ལམ་ལུགས་ཐོན་རིམ་ཚུ་"

#: SSLWarnings
msgid "SSL Warnings"
msgstr "ཨེསི་ཨེསི་ཨེལ་གྱི་ཉེན་བརྡ་"

#: SSLClientAuthMethod
msgid "Client Certificate Selection"
msgstr "ཞབས་ཏོག་སྤྱོད་མིའི་ལག་ཁྱེར་གྱི་སེལ་འཐུ་"

#: prefheader.title
msgid "Secure Sockets Layer (SSL)"
msgstr "སོ་ཀེཊི་བི་རིམ་མཐའ་བཙན་བཟོ་(ཨེསི་ཨེསི་ཨེལ་)"

#: enable.ssl2
msgid "Enable SSL version 2"
msgstr "ཨེསི་ཨེསི་ཨེལ་ ཐོན་རིམ་༢ པ་ ལྕོགས་ཅན་བཟོ་"

#: enable.ssl3
msgid "Enable SSL version 3"
msgstr "ཨེསི་ཨེསི་ཨེལ་ ཐོན་རིམ་༣ པ་ ལྕོགས་ཅན་བཟོ་"

#: enable.tls
msgid "Enable TLS"
msgstr "ཊི་ཨེལ་ཨེསི་ ལྕོགས་ཅན་བཟོ་"

#: edit.sslciphers
msgid "Edit Ciphers..."
msgstr "གསང་ཡིག་ཞུན་དག་རྐྱབས་་་"

#: edit.sslciphers.accesskey
msgid "E"
msgstr "E"

#: warn.description
msgid "&brandShortName; can alert you to the security status of the web page you are viewing. Set &brandShortName; to show a warning and ask permission before:"
msgstr "&brandShortName; གིས་ ཁྱོད་ཀྱིས་བལྟ་བའི་བསྒང་ཡོད་མི་ ཤོག་ལེབ་དེ་གི་ སྲུང་སྐྱོབ་གནས་སྟངས་ཀྱི་ དྲན་བརྡ་འབད་ཚུགས་ཨིན། &brandShortName; གཞི་སྒྲིག་འབད་ཞིནམ་ལས་ ཉེན་བརྡ་སྟོན་ གནང་བ་ལེན་:"

#: warn.enteringsecure
msgid "Loading a page that supports encryption"
msgstr "གསང་བཟོ་ལུ་རྒྱབ་སྐྱོར་འབད་མི་ ཤོག་ལེབ་ཅིག་མངོན་གསལ་མ་འབད་བའི་ཧེ་མར་"

#: warn.enteringweak
msgid "Loading a page that uses low-grade encryption"
msgstr "གསང་བཟོའི་སྡེ་རིམ་ཆུང་བའི་ཤོག་ལེབ་ཅིག་ མངོན་གསལ་མ་འབད་བའི་ཧེ་མ་"

#: warn.insecurepost
msgid "Sending form data from an unencrypted page to an unencrypted page"
msgstr "འབྲི་ཤོག་གནས་སྡུད་ གསང་བ་མ་བཟོ་བའི་ཤོག་ལེབ་ཅིག་ གསང་བ་མ་བཟོ་བའི་ ཤོག་ལེབ་ཅིག་ལུ་ མ་བཏང་པའི་ཧེ་མ་"

#: warn.leavingsecure
msgid "Leaving a page that supports encryption"
msgstr "གསང་བཟོ་ལུ་རྒྱབ་སྐྱོར་འབད་མི་ ཤོག་ལེབ་ཅིག་ བཞག་མ་འགྱོ་བའི་ཧེ་མ་"

#: warn.secureredirect
msgid "Redirecting from one encrypted page to another"
msgstr "གསང་བཟོས་ཤོག་ལེབ་གཅིག་ལས་ གཞན་མི་གཅིག་ལུ་ སླར་ལོག་མ་བཏང་པའི་ཧེ་མ་"

#: warn.secureredirecttoinsecure
msgid "Redirecting from an encrypted page to an unencrypted page"
msgstr "གསང་བཟོ་ཤོག་ལེབ་ཅིག་ལས་ གསང་བ་མ་བཟོ་བའིང་ཤོག་ལེབ་ཅིག་ལུ་ སླར་ལོག་མ་བཏང་པའི་ཧེ་མ་"

#: warn.viewmixed
msgid "Viewing a page with an encrypted/unencrypted mix"
msgstr "གསང་བཟོས་དང་གསང་བ་མ་བཟོཝ སླ་བསྲེས་འབད་མི་ཤོག་ལེབ་ཅིག་ལུ་ མ་བལྟའི་ཧེ་མ་"

#: certselect.description
msgid "Decide how &brandShortName; selects a security certificate to present to web sites that require one:"
msgstr "&brandShortName; གིས་ སྲུང་སྐྱོབ་ལག་ཁྱེར་ཅིག་ སེལ་འཐུ་འབད་དེ་ དེ་དགོ་མི་ཝེབ་ས་ཁོངས་ལུ་ ག་དེ་སྦེ་བྱིནམ་ཨིན་ན་ཐག་གཅད་:"

#: certselect.auto
msgid "Select Automatically"
msgstr "རང་བཞིན་གྱིས་སེལ་འཐུ་འབད་"

#: certselect.ask
msgid "Ask Every Time"
msgstr "ཨ་རྟག་རང་དྲིས་"

# Cipher pref window
#: cipher.title
msgid "SSL: Edit Ciphers"
msgstr "ཨེསི་ཨེསི་ཨེལ་: གསང་ཡིག་ཞུན་དག་རྐྱབས་"

#: cipher.ssl2.label
msgid "SSL2 Cipher Suites"
msgstr "ཨེསི་ཨེསི་ཨེལ་༢ གསང་ཡིག་ཆ་ཚང་"

#: cipher.ssl3.label
msgid "SSL3/TLS Cipher Suites"
msgstr "ཨེསི་ཨེསི་ཨེལ་༣/ཊི་ཨེལ་ཨེསི་ གསང་ཡིག་ཆ་ཚང་"

#: cipher.ssl3_extra.label
msgid "Extra SSL3/TLS Cipher Suites"
msgstr "ཨེསི་ཨེསི་ཨེལ་༣/ཊི་ཨེལ་ཨེསི་ གསང་ཡིག་ཆ་ཚང་ཐེབས་"

#: cipher.tab.ssl2
msgid "SSL2"
msgstr "ཨེསི་ཨེསི་ཨེལ་༢ །"

#: cipher.tab.ssl3tls
msgid "SSL3/TLS"
msgstr "ཨེསི་ཨེསི་ཨེལ་༣/ཊི་ཨེལི་ཨེསི་"

#: cipher.tab.ssl3tls_extra
msgid "Extra SSL3/TLS"
msgstr "ཨེསི་ཨེསི་ཨེལ་༣/ཊི་ཨེལ་ཨེསི་ཐེབས་"

# SSL2 Ciphers
# SSL3/TLS ciphers
#: cipher.ssl2.rc4_128
#: cipher.ssl3.rsa_rc4_128_md5
msgid "128-bit RC4 encryption with RSA and an MD5 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེམ་ཌི་༥ མེཀ་གི་ཅིག་ཐོག་ལས་ བིཊི་-༡༢༨ ཨར་སི་༤ གི་གསང་བཟོ།"

#: cipher.ssl2.rc2_128
msgid "128-bit RC2 encryption with RSA and an MD5 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེམ་ཌི་༥ མེཀ་གི་ཅིག་ཐོག་ལས་ བིཊི་-༡༢༨ ཨར་སི་༢ གི་གསང་བཟོ།"

#: cipher.ssl2.des_ede3_192
msgid "168-bit Triple DES encryption with RSA and MD5 MAC "
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེམ་ཌི་༥ མེཀ་གི་ཅིག་ཐོག་ལས་ བིཊི་-༡༦༨ ཌི་ཨི་ཨེསི་གསུམ་ལྡན་གསང་བཟོ།"

#: cipher.ssl2.des_64
msgid "56-bit DES encryption with RSA and an MD5 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེམ་ཌི་༥ མེཀ་གི་ཅིག་ཐོག་ལས་ བིཊི་-༥༦ ཌི་ཨི་ཨེསི་གི་གསང་བཟོ།"

#: cipher.ssl2.rc4_40
#: cipher.ssl3.rsa_rc4_40_md5
msgid "40-bit RC4 encryption with RSA and an MD5 MAC (export)"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེམ་ཌི་༥ མེཀ་གི་ཅིག་(ཕྱིར་འདྲེན་)ཐོག་ལས་ བིཊི་-༤༠ ཨར་སི་༤ གི་གསང་བཟོ།"

#: cipher.ssl2.rc2_40
#: cipher.ssl3.rsa_rc2_40_md5
msgid "40-bit RC2 encryption with RSA and an MD5 MAC (export)"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེམ་ཌི་༥ མེཀ་གི་ཅིག་ཐོག་ལས་(ཕྱིར་འདྲེན་) བིཊི་-༤༠ ཨར་སི་༢ ཀྱི་གསང་བཟོ།"

#: cipher.ssl3.rsa_rc4_128_sha
msgid "128-bit RC4 encryption with RSA and a SHA1 MAC"
msgstr "ཨེར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་ཅིག་གི་ཐོག་ལས་ བིཊི་-༡༦༨ ཨར་སི་༢༤ གི་གསང་བཟོ།"

#: cipher.ssl3.rsa_fips_des_ede3_sha
msgid "168-bit Triple DES with RSA and a SHA1 MAC (FIPS)"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་ཅིག་(ཨེཕ་ཨའི་པི་ཨེསི་)ཐོག་ལས་ བིཊི་-༡༦༨ ཌི་ཨི་ཨེསི་གསུམ་ལྡན་"

#: cipher.ssl3.rsa_des_ede3_sha
msgid "168-bit Triple DES with RSA and a SHA1 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་ཅིག་གི་ཐོག་ལས་ བིཊི་-༡༦༨ ཌི་ཨི་ཨེསི་གསུམ་ལྡན་"

#: cipher.ssl3.rsa_fips_des_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC (FIPS)"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་ཅིག་གི་ཐོག་ལས་ བིཊི་-༥༦ ཌི་ཨི་ཨེསི་གསང་བཟོ་"

#: cipher.ssl3.rsa_des_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་ཅིག་གི་ཐོག་ལས་ བིཊི་-༡༦༨ ཌི་ཨི་ཨེསི་གསང་བཟོ།"

#: cipher.ssl3.rsa_1024_rc4_56_sha
msgid "56-bit RC4 encryption with RSA and a SHA1 MAC (export)"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་(ཕྱིར་འདྲེན)ཅིག་གི་ཐོག་ལས་ བིཊི་-༡༦༨ ཨར་སི་༤ གི་གསང་བཟོ།"

#: cipher.ssl3.rsa_1024_des_cbc_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC (export)"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་(ཕྱིར་འདྲེན་)ཅིག་གི་ཐོག་ལས་ བིཊི་-༥༦ ཌི་ཨི་ཨེསི་གསང་བཟོ།"

# Extra SSL3/TLS ciphers
#: cipher.ssl3.dhe_rsa_aes_256_sha
msgid "256-bit AES encryption with RSA, DHE, and a SHA1 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཌི་ཨེཆ་ཨི་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་ཅིག་གི་ཐོག་ལས་ བིཊི་-༢༥༦ ཨེ་ཨི་ཨེསི་གསང་བཟོ།"

#: cipher.ssl3.dhe_dss_aes_256_sha
msgid "256-bit AES encryption with DSA, DHE, and a SHA1 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཌི་ཨེཆ་ཨི་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་ཅིག་གི་ཐོག་ལས་ བིཊི་-༢༥༦ ཨེ་ཨི་ཨེསི་གསང་བཟོ།"

#: cipher.ssl3.rsa_aes_256_sha
msgid "256-bit AES encryption with RSA and a SHA1 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་ཅིག་གི་ཐོག་ལས་ བིཊི་-༢༥༦ ཨེ་ཨི་ཨེསི་གསང་བཟོ།"

#: cipher.ssl3.dhe_rsa_aes_128_sha
msgid "128-bit AES encryption with RSA, DHE, and a SHA1 MAC"
