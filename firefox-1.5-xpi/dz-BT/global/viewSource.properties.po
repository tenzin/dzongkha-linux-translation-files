#: goToLineTitle
msgid "Go to line"
msgstr "གྱལ་རིམ་ལུ་སོང་"

#: goToLineText
msgid "Enter line number"
msgstr "གྱལ་རིམ་ཨང་བཙུགས་"

#: invalidInputTitle
msgid "Invalid input"
msgstr "ནུས་མེད་ཨིན་པུཊི་"

#: invalidInputText
msgid "The line number entered is invalid."
msgstr "གྱལ་རིམ་ཨང་བཙུགས་མི་དེ་ ནུས་མེད་ཨིན་པས།"

#: outOfRangeTitle
msgid "Line not found"
msgstr "གྱལ་རིམ་འཚོལ་མ་ཐོབ་"

#: outOfRangeText
msgid "The specified line was not found."
msgstr "གསལ་བཀོད་འབད་མི་གྱལ་རིམ་དེ་འཚོལ་མ་ཐོབ།"

#: statusBarLineCol
msgid "Line %1$S, Col %2$S"
msgstr "གྱལ་རིམ་%1$S། ཀེར་ཐིག་ %2$S།"

#: viewSelectionSourceTitle
msgid "DOM Source of Selection"
msgstr "སེལ་འཐུའི་ཌི་ཨོ་ཨེམ་འབྱུང་ཁུངས།"

#: viewMathMLSourceTitle
msgid "DOM Source of MathML"
msgstr "མེཐ་ཨེམ་ཨེལ་གྱི་ ཌི་ཨོ་ཨེམ་འབྱུང་ཁུངས།"

