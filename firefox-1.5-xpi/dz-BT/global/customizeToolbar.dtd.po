#: dialog.title
msgid "Customize Toolbar"
msgstr "ལག་ཆས་ཕྲ་རིང་སྲོལ་སྒྲིག་འབད་"

#: instructions.description
msgid "You can add or remove items by dragging to or from the toolbars."
msgstr "ཁྱོད་ཀྱིས་རྣམ་གྲངས་ཚུ་ ལག་ཆས་ཕྲ་རིང་ལུ་ འདྲུད་དེ་བསྐྱལ་བའམ་ ལག་ཆས་ཕྲ་རིང་ནང་ལས་ འདྲུད་པའི་སྒོ་ལས་ ཁ་སྐོང་རྐྱབ་པའམ་ རྩ་བསྐྲད་གཏང་བཏུབ།"

#: show.label
msgid "Show:"
msgstr "སྟོན་:"

#: iconsAndText.label
msgid "Icons and Text"
msgstr "ངོས་དཔར་དང་ཚིག་ཡིག་"

#: icons.label
msgid "Icons"
msgstr "ངོས་དཔར་"

#: text.label
msgid "Text"
msgstr "ཚིག་ཡིག་"

#: useSmallIcons.label
msgid "Use Small Icons"
msgstr "ངོས་དཔར་ཆུང་ཀུ་ལག་ལེན་འཐབ་"

#: restoreDefaultSet.label
msgid "Restore Default Set"
msgstr "སྔོན་སྒྲིག་སྒྲིག་སྟངས་སོར་ཆུད་འབད་"

#: addNewToolbar.label
msgid "Add New Toolbar"
msgstr "ལག་ཆས་ཕྲ་རིང་གསརཔ་ཁ་སྐོང་རྐྱབས་"

#: saveChanges.label
msgid "Done"
msgstr "འབད་ཚར་"

#: undoChanges.label
msgid "Undo Changes"
msgstr "བསྒྱུར་བཅོས་འབད་ནི་བཤོལ་"

