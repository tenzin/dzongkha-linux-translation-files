msgstr "&brandFullName; Options"

#: None:2
msgid "&brandFullName; &pref.pluralCaps;"
msgstr "&brandFullName; &pref.pluralCaps;"

#: None:3
msgid ""
"This document explains all of the &pref.plural; available in the  &pref."
"pluralCaps; window of &brandFullName;."
msgstr ""
"This document explains all of the &pref.plural; available in the  &pref."
"pluralCaps; window of &brandFullName;."

#: None:4
msgid "In this section:  <ul>"
msgstr "In this section:  <ul>"

#: None:5
msgid "<a href=\"#general_options\">General &pref.pluralCaps;</a>"
msgstr "<a href=\"#general_options\">General &pref.pluralCaps;</a>"

