#: tabsTab.label
msgid "Tabs"
msgstr "ཨེབ་ལྡེ་ཚུ་"

#: openExternalLinksIn.label
msgid "Open links from other applications in:"
msgstr "གློག་རིམ་གཞན་ཚུ་ནང་ལས་ འབྲེལ་ལམ་ཁ་ཕྱེ་"

#: newWindow.label
#: newWindow.accesskey
msgid "a ne&w window"
msgstr "སྒོ་སྒྲིག་གསརཔ་ཅིག་ནང་(&w)"

#: newTabInRecentWindow.label
#: newTabInRecentWindow.accesskey
msgid "a new &tab in the most recent window"
msgstr "འཕྲལ་གྱི་སྒོ་སྒྲིག་ཅིག་ནང་ ཨེབ་ལྡེ་གསརཔ་ཅིག་ནང་(&t)"

#: recentWindowOrTab.label
#: recentWindowOrTab.accesskey
msgid "the most &recent tab/window"
msgstr "འཕྲལ་གྱི་ཨེབ་ལྡེ་/སྒོ་སྒྲིག་ནང་(&r)"

#: forceNewWindowLinks.label
#: forceNewWindowLinks.accesskey
msgid "&Force links that open new windows to open in:"
msgstr "སྒོ་སྒྲིག་གསརཔ་ཁ་ཕྱེ་མི་ འབྲེལ་ལམ་ཚུ་ བང་ཅན་སྦེ་བཟོ་(&F)"

#: sameTabOrWindow.label
#: sameTabOrWindow.accesskey
msgid "the &same tab/window as the link"
msgstr "ཨེབ་ལྡེ་/སྒོ་སྒྲིག་གཅིག་པའི་ནང་ འབྲེལ་ལམ་སྦེ་སྟོན་ནི་ལུ་(&s)"

#: newTab.label
#: newTab.accesskey
msgid "a &new tab"
msgstr "ཨེབ་ལྡེ་གསརཔ་ཅིག་ནང་(&n)"

#: hideTabBar.label
#: hideTabBar.accesskey
msgid "Hi&de the tab bar when only one web site is open"
msgstr "ཝེབ་ས་ཁོངས་གཅིག་རྐྱངམ་གཅིག་ ཁ་ཕྱེ་བའི་སྐབས་ ཨེབ་ལྡེ་ཕྲ་རིང་དེ་སྦ་བཞག(&d)"

#: loadInBackground.label
#: loadInBackground.accesskey
msgid "Se&lect new tabs opened from links"
msgstr "འབྲེལ་ལམ་ནང་ལས་ཁ་ཕྱེ་མི་ ཨེབ་ལྡེ་གསརཔ་ཚུ་ སེལ་འཐུ་འབད་(&l)"

#: loadBookmarksInBackground.label
#: loadBookmarksInBackground.accesskey
msgid "Select new ta&bs opened from bookmarks or history"
msgstr "དེབ་རྟགས་དང་ལོ་རྒྱུས་ནང་ལས་ཁ་ཕྱེ་མི་ ཨེབ་ལྡེ་གསརཔ་ཚུ་ སེལ་འཐུ་འབད།(&b)"

#: warnOnClose.label
#: warnOnClose.accesskey
msgid "Warn when closing &multiple tabs"
msgstr "ཨེབ་ལྡེ་ལེ་ཤ་ཁ་བསྡམས་པའི་སྐབས་ ཉེན་བརྡ་འབད།(&m)"

