#: window.width
msgid ""
"_: Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger.\n"
"30em"
msgstr "ཨི་ཨེ་༣༠"

#: languages.customize.Header
#: languages.customize.label
msgid "Languages"
msgstr "སྐད་ཡིག་ཚུ་"

#: languages.customize.prefLangDescript
msgid "Web pages are sometimes offered in more than one language. Choose languages for displaying these web pages, in order of preference."
msgstr "རེ་ཚེསྐབས་ ཝེབ་ཤོག་ལེབ་ཚུ་ སྐད་ཡིག་གཅིག་ལས་མངམ་ནང་བྱིནམ་ཨིན། རང་གི་དགའ་གདམ་གྱི་གོ་རིམ་དང་འཁྲིལ་ཏེ་ བཀྲམ་སྟོན་འབད་ནིའི་ སྐད་ཡིག་ཚུ་གདམ་ཁ་རྐྱབས།"

#: languages.customize.active.label
msgid "Languages in order of preference:"
msgstr "དགའ་གདམ་གྱི་གོ་རིམ་དང་འཁྲིལ་བའི་སྐད་ཡིག་ཚུ་:"

#: languages.customize.moveUp.label
#: languages.customize.moveUp.accesskey
msgid "Move &Up"
msgstr "ཡར་སྤོ་(&U)"

#: languages.customize.moveDown.label
#: languages.customize.moveDown.accesskey
msgid "Move &Down"
msgstr "མར་སྤོ་(&D)"

#: languages.customize.deleteButton.label
#: languages.customize.deleteButton.accesskey
msgid "&Remove"
msgstr "རྩ་བསྐྲད་གཏང་(&R)"

#: languages.customize.selectLanguage.label
#: languages.customize.selectLanguage.accesskey
msgid "&Select a language to add..."
msgstr "ཁ་སྐོང་བརྐྱབ་ནིའི་སྐད་ཡིག་སེལ་འཐུ་འབད་་་་་(&S)"

#: languages.customize.addButton.label
#: languages.customize.addButton.accesskey
msgid "&Add"
msgstr "ཁ་སྐོང་རྐྱབས་(&A)"

