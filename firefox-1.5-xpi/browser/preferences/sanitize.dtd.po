#: sanitizeDialog.title
msgid "Clear Private Data"
msgstr "སྒེར་གྱི་གནས་སྡུ་བསལ་གཏང་"

#: window.width
msgid ""
"_: Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger.\n"
"30em"
msgstr "ཨི་ཨེམ་༣༠།"

#: items.label
msgid "Private Data"
msgstr "སྒེར་གྱི་གནས་སྡུད་"

#: sanitizeItems.label
msgid "Select the items to be cleared:"
msgstr "བསལ་གཏང་ནིའི་རྣམ་གྲངས་ཚུ་སེལ་འཐུ་འབད་:"

#: itemHistory.label
#: itemHistory.accesskey
msgid "&Browsing History"
msgstr "ལོ་རྒྱུས་བརྡ་འཚོལ་འབད་དོ་(&B)"

#: itemFormData.label
#: itemFormData.accesskey
msgid "Saved &Form Information"
msgstr "བསྲུངས་ཡོད་པའི་འབྲི་ཤོག་གི་བརྡ་དོན་(&F)"

#: itemPasswords.label
#: itemPasswords.accesskey
msgid "Saved &Passwords"
msgstr "བསྲུངས་ཡོད་པའི་ཆོག་ཡིག(&P)"

#: itemCookies.label
#: itemCookies.accesskey
msgid "&Cookies"
msgstr "ཀུ་ཀིསི་(&C)"

#: itemCache.label
#: itemCache.accesskey
msgid "C&ache"
msgstr "འདྲ་མཛོད་(&a)"

#: itemDownloads.label
#: itemDownloads.accesskey
msgid "&Download History"
msgstr "ལོ་རྒྱུས་ཕབ་ལེན་འབད་(&D)"

#: itemSessions.label
#: itemSessions.accesskey
msgid "Authenticated &Sessions"
msgstr "བདེན་བཤད་འབད་ཡོད་པའི་ལཱ་ཡུན་ཚུ་(&S)"

#: settings.label
msgid "Settings"
msgstr "སྒྲིག་སྟངས་"

#: sanitizeOnShutDown.label
#: sanitizeOnShutDown.accesskey
msgid "Cl&ear private data when closing &brandShortName;"
msgstr "&brandShortName; ཁ་བསྡམས་པའི་སྐབས་ སྒེར་གྱི་གནས་སྡུད་བསལ་གཏང་།(&e)"

#: promptOnSanitize.label
#: promptOnSanitize.accesskey
msgid "As&k me before clearing private data"
msgstr "སྒེར་གྱི་གནས་སྡུད་ བསལ་མ་བཏང་པའི་ཧེ་མར་ ང་ལུ་དྲིས།(&k)"

