#: newBookmark.title
msgid "Add Bookmark"
msgstr "དེབ་རྟགས་ཁ་སྐོང་རྐྱབས།"

#: newbookmark.label
msgid "&brandShortName; will add a bookmark to this page."
msgstr "&brandShortName; གིས་ ཤོག་ལེབ་འདི་ལུ་ དེབ་རྟགས་ཅིག་ཁ་སྐོང་བརྐྱབ་འོང་།"

#: name.label
#: name.accesskey
msgid "&Name:"
msgstr "མིང་།(&N)"

#: keyword.label
#: keyword.accesskey
msgid "&Keyword:"
msgstr "གཙོ་ཚིག།(&K)"

#: url.label
#: url.accesskey
msgid "&Location:"
msgstr "གནས་ཁོངས:(&L)"

#: button.createin.label
#: button.createin.accesskey
msgid "&Create In &gt;&gt;"
msgstr "&gt;&gt ནང་གསར་བསྐྲུན་འབད།(&C)"

#: button.createin2.label
msgid "Create In &lt;&lt;"
msgstr " &lt;&lt ནང་ གསར་བསྐྲུན་འབད།"

#: createin.label
#: createin.accesskey
msgid "&Create in:"
msgstr "གསར་བསྐྲུན་འབད།(&C)"

#: moveto.label
#: moveto.accesskey
msgid "&Move to:"
msgstr "ལུ་སྤེོ།(&M)"

#: button.newfolder.label
#: button.newfolder.accesskey
msgid "New F&older..."
msgstr "སྣོད་འཛིན་གསར་པའི་ནང་(&o)"

#: alwayscreateinfolder.label
#: alwayscreateinfolder.accesskey
msgid "Don't show this di&alog again"
msgstr "ཌའི་ལོག་འདི་ ལོག་སྟེ་མ་སྟོན།"

#: dontshowmessage.tooltip
msgid "When this option is selected, new Bookmarks will be added using the title provided by the page."
msgstr "གདམ་ཁ་འདི་སེལ་འཐུ་འབདཝ་ད་ ཤོག་ལེབ་དེ་གིས་བྱིན་མི་ མགོ་མིང་དེ་ལག་ལེན་འཐབ་པའི་ཐོག་ལས་ དེབ་རྟགས་ཁ་སྐོང་བརྐྱབ་འོང་།"

#: selectFolder.label
msgid "Choose Folder"
msgstr "སྣོེད་འཛིན་གདབ་ཁ་རྐྱབས།"

#: expanderDown.tooltip
msgid "Show all the bookmarks folders"
msgstr "དེབ་རྟགས་ཀྱི་སྣོད་འཛིན་ཆ་མཉམ་སྟོན།"

#: expanderUp.tooltip
msgid "Hide"
msgstr "སྦ།"

#: acceptButton.label
msgid "Add"
msgstr "ཁ་སྐོང་རྐྱབས།"

#: bookmarksToolbar.label
msgid "Bookmarks Toolbar"
msgstr "དེབ་རྟགས་ཀྱི་ལག་ཆས་ཕྲ་རིང་།"

