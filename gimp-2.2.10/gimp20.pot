#: ../app/main.c:422
#, c-format
msgid ""
"\n"
"Invalid option \"%s\"\n"
msgstr ""
"\n"
"ནུས་མེད་གདམ་ཁ་\"%s\"\n"

#: ../app/main.c:501
msgid "GIMP version"
msgstr "ཇི་ཨའི་ཨེམ་པི་ ཐོན་རིམ།"

#: ../app/main.c:509
#, c-format
msgid ""
"\n"
"Usage: %s [option ... ] [file ... ]\n"
"\n"
msgstr ""
"\n"
"ལག་ལེན་: %s [གདམ་ཁ་ ... ] [ཡིག་སྣོད་ ... ]\n"
"\n"

#: ../app/main.c:511
msgid "Options:\n"
msgstr "འདམ་ཁ་ཚུ་:\n"

#: ../app/main.c:512
msgid "  -h, --help               Output this help.\n"
msgstr "  -h, --help               འ་ནི་གྲོགས་རམ་འདི་ཨའུཊི་པུཊི་འབད། \n"

#: ../app/main.c:513
msgid "  -v, --version            Output version information.\n"
msgstr "  -v, --version            ཨའུཊི་པུཊི་ཐོན་རིམ་བརྡ་དོན། \n"

#: ../app/main.c:514
msgid "  --verbose                Show startup messages.\n"
msgstr "  --verbose                འགོ་བཙུགས་ཡོད་པའི་འཕྲིན་དོན་ཚུ་སྟོན། \n"

#: ../app/main.c:515
msgid "  --no-shm                 Do not use shared memory between GIMP and plugins.\n"
msgstr "  --no-shm                 ཇི་ཨའི་ཨེམ་པི་དང་ པ་ལག་ཨིནསི་ གྱི་བར་ན་ རུབ་སྤྱོད་འབད་ཡོད་པའི་དྲན་ཚད་ལག་ལེན་མ་འཐབ། \n"

#: ../app/main.c:516
msgid "  --no-cpu-accel           Do not use special CPU accelerations.\n"
msgstr "  --no-cpu-accel           དམིགས་བསལ་གྱི ་སི་པི་ཡུ ་མགྱོགས་བསྐྱོད་འབད་ནི་ཚུ་ལག་ལེན་མ་འཐབ། \n"

#: ../app/main.c:517
msgid "  -d, --no-data            Do not load brushes, gradients, palettes, patterns.\n"
msgstr "  -d, --no-data            དཔེ་གཞི་ཚུ་ པེ་ལེཊི་ཚུ་ སྟེགས་རིས་ཚུ་དང་པྱིར་ཚུ་མངོན་གསལ་མ་འབད། \n"

#: ../app/main.c:518
msgid "  -f, --no-fonts           Do not load any fonts.\n"
msgstr "  -f, --no-fonts           གང་རུང་གི་ཡིག་གཟུགས་ཚུ་མངོན་གསལ་མ་འབད། \n"

#: ../app/main.c:519
msgid "  -i, --no-interface       Run without a user interface.\n"
msgstr "  -i, --no-interface       ངོས་འདྲ་བའི་ལག་ལེན་པ་མེད་པར་གཡོག་བཀོལ། \n"

#: ../app/main.c:520
msgid "  --display <display>      Use the designated X display.\n"
msgstr "  --display <བཀྲམ་སྟོན་>      བཀོད་བསྒྲིག་འབད་ཡོད་པའི་ཨེཀསི་བཀྲམ་སྟོན་འདི་ལག་ལེན་འཐབ། \n"

#: ../app/main.c:521
msgid "  -s, --no-splash          Do not show the startup window.\n"
msgstr "  -s, --no-splash          འགོ་བཙུགས་ནིའི་སྒོ་སྒྲིག་མ་སྟོན། \n"

#: ../app/main.c:522
msgid "  --session <name>         Use an alternate sessionrc file.\n"
msgstr "  --session <མིང་>         སྤེལ་མའི་ལཱ་ཡུན་ཨཱར་སི་ཡིག་སྣོད་འདི་ལག་ལེན་འཐབ། \n"

#: ../app/main.c:523
msgid "  -g, --gimprc <gimprc>    Use an alternate gimprc file.\n"
msgstr "  -g, --gimprc <gimprc>    ཇི་ཨའི་ཨེམ་པི་ཨཱར་སི་ཡིག་སྣོད་དེ་ལག་ལེན་འཐབ། \n"

#: ../app/main.c:524
msgid "  --system-gimprc <gimprc> Use an alternate system gimprc file.\n"
msgstr "  --system-gimprc <gimprc>སྤེལ་མའི་རིམ་ལུགས་ཇི་ཨཡི་ཨེམ་པི་ཨཱར་སི་ཡིག་སྣོད་དེ་ལག་ལེན་འཐབ། \n"

#: ../app/main.c:525
msgid "  --dump-gimprc            Output a gimprc file with default settings.\n"
msgstr "  --dump-gimprc           སྔོན་སྒྲིག་སྒྲིག་སྟངས་ཚུ་དང་ཅིག་ཁར་ཇི་ཨའི་ཨེམ་པི་ཨཱར་སི་ཡིག་སྣོད་ཨའུཊི་པུཊི་འབད། \n"

#: ../app/main.c:526
msgid "  -c, --console-messages   Display warnings to console instead of a dialog box.\n"
msgstr "  -c, --མ་སྒྲོམ་-འཕྲིན་དོན་ཚུ་   ཌའི་ལོག་སྒྲོམ་གྱི་ཚབ་སྦེ་མ་སྒྲོམ་ལུ་ཉེན་བརྡ་ཚུ་བཀྲམ་སྟོན་འབད། \n"

#: ../app/main.c:527
msgid "  --debug-handlers         Enable non-fatal debugging signal handlers.\n"
msgstr "  --debug-handlers         ཚབས་ཆེན་མ་ཡིན་པ་རྐྱེན་སེལ་འབད་ནིའི་བརྡ་རྟགས་ལེགས་སྐྱོང་པ་ཚུ་ལྕོགས་ཅན་བཟོ། \n"

#: ../app/main.c:528
msgid ""
"  --stack-trace-mode <never | query | always>\n"
"                           Debugging mode for fatal signals.\n"
msgstr ""
"  --བstack-trace-mode <ནམ་ཡང་ | འདྲི་དཔྱད་ | ཨ་རྟག་རང་>\n"
"                           ཚབས་ཆེན་བརྡ་རྟགས་ཚུ་གི་དོན་ལུ་རྐྱེན་སེལ་འབད་ནིའི་ཐབས་ལམ། \n"

#: ../app/main.c:530
msgid ""
"  --pdb-compat-mode <off | on | warn>\n"
"                           Procedural Database compatibility mode.\n"
msgstr ""
"  --pdb-compat-mode <ཨོཕ་ | ཨཱོན་ | ཉེན་བརྡ་>\n"
"                           བྱ་སྒོའི་ལམ་ལུགས་ཀྱི་གནད་སྡུད་གཞི་རྟེན་མཐུན་འགྱུར་ཐབས་ལམ། \n"

#: ../app/main.c:532
msgid ""
"  --batch-interpreter <procedure>\n"
"                           The procedure to process batch commands with.\n"
msgstr ""
"  --batch-interpreter <བྱ་སྒོའི་ལམ་ལུགས་>\n"
"                           རུ་ཚན་བརྡ་བཀོད་ཚུ་དང་མཉམ་ལས་སྦྱོར་འབད་ནི་ལུ་བྱ་སྒོའི་ལམ་ལུགས། \n"

#: ../app/main.c:534
msgid "  -b, --batch <commands>   Process commands in batch mode.\n"
msgstr "  -b, --batch <བརྡ་བཀོད་ཚུ་>   རུ་ཚན་ཐབས་ལམ་ནང་ལས་སྦྱོར་བརྡ་བཀོད་ཚུ། \n"

#: ../app/sanity.c:194
#, c-format
msgid ""
"The configured filename encoding cannot be converted to UTF-8: %s\n"
"\n"
"Please check the value of the environment variable G_FILENAME_ENCODING."
msgstr ""
"རིམ་སྒྲིག་་འབད་ཚར་མི་ཡིག་སྣོད་ཀྱི་མིང་ཨིན་ཀོ་ཌིང་དེ་ ཡུ་ཊི་ཨེཕི་-༨:%s\n"
"\n"
"ལུ་ངཞི་བསྒྱུར་འབད་མི་བཏུབ།མཐའ་འཁོར་འགྱུར་ཅན་གྱི་གནས་གོང་དེ་ཞིབ་དཔྱད་འབད་གནང་ ཇི_ཡིག་སྣོད་_ཨིན་ཀོ་ཌིང་།"

#: ../app/sanity.c:213
#, c-format
msgid ""
"The name of the directory holding the GIMP user configuration cannot be converted to UTF-8: %s\n"
"\n"
"Most probably your filesystem stores files in an encoding different from UTF-8 and you didn't tell GLib about this. Please set the environment variable G_FILENAME_ENCODING."
msgstr ""
"སྣོད་ཐོ་འདི་གི་མིང་ཇི་ཨའི་ཨེམ་པི་ལག་ལེན་པའི་རིམ་སྒྲིག་འཆང་མི་འདི་ UTF-8: %s ལུ་གཞི་བསྒྱུར་འབད་མི་བཏུབ། \n"
"\n"
"མང་ཆེ་ཤོས་ཅིག་ཁྱོད་ཀྱི་ཡིག་སྣོད་རིམ་ལུགས་ཀྱིས་ཡུ་ཊི་ཨེཕི་-8 ལས་སོ་སོ་ཨིན་མི་ ཨེན་ཀོ་ཌིང་ནང་གསོག་འཇོག་འབད་ཡོདཔ་ དང་ ཁྱོད་ཀྱིས་འདི་གི་སྐོར་ལས་ཇི་ཨེལ་ཨའི་བི་ལུ་མ་སླབ། མཐའ་འཁོར་འགྱུར་ཅན་ ཇི་ ཡིག་སྣོད་མིང་ ཨིན་ཀོ་ཌིང་འདི་གཞི་སྒྲིག་འབད་གནང་།"

#. initialize the list of gimp brushes
#: ../app/actions/actions.c:94
#: ../app/core/gimp.c:856
#: ../app/dialogs/dialogs.c:138
#: ../app/dialogs/preferences-dialog.c:2297
#: ../app/pdb/internal_procs.c:92
msgid "Brushes"
