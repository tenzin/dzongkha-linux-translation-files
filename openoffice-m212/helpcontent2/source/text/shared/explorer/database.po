msgstr "ཆ་ཤས་བརླག་སྟོར་ཞུགས་པ།"

#: 02000002.xhp#bm_id3150445.help.text
msgid "\\<bookmark_value\\>queries; missing elements (Base)\\</bookmark_value\\>"
msgstr "\\<bookmark_value\\>འདྲི་དཔྱད་ཚུ། བརླག་སྟོར་ཞུགས་མི་ཆ་ཤས་ཚུ་ (གཞི་རྟེན་)\\</bookmark_value\\>"

