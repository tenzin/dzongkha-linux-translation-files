msgstr "ཐིག་ཁྲམ་ངེས་ཚིག་གི་མངའ་ཁོངས།"

#: 05010000.xhp#par_id3153681.5.help.text
msgid "\\<ahelp hid=\\\"HID_TABDESIGN_BACKGROUND\\\"\\>This area is where you define the table structure.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\"HID_TABDESIGN_BACKGROUND\\\"\\>འ་ནི་མངའ་ཁོངས་དེ་ ཁྱོད་ཀྱིས་ཐིག་ཁྲམ་གཞི་བཀོད་ངེས་འཛིན་འབད་ས་ཨིན་།\\</ahelp\\>"

#: 05010000.xhp#hd_id3153031.6.help.text
msgid "Field Name"
msgstr "ས་སྒོའི་མིང་།"

#: 05010000.xhp#par_id3156113.7.help.text
msgid "\\<ahelp hid=\\\"HID_TABDESIGN_NAMECELL\\\"\\>Specifies the name of the data field. Note the database restrictions, such as the length of the name, special characters and spaces.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\"HID_TABDESIGN_NAMECELL\\\"\\>གནད་སྡུད་ས་སྒོའི་མིང་འདི་གསལ་བཀོད་འབདཝ་ཨིན།  གནད་སྡུད་གཞི་རྟེན་གྱི་བཀག་དམ་  མིང་གི་རིང་ཚད་དང་དམིགས་བསལ་གྱི་ཡིག་འབྲུ་ཚུ་ དེ་ལས་ བར་སྟོང་ཚུ་དྲེན་འཛིན་འབད།\\</ahelp\\>"

