msgid ""
"_: tablewizard02.xhp#par_idN10586.help.text\n"
"Field name"
msgstr "ས་སྒོའི་མིང་།"

#: tablewizard02.xhp#par_idN1058A.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Displays the name of the selected data field. If you want, you can enter a new name.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>གིས་  སེལ་འཐུ་འབད་ཡོད་པའི་  གནད་སྡུད་ས་སྒོ་གི་མིང་འདི་  བཀྲམ་སྟོན་འབདཝ་ཨིན། ཁྱོད་རང་དགོ་པ་ཅིན་  ཁྱོད་ཀྱིས་མིང་གསརཔ་ཅིག་བཙུགས་བཏུབ།\\</ahelp\\>"

#: tablewizard02.xhp#par_idN1058D.help.text
msgid ""
"_: tablewizard02.xhp#par_idN1058D.help.text\n"
"Field type"
msgstr "ས་སྒོའི་དབྱེ་བ།"

#: tablewizard02.xhp#par_idN10591.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Select a field type.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>ས་སྒོའི་དབྱེ་བ་སེལ་འཐུ་འབད།\\</ahelp\\>"

#: tablewizard02.xhp#hd_id5486922.help.text
msgid "AutoValue"
msgstr "རང་བཞིན་གནས་གོང་།"

#: tablewizard02.xhp#par_id4198736.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>If set to Yes, the values for this data field are generated by the database engine.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>ཨིན་ལུ་གཞི་སྒྲིག་འབད་བ་ཅིན་ ཨ་ནི་གནད་སྡུད་ས་སྒོ་གི་དོན་ལས་གནས་གོང་ཚུ་གནད་སྡུད་གཞི་རྟེན་མ་འཕྲུལ་གྱིས་བཟོ་སྟོན་འབདཝ་ཨིན།\\</ahelp\\>"

#: tablewizard02.xhp#par_idN106A0.help.text
msgid "Entry required"
msgstr "ཐོ་བཀོད་དགོཔ་ཨིན།"

#: tablewizard02.xhp#par_idN106A6.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>If set to Yes, this field must not be empty.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>ཨིན་ལུ་གཞི་སྒྲིག་འབད་དེ་ཡོད་པ་ཅིན་  ས་སྒོ་འདི་སྟོངམ་མེདཔ་སྦེ་དགོ།\\</ahelp\\>"

#: tablewizard02.xhp#par_idN10594.help.text
msgid ""
"_: tablewizard02.xhp#par_idN10594.help.text\n"
"Length"
msgstr "རིང་ཚད།"

#: tablewizard02.xhp#par_idN10598.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Specifies the number of characters for the data field.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>གནད་སྡུད་ས་སྒོའི་དོན་ལུ་  ཡིག་འབྲུ་གི་ཨང་གྲངས་འདི་  གསལ་བཀོད་འབདཝ་ཨིན། \\</ahelp\\>"

#: tablewizard02.xhp#par_idN1059B.help.text
msgid ""
"_: tablewizard02.xhp#par_idN1059B.help.text\n"
"Decimal places"
msgstr "བཅུ་ཚགས་གནས་ས་ཚུ།"

#: tablewizard02.xhp#par_idN1059F.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Specifies the number of decimal places for the data field. This option is only available for numerical or decimal data fields.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>གིས་  གནད་སྡུད་ས་སྒོའི་དོན་ལུ་  བཅུ་ཚག་གནས་ས་གི་ཨང་གྲངས་འདི་གསལ་བཀོད་འབདཝ་ཨིན།  གདམ་ཁ་འདི་  ཨང་གྲངས་དོན་ལུ་ ཡང་ན་  བཅུ་ཚག་གནད་སྡུད་ས་སྒོའི་དོན་ལུ་རྐྱངམ་ཅིག་  འཐོབ་ཚུགསཔ་ཨིན།\\</ahelp\\>"

#: tablewizard02.xhp#par_idN105A2.help.text
msgid ""
"_: tablewizard02.xhp#par_idN105A2.help.text\n"
"Default value"
msgstr "སྔོན་སྒྲིག་གནས་གོང་།"

#: tablewizard02.xhp#par_idN105A6.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Specifies the default value for a Yes/No field.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>ཨིན་དང་/མེན་གྱི་ས་སྒོའི་དོན་ལུ་  སྔོན་སྒྲིག་གནས་གོང་འདི་གསལ་བཀོད་འབདཝ་ཨིན།\\</ahelp\\>"

#: tablewizard02.xhp#par_idN10730.help.text
msgid ""
"_: tablewizard02.xhp#par_idN10730.help.text\n"
"Auto-increment statement"
msgstr "རང་བཞིན་ཡར་འཕར་གསལ་བཤད།"

#: tablewizard02.xhp#par_idN10736.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Enter the statement used by the database to auto-increment this field.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>ས་སྒོ་འདི་རང་བཞིན་གྱི་ཡར་འཕར་གཏང་ནི་དོན་ལུ་  གནད་སྡུད་གཞི་རྟེན་གྱིས་ལག་ལེན་འཐབ་སྟེ་ཡོད་པའི་  གསལ་བཤད་འདི་བཙུགས།\\</ahelp\\>"

#: tablewizard02.xhp#par_idN105A9.help.text
