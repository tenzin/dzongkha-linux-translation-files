#: querywizard02.xhp#par_idN10543.help.text
msgid "\\<link href=\\\"text/shared/explorer/database/querywizard02.xhp\\\"\\>Query Wizard - Sorting Order\\</link\\>"
msgstr "\\<link href=\\\"text/shared/explorer/database/querywizard02.xhp\\\"\\>འདྲི་དཔྱད་ཝི་ཛརཌི་ - རིམ་པའི་དབྱེ་སེལ།\\</link\\>"

#: querywizard02.xhp#par_idN10553.help.text
msgid "Specifies the sorting order for the data records in your query."
msgstr "ཁྱོད་རའི་འདྲི་དཔྱད་ནང་ལུ་  གནད་སྡུད་དྲན་ཐོའི་དོན་ལུ་དབྱེ་བའི་རིམ་པ་འདི་  གསལ་བཀོད་འབདཝ་ཨིན།"

#: querywizard02.xhp#par_idN10556.help.text
msgid "Sort by"
msgstr "གིས་དབྱེ་སེལ།"

#: querywizard02.xhp#par_idN105B2.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Specifies the field by which the created query is sorted.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>གསར་བསྐྲུན་འབད་ཡོད་པའི་ འདྲི་དཔྱད་དབྱེ་སེལ་འབད་མི་  ས་སྒོ་འདི་གསལ་བཀོད་འབདཝ་ཨིན།\\</ahelp\\>"

#: querywizard02.xhp#par_idN1055C.help.text
msgid ""
"_: querywizard02.xhp#par_idN1055C.help.text\n"
"Ascending"
msgstr "ཡར་འཛེགས།"

#: querywizard02.xhp#par_idN105BF.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Click to sort in alphabetically or numerically ascending order.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>ཀ་ཁའི་གོ་རིམ་བཞིན་ཡང་ན་ཨང་གྲངས་ཀྱིས་ཡར་འཛེགས་རིམ་པའི་ནང་ དབྱེ་སེལ་འབད་ནི་དོན་ལུ་ཨེབ་གཏང་།\\</ahelp\\>"

#: querywizard02.xhp#par_idN10562.help.text
msgid ""
"_: querywizard02.xhp#par_idN10562.help.text\n"
"Descending"
msgstr "མར་འབབ།"

#: querywizard02.xhp#par_idN105CC.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Click to sort in alphabetically or numerically descending order.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>ཀ་ཁའི་གོ་རིམ་ཡང་ན་ ཨང་གྲངས་མར་འབབ་རིམ་པའི་ནང་  དབྱེ་སེལ་འཐབ་ནི་དོན་ལུ་ཨེབ་གཏང་།\\</ahelp\\>"

#: querywizard02.xhp#par_idN10568.help.text
msgid "And then by"
msgstr "དང་དེ་ལས་གིས།"

#: querywizard02.xhp#par_idN105D9.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Specifies additional fields by which the created query is sorted, if previous sort fields are equal.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>ཧེ་མ་དབྱེ་སེལ་འབད་ཡོད་པའི་ས་སྒོ་ཚུ་  མཉམ་པ་སྦེ་ཡོད་པ་ཅིན་  གསར་བསྐྲུན་འབད་དེ་ཡོད་པའི་ འདྲི་དཔྱད་འདི་དབྱེ་སེལ་མི་གི་   ཁ་སྐོང་ས་སྒོ་ཚུ་གསལ་བཀོད་འབདཝ་ཨིན། \\</ahelp\\>"

#: querywizard02.xhp#par_idN1056E.help.text
msgid "\\<link href=\\\"text/shared/explorer/database/querywizard03.xhp\\\" name=\\\"Query Wizard - Search conditions\\\"\\>Query Wizard - Search conditions\\</link\\>"
msgstr "\\<link href=\\\"text/shared/explorer/database/querywizard03.xhp\\\" name=\\\"Query Wizard - Search conditions\\\"\\>འདྲི་དཔྱད་ཝི་ཛརཌི་ - གནས་སྟངས་ཚུ་འཚོལ་ཞིབ་འབད།\\</link\\>"

#: querywizard03.xhp#tit.help.text
msgid "Query Wizard - Search Conditions"
msgstr "འདྲི་དཔྱད་ཝི་ཛརཌི་-གནས་སྟངས་ཚུ་འཚོལ་ཞིབ་འབད།"

#: querywizard03.xhp#par_idN10543.help.text
msgid "\\<link href=\\\"text/shared/explorer/database/querywizard03.xhp\\\"\\>Query Wizard - Search Conditions\\</link\\>"
msgstr "\\<link href=\\\"text/shared/explorer/database/querywizard03.xhp\\\"\\>འདྲི་དཔྱད་ཝི་ཛརཌི་ - གནས་སྟངས་འཚོལ་ཞིབ་ཚུ།\\</link\\>"

#: querywizard03.xhp#par_idN10553.help.text
msgid "Specifies the search conditions to filter the query. "
msgstr "འདྲི་དཔྱད་འདི་ཚགས་མ་བཙུགས་ནིའི་དོན་ལུ་  གནས་སྟངས་འཚོལ་ཞིབ་འདི་  གསལ་བཀོད་འབདཝ་ཨིན།"

#: querywizard03.xhp#par_idN10556.help.text
msgid ""
"_: querywizard03.xhp#par_idN10556.help.text\n"
"Match all of the following"
msgstr "འོག་ལུ་ཡོད་མི་ཚུ་ཆ་མཉམ་རང་མཐུན་སྒྲིག་འབད།"

#: querywizard03.xhp#par_idN105B2.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Select to filter the query by all the conditions using a logical AND.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\> གནས་སྟངས་ཆ་མཉམ་གྱིས་འདྲི་དཔྱད་ཀྱིས་   གཏན་ཚིག་ཅན་ཨེ་ཨེན་ཌི་ལག་ལེན་འཐབ་སྟེ་   ཚགས་མ་བཙུགས་ནིའི་དོན་ལུ་  སེལ་འཐུ་འབད།\\</ahelp\\>"

#: querywizard03.xhp#par_idN1055C.help.text
msgid ""
"_: querywizard03.xhp#par_idN1055C.help.text\n"
"Match any of the following"
msgstr "འོག་གི་གང་རུང་ཅིག་ལུ་མཐུན་སྒྲིག་འབད།"

#: querywizard03.xhp#par_idN105BF.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Select to filter the query by any of the conditions using a logical OR.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>གནས་སྟངས་གང་རུང་ཅིག་གིས་  གཏན་ཚིག་ཅན་གྱི་ཨོ་ཨར་ཨེལ་ལག་ལེན་འཐབ་སྟེ་   འདྲི་དཔྱད་འདི་ཚགས་མ་བཙུགས་ནིའི་དོན་ལུ་ སེལ་འཐུ་འབད། \\</ahelp\\>"

#: querywizard03.xhp#par_idN10562.help.text
msgid ""
"_: querywizard03.xhp#par_idN10562.help.text\n"
"Field"
msgstr "ས་སྒོ།"

#: querywizard03.xhp#par_idN105CC.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Select the field name for the filter condition.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>ཚགས་མའི་གནས་སྟངས་དོན་ལུ་ ས་སྒོའི་མིང་སེལ་འཐུ་འབད།\\</ahelp\\>"

#: querywizard03.xhp#par_idN10568.help.text
msgid ""
"_: querywizard03.xhp#par_idN10568.help.text\n"
"Condition"
msgstr "གནས་སྟངས།"

#: querywizard03.xhp#par_idN105D9.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Select the condition for the filter.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>ཚགས་མའི་དོན་ལུ་  གནས་སྟངས་འདི་སེལ་འཐུ་འབད།\\</ahelp\\>"

#: querywizard03.xhp#par_idN1056E.help.text
msgid ""
"_: querywizard03.xhp#par_idN1056E.help.text\n"
"Value"
msgstr "གནས་གོང་།"

#: querywizard03.xhp#par_idN105E6.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Enter the value for the filter condition.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>ཚགས་མའི་གནས་སྟངས་དོན་ལུ་  གནས་གོང་འདི་བཙུགས།\\</ahelp\\>"

#: querywizard03.xhp#par_idN10574.help.text
msgid "\\<link href=\\\"text/shared/explorer/database/querywizard04.xhp\\\" name=\\\"Query Wizard - Detail or Summary\\\"\\>Query Wizard - Detail or summary\\</link\\>"
msgstr "\\<link href=\\\"text/shared/explorer/database/querywizard04.xhp\\\" name=\\\"Query Wizard - Detail or Summary\\\"\\>འདྲི་དཔྱད་ཝི་ཛརཌི་ - རྒྱས་བཤད་ཡང་ན་ བཅུད་དོན།\\</link\\>"

#: querywizard04.xhp#tit.help.text
msgid "Query Wizard - Detail or Summary"
msgstr "འདྲི་དཔྱད་ཝི་ཛརཌི་- རྒྱས་བཤད་ཡང་ན་ བཅུད་དོན།"

#: querywizard04.xhp#par_idN10543.help.text
msgid "\\<link href=\\\"text/shared/explorer/database/querywizard04.xhp\\\"\\>Query Wizard - Detail or Summary\\</link\\>"
msgstr "\\<link href=\\\"text/shared/explorer/database/querywizard04.xhp\\\"\\>འདྲི་དཔྱད་ཝི་ཛརཌི་ - རྒྱས་བཤད་ཡང་ན་བཅུད་དོན།\\</link\\>"

#: querywizard04.xhp#par_idN10553.help.text
msgid "Specifies whether to display all records of the query, or only the results of aggregate functions. "
msgstr "འདྲི་དཔྱད་ཀྱི་དྲན་ཐོ་ཚུ་ཆ་མཉམ་རང་དང་  ཡང་ན་  སྤྱི་སྙོམས་ལས་འགན་གྱི་གྲུབ་འབྲས་ཚུ་རྐྱངམ་ཅིག་  བཀྲམ་སྟོན་འབད་ནི་ཨིན་ན་མེན་ན་འདི་གསལ་བཀོད་འབདཝ་ཨིན།"

#: querywizard04.xhp#par_idN10556.help.text
msgid "This page is only displayed when there are numerical fields in the query that allow the use of aggregate functions."
msgstr "སྤྱི་སྙོམས་ཀྱི་ལས་འགན་ཚུ་  ལག་ལེན་འཐབ་བཅུག་པའི་འདྲི་དཔྱད་ནང་ལུ་  ཨང་གྲངས་ཀྱི་ས་སྒོ་ཚུ་ཡོད་པ་ཅིན་རྐྱངམ་ཅིག་  ཤོག་ལེབ་དེ་གིས་བཀྲམ་སྟོན་འབདཝ་ཨིན།"

#: querywizard04.xhp#par_idN10559.help.text
msgid "Detailed query"
msgstr "འདྲི་དཔྱད་རྒྱས་བཤད།"

#: querywizard04.xhp#par_idN105BD.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Select to show all records of the query.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>འདྲི་དཔྱད་ཀྱི་དྲན་ཐོ་ཚུ་ཆ་མཉམ་སྟོན་ནི་དོན་ལུ་སེལ་འཐུ་འབད།Select to show all records of the query.\\</ahelp\\>"

#: querywizard04.xhp#par_idN105C2.help.text
msgid "Summary query"
msgstr "འདྲི་དཔྱད་ཀྱི་བཅུད་དོན།"

#: querywizard04.xhp#par_idN105C8.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Select to show only results of aggregate functions.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>སྤྱི་སྙོམས་ལས་འགན་གྱི་གྲུབ་འབྲས་རྐྱངམ་ཅིག་སྟོན་ནི་དོན་ལུ་སེལ་འཐུ་འབད།\\</ahelp\\>"

#: querywizard04.xhp#par_idN105D7.help.text
msgid "Select the aggregate function and the field name of the numeric field in the list box. You can enter as many aggregate functions as you want, one in each row of controls."
msgstr "ཐོ་ཡིག་སྒྲོམ་ནང་གི་  སྤྱི་སྙོམས་ལས་འགན་དང་  ཨང་གྲངས་ས་སྒོ་གི་ས་སྒོ་མིང་འདི་སེལ་འཐུ་འབད།  ཚད་འཛིན་གྱི་གྲལ་ཐིག་རེ་རེ་ནང་  ཁྱོད་ཀྱིས་  སྤྱི་སྙོམས་ལས་འགན་ཚུ་  ཁྱོད་ལུ་ག་དེ་དགོཔ་ཅིག་བཙུགས་བཏུབ།"

#: querywizard04.xhp#par_idN1055D.help.text
msgid "Aggregate function"
msgstr "སྤྱི་སྙོམ་ལས་འགན།"

#: querywizard04.xhp#par_idN105E4.help.text
msgid "\\<ahelp hid=\\\".\\\"\\>Select the aggregate function.\\</ahelp\\>"
msgstr "\\<ahelp hid=\\\".\\\"\\>སྤྱི་སྙོམས་ལས་འགན་འདི་སེལ་འཐུ་འབད།\\</ahelp\\>"

#: querywizard04.xhp#par_idN105DD.help.text
msgid ""
"_: querywizard04.xhp#par_idN105DD.help.text\n"
"Field name"
msgstr "ས་སྒོའི་མིང་།"

