#: src/ui.cc:2137
msgid "Reload the package cache"
msgstr "ཐུམ་སྒྲིལ་འདྲ་མཛོད་དེ་ཡང་བསྐྱར་མངོན་གསལ་འབད།"

#: src/ui.cc:2141
msgid "^Play Minesweeper"
msgstr "འགས་རྫས་འཕྱག་མི་གཏང་། (^P)"

#: src/ui.cc:2146
msgid "^Become root"
msgstr "རྩ་བ་ལུ་འགྱུར་འོང་། (^B)"

#: src/ui.cc:2147
msgid "Run 'su' to become root; this will restart the program, but your settings will be preserved"
msgstr " 'ཨེསི་ཡུ་' དེ་རྩ་བ་ལུ་འགྱུར་ནིའི་དོན་ལུ་གཡོག་བཀོལ། འདི་གིས་ལས་རིམ་དེ་སླར་འགོ་བཙུགསཔ་ཨིན་ དེ་འབདཝ་ད་ ཁྱོད་ཀྱི་སྒྲིག་སྟངས་དེ་ཉམས་སྲུང་འབད་བཞག་འོང་།"

#: src/ui.cc:2149
msgid "^Quit"
msgstr "སྤང་།(^Q)"

#: src/ui.cc:2150
msgid "Exit the program"
msgstr "ལས་རིམ་དེ་ཕྱིར་འཐོན་འབད།"

#: src/ui.cc:2156
#: src/ui.cc:2524
msgid "Undo"
msgstr "འབད་བཤོལ།"

#: src/ui.cc:2157
msgid "Undo the last package operation or group of operations"
msgstr "མཇུག་གི་ཐུམ་སྒྲིལ་བཀོལ་སྤྱོད་ ཡང་ན་ བཀོལ་སྤྱོད་སྡེ་ཚན་ཚུ་འབད་བཤོལ་བཞག"

#: src/ui.cc:2165
msgid "^Install"
msgstr "གཞི་བཙུགས།(^I)"

#: src/ui.cc:2166
msgid "Flag the currently selected package for installation or upgrade"
msgstr "གཞི་བཙུགས་དང་ཡར་བསྐྱེད་འབད་ནིའི་དོན་ལུ་ ད་ལྟོ་སེལ་འཐུ་འབད་ཡོའ་པའི་ཐུམ་སྒྲིལ་དེ་ལུ་ཟུར་རྟགས་བཀོད།"

#: src/ui.cc:2169
msgid "^Remove"
msgstr "རྩ་བསྐྲད། (^R)"

#: src/ui.cc:2170
msgid "Flag the currently selected package for removal"
msgstr "རྩ་བསྐྲད་གཏང་ནིའི་དོན་ལུ་ ད་ལྟོ་སེལ་འཐུ་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་དེ་ལུ་ཟུར་རྟགས་བཀོད།"

#: src/ui.cc:2173
msgid "^Purge"
msgstr "ཕྱིར་བསྐྲད། (^P)"

#: src/ui.cc:2174
msgid "Flag the currently selected package and its configuration files for removal"
msgstr "རྩ་བསྐྲད་གཏང་ནིའི་དོན་ལུ་ད་ལྟོ་སེལ་འཐུ་འབད་ཡོད་མི་ཐུམ་སྒྲིལ་དང་ དེའི་རིམ་སྒྲིག་ཡིག་སྣོད་དེ་ལུ་ ཟུར་རྟགས་བཀོད།"

#: src/ui.cc:2177
msgid "^Keep"
msgstr "བཞག(^K)"

#: src/ui.cc:2178
msgid "Cancel any action on the selected package"
msgstr "བྱ་བ་ག་འབད་རུང་ཅིག་སེལ་འཐུ་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་གུར་ཆ་མེད་གཏང་།"

#: src/ui.cc:2181
msgid "^Hold"
msgstr "འཆང། (^H)"

#: src/ui.cc:2182
msgid "Cancel any action on the selected package, and protect it from future upgrades"
msgstr "བྱ་བ་ག་འབད་རུང་ཅིག་སེལ་འཐུ་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་གུར་ཆ་མེད་གཏང་ཞིནམ་ལས་ འ་ནི་དེ་མ་འོངས་པ་ལུ་ཡར་བསྐྱེད་འབད་ནི་ལས་ཉེན་སྐྱོབ་འབད།"

#: src/ui.cc:2185
msgid "Mark ^Auto"
msgstr "རང་བཞིན་ལུ་རྟགས་བཀལ། (^A)"

#: src/ui.cc:2186
msgid "Mark the selected package as having been automatically installed; it will automatically be removed if no other packages depend on it"
msgstr "སེལ་འཐུ་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་ཚུ་རང་བཞིན་གྱིས་ཐོག་ལས་གཞི་བཙུགས་འབད་ཡོདཔ་བཟུམ་སྦེ་རྟགས་བཀལ། ཐུམ་སྒྲིལ་གཞན་མི་ཚུ་གིས་དེ་ལུ་མ་བརྟེན་པ་ཅིན་ འ་ནི་དེ་རང་བཞིན་གྱིས་རྩ་བསྐྲད་གཏང་འོང་།"

#: src/ui.cc:2189
msgid "Mark ^Manual"
msgstr "ལག་དེབ་ལུ་རྟགས་བཀལ།(^M)"

#: src/ui.cc:2190
msgid "Mark the selected package as having been manually installed; it will not be removed unless you manually remove it"
msgstr "སེལ་འཐུ་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་ཚུ་ལག་ཐོག་ལས་གཞི་བཙུགས་འབད་དེ་ཡོད་དོ་བཟུམ་སྦེ་རྟགས་བཀལ། ཁྱོད་ཀྱིས་ལག་ཐོག་ལས་རྩ་བསྐྲད་མ་གཏང་ཚུན་ཚོད་ དེ་གིས་རྩ་བསྐྲད་མི་འགྱོ།"

#: src/ui.cc:2193
msgid "^Forbid Version"
msgstr "ཐོན་རིམ་མ་ཆོགཔ་བཟོ།(^F)"

#: src/ui.cc:2194
msgid "Forbid the candidate version of the selected package from being installed; newer versions of the package will be installed as usual"
msgstr "སེལ་འཐུ་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་ཚུ་གིས་མི་ངོའི་ཐོན་རིམ་དེ་གཞི་བཙུགས་འབད་ནི་ལས་མ་ཆོགཔ་བཟོ། ཐུམ་སྒྲིལ་དེ་གི་ཐོན་རིམ་གསརཔ་ཚུ་ཨ་རྟག་བཟུམ་སྦེ་གཞི་བཙུགས་འབད་ནི་ཨིན།"

#: src/ui.cc:2198
msgid "I^nformation"
msgstr "བརྡ་དོན།(^n)"

#: src/ui.cc:2199
msgid "Display more information about the selected package"
msgstr "སེལ་འཐུ་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་དེ་གི་སྐོར་ལས་བརྡ་དོན་ཧེང་བཀལ་བཀྲམ་སྟོན་འབད།"

#: src/ui.cc:2202
msgid "^Changelog"
msgstr "བསྒྱུར་བཅོས་དྲན་དེབ། (^C)"

#: src/ui.cc:2203
msgid "Display the Debian changelog of the selected package"
msgstr "སེལ་འཐུ་འབད་ཡོད་པའི་ཐུམ་སྒྲིལ་དེ་གི་ཌེ་བི་ཡཱན་བསྒྱུར་བཅོས་དྲན་དེབ་བཀྲམ་སྟོན་འབད།"

#: src/ui.cc:2210
msgid "^Examine Solution"
msgstr "ཐབས་ཤེས་བརྟག་དཔྱད་འབད།"

#: src/ui.cc:2211
msgid "Examine the currently selected solution to the dependency problems."
msgstr "ད་ལྟོ་སེལ་འཐུ་འབད་ཡོད་པའི་ཐབས་ཤེས་དེ་རྟེན་འབྲེལ་གྱི་དཀའ་ངལ་དེ་ལུ་བརྟག་དཔྱད་འབད།"

#: src/ui.cc:2214
msgid "Apply ^Solution"
msgstr "ཐབས་ཤེས་འཇུག་སྤྱོད་འབད།"

#: src/ui.cc:2215
msgid "Perform the actions contained in the currently selected solution."
msgstr "ད་ལྟོ་སེལ་འཐུ་འབད་ཡོད་པའི་ཐབས་ཤེས་ཚུ་ནང་ལུ་ཡོད་པའི་བྱ་བ་ཚུ་ལག་ལེན་འཐབ།"

#: src/ui.cc:2218
msgid "^Next Solution"
msgstr "ཤུལ་མམ་གྱི་ཐབས་ལམ། (^N)"

#: src/ui.cc:2219
msgid "Select the next solution to the dependency problems."
msgstr "ཤུལ་མམ་གྱི་ཐབས་ཤེས་དེ་རྟེན་འབྲེལ་གྱི་དཀའ་ངལ་དེ་ལུ་སེལ་འཐུ་འབད།"

#: src/ui.cc:2222
msgid "^Previous Solution"
msgstr "ཧེ་མམ་གྱི་་ཐབས་ཤེས། (^P)"

#: src/ui.cc:2223
msgid "Select the previous solution to the dependency problems."
msgstr "ཧེ་མམ་གྱི་ཐབས་ཤེས་དེ་རྟེན་འབྲེལ་གྱི་དཀའ་ངལ་དེ་ལུ་སེལ་འཐུ་འབད།"

#: src/ui.cc:2226
msgid "^First Solution"
msgstr "ཐབས་དང་པ། (^F)"

#: src/ui.cc:2227
msgid "Select the first solution to the dependency problems."
msgstr "ཐབས་ཤེས་དང་པམ་དེ་རྟེན་འབྲེལ་གྱི་དཀའ་ངལ་དེ་ལུ་སེལ་འཐུ་འབད།"

#: src/ui.cc:2230
msgid "^Last Solution"
msgstr "མཇུག་གི་ཐབས་ཤེས།"

#: src/ui.cc:2231
msgid "Select the last solution to the dependency problems that has been generated so far."
msgstr "མཇུག་གི་སེལ་འཐུ་དེ་ད་ལྟོ་ཚུན་ཚོད་བཟོ་བཏོན་འབད་ཡོད་པའི་རྟེན་འབྲེལ་གྱི་དཀའ་ངལ་དེ་ལུ་སེལ་འཐུ་འབད།"

#: src/ui.cc:2237
msgid "Toggle ^Rejected"
msgstr "སོར་སྟོན་སྣང་མེད་སྦེ་བཞག་ཡོདཔ།(^R)"

#: src/ui.cc:2238
msgid "Toggle whether the currently selected action is rejected."
msgstr "ད་ལྟོ་སེལ་འཐུ་འབཐ་ཡོད་པའི་བྱ་བ་དེ་སྣང་མེད་སྦེ་བཞག་སྟེ་ཡོད་དང་མེད་སོར་སྟོན་འབད།"

#: src/ui.cc:2242
msgid "Toggle ^Approved"
msgstr "སོར་སྟོན་གནང་བ་གྲོལ་ཡོདཔ། (^A)"

#: src/ui.cc:2243
msgid "Toggle whether the currently selected action is approved."
msgstr "ད་ལྟོ་སེལ་འཐུ་འབད་ཡོད་པའི་བྱ་བ་དེ་གི་གནང་བ་གྲོལ་ཡོད་དང་མེད་སོར་སྟོན་འབད།"

#: src/ui.cc:2247
msgid "^View Target"
msgstr "དམིགས་གཏད་སྟོན། (^V)"

#: src/ui.cc:2248
msgid "View the package which will be affected by the selected action"
msgstr "ཐུམ་སྒྲིལ་ག་དེ་ལུ་སེལ་འཐུ་འབད་ཡོད་པའི་བྱ་བ་གིས་གནོད་སྐྱོན་འབྱུང་ནི་ཨིན་པས་ག་སྟོན།"

#: src/ui.cc:2256
msgid "^Find"
msgstr "འཚོལ།(^F)"

#: src/ui.cc:2257
msgid "Search forwards"
msgstr "གདོང་བསྐྱོད་འབད་དེ་འཚོལ་ཞིབ་འབད།"

#: src/ui.cc:2260
msgid "^Find Backwards"
msgstr "རྒྱབ་བསྐྱོད་དེ་འཚོལ། (^F)"

#: src/ui.cc:2261
msgid "Search backwards"
msgstr "རྒྱབ་བསྐྱོད་འབད་དེ་ཚའོལ་ཞིབ་འབད།"

#: src/ui.cc:2264
msgid "Find ^Again"
msgstr "ལོག་སྟེ་འཚོལ།(^A)"

#: src/ui.cc:2265
msgid "Repeat the last search"
msgstr "མཇུག་གི་འཚོལ་ཞིབ་འབད་ཡོད་མི་དེ་ཡང་བསྐྱར་འབད།"

#: src/ui.cc:2268
msgid "Find Again ^Backwards"
msgstr "རྒྱབ་བསྐྱོད་དེ་ལོག་འཚོལ། (^B)"

#: src/ui.cc:2269
msgid "Repeat the last search in the opposite direction"
msgstr "མཇུག་གི་འཚོལ་ཞིབ་འབད་ཡོད་མི་དེ་རྒྱབ་འགལ་ཕྱོགས་སུ་་ཡང་བསྐྱར་འབད།"

#: src/ui.cc:2273
msgid "^Limit Display"
msgstr "བཀྲམ་སྟོན་འབད་ནི་ལུ་་ཚད་བཀལ།(^L)"

#: src/ui.cc:2274
msgid "Apply a filter to the package list"
msgstr "ཐུམ་སྒྲིལ་ཐོ་ཡིག་དེ་ལུ་ཚགས་མ་ཅིག་འཇུག་སྤྱོད་འབད།"

#: src/ui.cc:2277
msgid "^Un-Limit Display"
msgstr "བཀྲམ་སྟོན་འབད་ནི་ལུ་ཚད་བཤོལ་འབད།"

#: src/ui.cc:2278
msgid "Remove the filter from the package list"
msgstr "ཐུམ་སྒྲིལ་གྱི་ཐོ་ཡིག་དེ་ནང་ལས་ཚགས་མ་དེ་སྟོན་གཏང་།"

