#: references.xhp#par_id3156257.45.help.text
msgid "Leave the dialog open and proceed to the next section."
msgstr "ཌའི་ལོག་འདི་ཁ་ཕྱེ་བཞག་ཞིནམ་ལས་ དབྱེ་ཚན་ཤུལ་མམ་ལུ་འཕྲོ་མཐུད།"

#: references.xhp#hd_id3153370.53.help.text
msgid "To create a cross-reference to a target:"
msgstr "དམིགས་གཏད་ཅིག་ལུ་ཕན་ཚུན་གཞི་བསྟུན་ཅིག་གསར་བསྐྲུན་འབད་ནིའི་དོན་ལས་:"

#: references.xhp#par_id3153392.46.help.text
msgid "In the \\<emph\\>Type\\</emph\\> list, select \"Insert Reference\"."
msgstr "\\<emph\\>དབྱེ་བ་\\</emph\\> ཐོ་ཡིག་ནང་ལུ་  \"Insert Reference\" སེལ་འཐུ་འབད།"

#: references.xhp#par_id3154256.47.help.text
msgid "In the \\<emph\\>Selection\\</emph\\> list, select the target that you want to cross-reference."
msgstr "\\<emph\\>སེལ་འཐུ་འབད་ནིའི་\\</emph\\> ཐོ་ཡིག་འདི་ནང་ ཁྱོད་ཀྱིས་ཕན་ཚུན་གཞི་བསྟུན་འབད་ནི་ཨིན་མི་ དམིགས་གཏད་འདི་སེལ་འཐུ་འབད།"

#: references.xhp#par_id3154856.48.help.text
msgid "In the \\<emph\\>Format\\</emph\\> list, select the format for the cross-reference. The \\<link href=\\\"text/swriter/01/04090002.xhp\\\" name=\\\"format\\\"\\>format\\</link\\> specifies the type of information that is displayed as the cross-reference. For example, \"Reference\" inserts the target text, and \"Page\" inserts the page number where the target is located."
msgstr "\\<emph\\>རྩ་སྒྲིག་\\</emph\\> ཐོ་ཡིག་ནང་ལུ་ ཕན་ཚུན་གཞི་བསྟུན་གྱི་དོན་ལུ་ རྩ་སྒྲིག་འདི་སེལ་འཐུ་འབད།  \\<link href=\\\"text/swriter/01/04090002.xhp\\\" name=\\\"format\\\"\\>རྩ་སྒྲིག་\\</link\\> འདི་གིས་ ཕན་ཚུན་གཞི་བསྟུན་སྦེ་བཀྲམ་སྟོན་འབད་ཡོད་མི་ བརྡ་དོན་གྱི་དབྱེ་བ་འདི་ གསལ་བཀོད་འབདཝ་ཨིན། དཔེར་ན་  \"Reference\" གིས་དམིགས་གཏད་ཚིག་ཡིག་འདི་བཙུགསཔ་ཨིན་ དེ་ལས་ \"Page\" གིས་ དམིགས་གཏད་འདི་ག་ཡོད་འཚོལ་ཡོད་སའི་ཤོག་ལེབ་ཨང་འདི་བཙུགསཔ་ཨིད།"

#: references.xhp#par_id3155895.49.help.text
msgid ""
"_: references.xhp#par_id3155895.49.help.text\n"
"Click \\<emph\\>Insert\\</emph\\>."
msgstr "\\<emph\\>བཙུགས་\\</emph\\>ཨེབ་གཏང་།"

#: references.xhp#par_id3148685.54.help.text
msgid ""
"_: references.xhp#par_id3148685.54.help.text\n"
"Click \\<emph\\>Close\\</emph\\> when finished."
msgstr "མཇུག་བསྡུ་བའི་སྐབས་\\<emph\\>ཁ་བསྡམ་\\</emph\\> ཨེབ་གཏང་།"

#: references.xhp#hd_id3149980.29.help.text
msgid "Cross-Referencing an Object"
msgstr "དངོས་པོ་ཅིག་ཕན་ཚུན་གཞི་བསྟུན་འབད་དོ།"

#: references.xhp#par_id3149992.30.help.text
msgid "You can cross-reference most objects in your document, such as graphics, drawing objects, OLE objects, and tables, so long as they have a caption. To add a caption to an object, select the object, and then choose \\<link href=\\\"text/swriter/guide/captions.xhp\\\" name=\\\"Insert - Caption\\\"\\>\\<emph\\>Insert - Caption\\</emph\\>\\</link\\>."
msgstr "ཁྱོད་ཀྱིས་དཔེར་ན་ ཚད་རིས་ འབྲི་ནིའི་དངོས་པོ་ ཨོ་ཨེལ་ཨི་དངོས་པོ་ དང་ཐིག་ཁྲམ་བཟུམ་ཚུ་ལུ་ འགོ་བརྗོད་ཡོད་རིང་ ཁྱོད་རའི་ཡིག་ཆ་ནང་གི་དངོས་པོ་མང་ཤོས་ལུ་ ཕན་ཚུན་གཞི་བསྟུན་འབད་བཏུབ། འོག་བརྗོད་ཅིག་དངོས་པོ་ལུ་ཁ་སྐོང་རྐྱབས་ནིའི་དོན་ལས་ དངོས་པོ་འདི་སེལ་འཐུ་འབད་ དེ་ལས་ \\<link href=\\\"text/swriter/guide/captions.xhp\\\" name=\\\"Insert - Caption\\\"\\>\\<emph\\>བཙུགས་ - འགོ་བརྗོད་\\</emph\\>\\</link\\>གདམ་ཁ་རྐྱབས།"

#: references.xhp#par_id3147123.14.help.text
msgid "Click in the document where you want to insert the cross-reference."
msgstr "ཁྱོད་ཀྱིས་ཕན་ཚུན་གཞི་བསྟུན་འདི་བཙུགས་དགོ་མནོ་སའི་ ཡིག་ཆ་འདི་ནང་ཨེབ་གཏང་།"

#: references.xhp#par_id3150212.11.help.text
msgid ""
"_: references.xhp#par_id3150212.11.help.text\n"
"Choose \\<emph\\>Insert - Cross-reference\\</emph\\>."
msgstr "\\<emph\\>བཙུགས་ - ཕན་ཚུན་གཞི་བསྟུན་\\</emph\\>གདམ་ཁ་རྐྱབས།"

#: references.xhp#par_id3150236.32.help.text
msgid "In the \\<emph\\>Type\\</emph\\> list, select the caption category of the object."
msgstr "\\<emph\\>དབྱེ་བའི་\\</emph\\> ཐོ་ཡིག་ནང་ལུ་ དངོས་པོ་འདི་གི་འགོ་བརྗོད་དབྱེ་རིམ་འདི་སེལ་འཐུ་འབད།"

#: references.xhp#par_id3150942.33.help.text
msgid "In the \\<emph\\>Selection\\</emph\\> list, select the caption number of the object that you want to cross-reference."
msgstr "\\<emph\\>སེལ་འཐུ་འབད་ནིའི་\\</emph\\> ཐོ་ཡིག་ནང་ལུ་ ཁྱོད་ཀྱིས་ཕན་ཚུན་གཞི་བསྟུན་འབད་ནི་ཨིན་མི་ དངོས་པོ་འདི་གི་འགོ་བརྗོད་ཨང་འདི་སེལ་འཐུ་འབད།"

#: references.xhp#par_id3150968.34.help.text
msgid "In the \\<emph\\>Format\\</emph\\> list, select the format of the cross-reference. The \\<link href=\\\"text/swriter/01/04090002.xhp\\\" name=\\\"format\\\"\\>format\\</link\\> specifies the type of information that is displayed as the cross-reference. For example, \"Reference\" inserts the caption category and caption text of the object."
msgstr "\\<emph\\>རྩ་སྒྲིག་\\</emph\\> ཐོ་ཡིག་ནང་ལུ་ ཕན་ཚུན་གཞི་བསྟུན་འདི་གི་རྩ་སྒྲིག་འདི་སེལ་འཐུ་འབད།  \\<link href=\\\"text/swriter/01/04090002.xhp\\\" name=\\\"format\\\"\\>རྩ་སྒྲིག་\\</link\\> འདི་གིས་ ཕན་ཚུན་གཞི་བསྟུན་སྦེ་བཀྲམ་སྟོན་འབད་ཡོད་མི་ བརྡ་དོན་གྱི་དབྱེ་བ་འདི་གསལ་བཀོད་འབདཝ་ཨིན། དཔེར་ན་  \"Reference\" གིས་དངོས་པོ་གི་ འགོ་བརྗོད་དབྱེ་རིམ་དང་འགོ་བརྗོད་ཚིག་ཡིག་བཙུགསཔ་ཨིན།"

#: references.xhp#par_id3150535.35.help.text
msgid ""
"_: references.xhp#par_id3150535.35.help.text\n"
"Click \\<emph\\>Insert\\</emph\\>."
msgstr "\\<emph\\>བཙུགས་\\</emph\\>ཨེབ་གཏང་།"

#: references.xhp#par_id3151092.55.help.text
msgid ""
"_: references.xhp#par_id3151092.55.help.text\n"
"Click \\<emph\\>Close\\</emph\\> when finished."
msgstr "མཇུག་བསྡུ་བའི་སྐབས་ \\<emph\\>ཁ་བསྡམ་\\</emph\\>ཨེབ་གཏང་།"

#: references.xhp#hd_id3151115.56.help.text
msgid "Updating Cross-References"
msgstr "ཕན་ཚུན་གཞི་བསྟུན་དུས་མཐུན་བཟོ་དོ།"

#: references.xhp#par_id3153594.51.help.text
msgid "To manually update the cross-references in a document, press F9."
msgstr "ལག་ཐོག་ལས་ཕན་ཚུན་གཞི་བསྟུན་འདི་ཡིག་ཆ་ཅིག་ནང་ལུ་དུས་མཐུན་བཟོ་ནིའི་དོན་ལས་  F9 ཨེབ།"

#: references_modify.xhp#tit.help.text
msgid "Modifying Cross-References"
msgstr "ཕན་ཚུན་གཞི་བསྟུན་ལེགས་བཅོས་འབད་དོ།"

#: references_modify.xhp#bm_id3149291.help.text
msgid "\\<bookmark_value\\>references; modifying cross-references\\</bookmark_value\\>\\<bookmark_value\\>cross-references; modifying\\</bookmark_value\\>\\<bookmark_value\\>editing;cross-references\\</bookmark_value\\>\\<bookmark_value\\>searching;cross-references\\</bookmark_value\\>"
msgstr "\\<bookmark_value\\>གཞི་བསྟུན་ཚུ་ ཕན་ཚུན་གཞི་བསྟུན་\\</bookmark_value\\>ཚུ་ལེགས་བཅོས་འབད་དོ་\\<bookmark_value\\>ཕན་ཚུན་གཞི་བསྟུན་ཚུ་ ལེགས་བཅོས་འབད་དོ་\\</bookmark_value\\>\\<bookmark_value\\>ཞུན་དག་འབད་དོ་ ཕན་ཚུན་གཞི་བསྟུན་\\</bookmark_value\\>ཚུ་\\<bookmark_value\\>འཚོལ་ཞིབ་འབད་དོ་ ཕན་ཚུན་གཞི་བསྟུན་\\</bookmark_value\\>ཚུ།"

#: references_modify.xhp#hd_id3149291.6.help.text
msgid "\\<variable id=\\\"references_modify\\\"\\>\\<link href=\\\"text/swriter/guide/references_modify.xhp\\\" name=\\\"Modifying Cross-References\\\"\\>Modifying Cross-References\\</link\\>\\</variable\\>"
msgstr "\\<variable id=\\\"references_modify\\\"\\>\\<link href=\\\"text/swriter/guide/references_modify.xhp\\\" name=\\\"Modifying Cross-References\\\"\\>ཕན་ཚུན་གཞི་བསྟུན་ལེགས་བཅོས་འབད་དོ།\\</link\\>\\</variable\\>"

#: references_modify.xhp#par_id3153132.2.help.text
msgid "Click in front of the cross-reference that you want to modify."
msgstr "ཁྱོད་ཀྱིས་ལེགས་བཅོས་འབད་ནི་ཨིན་མི་ ཕན་ཚུན་གཞི་བསྟུན་འདི་གི་གདོང་ཁར་ཨེབ་གཏང་།"

#: references_modify.xhp#par_id3149632.3.help.text
msgid "If you cannot see the field shading of the cross-reference, choose \\<emph\\>View - Field Shadings\\</emph\\> or press \\<emph\\>Ctrl+F8\\</emph\\>."
msgstr "ཁྱོད་ཀྱིས་ཕན་ཚུན་གཞི་བསྟུན་འདི་གི ས་སྒོ་ནག་གྲིབ་བཟོ་ནི་འདི་མ་མཐོངམ་ཨིན་པ་ཅིན་  \\<emph\\>མཐོང་སྣང་ - ས་སྒོ་ནག་གྲིབ་བཟོ་ནི་\\</emph\\> གདམ་ཁ་རྐྱབས་ ཡང་ན་  \\<emph\\>ཚད་འཛིན་+F8\\</emph\\>ཨེབ།"

#: references_modify.xhp#par_id3149611.4.help.text
msgid "Choose \\<emph\\>Edit - Fields\\</emph\\>."
msgstr "\\<emph\\>ཞུན་དག་ - ས་སྒོ་ཚུ་\\</emph\\>གདམ་ཁ་རྐྱབས།"

#: references_modify.xhp#par_id3145101.7.help.text
msgid "Set the options that you want, and then click \\<emph\\>OK\\</emph\\>."
msgstr "ཁྱོད་ལུ་དགོ་མི་གདམ་ཁ་འདི་གཞི་སྒྲིག་འབད་ དེ་ལས་ \\<emph\\>བཏུབ་\\</emph\\>ཨེབ་གཏང་།"

#: references_modify.xhp#par_id3154255.5.help.text
msgid "Use the arrow buttons in the \\<emph\\>Edit Fields \\</emph\\>dialog to browse through the cross-references in the current document."
msgstr "\\<emph\\>ཞུན་དག་ས་སྒོའི་ \\</emph\\>ཌའི་ལོག་ནང་གི་མདའ་རྟགས་ཨེབ་རྟ་འདི་ ད་ལྟོའི་ཡིག་ཆ་ནང་གི་ ཕན་ཚུན་གཞི་བསྟུན་འདི་བརྒྱུད་དེ་བརྡ་འཚོལ་འབད་ནིའི་དོན་ལས་ལག་ལེན་འཐབ།"

#: registertrue.xhp#tit.help.text
msgid "Printing Register-true"
msgstr "ཐོ་འགོད་-བདེན་པ་ དཔར་བསྐྲུན་འབད་དོ།"

#: registertrue.xhp#bm_id4825891.help.text
msgid "\\<bookmark_value\\>rows; register-true text\\</bookmark_value\\>\\<bookmark_value\\>lines of text; register-true\\</bookmark_value\\>\\<bookmark_value\\>pages;register-true\\</bookmark_value\\>\\<bookmark_value\\>paragraphs;register-true\\</bookmark_value\\>\\<bookmark_value\\>register-true;pages and paragraphs\\</bookmark_value\\>\\<bookmark_value\\>spacing;register-true text\\</bookmark_value\\>\\<bookmark_value\\>formatting;register-true text\\</bookmark_value\\>"
msgstr "\\<bookmark_value\\>གྱལ་ཚུ་; ཐོ་འགོད་-ངེས་བདེན་ཚིག་ཡིག\\</bookmark_value\\>\\<bookmark_value\\>ཚིག་ཡིག་གི་གྱལ་ཚུ་;ཐོ་འགོད་ -ངེས་བདེན་\\</bookmark_value\\>\\<bookmark_value\\>ཤོག་ལེབ་ཚུ་;ཐོ་འགོད་-ངེས་བདེན་\\</bookmark_value\\>\\<bookmark_value\\>མཚམས་དོན་ཚུ་;ཐོ་འོགད་-ངེས་བདེན་\\</bookmark_value\\>\\<bookmark_value\\>ཐོ་འགོད་-ངོས་བདེན་;ཤོག་ལེབ་ཚུ་དང་མཚམས་དོན་ཚུ་\\</bookmark_value\\>\\<bookmark_value\\>བར་སྟོང་བཞག་དོ་;ཐོ་འགོད་-ངེས་བདེན་ཚིག་ཡིག་\\</bookmark_value\\>\\<bookmark_value\\>རྩ་སྒྲིག་འབད་དོ་;ཐོ་འགོད་-ངེས་བདེན་ཚིག་ཡིག\\</bookmark_value\\>"

#: registertrue.xhp#par_idN10652.help.text
msgid "\\<variable id=\\\"registertrue\\\"\\>\\<link href=\\\"text/swriter/guide/registertrue.xhp\\\"\\>Printing Register-true\\</link\\>\\</variable\\>"
msgstr "\\<variable id=\\\"registertrue\\\"\\>\\<link href=\\\"text/swriter/guide/registertrue.xhp\\\"\\>ཐོ་འགོད་-བདེན་པ་ དཔར་བསྐྲུན་འབད་དོ།\\</link\\>\\</variable\\>"

#: registertrue.xhp#par_idN1065E.help.text
msgid "Setting a document to register-true printing"
