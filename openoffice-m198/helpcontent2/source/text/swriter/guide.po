msgid "\\<image id=\\\"img_id7366557\\\" src=\\\"res/helpimg/border_wr_2.png\\\" width=\\\"1.2709inch\\\" height=\\\"1.6146inch\\\"\\>\\<alt id=\\\"alt_id7366557\\\"\\>one selected cell border\\</alt\\>\\</image\\>"
