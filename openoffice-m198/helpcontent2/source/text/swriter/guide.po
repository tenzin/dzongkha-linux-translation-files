msgid "\\<bookmark_value\\>fill format mode\\</bookmark_value\\>\\<bookmark_value\\>copying; styles\\</bookmark_value\\>\\<bookmark_value\\>brush for copying styles\\</bookmark_value\\>\\<bookmark_value\\>styles; transferring\\</bookmark_value\\>\\<bookmark_value\\>formats; copying and pasting\\</bookmark_value\\>\\<bookmark_value\\>text formats; copying and pasting\\</bookmark_value\\>"
msgstr "\\<bookmark_value\\>རྩ་སྒྲིག་ཐབས་ལམ་བཀང་\\</bookmark_value\\>\\<bookmark_value\\>བཟོ་རྣམ་ཚུ་; འདྲ་བཤུས་འབད་དོ\\</bookmark_value\\>\\<bookmark_value\\>བཟོ་རྣམ་ཚུ་འདྲ་བཤུས་འབད་ནི་དོན་ལུ་པྱིར་\\</bookmark_value\\>\\<bookmark_value\\>བཟོ་རྣམ་ཚུ་;གནས་སོར་གཏང་དོ་\\</bookmark_value\\>\\<bookmark_value\\>རྩ་སྒྲིག་ཚུ་; འདྲ་བཤུས་དང་སྦྱར་དོ་\\</bookmark_value\\>\\<bookmark_value\\>ཚིག་ཡིག་རྩ་སྒྲིག་ཚུ་; འདྲ་བཤུས་དང་སྦྱར་དོ།\\</bookmark_value\\>"

#: stylist_fillformat.xhp#hd_id3145084.31.help.text
msgid "\\<variable id=\\\"stylist_fillformat\\\"\\>\\<link href=\\\"text/swriter/guide/stylist_fillformat.xhp\\\" name=\\\"Applying Styles in Fill Format Mode\\\"\\>Applying Styles in Fill Format Mode\\</link\\>\\</variable\\>"
msgstr "\\<variable id=\\\"stylist_fillformat\\\"\\>\\<link href=\\\"text/swriter/guide/stylist_fillformat.xhp\\\" name=\\\"Applying Styles in Fill Format Mode\\\"\\>བཀང་ནིའི་རྩ་སྒྲིག་གི་ཐབས་ལམ་ནང་ལུ་བཟོ་རྣམ་ཚུ་འཇུག་སྤྱོད་འབད་དོ།\\</link\\>\\</variable\\>"

#: stylist_fillformat.xhp#par_id3155855.42.help.text
msgid "You can quickly apply styles, such as paragraph and character styles, in your document by using the Fill Format Mode in the Styles and Formatting window."
msgstr "ཁྱོད་ཀྱིས་གང་མགྱོགས་སྦེ་ བཟོ་རྣམ་ཚུ་ དཔེར་ན་ དོན་མཚམས་དང་ཡིག་འབྲུ་བཟོ་རྣམ་ཚུ་བཟུམ་ ཁྱོད་རའི་ཡིག་ཆ་ནང་ལུ་ བཟོ་རྣམ་ཚུ་དང་རྩ་སྒྲིག་འབད་ནིའི་སྒོ་སྒྲིག་ནང་གི་ བཀང་ནིའི་རྩ་སྒྲིག་ཐབས་ལམ་ལག་ལེན་འཐབ་ཐོག་ འཇུག་སྤྱོད་འབད་ཚུགས།"

#: stylist_fillformat.xhp#par_id3156114.32.help.text
msgid ""
"_: stylist_fillformat.xhp#par_id3156114.32.help.text\n"
"Choose \\<emph\\>Format - Styles and Formatting\\</emph\\>."
msgstr "\\<emph\\>རྩ་སྒྲིག་ - བཟོ་རྣམ་ཚུ་དང་རྩ་སྒྲིག་འབད་ནི་\\</emph\\>གདམ་ཁ་རྐྱབས།"

#: stylist_fillformat.xhp#par_id3153128.33.help.text
msgid "Click the icon of the style category that you want to apply."
msgstr "ཁྱོད་ཀྱིས་བཙུགས་དགོ་མནོ་མི་བཟོ་རྣམ་དབྱེ་རིམ་འདི་གི་ངོས་དཔར་འདི་ཨེབ་གཏང་།"

#: stylist_fillformat.xhp#par_id3145090.17.help.text
msgid "Click the style, and then click the \\<emph\\>Fill Format Mode\\</emph\\> icon \\<image id=\\\"img_id3149644\\\" src=\\\"res/commandimagelist/sc_fillstyle.png\\\" width=\\\"5.64mm\\\" height=\\\"5.64mm\\\"\\>\\<alt id=\\\"alt_id3149644\\\"\\>Icon\\</alt\\>\\</image\\> in the \\<emph\\>Styles and Formatting\\</emph\\> window."
msgstr "བཟོ་རྣམ་འདི་ཨེབ་གཏང་འབད་ནི་དང་དེ་ལས་ \\<emph\\>བཀང་རྩ་སྒྲིག་ཐབས་ལམ་\\</emph\\> ངོས་དཔར་ \\<image id=\\\"img_id3149644\\\" src=\\\"res/commandimagelist/sc_fillstyle.png\\\" width=\\\"5.64mm\\\" height=\\\"5.64mm\\\"\\>\\<alt id=\\\"alt_id3149644\\\"\\>ངོས་དཔར་\\</alt\\>\\</image\\> བཟོ་རྣམ་ཚུ་ནང་དང་ \\<emph\\>ཝིནཌོ་རྩ་སྒྲིག་\\</emph\\> འདི་ཨེབ་གཏང་འབད་དོ།"

#: stylist_fillformat.xhp#par_id3153371.29.help.text
msgid "Move the mouse pointer to where you want to apply the style in the document, and click. To apply the style to more than one item, drag to select the items, and then release."
msgstr "མའུསི་དཔག་བྱེད་འདི་ ཁྱོད་ཀྱིས་ ཡིག་ཆ་ནང་གི་བཟོ་རྣམ་འདི་ག་སྟེ་འཇུག་སྤྱོད་འབད་ནི་ཨིན་ས་ལུ་བཤུད་ཞིནམ་ལས་ ཨེབ་གཏང་། བཟོ་རྣམ་འདི་ རྣམ་གྲངས་གཅིག་ལས་ལྷག་སྟེ་འཇུག་སྤྱོད་འབད་ནིའི་དོན་ལས་ རྣམ་གྲས་ཚུ་སེལ་འཐུ་འབད་ནིའི་དོན་ལས་འདྲུད་ དེ་ལས་གསར་བཏོན་འབད།"

#: stylist_fillformat.xhp#par_id3154263.34.help.text
msgid "Press \\<item type=\\\"keycode\\\"\\>Esc\\</item\\> when finished."
msgstr "\\<item type=\\\"keycode\\\"\\>ཐར་\\</item\\> འདི་མཇུག་བསྡུ་ཡོད་པའི་སྐབས་ཨེབ།"

