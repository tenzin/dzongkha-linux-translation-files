msgstr "\\<variable id=\\\"keyboard\\\"\\>\\<link href=\\\"text/swriter/guide/keyboard.xhp\\\" name=\\\"Using Shortcut Keys ($[officename] Writer Accessibility)\\\"\\>མགྱོགས་ཐབས་ལྡེ་མིག་ལག་ལེན་འཐབ་དོ་ ($[officename] རྩོམ་འབྲི་པ་འཛུལ་སྤྱོད་)\\</link\\>\\</variable\\>"

#: keyboard.xhp#par_id3149685.3.help.text
msgid "Press the keys Alt+<underlined character> to open a menu. In an open menu, press the underlined character to run a command. For example, press Alt+I to open the \\<emph\\>Insert \\</emph\\>menu, and then T to insert a table."
msgstr "ལྡེ་མིག་ གདམ་ལྡེ་+<underlined character> འདི་དཀར་ཆག་ཅིག་ཁ་ཕྱེ་ནིའི་དོན་ལས་ཨེབ། ཁ་ཕྱེ་ཡོད་པའི་དཀར་ཆག་ཅིག་ནང་ལུ་ བརྡ་བཀོད་ཅིག་གཡོག་བཀལ་ནིའི་དོན་ལས་ འོག་ཐིག་རྐྱབས་ཡོད་པའི་ཡིག་འབྲུ་འདི་ཨེབ། དཔེར་ན་ གདམ་ལྡེ་+I འདི་  \\<emph\\>བཙུགས་ \\</emph\\>དཀར་ཆག་ཁ་ཕྱེ་ནིའི་དོན་ལས་ཨེབ་ དེ་ལས  T འདི་ཐིག་ཁྲམ་ཅིག་བཙུགས་ནིའི་དོན་ལས་ཨེབ།"

#: keyboard.xhp#par_id3155186.4.help.text
msgid "To open a context menu, press Shift+F10. To close a context menu, press Escape."
msgstr "སྐབས་དོན་དཀར་ཆག་ཅིག་ཁ་ཕྱེ་ནིའི་དོན་ལས་ སོར་ལྡེ་+F10 ཨེབ། སྐབས་དོན་དཀར་ཆག་འདི་ཁ་བསྡམ་ནིའི་དོན་ལས་ ཐར་ཨེབ།"

#: keyboard.xhp#hd_id3155918.5.help.text
msgid ""
"_: keyboard.xhp#hd_id3155918.5.help.text\n"
"Inserting Sections"
msgstr "དབྱེ་ཚན་ཚུ་བཙུགས་དོ།"

#: keyboard.xhp#par_idN106AA.help.text
msgid "Choose \\<emph\\>View - Toolbars - Insert\\</emph\\> to open the \\<emph\\>Insert\\</emph\\> toolbar."
msgstr "\\<emph\\>མཐོང་སྣང་ - ལག་ཆས་ཕྲ་རིང་ཚུ་ - བཙུགས་\\</emph\\> འདི་  \\<emph\\>བཙུགས་\\</emph\\>ལག་ཆས་ཕྲ་རིང་ཁ་ཕྱེ་ནིའི་དོན་ལས་གདམ་ཁ་རྐྱབས།"

#: keyboard.xhp#par_id3155870.6.help.text
msgid "Press F6 until the focus is on the\\<emph\\> Insert\\</emph\\> toolbar."
msgstr "ཆེད་དམིགས་འདི་  \\<emph\\> བཙུགས་\\</emph\\> ལག་ཆས་ཕྲ་རིང་གུ་ལུ་མ་ལྷོད་ཚུན་  F6 ཨེབ།"

#: keyboard.xhp#par_id3149630.8.help.text
msgid "Press the right arrow key until the \\<emph\\>Section\\</emph\\> icon is selected."
msgstr "\\<emph\\>དབྱེ་ཚན་\\</emph\\> ངོས་དཔར་འདི་སེལ་འཐུ་མ་འབད་ཚུན་ གཡས་མདའ་རྟགས་ལྡེ་མིག་འདི་ཨེབ།"

#: keyboard.xhp#par_id3145100.20.help.text
msgid "Press the down arrow key, and then press the right arrow key to set the width of the section that you want to insert."
msgstr "མར་གྱི་མདའ་རྟགས་ལྡེ་མིག་འདི་ཨེབ་ དེ་ལས་ ཁྱོད་ཀྱི་བཙུགས་ནི་ཨིན་མི་ དབྱེ་ཚན་གྱི་རྒྱ་ཚད་འདི་གཞི་སྒྲིག་འབད་ནིའི་དོན་ལས་ གཡས་མདའ་རྟགས་ལྡེ་མིག་འདི་ཨེབ།"

#: keyboard.xhp#par_id3156237.11.help.text
msgid ""
"_: keyboard.xhp#par_id3156237.11.help.text\n"
"Press Enter."
msgstr "ལོག་ལྡེ་ཨེབ།"

#: keyboard.xhp#par_id3156253.19.help.text
msgid ""
"_: keyboard.xhp#par_id3156253.19.help.text\n"
"Press F6 to place the cursor inside the document."
msgstr "འོད་རྟགས་འདི་ ཡིག་ཆ་གི་ནང་ན་བཞག་ནིའི་དོན་ལས་  F6 ཨེབ།"

#: keyboard.xhp#hd_id3153367.13.help.text
msgid "Inserting Text Tables"
msgstr "ཚིག་ཡིག་གི་ཐིག་ཁྲམ་ཚུ་བཙུགས་དོ།"

#: keyboard.xhp#par_id3153388.14.help.text
msgid "Press F6 until the focus is on the\\<emph\\> Standard\\</emph\\> toolbar."
msgstr "ཆེད་དམིགས་འདི་\\<emph\\> ཚད་ལྡན་\\</emph\\> ལག་ཆས་ཕྲ་རིང་གུ་ལུ་མ་ལྷོད་ཚུན་  F6 ཨེབ།"

#: keyboard.xhp#par_id3154849.21.help.text
msgid "Press the right arrow key until the \\<emph\\>Table\\</emph\\> icon is selected."
msgstr "གཡས་མདའ་རྟགས་ལྡེ་མིག་འདི་ \\<emph\\>ཐིག་ཁྲམ་\\</emph\\> ངོས་དཔར་འདི་སེལ་འཐུ་མ་འབད་ཚུན་ ཨེབ།"

#: keyboard.xhp#par_id3155872.22.help.text
msgid "Press the down arrow key, and then use the arrow keys to select the number of columns and rows to include in the table."
msgstr "མར་གྱི་མདའ་རྟགས་ལྡེ་མིག་འདི་ཨེབ་ དེ་ལས་མདའ་རྟགས་ལྡེ་མིག་ཚུ་ ཀེར་ཐིག་དང་གྲལ་ཐིག་ཚུའི་ཨངའདི་ ཐིག་ཁྲམ་གྱི་གྲངས་སུ་བཙུགས་ནིའི་དོན་ལས་ ལག་ལེན་འཐབ།"

#: keyboard.xhp#par_id3155892.24.help.text
msgid ""
"_: keyboard.xhp#par_id3155892.24.help.text\n"
"Press Enter."
msgstr "ལོག་ལྡེ་ཨེབ།"

#: keyboard.xhp#par_idN1072C.help.text
msgid ""
"_: keyboard.xhp#par_idN1072C.help.text\n"
"Press F6 to place the cursor inside the document."
msgstr "འོད་རྟགས་འདི་ཡིག་ཆ་གི་ནང་ན་བཞག་ནིའི་དོན་ལས་  F6 ཨེབ།"

#: link_edit.xhp#tit.help.text
msgid "Editing  Hyperlinks"
msgstr "ཚད་བརྒལ་འབྲེལ་རྟགས་ཚུ་ཞུན་དག་འབད་དོ།"

#: link_edit.xhp#bm_id3149695.help.text
msgid "\\<bookmark_value\\>editing; hyperlinks on/off\\</bookmark_value\\>\\<bookmark_value\\>selecting; hyperlinks\\</bookmark_value\\>\\<bookmark_value\\>hyperlinks; switching on and off\\</bookmark_value\\>"
msgstr "\\<bookmark_value\\>ཞུན་དག་འབད་དོ་ ཚད་བརྒལ་འབྲེལ་རྟགས་ཚུ་ ཨཱོན་/ཨོཕ་\\</bookmark_value\\>\\<bookmark_value\\>སེལ་འཐུ་འབད་དོ་ ཚད་བརྒལ་འབྲེལ་རྟགས་ཚུ་\\</bookmark_value\\>\\<bookmark_value\\>ཚད་བརྒལ་འབྲེལ་རྟགས་ཚུ་ སུ་ཡི་ཅིང་ ཨཱོན་དང་ཨོཕ་\\</bookmark_value\\>"

#: link_edit.xhp#hd_id3149695.74.help.text
msgid "\\<variable id=\\\"link_edit\\\"\\>\\<link href=\\\"text/swriter/guide/link_edit.xhp\\\" name=\\\"Editing  Hyperlinks\\\"\\>Editing Hyperlinks\\</link\\>\\</variable\\>"
msgstr "\\<variable id=\\\"link_edit\\\"\\>\\<link href=\\\"text/swriter/guide/link_edit.xhp\\\" name=\\\"Editing  Hyperlinks\\\"\\>ཚད་བརྒལ་འབྲེལ་རྟགས་ཚུ་ཞུན་དག་འབད་དོ།\\</link\\>\\</variable\\>"

#: link_edit.xhp#par_id3155867.119.help.text
msgid ""
"_: link_edit.xhp#par_id3155867.119.help.text\n"
"Do one of the following:"
msgstr "འོག་གི་ཚུ་ལས་གཅིག་འབད་:"

#: link_edit.xhp#par_id3149849.75.help.text
msgid "Hold down the \\<switchinline select=\\\"sys\\\"\\>\\<caseinline select=\\\"MAC\\\"\\>Option key\\</caseinline\\>\\<defaultinline\\>Alt key\\</defaultinline\\>\\</switchinline\\>, and then click in the hyperlink."
msgstr "\\<switchinline select=\\\"sys\\\"\\>\\<caseinline select=\\\"MAC\\\"\\>གདམ་ཁའི་ལྡེ་མིག་\\</caseinline\\>\\<defaultinline\\>གདམ་ལྡེའི་ལྡེ་མིག་\\</defaultinline\\>\\</switchinline\\>འདི་འཆང་བཞག་ དེ་ལས་ ཚད་བརྒལ་འབྲེལ་རྟགས་ནང་ལུ་ཨེབ་གཏང་།"

#: link_edit.xhp#par_id3149644.118.help.text
msgid "On the \\<emph\\>Status Bar\\</emph\\>, click the \\<emph\\>HYP \\</emph\\>field, and then click in the hyperlink. If the \\<emph\\>SEL \\</emph\\>field is already displayed in the \\<emph\\>Status Bar\\</emph\\>, click in the hyperlink."
msgstr "\\<emph\\>གནས་ཚད་ཕྲ་རིང་\\</emph\\>གུ་ལུ་  \\<emph\\>ཨེཆ་ཝའི་པི་ \\</emph\\>ས་སྒོ་འདི་ཨེབ་གཏང་ དེ་ལས་ཚད་བརྒལ་འབྲེལ་རྟགས་ནང་ལུ་ཨེབ་གཏང་། \\<emph\\>ཨེསི་ཨི་ཨེལ་ \\</emph\\>ས་སྒོ་འདི་ཧེ་ལས་རང་ \\<emph\\>གནས་ཚད་ཕྲ་རིང་\\</emph\\>ནང་ལུ་ བཀྲམ་སྟོན་འབད་ཡོདཔ་ཨིན་པ་ཅིན་ ཚད་བརྒལ་འབྲེལ་རྟགས་ནང་ལུ་ཨེབ་གཏང་།"

#: link_edit.xhp#par_id3156255.120.help.text
msgid "Choose \\<emph\\>Insert - Hyperlink\\</emph\\>."
msgstr "\\<emph\\>བཙུགས་ - ཚད་བརྒལ་འབྲེལ་རྟགས་\\</emph\\>གདམ་ཁ་རྐྱབས།"

#: load_styles.xhp#tit.help.text
msgid "Using Styles From Another Document or Template"
msgstr "བཟོ་རྣམ་ཚུ་ ཡིག་ཆ་ཡང་ན་ཊེམ་པེལེཊི་གཞན་མི་ཅིག་ལས་ ལག་ལེན་འཐབ་དོ།"

#: load_styles.xhp#bm_id3145086.help.text
msgid "\\<bookmark_value\\>formatting styles; importing\\</bookmark_value\\>\\<bookmark_value\\>styles; importing from other files\\</bookmark_value\\>\\<bookmark_value\\>importing;styles from other files\\</bookmark_value\\>\\<bookmark_value\\>loading;styles from other files\\</bookmark_value\\>"
msgstr "\\<bookmark_value\\>རྩ་སྒྲིག་འབད་ནིའི་བཟོ་རྣམ་ཚུ་ ནང་འདྲེན་འབད་དོ་\\</bookmark_value\\>\\<bookmark_value\\>བཟོ་རྣམ་ཚུ་ ཡིག་སྣོད་གཞན་མི་ལས་ནང་འདྲེན་འབད་དོ་\\</bookmark_value\\>\\<bookmark_value\\>ནང་འདྲེན་འབད་དོ་ ཡིག་སྣོད་གཞན་མི་ཚུ་ལས་བཟོ་རྣམ་ཚུ་\\</bookmark_value\\>\\<bookmark_value\\>མངོན་གསལ་འབད་དོ་ ཡིག་སྣོད་གཞན་མི་ཚུ་ལས་བཟོ་རྣམ་ཚུ་\\</bookmark_value\\>"

#: load_styles.xhp#hd_id3145086.10.help.text
msgid "\\<variable id=\\\"load_styles\\\"\\>\\<link href=\\\"text/swriter/guide/load_styles.xhp\\\" name=\\\"Using Styles From Another Document or Template\\\"\\>Using Styles From Another Document or Template\\</link\\>\\</variable\\>"
msgstr "\\<variable id=\\\"load_styles\\\"\\>\\<link href=\\\"text/swriter/guide/load_styles.xhp\\\" name=\\\"Using Styles From Another Document or Template\\\"\\>བཟོ་རྣམ་ཚུ་ ཡིག་ཆ་ཡང་ན་ཊེམ་པེལེཊི་གཞན་མི་ཅིག་ལས་ ལག་ལེན་འཐབ་དོ།\\</link\\>\\</variable\\>"

#: load_styles.xhp#par_id3154491.11.help.text
msgid "You can import styles from another document or template into the current document."
msgstr "ཁྱོད་ཀྱིས་ བཟོ་རྣམ་ཚུ་ ཡིག་ཆ་ཡང་ན་ཊེམ་པེལེཊི་གཞན་མི་ལས་ ད་ལྟོའི་ཡིག་ཆ་འདི་ནང་ལུ་ ནང་འདྲེན་འབད་ཚུགས།"

#: load_styles.xhp#par_id3155910.12.help.text
msgid "Choose \\<emph\\>Format - Styles and Formatting\\</emph\\> to open the \\<emph\\>Styles and Formatting\\</emph\\> window."
msgstr "\\<emph\\>རྩ་སྒྲིག་ - བཟོ་རྣམ་ཚུ་དང་རྩ་སྒྲིག་འབད་ནི་\\</emph\\> འདི་ \\<emph\\>བཟོ་རྣམ་ཚུ་དང་རྩ་སྒྲིག་འབད་ནི་\\</emph\\> སྒོ་སྒྲིག་ཁ་ཕྱེ་ནིའི་དོན་ལས་ གདམ་ཁ་རྐྱབས།"

#: load_styles.xhp#par_idN10703.help.text
msgid "Click the arrow next to the \\<emph\\>New Style from Selection\\</emph\\> icon to open the submenu."
msgstr "མདའ་རྟགས་ \\<emph\\>སེལ་འཐུ་འབད་ནི་ལས་བཟོ་རྣམ་གསརཔ་\\</emph\\> ངོས་དཔར་དེ་གི་ཤུལ་མ་ཡོད་མི་འདི་ ཡན་ལག་དཀར་ཆག་ཁ་ཕྱེ་ནིའི་དོན་ལས་ལག་ལེན་འཐབ།"

#: load_styles.xhp#par_idN1070B.help.text
msgid "Choose \\<emph\\>Load styles\\</emph\\>."
msgstr "\\<emph\\>མངོན་གསལ་འབད་ནིའི་བཟོ་རྣམ་ཚུ་\\</emph\\>གདམ་ཁ་རྐྱབས།"

#: load_styles.xhp#par_id3149632.15.help.text
msgid "Use the check boxes at the bottom of the dialog to select the style types that you want to import. To replace styles in the current document that have the same name as the ones you are importing, select \\<emph\\>Overwrite\\</emph\\>."
msgstr "ཌའི་ལོག་འདི་གི་མཇུག་ལུ་ཡོད་པའི་དཔྱད་སྒྲོམ་ཚུ་ ཁྱོད་ལུ་ནང་འདྲེན་འབད་ནིའི་དོན་ལས་དགོ་ནི་ཨིན་མི་ བཟོ་རྣམ་གྱི་དབྱེ་བ་ཚུ་སེལ་འཐུ་འབད་ནིའི་དོན་ལས་ ལག་ལེན་འཐབ། ཁྱོད་ཀྱིས་ནང་འདྲེན་འབད་མི་ཚུ་དང་མིང་གཅིག་པ་ཡོད་མི་ ད་ལྟོའི་ཡིག་ཆ་འདི་ནང་ལུ་ བཟོ་རྣམ་ཚུ་ཚབ་བཙུགས་ནིའི་དོན་ལས་  \\<emph\\>ཚབ་སྲུང་འབད་ནི་\\</emph\\>སེལ་འཐུ་འབད།"

