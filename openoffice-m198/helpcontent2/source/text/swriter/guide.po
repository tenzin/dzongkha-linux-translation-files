#: fields.xhp#tit.help.text
msgid "About Fields"
msgstr "ས་སྒོ་ཚུ་གི་སྐོར་ལས།"

#: fields.xhp#bm_id3145576.help.text
msgid "\\<bookmark_value\\>fields;updating/viewing\\</bookmark_value\\>\\<bookmark_value\\>updating;fields\\</bookmark_value\\>\\<bookmark_value\\>Help tips;fields\\</bookmark_value\\>\\<bookmark_value\\>properties;fields\\</bookmark_value\\>\\<bookmark_value\\>disabling;field highlighting\\</bookmark_value\\>\\<bookmark_value\\>changing;field shadings\\</bookmark_value\\>\\<bookmark_value\\>viewing;fields\\</bookmark_value\\>"
msgstr "\\<bookmark_value\\>ས་སྒོ་ཚུ་;དུས་མཐུན་/བལྟ་དོ\\</bookmark_value\\>\\<bookmark_value\\>ས་སྒོ་ཚུ་;དུས་མཐུན་བཟོ་དོ་\\</bookmark_value\\>\\<bookmark_value\\>གྲོགས་རམ་་ཕན་བསླབ་;ས་སྒོ་ཚུ་\\</bookmark_value\\>\\<bookmark_value\\>རྒྱུ་དངོས་;ས་སྒོ་ཚུ་\\</bookmark_value\\>\\<bookmark_value\\>ས་སྒོ་གཙོ་དམིགས་འབད་ནི་་;ལྕོགས་མིན་བཟོ་དོ་\\</bookmark_value\\>\\<bookmark_value\\>ས་སྒོ་ནག་གྲིབ་བཟོ་ནི་ཚུ་;བསྒྱུར་བཅོས་འབད་དོ་\\</bookmark_value\\>\\<bookmark_value\\>ས་སྒོ་ཚུ་;བལྟ་དོ།\\</bookmark_value\\>"

#: fields.xhp#hd_id3145576.3.help.text
msgid "\\<variable id=\\\"fields\\\"\\>\\<link href=\\\"text/swriter/guide/fields.xhp\\\" name=\\\"About Fields\\\"\\>About Fields\\</link\\>\\</variable\\>"
msgstr "\\<variable id=\\\"fields\\\"\\>\\<link href=\\\"text/swriter/guide/fields.xhp\\\" name=\\\"About Fields\\\"\\>ས་སྒོ་ཚུ་གི་སྐོར་ལས་\\</link\\>\\</variable\\>"

#: fields.xhp#par_id3154246.4.help.text
msgid "Fields are used for data that changes in a document, such as the current date or the total number of pages in a document."
msgstr "ས་སྒོ་ཚུ་ ཡིག་ཆ་ནང་ལུ་བསྒྱུར་བཅོས་འབད་མི་གནད་སྡུད་ཀྱི་དོན་ལུ་ལག་ལེན་འཐབ་ཡོདཔ་ཨིན་ དཔེར་ན་ ཡིག་ཆ་ནང་གི་ཤོག་ལེབ་ཀྱི་གྲངས་བསྡོམས་དང་ཡང་ན་ ད་ལྟོའི་ཚེས་གྲངས་བཟུམ།"

#: fields.xhp#hd_id3154262.5.help.text
msgid "Viewing Fields"
msgstr "ས་སྒོ་ཚུ་སྟོན་དོ།"

#: fields.xhp#par_id3150509.6.help.text
msgid "Fields consist of a field name and the field content. To switch the field display between the field name or the field content, choose \\<link href=\\\"text/swriter/01/03090000.xhp\\\" name=\\\"View - Fields\\\"\\>\\<emph\\>View - Field Names\\</emph\\>\\</link\\>."
msgstr "ས་སྒོའི་མིང་དང་ས་སྒོའི་ནང་དོན་ཡོད་མི་ས་སྒོ་ཚུ། ས་སྒོའི་བཀྲམ་སྟོན་འདི་ ས་སྒོའི་མིང་ཡང་ན་ས་སྒོའི་ནང་དོན་གྱི་བར་ན་ སོར་བསྒྱུར་འབད་ནིའི་དོན་ལས་  \\<link href=\\\"text/swriter/01/03090000.xhp\\\" name=\\\"View - Fields\\\"\\>\\<emph\\>མཐོང་སྣང་ - ས་སྒོའི་མིང་ཚུ་\\</emph\\>\\</link\\>གདམ་ཁ་རྐྱབས།"

#: fields.xhp#par_id3150536.195.help.text
msgid "To display or hide field highlighting in a document, choose \\<emph\\>View - Field Shadings\\</emph\\>. To permanently disable this feature, choose \\<emph\\>Tools - Options - $[officename] - Appearance\\</emph\\>, and clear the check box in front of \\<emph\\>Field shadings\\</emph\\>."
msgstr "ཡིག་ཆ་ནང་ལུ་ ས་སྒོ་གཙོ་དམིགས་འབད་ནི་ བཀྲམ་སྟོན་ཡང་ན་སྦ་བཞག་ནིའི་དོན་ལས་  \\<emph\\>མཐོང་སྣང་ - ས་སྒོ་ནག་གྲིབ་བཟོ་ནི་\\</emph\\>ཚུ་གདམ་ཁ་རྐྱབས། འ་ནི་ཁྱད་རྣམ་འདི་རྟག་བརྟན་སྦེ་ལྕོགས་མིན་བཟོ་ནིའི་དོན་ལས་  \\<emph\\>ལག་ཆས་ཚུ་ - གདམ་ཁ་ཚུ་ - $[officename] - འབྱུང་སྣང་\\</emph\\>གདམ་ཁ་རྐྱབས་ དེ་ལས་\\<emph\\>ས་སྒོ་ནག་གྲིབ་བཟོ་ནི་\\</emph\\>ཚུ་གི་གདོང་ཁ་ཡོད་མི་དཔྱད་སྒྲོམ་འདི་བསལ།"

#: fields.xhp#par_id3152885.7.help.text
msgid "To change the color of field shadings, choose \\<link href=\\\"text/shared/optionen/01012000.xhp\\\" name=\\\"Tools - Options - $[officename] - Appearance\\\"\\>\\<emph\\>Tools - Options - $[officename] - Appearance\\</emph\\>\\</link\\>, locate the \\<emph\\>Field shadings\\</emph\\> option, and then select a different color in the \\<emph\\>Color setting \\</emph\\>box."
msgstr "ས་སྒོ་ནག་གྲིབ་བཟོ་ནི་གི་ཚོས་གཞི་འདི་བསྒྱུར་བཅོས་འབད་ནིའི་དོན་ལས་  \\<link href=\\\"text/shared/optionen/01012000.xhp\\\" name=\\\"Tools - Options - $[officename] - Appearance\\\"\\>\\<emph\\>ལག་ཆས་ཚུ་ - གདམ་ཁ་ཚུ་ - $[officename] - འབྱུང་སྣང་\\</emph\\>\\</link\\>གདམ་ཁ་རྐྱབས་   \\<emph\\>ས་སྒོ་ནག་གྲིབ་བཟོ་ནི་\\</emph\\>ཚུ་ག་ཡོད་འཚོལ་ དེ་ལས་ \\<emph\\>ཚོས་གཞིའི་སྒྲིག་སྟངས་ \\</emph\\>སྒྲོམ་འདི་ནང་ལུ་ ཚོས་གཞི་སོ་སོ་ཅིག་སེལ་འཐུ་འབད།"

#: fields.xhp#hd_id3153166.8.help.text
msgid "Field Properties"
msgstr "ས་སྒོ་རྒྱུ་དངོས་ཚུ།"

#: fields.xhp#par_id3153180.181.help.text
msgid "Most field types in a document, including database fields, store and display variable values."
msgstr "གནད་སྡུད་གཞི་རྟེན་ས་སྒོ་ཚུ་གྲངས་སུ་བཙུགས་ཏེ་ ཡིག་ཆ་ནང་གི་ས་སྒོའི་དབྱེ་བ་མང་ཤོས་ཅིག་གིས་ འགྱུར་ཅན་གནས་གོང་ཚུ་ གསོག་འཇོག་དང་བཀྲམ་སྟོན་འབདཝ་ཨིན།"

#: fields.xhp#par_id3155533.182.help.text
msgid "The following field types execute an action when you click the field:"
msgstr "འོག་གི་ས་སྒོའི་དབྱེ་བ་འདི་གིས་ཁྱོད་ཀྱིས་ས་སྒོ་ཅིག་ཨེབ་གཏང་པའི་སྐབས་ བྱ་བ་ཅིག་ལག་ལེན་འཐབ་ཨིན་:"

#: fields.xhp#par_id3155582.183.help.text
msgid "Field Type"
msgstr "ས་སྒོ་དབྱེ་བ།"

#: fields.xhp#par_id3147760.184.help.text
msgid "Property"
msgstr "རྒྱུ་དངོས།"

#: fields.xhp#par_id3147789.185.help.text
msgid "Placeholder"
msgstr "ས་གནས་འཛིན་མི།"

#: fields.xhp#par_id3147812.186.help.text
msgid "Opens a dialog to insert the object for which the placeholder was set."
msgstr "ས་གནས་འཛིན་མི་དེ་གཞི་སྒྲིག་འབད་ཡོད་མི་གི་དོན་ལུ་དངོས་པོ་འདི་བཙུགས་ནིའི་དོན་ལས་ཌའི་ལོག་ཅིག་ཁ་ཕྱེཝ་ཨིན།"

#: fields.xhp#par_id3147216.187.help.text
msgid "Insert Reference"
msgstr "གཞི་བསྟུན་བཙུགས།"

#: fields.xhp#par_id3147239.188.help.text
msgid "Moves the mouse pointer to the reference."
msgstr "མའུསི་དཔག་བྱེད་འདི་གཞི་བསྟུན་ལུ་བཤུདཔ་ཨིན།"

#: fields.xhp#par_id3147267.189.help.text
msgid "Run macro"
msgstr "མེཀ་རོ་གཡོག་བཀོལ།"

#: fields.xhp#par_id3147290.190.help.text
msgid "Runs a macro."
msgstr "མེཀ་རོ་གཅིག་གཡོག་བཀོལཝ་ཨིན།"

#: fields.xhp#par_id3145614.191.help.text
msgid "Input Field"
msgstr "ཨིན་པུཊི་ས་སྒོ།"

#: fields.xhp#par_id3145637.192.help.text
msgid "Opens a dialog to edit the contents of the field."
msgstr "ས་སྒོ་འདི་གི་ནང་དོན་ཚུ་ཞུན་དག་འབད་ནིའི་དོན་ལས་ཌའི་ལོག་ཅིག་ཁ་ཕྱེཝ་ཨིན།"

#: fields.xhp#par_id3151244.10.help.text
msgid "Placeholder, hidden text, insert reference, variable, database, and user-defined fields display a help tip when you rest the mouse pointer over the field in a document. To enable this feature, ensure that the \\<emph\\>Extended Tips \\</emph\\>option is selected in the \\<emph\\>Help\\</emph\\> menu."
msgstr "ས་གནས་འཛིན་མི་ སྦ་བཞག་ཡོད་པའི་ཚིག་ཡིག་ བཙུགས་ནིའི་གཞི་བསྟུན་ འགྱུར་ཅན་ གནད་སྡུད་གཞི་རྟེན་ དང་ལག་ལེན་པ་ངེས་འཛིན་འབད་ཡོད་མི་ས་སྒོ་ཚུ་གིས་ ཡིག་ཆ་ནང་ལུ་ས་སྒོ་ཅིག་གུ་ལས་མའུསི་དཔག་བྱེད་འདི་ ཁྱོད་ཀྱིས་ངལ་འཚོ་བཞག་པའི་སྐབས་ གྲོགས་རམ་ཕན་བསླབ་བཀྲམ་སྟོན་འབདཝ་ཨིན། འ་ནི་ཁྱད་རྣམ་འདི་ལྕོགས་ཅན་བཟོ་ནིའི་དོན་ལས་  \\<emph\\>རྒྱ་བསྐྱེད་ཅན་ཕན་བསླབ་ཀྱི་ \\</emph\\>གདམ་ཁ་འདི་ \\<emph\\>གྲོགས་རམ་\\</emph\\> དཀར་ཆག་ནང་ལུ་ སེལ་འཐུ་འབད་ཡོདཔ་ཨིནམ་ངེས་ཏིག་བཟོ།"

#: fields.xhp#hd_id3155937.11.help.text
msgid "Updating Fields"
msgstr "ས་སྒོ་ཚུ་དུས་མཐུན་བཟོ་དོ།"

#: fields.xhp#par_id3155963.12.help.text
msgid "To update all of the fields in a document, press F9, or choose \\<emph\\>Edit - Select All\\</emph\\>, and then press F9."
msgstr "ཡིག་ཆ་ནང་གི་ས་སྒོ་ཚུ་ཆ་མཉམ་དུས་མཐུན་བཟོ་ནིའི་དོན་ལས་ F9 ཨེབ་ ཡང་ན་ \\<emph\\>ཞུན་དག་ - ཆ་མཉམ་སེལ་འཐུ་འབད་l\\</emph\\>གདམ་ཁ་རྐྱབས་ དེ་ལས་ F9 ཨེབ་གཏང་།"

#: fields.xhp#par_id3155984.13.help.text
msgid "To update a field that was inserted from a database, select the field, and then press F9."
msgstr "གནད་སྡུད་གཞི་རྟེན་ལས་བཙུགས་ཡོད་པའི་ས་སྒོ་འདི་དུས་མཐུན་བཟོ་ནིའི་དོན་ལས་ ས་སྒོ་འདི་སེལ་འཐུ་འབད་ དེ་ལས་ F9 ཨེབ།"

#: fields_date.xhp#tit.help.text
msgid "Inserting a Fixed or Variable Date Field"
msgstr "གཏན་བཟོས་ཡང་ན་འགྱུར་ཅན་གནད་སྡུད་ས་སྒོ་ཅིག་བཙུགས་དོ།"

#: fields_date.xhp#bm_id5111545.help.text
msgid "\\<bookmark_value\\>inserting;date fields\\</bookmark_value\\>\\<bookmark_value\\>dates;inserting\\</bookmark_value\\>\\<bookmark_value\\>date fields;fixed/variable\\</bookmark_value\\>\\<bookmark_value\\>fixed dates\\</bookmark_value\\>\\<bookmark_value\\>variable dates\\</bookmark_value\\>"
msgstr "\\<bookmark_value\\>བཙུགས་དོ་ ས་སྒོའི་ཚེས་གྲངས་\\</bookmark_value\\>ཚུ་\\<bookmark_value\\>ཚེས་གྲངས་ཚུ་ བཙུགས་དོ་\\</bookmark_value\\>\\<bookmark_value\\>ཚེས་གྲངས་ས་སྒོ་ཚུ་ གཏན་བཟོས་/འགྱུར་ཅན་\\</bookmark_value\\>\\<bookmark_value\\>གཏན་བཟོས་ཚེས་གྲངས་\\</bookmark_value\\>ཚུ་\\<bookmark_value\\>འགྱུར་ཅན་ཚེས་གྲངས་\\</bookmark_value\\>ཚུ།"

#: fields_date.xhp#hd_id3155165.1.help.text
