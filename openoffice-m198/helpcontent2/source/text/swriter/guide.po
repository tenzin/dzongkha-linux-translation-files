#: page_break.xhp#tit.help.text
msgid "Inserting and Deleting Page Breaks"
msgstr "ཤོག་ལེབ་ཀྱི་བར་མཚམས་ཚུ་ བཙུགས་ནི་དང་བཏོན་གཏང་དོ།"

#: page_break.xhp#bm_id3155183.help.text
msgid "\\<bookmark_value\\>page breaks; inserting and deleting\\</bookmark_value\\>\\<bookmark_value\\>inserting; page breaks\\</bookmark_value\\>\\<bookmark_value\\>deleting;page breaks\\</bookmark_value\\>\\<bookmark_value\\>pages; inserting/deleting page breaks\\</bookmark_value\\>\\<bookmark_value\\>manual page breaks\\</bookmark_value\\>"
msgstr "\\<bookmark_value\\>ཤོག་ལེབ་བར་མཚམས་ཚུ་ བཙུགས་ནི་དང་བཏོན་གཏང་དོ་\\</bookmark_value\\>\\<bookmark_value\\>བཙུགས་དོ་ ཤོག་བེལ་བར་མཚམས་ཚུ་\\</bookmark_value\\>\\<bookmark_value\\>བཏོན་གཏང་དོ་ ཤོག་ལེབབར་མཚམས་ཚུ་\\</bookmark_value\\>\\<bookmark_value\\>ཤོག་ལེབ་ཚུ་ ཤོག་ལེབ་བར་མཚམས་\\</bookmark_value\\>ཚུ་བཙུགས་དོ་/བཏོན་གཏང་དོ་\\<bookmark_value\\>ལག་ཐོག་ཤོག་ལེབ་བར་མཚམས་ཚུ་\\</bookmark_value\\>"

#: page_break.xhp#hd_id3155183.1.help.text
msgid "\\<variable id=\\\"page_break\\\"\\>\\<link href=\\\"text/swriter/guide/page_break.xhp\\\" name=\\\"Inserting and Deleting Page Breaks\\\"\\>Inserting and Deleting Page Breaks\\</link\\>\\</variable\\>"
msgstr "\\<variable id=\\\"page_break\\\"\\>\\<link href=\\\"text/swriter/guide/page_break.xhp\\\" name=\\\"Inserting and Deleting Page Breaks\\\"\\>ཤོག་ལེབ་ཀྱི་བར་མཚམས་ཚུ་ བཙུགས་ནི་དང་བཏོན་གཏང་དོ།\\</link\\>\\</variable\\>"

#: page_break.xhp#hd_id3149833.2.help.text
msgid "To insert a manual page break:"
msgstr "ལག་ཐོག་ཤོག་ལེབ་བར་མཚམས་ཅིག་བཙུགས་ནིའི་དོན་ལས་:"

#: page_break.xhp#par_id3153402.3.help.text
msgid "Click in your document where you want the new page to begin."
msgstr "ཁྱོད་རའི་ཡིག་ཆ་ནང་ལུ་ ཁྱོད་ཀྱིས་ཤོག་ལེབ་གསརཔ་འདི་འགོ་བཙུགས་དགོ་མནོ་སར་ཨེབ་གཏང་།"

#: page_break.xhp#par_id3153119.4.help.text
msgid "Press Ctrl+Enter."
msgstr "ཚད་འཛིན་+ལོག་ལྡེ་ ཨེབ།"

#: page_break.xhp#hd_id3153135.6.help.text
msgid "To delete a manual page break:"
msgstr "ལག་ཐོག་ཤོག་ལེབ་ཀྱི་བར་མཚམས་ཅིག་བཏོན་གཏང་ནིའི་དོན་ལས་:"

#: page_break.xhp#par_id3149641.7.help.text
msgid "Click in front of the first character on the page that follows the manual page break."
msgstr "ལག་ཐོག་ཤོག་ལེབ་བར་མཚམས་འདི་རྗེས་སུ་འབྲང་མི་ ཤོག་ལེབ་གུའི་ཡིག་འབྲུ་དང་པ་འདི་གི་གདོང་ཁ་ལུ་ཨེབ་གཏང་།"

#: page_break.xhp#par_id3149608.8.help.text
msgid "Press Backspace."
msgstr "རྒྱབ་བཤུད་ཨེབ།"

#: page_break.xhp#hd_id3149624.10.help.text
msgid "To delete a manual page break that occurs before a table:"
msgstr "ཐིག་ཁྲམ་ཅིག་གི་ཧེ་མ་འབྱུང་མི་ ལག་ཐོག་ཤོག་ལེབ་བར་མཚམས་ཅིག་བཏོན་གཏང་ནིའི་དོན་ལས་:"

#: page_break.xhp#par_id3145111.11.help.text
msgid "Right-click in the table, and choose \\<emph\\>Table\\</emph\\>."
msgstr "ཐིག་ཁྲམ་འདི་ནང་གཡས་-ཨེབ་གཏང་འབད་ དེ་ལས་ \\<emph\\>ཐིག་ཁྲམ་\\</emph\\>གདམ་ཁ་རྐྱབས།"

#: page_break.xhp#par_id3156254.13.help.text
msgid "Click the \\<link href=\\\"text/swriter/01/05090300.xhp\\\" name=\\\"Text Flow\\\"\\>\\<emph\\>Text Flow\\</emph\\>\\</link\\> tab."
msgstr "\\<link href=\\\"text/swriter/01/05090300.xhp\\\" name=\\\"Text Flow\\\"\\>\\<emph\\>ཚིག་ཡིག་འབབ་རྒྱུན་\\</emph\\>\\</link\\> མཆོང་ལྡེ་འདི་ཨེབ་གཏང་།"

#: page_break.xhp#par_id3153380.14.help.text
msgid "Clear the \\<emph\\>Break\\</emph\\> check box."
msgstr "\\<emph\\>བར་མཚམས་\\</emph\\> དཔྱད་སྒྲོམ་འདི་བསལ།"

#: page_break.xhp#par_id3154249.15.help.text
msgid ""
"_: page_break.xhp#par_id3154249.15.help.text\n"
"\\<link href=\\\"text/swriter/01/04010000.xhp\\\" name=\\\"Insert Break dialog\\\"\\>Insert Break dialog\\</link\\>"
msgstr "\\<link href=\\\"text/swriter/01/04010000.xhp\\\" name=\\\"Insert Break dialog\\\"\\>བར་མཚམས་ཌའི་ལོག་བཙུགས་\\</link\\>"

#: pagebackground.xhp#tit.help.text
msgid "Changing Page Backgrounds"
msgstr "ཤོག་ལེབ་ཀྱི་རྒྱབ་གཞི་ཚུ་བསྒྱུར་བཅོས་འབད་དོ།"

#: pagebackground.xhp#bm_id8431653.help.text
msgid "\\<bookmark_value\\>page backgrounds\\</bookmark_value\\>\\<bookmark_value\\>page styles;backgrounds\\</bookmark_value\\>\\<bookmark_value\\>backgrounds; different pages\\</bookmark_value\\>\\<bookmark_value\\>changing;page backgrounds\\</bookmark_value\\>\\<bookmark_value\\>pages;backgrounds\\</bookmark_value\\>"
msgstr "\\<bookmark_value\\>ཤོག་ལེབ་རྒྱབ་གཞི་\\</bookmark_value\\>ཚུ་\\<bookmark_value\\>ཤོག་ལེབ་བཟོ་རྣམ་ཚུ་ རྒྱབ་གཞི་\\</bookmark_value\\>ཚུ་\\<bookmark_value\\>རྒྱབ་གཞི་ཚུ ཤོག་ལེབ་སོ་སོ་\\</bookmark_value\\>ཚུ་\\<bookmark_value\\>བསྒྱུར་བཅོས་འབད་དོ་ ཤོག་ལེབ་རྒྱབ་གཞི་\\</bookmark_value\\>ཚུ་\\<bookmark_value\\>ཤོག་ལེབ་ཚུ་ རྒྱབ་གཞི་\\</bookmark_value\\>ཚུ།"

#: pagebackground.xhp#par_idN107F4.help.text
msgid "\\<variable id=\\\"pagebackground\\\"\\>\\<link href=\\\"text/swriter/guide/pagebackground.xhp\\\"\\>Changing Page Background\\</link\\>\\</variable\\>"
msgstr "\\<variable id=\\\"pagebackground\\\"\\>\\<link href=\\\"text/swriter/guide/pagebackground.xhp\\\"\\>ཤོག་ལེབ་ཀྱི་རྒྱབ་གཞི་བསྒྱུར་བཅོས་འབད་དོ།\\</link\\>\\</variable\\>"

#: pagebackground.xhp#par_idN10812.help.text
msgid "$[officename] uses page styles to specify the background of the pages in a document. For example, to change the page background of one or more pages in a document to a watermark, you need to create a page style that uses the watermark background, and then apply the page style to the pages."
msgstr "$[officename] གིས་ ཡིག་ཆ་ཅིག་ནང་གི་ཤོག་ལེབ་འདི་ཚུ་གི་རྒྱབ་གཞི་འདི་གསལ་བཀོད་འབད་ནིའི་དོན་ལས་ ཤོག་ལེབ་ཀྱི་བཟོ་རྣམ་ཚུ་ལག་ལེན་འཐབ་ཨིན། དཔེར་ན་ ཡིག་ཆ་ཅིག་ནང་གི་ གཅིག་ཡང་ན་དེ་ལས་ལྷག་པའི་ཤོག་ལེབ་ཚུ་གི་ ཤོག་ལེབ་རྒྱབ་གཞི་འདི་ ཆུ་རྟགས་ཅིག་ལུ་བསྒྱུར་བཅོས་འབད་ནིའི་དོན་ལས་ ཁྱོད་ཀྱིས་ ཆུ་རྟགས་རྒྱབ་གཞི་འདི་ལག་ལེན་འཐབ་མི་ ཤོག་ལེབ་ཀྱི་བཟོ་རྣམ་ཅིག་གསར་བསྐྲུན་འབད་དགོ་ དེ་ལས་ཤོག་ལེབ་ཀྱི་བཟོ་རྣམ་འདི་ཤོག་ལེབ་ཚུ་ལུ་འཇུག་སྤྱོད་འབད་དགོ།"

#: pagebackground.xhp#par_idN10820.help.text
msgid "To Change the Page Background"
msgstr "ཤོག་ལེབ་ཀྱི་རྒྱབ་གཞི་བསྒྱུར་བཅོས་འབད་ནིའི་དོན་ལས་:"

#: pagebackground.xhp#par_idN10827.help.text
msgid ""
"_: pagebackground.xhp#par_idN10827.help.text\n"
"Choose \\<emph\\>Format - Styles and Formatting\\</emph\\>."
msgstr "\\<emph\\>རྩ་སྒྲིག་ - བཟོ་རྣམ་ཚུ་དང་རྩ་སྒྲིག་འབད་ནི་\\</emph\\>གདམ་ཁ་རྐྱབས།"

#: pagebackground.xhp#par_idN1082F.help.text
msgid ""
"_: pagebackground.xhp#par_idN1082F.help.text\n"
"Click the \\<emph\\>Page Styles\\</emph\\> icon."
msgstr "\\<emph\\>ཤོག་ལེབ་བཟོ་རྣམ་ཚུའི་\\</emph\\> ངོས་དཔར་འདི་ཨེབ་གཏང་།"

#: pagebackground.xhp#par_idN10837.help.text
msgid ""
"_: pagebackground.xhp#par_idN10837.help.text\n"
"In the list of page styles, right-click an item, and then choose \\<emph\\>New\\</emph\\>."
msgstr "ཤོག་ལེབ་བཟོ་རྣམ་ཚུ་གི་ཐོ་ཡིག་འདི་ནང་ལུ་ རྣམ་གྲངས་ཅིག་གཡས་-ཨེབ་གཏང་འབད་ དེ་ལས་ \\<emph\\>གསརཔ་\\</emph\\>གདམ་ཁ་རྐྱབས།"

#: pagebackground.xhp#par_idN1083F.help.text
msgid "On the \\<emph\\>Organizer\\</emph\\> tab page, type a name for the page style in the \\<emph\\>Name\\</emph\\> box."
msgstr " \\<emph\\>འགོ་འདྲེན་པ་\\</emph\\> མཆོང་ལྡེ་ཤོག་ལེབ་འདི་གུ་ ཤོག་ལེབ་བཟོ་རྣམ་དོན་ལུ་ \\<emph\\>སྒྲོམ་\\</emph\\> མིང་འདི་ནང་མིང་ཅིག་ཡིག་དཔར་རྐྱབས།"

#: pagebackground.xhp#par_idN1084B.help.text
msgid ""
"_: pagebackground.xhp#par_idN1084B.help.text\n"
"In the \\<emph\\>Next Style\\</emph\\> box, select the page style that you want to apply to the next page."
msgstr "\\<emph\\>ཤུལ་མའི་བཟོ་རྣམ་\\</emph\\> སྒྲོམ་ནང་ལུ་ ཁྱོད་ཀྱིས་ཤུལ་མའི་ཤོག་ལེབ་འདི་ལུ་ འཇུག་སྤྱོད་འབད་ནི་ཨིན་མི་ ཤོག་ལེབ་བཟོ་རྣམ་འདི་སེལ་འཐུ་འབད།"

#: pagebackground.xhp#par_idN10855.help.text
msgid "To only apply the new page style to a single page, select \"Default\"."
msgstr "ཤོག་ལེབ་བཟོ་རྣམ་གསརཔ་འདི་འཇུག་སྤྱོད་འབད་ནི་ལུ་རྐྱངམ་ཅིག་ཤོག་ལེབ་རྐྱང་པ་ཅིག་ལུ་ \"Default\"སེལ་འཐུ་འབད།"

#: pagebackground.xhp#par_idN10859.help.text
msgid ""
"_: pagebackground.xhp#par_idN10859.help.text\n"
"To apply the new page style to all subsequent pages, select the name of the new page style."
msgstr "ཤུལ་ལུའི་ཤོག་ལེབ་ཆ་མཉམ་ལུ་ ཤོག་ལེབ་བཟོ་རྣམ་གསརཔ་འདི་འཇུག་སྤྱོད་འབད་ནིའི་དོན་ལས་ ཤོག་ལེབ་བཟོ་རྣམ་གསརཔ་འདི་གི་མིང་འདི་སེལ་འཐུ་འབད།"

#: pagebackground.xhp#par_idN1085F.help.text
msgid "Click the \\<emph\\>Background\\</emph\\> tab."
msgstr "\\<emph\\>རྒྱབ་གཞི་\\</emph\\> མཆོང་ལྡེ་འདི་ཨེབ་གཏང་།"

#: pagebackground.xhp#par_idN10867.help.text
msgid "In the list box at the top, select whether you want a solid color or a graphic. Then select your options from the tab page. "
msgstr "མགོ་གི་ཐོ་ཡིག་སྒྲོམ་ནང་ལུ་ ཁྱོད་ལུ་ཚོས་གཞི་རགས་པ་ཡང་ན་ཚད་རིས་དགོ་ནི་ཨིན་ན་ སེལ་འཐུ་འབད། དེ་ལས་ མཆོང་ལྡེའི་ཤོག་ལེབ་འདི་ལས་ ཁྱོད་རའི་གདམ་ཁ་སེལ་འཐུ་འབད།"

#: pagebackground.xhp#par_idN1086B.help.text
msgid ""
"_: pagebackground.xhp#par_idN1086B.help.text\n"
"Click \\<emph\\>OK\\</emph\\>."
msgstr "\\<emph\\>བཏུབ་\\</emph\\>ཨེབ་གཏང་།"

#: pagebackground.xhp#par_idN1087A.help.text
msgid "To Change the Page Background of All Pages in a Document"
msgstr "ཡིག་ཆ་ཅིག་ནང་གི་ ཤོག་ལེབ་ཆ་མཉམ་གྱི་ ཤོག་ལེབ་རྒྱབ་གཞི་འདི་བསྒྱུར་བཅོས་འབད་ནིའི་དོན་ལས་"

#: pagebackground.xhp#par_idN1087E.help.text
msgid ""
"_: pagebackground.xhp#par_idN1087E.help.text\n"
"Before you begin, ensure that you have created a page style that uses a page background. See \\<link href=\\\"text/swriter/guide/pagebackground.xhp#define\\\"\\>To Change the Page Background\\</link\\> for details."
msgstr "ཁྱོད་ཀྱིས་འགོ་མ་བཙུགས་པའི་ཧེ་མ་ ཤོག་ལེབ་རྒྱབ་གཞི་ཅིག་ལག་ལེན་འཐབ་མི་ ཤོག་ལེབ་བཟོ་རྣམ་ཅིག་ གསར་བསྐྲུན་འབད་ཡོདཔ་སྦེ་ ངེས་ཏིག་བཟོ།  རྒྱས་བཤད་ཀྱི་དོན་ལུ་\\<link href=\\\"text/swriter/guide/pagebackground.xhp#define\\\"\\>ཤོག་ལེབ་རྒྱབ་གཞི་འདི་བསྒྱུར་བཅོས་འབད་ནི་\\</link\\> བལྟ།"

#: pagebackground.xhp#par_idN10892.help.text
msgid ""
"_: pagebackground.xhp#par_idN10892.help.text\n"
"Choose \\<emph\\>Format - Styles and Formatting\\</emph\\>."
msgstr "\\<emph\\>རྩ་སྒྲིག་ - བཟོ་རྣམ་ཚུ་དང་རྩ་སྒྲིག་འབད་ནི་\\</emph\\>གདམ་ཁ་རྐྱབས།"

#: pagebackground.xhp#par_idN1089A.help.text
msgid ""
"_: pagebackground.xhp#par_idN1089A.help.text\n"
"Click the \\<emph\\>Page Styles\\</emph\\> icon."
msgstr "\\<emph\\>ཤོག་ལེབ་བཟོ་རྣམ་ཚུའི་\\</emph\\> ངོས་དཔར་འདི་ཨེབ་གཏང་།"

#: pagebackground.xhp#par_idN108A2.help.text
msgid "Double-click the page style that uses the page background that you want to apply."
msgstr "ཁྱོད་ཀྱིས་འཇུག་སྤྱོད་འབད་ནི་ཨིན་མི་ ཤོག་ལེབ་ཀྱི་རྒྱབ་གཞི་འདི་ལག་ལེན་འཐབ་མི་ ཤོག་ལེབ་བཟོ་རྣམ་འདི་ཚར་གཉིས་ཨེབ་གཏང་།"

#: pagebackground.xhp#par_idN108A5.help.text
msgid "To Use Different Page Backgrounds in the Same Document"
msgstr "ཡིག་ཆ་གཅིག་པ་ནང་ལུ་ ཤོག་ལེབ་ཀྱི་རྒྱབ་གཞི་སོ་སོ་ཚུ་ལག་ལེན་འཐབ་ནིའི་དོན་ལས་"

#: pagebackground.xhp#par_idN108A9.help.text
msgid ""
"_: pagebackground.xhp#par_idN108A9.help.text\n"
"Before you begin, ensure that you have created a page style that uses a page background. See \\<link href=\\\"text/swriter/guide/pagebackground.xhp#define\\\"\\>To Change the Page Background\\</link\\> for details."
msgstr "ཁྱོད་ཀྱིས་འགོ་མ་བཙུགས་པའི་ཧེ་མ་ ཤོག་ལེབ་ཀྱི་རྒྱབ་གཞི་ཅིག་ལག་ལེན་འཐབ་མི་ ཤོག་ལེབ་བཟོ་རྣམ་ཅིག་ ཁྱོད་ཀྱིས་གསར་བསྐྲུན་འབད་ཡོད་སྦེ་ ངེས་ཏིག་བཟོ། རྒྱས་བཤད་ཀྱི་དོན་ལུ་  \\<link href=\\\"text/swriter/guide/pagebackground.xhp#define\\\"\\>ཤོག་ལེབ་རྒྱབ་གཞི་འདི་བསྒྱུར་བཅོས་འབད་ནིའི་དོན་ལས་\\</link\\> བལྟ།"

#: pagebackground.xhp#par_idN108BD.help.text
msgid "Click in front of the first character of the paragraph where you want to change the page background."
msgstr "ཁྱོད་ཀྱིས་ཤོག་ལེབ་རྒྱབ་གཞི་འདི་བསྒྱུར་བཅོས་འབད་ནི་ཨིན་མི་ དོན་མཚམས་འདི་གི་ ཡིག་འབྲུ་དང་པ་འདི་གི་གདོང་ཁ་ལུ་ཨེབ་གཏང་།"

#: pagebackground.xhp#par_idN108C1.help.text
msgid ""
"_: pagebackground.xhp#par_idN108C1.help.text\n"
"Choose \\<emph\\>Insert - Manual Break\\</emph\\>."
msgstr "\\<emph\\>བཙུགས་ - ལག་ཐོག་བར་མཚམས་\\</emph\\>གདམ་ཁ་རྐྱབས།"

#: pagebackground.xhp#par_idN108C9.help.text
msgid ""
"_: pagebackground.xhp#par_idN108C9.help.text\n"
"Select \\<emph\\>Page break\\</emph\\>."
msgstr "\\<emph\\>ཤོག་ལེབ་བར་མཚམས་\\</emph\\>སེལ་འཐུ་འབད།"

#: pagebackground.xhp#par_idN108D1.help.text
msgid "In the \\<emph\\>Style \\</emph\\>box, select a page style that uses the page background."
msgstr "\\<emph\\>བཟོ་རྣམ་ \\</emph\\>སྒྲོམ་ནང་ལུ་ ཤོག་ལེབ་ཀྱི་རྒྱབ་གཞི་ལག་ལེན་འཐབ་མི་ ཤོག་ལེབ་བཟོ་རྣམ་ཅིག་སེལ་འཐུ་འབད།"

#: pagebackground.xhp#par_idN108DB.help.text
msgid "To change the background of the current page only, select a page style where the Next Style option is set to \"Default\"."
msgstr "ད་ལྟོའི་ཤོག་ལེབ་འདི་གི་རྒྱབ་གཞི་རྐྱངམ་ཅིག་བསྒྱུར་བཅོས་འབད་ནིའི་དོན་ལས་ ཤུལ་མའི་བཟོ་རྣམ་གདམ་ཁ་འདི་  \"Default\" ལུ་གཞི་སྒྲིག་འབད་ཡོད་མི་ ཤོག་ལེབ་བཟོ་རྣམ་ཅིག་སེལ་འཐུ་འབད།"

#: pagebackground.xhp#par_idN108DF.help.text
msgid "To change the background of the current and subsequent pages, select a page style where the Next Style option is set to the name of the page style."
msgstr "ད་ལྟོ་དང་ཤུལ་ལུའི་ཤོག་ལེབ་ཚུ་གི་རྒྱབ་གཞི་བསྒྱུར་བཅོས་འབད་ནིའི་དོན་ལས་ ཤུལ་མའི་ཤོག་ལེབ་འདི་ ཤོག་ལེབ་བཟོ་རྣམ་གི་མིང་ལུ་གཞི་སྒྲིག་འབད་ཡོད་མི་ ཤོག་ལེབ་བཟོ་རྣམ་ཅིག་སེལ་འཐུ་འབད།"

#: pagebackground.xhp#par_idN108E2.help.text
msgid "If you want to change the page background later on in the document, repeat steps 1 to 3."
msgstr "ཁྱོད་ཀྱིས་ ཤུལ་ལས་ཡིག་ཆ་ནང་ལུ་ ཤོག་ལེབ་ཀྱི་རྒྱབ་གཞི་འདི་བསྒྱུར་བཅོས་འབད་ནི་ཨིན་པ་ཅིན་ རིམ་པ་ ༡ ལས་ ༣ ཚུན་ཡང་བསྐྱར་འབད།"

#: pagebackground.xhp#par_idN108E8.help.text
msgid ""
"_: pagebackground.xhp#par_idN108E8.help.text\n"
"Click \\<emph\\>OK\\</emph\\>."
msgstr "\\<emph\\>བཏུབ་\\</emph\\>ཨེབ་གཏང་།"

#: pagenumbers.xhp#tit.help.text
msgid "Page Numbers "
msgstr "ཤོག་ལེབ་ཨང་ཚུ།"

#: pagenumbers.xhp#bm_id5918759.help.text
msgid "\\<bookmark_value\\>page numbers;inserting/defining/formatting\\</bookmark_value\\>\\<bookmark_value\\>page styles;page numbering\\</bookmark_value\\>\\<bookmark_value\\>starting page numbers\\</bookmark_value\\>\\<bookmark_value\\>formatting;page numbers\\</bookmark_value\\>\\<bookmark_value\\>defining;starting page numbers\\</bookmark_value\\>\\<bookmark_value\\>inserting;page numbers\\</bookmark_value\\>"
msgstr "\\<bookmark_value\\>ཤོག་ལེབ་ཨང་ཚུ་བཙུགས་དོ/ངེས་འཛིན་འབད་དོ/རྩ་སྒྲིག་འབད་དོ\\</bookmark_value\\>\\<bookmark_value\\>ཤོག་ལེབ་བཟོ་རྣམ་ ཤོག་ལེབ་ཨང་བཏགས་དོ་\\</bookmark_value\\>\\<bookmark_value\\>ཤོག་ལེབ་ཨང་ཚུ་འགོ་བཙུགས་དོ་\\</bookmark_value\\>\\<bookmark_value\\>ཤོག་ལེབ་ཨང་ཚུ་རྩ་སྒྲིག་འབད་དོ\\</bookmark_value\\>\\<bookmark_value\\>ངོས་འཛིན་འབད་དོ་ ཤོག་ལེབ་ཨང་ཚུ་འགོ་བཙུགས་དོ་\\</bookmark_value\\>\\<bookmark_value\\>ཤོག་ལེབ་ཨང་ཚུ་བཙུགས་ད།\\</bookmark_value\\>"

#: pagenumbers.xhp#hd_id413830.help.text
msgid "\\<variable id=\\\"pagenumbers\\\"\\>\\<link href=\\\"text/swriter/guide/pagenumbers.xhp\\\" name=\\\"Page Numbers\\\"\\>Page Numbers\\</link\\>\\</variable\\>"
msgstr "\\<variable id=\\\"pagenumbers\\\"\\>\\<link href=\\\"text/swriter/guide/pagenumbers.xhp\\\" name=\\\"Page Numbers\\\"\\>ཤོག་ལེབ་ཨང་ཚུ།\\</link\\>\\</variable\\>"

#: pagenumbers.xhp#par_id1617175.help.text
msgid "In Writer, a page number is a field that you can insert into your text."
msgstr "རྩོམ་འབྲི་པ་ནངཤོག་ལེབ་ཨང་ཅིག་ཨིན་མི་་ཁྱོད་ཀྱི་ཚིག་ཡིག་བཙུགས་བཏུབ་པའི་ས་སྒོ་ཅིག་ཨིན།"

#: pagenumbers.xhp#hd_id6091494.help.text
msgid "Inserting page numbers"
msgstr "ཤོག་ལེབ་ཨང་ཚུ་བཙུགས་དོ།"

#: pagenumbers.xhp#par_id8611102.help.text
msgid "Choose \\<emph\\>Insert - Fields - Page Number\\</emph\\> to insert a page number at the current cursor position."
msgstr "\\<emph\\>ཤོག་ལེབ་ཨང་- ས་སྒོ་་ཚུ་ - བཙུགས་\\</emph\\> ད་ལྟོའི་འོད་གནས་ས་ལུ་ཤོག་ལེབ་ཨང་ཅིག་བཙུགས་ནི་ལུ་གདམ།"

#: pagenumbers.xhp#par_id6604510.help.text
msgid "If you see the text \"Page number\" instead of the number, choose \\<emph\\>View - Field names\\</emph\\>."
msgstr "ཁྱོད་ཀྱིས་ཨང་གི་ཚབ་མ་ཚིག་ཡིག་ \"Page number\" མཐོང་པ་ཅིན་ \\<emph\\>ས་སྒོ་མིང་ཚུ་ - མཐོང་སྣང་\\</emph\\>གདམ།"

#: pagenumbers.xhp#par_id2678914.help.text
msgid "However, these fields will change position when you add or remove text. So it is best to insert the page number field into a header or footer that has the same position and that is repeated on every page."
msgstr "ག་དེ་ཨིན་རུང་ས་སྒོ་དེ་ཚུ་ཁྱོད་ཀྱིས་ཚིག་ཡིག་ཁ་སྐོང་ཡང་ན་རྩ་བསྐྲད་གཏངམ་ད་གནས་ས་བསྒྱུར་འབདཝ་འོང་། དེ་འབདཝ་ལས་གནས་ས་ཡོད་མི་མགོ་ཡིག་ཡང་ན་མཇུག་ཡིག་ཅིག་གི་ནང་ན་ཤོག་ལེབ་ཨང་ས་སྒོ་འདི་བཙུགས་པ་ཅིན་ཤོག་ལེབ་ག་ར་གུ་ཡང་བསྐྱར་་འབདཝ་ད་གནས་ས་ཅོག་འཐདཔ་འོང་།"

#: pagenumbers.xhp#par_id614642.help.text
msgid "Choose \\<emph\\>Insert - Header - (name of page style)\\</emph\\> or \\<emph\\>Insert - Footer - (name of page style)\\</emph\\> to add a header or footer to all pages with the current Page Style."
msgstr " \\<emph\\>མགོ་ཡིག་ - བཙུགས་ - (ཤོག་ལེབ་བཟོ་རྣམ་གྱི་མིང་)\\</emph\\> ཡང་ \\<emph\\>མཇུག་ཡིག་ - བཙུགས་ - (ཤོག་ལེབ་བཟོ་རྣམ་གྱི་མིང་)\\</emph\\> ཤོག་ལེབ་ཚུ་ཆ་མཉམ་ལུ་མགོ་ཡིག་ཡང་ན་མཇུག་ཡིག་ཁ་སྐོང་འབད་ནི་དང་གཅིག་ཁར་ད་ལྟོའི་ཤོག་ལེབ་བཟོ་རྣམ་འདི་གདམ།"

#: pagenumbers.xhp#hd_id2551652.help.text
msgid "Starting with a defined page number"
msgstr "ངེས་འཛིན་འབད་ཡོད་མི་ཤོག་ལེབ་ཨང་ད་གཅིག་ཁར་འགོ་བཙུགས་དོ།"

#: pagenumbers.xhp#par_id6111941.help.text
msgid "Now you want some more control on page numbers. You are writing a text document that should start with page number 12."
msgstr "ད་ལྟོ་ཁྱོད་ལུ་ཤོག་་ལེབ་ཨང་ཚུ་གུ་ཚད་འཛིན་ལ་ལོ་དགོཔ་ཨིན། ཁྱོད་ཀྱིས་ཚིག་ཡིག་ཡིག་ཆ་ཤོག་ལེབ་ཨང་༡༢་འགོ་བཙུགསཔ་སྦེ་འབྲི་བསྒང་ཡོད།"

#: pagenumbers.xhp#par_id5757621.help.text
msgid "Click into the first paragraph of your document."
msgstr "ཁྱོད་རའི་ཡིག་ཆ་གི་མཚམས་དོན་དང་པ་འདི་ནང་ཨེབ་གཏང་འབད།"

#: pagenumbers.xhp#par_id2632831.help.text
msgid "Choose \\<emph\\>Format - Paragraph - Text flow\\</emph\\>."
msgstr "རྩ་སྒྲིག་-མཚམས་དོན་-ཚིག་ཡིག་འབབ་རྒྱུན་\\<emph\\>གདམ།\\</emph\\>."

#: pagenumbers.xhp#par_id4395275.help.text
msgid "In the Breaks area, enable \\<emph\\>Insert\\</emph\\>. Enable \\<emph\\>With Page Style\\</emph\\> just to be able to set the new \\<emph\\>Page number\\</emph\\>. Click \\<emph\\>OK\\</emph\\>."
msgstr "མཚམས་མངའ་ཁོངས་ཚུ་ནང་ \\<emph\\>བཙུགས་ལྕོགས་ཅན་\\</emph\\>བཟོ།  \\<emph\\>ཤོག་ལེབ་བཟོ་རྣམ་དང་གཅིག་ཁར་\\</emph\\> གཞི་སྒྲིག་གསརཔ་འདི་འཐོབ་ཚུགས་ \\<emph\\>ཤོག་ལེབ་ཨང་ལུ་ལྕོགས་ཅན་བཟོ།\\</emph\\>བཏུབ་\\<emph\\>ཨེབ་གཏང་འབད།\\</emph\\>."

#: pagenumbers.xhp#par_id1654408.help.text
msgid "The new page number is an attribute of the first paragraph of the page."
msgstr "ཤོག་ལེབ་ཨང་གསརཔ་འདི་ཤོག་ལེབ་ཀྱི་མཚམས་དོན་དང་པམ་གྱི་ཁྱད་ཆོས་ཅིག་ཨིན།"

#: pagenumbers.xhp#hd_id7519150.help.text
msgid "Formatting the page number style"
msgstr "ཤོག་ལེབ་ཨང་བཟོ་རྣམ་འདི་རྩ་སྒྲིག་དོ།"

#: pagenumbers.xhp#par_id9029206.help.text
msgid "You want roman page numbers running i, ii, iii, iv, and so on."
msgstr "ཁྱོད་རོ་མཱན་ཤོག་ལེབ་ཨང་ཚུ་i, ii, iii, iv,དང་སྦེ་གཡོག་བཀོལ་དགོ་ནི་ཨིན།"

#: pagenumbers.xhp#par_id3032319.help.text
msgid "Double-click directly before the page number field. You see the \\<emph\\>Edit Fields\\</emph\\> dialog."
msgstr "ཤོག་ལེབ་ཨང་ས་སྒོ་འདིའི་ཧེ་མ་ཐད་ཀར་གཉིཧ་ལྡན་ཨེབ་གཏང་འབད། ཁྱོད་ཀྱིས་\\<emph\\>ཞུན་དག་ས་སྒོ་\\</emph\\>ཌའི་ལོག་འདི་བལྟ།"

#: pagenumbers.xhp#par_id9139378.help.text
msgid "Select a number format and click \\<emph\\>OK\\</emph\\>."
msgstr "ཨང་རྩ་སྒྲིག་ཅིག་སེལ་འཐུ་འབད་སྦེ་ \\<emph\\>བཏུབ་ཨེབ་གཏང་འབད།\\</emph\\>."

#: pagenumbers.xhp#hd_id5051728.help.text
msgid "Using different page number styles"
msgstr "ཤོག་ལེབ་ཨང་བཟོ་རྣམ་སོ་སོ་ལག་ལེན་འཐབ་དོ།"

#: pagenumbers.xhp#par_id1558885.help.text
msgid "You need some pages with the roman numbering style, followed by the remaining pages in another style."
msgstr "ཁྱོད་ལུ་ཤོག་ལེབ་ཚུ་དང་གཅིག་ཁར་རོ་མཱན་ཨང་བཟོ་རྣམ་གཞན་བཟོ་རྣམ་ནང་ལྷག་ལུས་མི་ཤོག་ལེབ་ཚུ་གིས་འབད་འཇུག་ཡོདཔ་དགོཔ་ཨིན།"

#: pagenumbers.xhp#par_id1541184.help.text
msgid "In Writer, you will need different Page Styles. The first Page Style has a footer with a page number field formatted for roman numbers. The following Page Style has a footer with a page number field formatted in another look."
msgstr "རྩོམ་འབྲིཔ་ནང་ཁྱོད་ལུ་ཤོག་ལེབ་བཟོ་རྣམ་ཚུ་སོ་སོ་དགོཔ་ཨིན། ཤོག་ལེབ་བཟོ་རྣམ་དང་པམ་འདི་ལུ་མཇུག་ཡིག་དང་གཅིག་ཁར་རོ་མཱན་ཨང་ཚུ་གི་དོན་ལུ་ཤོག་ལེབ་ཨང་ས་སྒོ་རྩ་སྒྲིག་ཡོད་མི་ཡོདཔ་ཨིན། འོག་གི་ཤོག་ལེབ་བཟོ་རྣམ་འདི་ལུ་མཇུག་ཡིག་དང་གཅིག་ཁར་གཞན་སྣང་ཚུལ་ནང་ཤོག་ལེབ་ཨང་ས་སྒོ་རྩ་སྒྲིག་ཡོད་མི་ཡོདཔ་ཨིན།"

#: pagenumbers.xhp#par_id18616.help.text
msgid "Both Page Styles must be separated by a page break. In Writer, you can have automatic page breaks and manually inserted page breaks."
msgstr "ཤོག་ལེབ་བཟོ་རྣམ་ཚུ་གཉིས་ཆ་རང་ཤོག་ལེབ་མཚམས་ཅིག་གིས་ཁ་དབྱེ་དགོཔ་ཨིན། རྩོམ་འབྲིཔ་ནང་ཁྱོད་ལུ་རང་བཞིན་ཤོག་ལེབ་མཚམས་ཚུ་དང་ལག་དེབ་སྦེ་བཙུགས་ཡོད་མི་ཤོག་ལེབ་མཚམས་ཚུ་ཡོདཔ་ཨིན།"

#: pagenumbers.xhp#par_id6138492.help.text
msgid "An \\<emph\\>automatic page break\\</emph\\> appears at the end of a page when the Page Style has a different \"next style\"."
msgstr " \\<emph\\>རང་བཞིན་ཤོག་ལེབ་མཚམས་ཅིག་\\</emph\\> ཤོག་ལེབ་བཟོ་རྣམ་འདི་སོ་སོ་ཡོདཔ་ད་ཤོག་ལེབ་ཀྱི་མཇུག་ལུ་འབྱུངམ་ཨིན \"next style\"."

#: pagenumbers.xhp#par_id4569231.help.text
msgid "For example, the \"First Page\" Page Style has \"Default\" as the next style. To see this, you may press F11 to open the \\<emph\\>Styles and Formatting\\</emph\\> window, click the \\<emph\\>Page Styles\\</emph\\> icon, right-click the First Page entry. Choose \\<emph\\>Modify\\</emph\\> from the context menu. On the Organizer tab, you can see the \"next style\"."
msgstr "དཔེར་ན་ \"First Page\" ཤོག་ལེབ་བཟོ་རྣམ་འདི་ལུ་ \"Default\" ཤུལ་མམ་གྱི་བཟོ་རྣམ་སྦེ་ཡོད། འདི་བལྟ་ནི་ལུ་ ཁྱོད་ཀྱིས་ \\<emph\\>བཟོ་རྣམ་ཚུ་དང་རྩ་སྒྲིག་འབད་ཐངས་\\</emph\\> ཝིནཌོ་ཁ་ཕྱེ་ནི་ལུ་ཨེཕ་༡༡་ཨེབ་གཏང་  \\<emph\\>ཤོག་ལེབ་བཟོ་རྣམ་ཚུ་\\</emph\\> ངོས་དཔར་ གཡས་-ཨེབ་གཏང་ཤོག་ལེབ་དང་པ་བཙུགས་འདི་ཨེབ་གཏང་འབད། སྐབས་དོན་དཀར་ཆག་འདི་ལས་\\<emph\\>ལེགས་བཅོས་\\</emph\\> གདམ།  འགོ་འདྲེན་འཐབ་ནི་མཆོང་ལྡེ་གུ་ཁྱོད་ཀྱིས་\"next style\"འདི་བལྟ།"

#: pagenumbers.xhp#par_id291451.help.text
msgid "A \\<emph\\>manually inserted page break\\</emph\\> can be applied without or with a change of page styles."
msgstr " \\<emph\\>ལག་དེབ་སྦེ་བཙུགས་ཡོད་མི་ཤོག་ལེབ་མཚམས་ཅིག་\\</emph\\>མེདཔ་སྦེ་ཡང་ན་ཤོག་ལེབ་བཟོ་རྣམ་ཚུ་གི་བསྒྱུར་བཅོས་ཅིག་དང་གཅིག་ཁར་འཇུག་སྤྱོད་འབད་བཏུབ་ཨིན། "

#: pagenumbers.xhp#par_id3341776.help.text
msgid "If you just press \\<item type=\\\"keycode\\\"\\>Ctrl+Enter\\</item\\>, you apply a page break without a change of styles."
msgstr "ཁྱོད་ཀྱིས་ \\<item type=\\\"keycode\\\"\\>ཚད་འཛིན་+བཙུགས་ཨེབ་པ་ཅིན་\\</item\\> ཁྱོད་རང་བཟོ་རྣམ་ཚུ་གི་བསྒྱུར་བཅོས་ཅིག་མེདཔ་སྦེ་ཤོག་ལེབ་མཚམས་ཅིག་འཇུག་སྤྱོད་འབད།"

#: pagenumbers.xhp#par_id5947141.help.text
msgid "If you choose \\<emph\\>Insert - Manual break\\</emph\\>, you can insert a page break without or with a change of style or with a change of page number."
msgstr "ཁྱོད་ཀྱིས་ \\<emph\\>ལག་དེབ་མཚམས་ - བཙུགས་འདི་གདམ་པ་ཅིན་\\</emph\\>ཁྱོད་རང་ཤོག་ལེབ་མཚམས་མེདཔ་སྦེ་བཙུགས་ཡང་ན་བཟོ་རྣམ་གྱི་བསྒྱུར་བཅོས་ཅིག་དང་གཅིག་ཁར་ཡང་ན་ཤོག་ལེབ་ཨང་གི་བསྒྱུར་བཅོས་ཅིག་དང་གཅིག་ཁར་བཙུགས་བཏུབ་ཨིན།"

#: pagenumbers.xhp#par_id1911679.help.text
msgid "It depends on your document what is best: to use a manually inserted page break between Page Styles, or to use an automatic change. If you just need one title page with a different style than the other pages, you can use the automatic method:"
msgstr "ཁྱོད་རའི་ཡིག་ཆ་དྲག་ཤོས་འདི་: ཤོག་ལེབ་མཚམས་བཙུགས་ཡོད་མི་ལག་དེབ་ཤོག་ལེབ་བཟོ་རྣམ་ཚུ་གི་བར་ན་ལག་ལེན་འཐབ་ནི་ལུ་ ཡང་ན་རང་བཞིན་བསྒྱུར་བཅོས་ཅིག་ལག་ལེན་འཐབ་ནི་ལུ་བརྟེནམ་ཨིན།  ཁྱོད་ལུ་གཞན་ཤོག་ལེབ་ཚུ་ལས་ཤོག་ལེབ་མགོ་མིང་དང་གཅིག་ཁར་བཟོ་རྣམ་སོ་སོ་དགོ་པ་ཅིན་ཁྱོད་རང་རང་བཞིན་ཐབས་ལམ་འདི་ལག་ལེན་འཐབ་བཏུབ་ཨིན།:"

#: pagenumbers.xhp#hd_id9364909.help.text
msgid "To apply a different page style to the first page"
msgstr "ཤོག་ལེབ་དང་པམམ་འདི་ལུ་ཤོག་ལེབ་བཟོ་རྣམ་སོ་སོ་འཇུག་སྤྱོདའབད་ནི་ལུ།"

#: pagenumbers.xhp#par_id4473403.help.text
msgid "Click into the first page of your document."
msgstr "ཁྱོད་རའི་ཡིག་ཆ་གི་ཤོག་ལེབ་དང་པམ་འདི་ནང་ཨེབ་གཏང་འབད།"

#: pagenumbers.xhp#par_id4313791.help.text
msgid "Choose \\<emph\\>Format - Styles and Formatting\\</emph\\>. "
msgstr "རྩ་སྒྲིག་-བཟོ་རྣམ་ཚུ་དང་རྩ་སྒྲིག་ཐངས་ \\<emph\\>གད་མ།\\</emph\\>. "

#: pagenumbers.xhp#par_id4331797.help.text
msgid "In the \\<emph\\>Styles and Formatting\\</emph\\> window, click the \\<emph\\>Page Styles\\</emph\\> icon."
msgstr "བཟོ་རྣམ་ཚུ་དང་རྩ་སྒྲིག་ཐངས་ \\<emph\\>ཝིནཌོ་འདི་\\</emph\\> ནང་ \\<emph\\>ཤོག་ལེབ་བཟོ་རྣམ་ཚུ་\\</emph\\> ངོས་དཔར་འདི་ཨེབ་གཏང་འབད།"

#: pagenumbers.xhp#par_id4191717.help.text
msgid "Double-click the \"First Page\" style."
msgstr " \"First Page\" བཟོ་རྣམ་འདི་གཉིས་ལྡན་ཨེབ་གཏང་འབད།"

#: pagenumbers.xhp#par_id2318796.help.text
msgid "Now your title page has the style \"First Page\", and the next pages automatically have the \"Default\" style."
msgstr "ད་ཁྱོད་ཀྱི་ཤོག་ལེབ་མགོ་མིང་འདི་ལུ་ \"First Page\"བཟོ་རྣམ་ཡོདཔ་དང་ ཤུལ་མམ་གྱི་་ཤོག་ལེབ་ཚུ་རང་བཞིན་གྱིས་ \"Default\" བཟོ་རྣམ་འདི་ཡོདཔ་ཨིན།"

#: pagenumbers.xhp#par_id399182.help.text
msgid "You can now for example insert a footer for the \"Default\" Page Style only, or insert footers to both Page Styles, but with differently formatted page number fields."
msgstr "ཁྱོད་ཀྱིས་ད་ལྟོ་མཇུག་ཡིག་ཅིག་ དཔེར་ན་\"Default\" ཤོག་ལེབ་བཟོ་རྣམ་་ཅིག་དོན་ལུརྐྱངམ་ཅིག་བཙུགས་ཡང་ན་ ཤོག་ལེབ་བཟོ་རྣམ་གཉིས་ཆ་རང་ལུ་མཇུག་ཡིག་ཚུ་བཙུགས་ཨིན་རུང་ ཤོག་ལེབ་ཨང་ས་སྒོ་སོ་སོ་སྦེ་རྩསྒྲིག་ཡོད་མི་དགོ།"

#: pagenumbers.xhp#hd_id5298318.help.text
msgid "To apply a manually inserted Page Style change"
