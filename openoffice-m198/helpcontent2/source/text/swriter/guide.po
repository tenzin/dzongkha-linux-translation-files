#: stylist_update.xhp#hd_id3155915.38.help.text
msgid "\\<variable id=\\\"stylist_update\\\"\\>\\<link href=\\\"text/swriter/guide/stylist_update.xhp\\\" name=\\\"Updating Styles From Selections\\\"\\>Updating Styles From Selections\\</link\\>\\</variable\\>"
msgstr "\\<variable id=\\\"stylist_update\\\"\\>\\<link href=\\\"text/swriter/guide/stylist_update.xhp\\\" name=\\\"Updating Styles From Selections\\\"\\>སེལ་འཐུ་འབད་ནི་ལས་བཟོ་རྣམ་ཚུ་དུས་མཐུན་འབད་དོ་\\</link\\>\\</variable\\>"

#: stylist_update.xhp#par_id3149838.40.help.text
msgid ""
"_: stylist_update.xhp#par_id3149838.40.help.text\n"
"Choose \\<emph\\>Format - Styles and Formatting\\</emph\\>."
msgstr "\\<emph\\>རྩ་སྒྲིག་ - བཟོ་རྣམ་ཚུ་དང་རྩ་སྒྲིག་འབད་ནི་\\</emph\\>གདམ་ཁ་རྐྱབས།"

#: stylist_update.xhp#par_id3156107.47.help.text
msgid "Click the icon of the style category that you want to update."
msgstr "ཁྱོད་ཀྱིས་དུས་མཐུན་བཟོ་ནི་ཨིན་མི་ བཟོ་རྣམ་གྱི་དབྱེ་རིམ་འདི་གི་ངོས་དཔར་འདི་ཨེབ་གཏང་།"

#: stylist_update.xhp#par_id3149283.41.help.text
msgid "In the document, click where you want to copy the updated style from. For example, click a paragraph that you applied manual formatting to."
msgstr "ཡིག་ཆ་འདི་ནང་དུས་མཐུན་བཟོ་ཡོད་མི་བཟོ་རྣམ་ལས་འདྲ་བཤུས་འབད་ནིཨིན་ནཨེབ་གཏང་འབད། དཔེར་ན་མཚམས་དོན་ཅིག་ཁྱོད་ཀྱིས་ལག་དེབ་སྦེ་རྩ་སྒྲིག་འབད་ནི་ལུ་འཇུག་སྤྱོད་འབད་མི་ནང་ཨེབ་གཏང་འབད།"

#: stylist_update.xhp#par_id3153402.42.help.text
msgid "In the Styles and Formatting window, click the style that you want to update."
msgstr "ཝིནཌོ་རྩ་སྒྲིག་དང་བཟོ་རྣམ་ཚུ་ནང་ཁྱོད་རང་དུས་མཐུན་བཟོ་ནི་ཨིན་མི་བཟོ་རྣམ་འདི་ཨེབ་གཏང་འབད།"

#: stylist_update.xhp#par_id3153119.43.help.text
msgid "Click the arrow next to the \\<emph\\>New Style from Selection\\</emph\\> icon and choose \\<emph\\>Update Style\\</emph\\> from the submenu."
msgstr " \\<emph\\>སེལ་འཐུ་ངོས་དཔར་ལས་བཟོ་རྣམ་གསརཔ་་འདིའི་ཤུལ་མའི་མདའ་རྟགས་འདི་\\</emph\\>ཨེབ་གཏང་འབད་ནི་དང་ \\<emph\\>དཀར་ཆག་འོག་མ་འདི་ལས་\\</emph\\> བཟོ་རྣམ་དུས་མཐུན་བཟོ་གདམ།"

#: stylist_update.xhp#par_id3155498.45.help.text
msgid ""
"_: stylist_update.xhp#par_id3155498.45.help.text\n"
"\\<link href=\\\"text/swriter/01/05140000.xhp\\\" name=\\\"Styles and Formatting\\\"\\>Styles and Formatting\\</link\\>"
msgstr "\\<link href=\\\"text/swriter/01/05140000.xhp\\\" name=\\\"Styles and Formatting\\\"\\>བཟོ་རྣམ་ཚུ་དང་རྩ་སྒྲིག་འབད་ནི་ཚུ་\\</link\\>"

#: subscript.xhp#tit.help.text
msgid "Making Text Superscript or Subscript"
msgstr "ཚིག་ཡིག་སྟེང་ཡིག་ཡང་ན་འོག་ཡིག་བཟོ་དོ།"

#: subscript.xhp#bm_id3155174.help.text
msgid "\\<bookmark_value\\>text; subscript and superscript\\</bookmark_value\\>\\<bookmark_value\\>superscript text\\</bookmark_value\\>\\<bookmark_value\\>subscript text\\</bookmark_value\\>\\<bookmark_value\\>characters;subscript and superscript\\</bookmark_value\\>"
msgstr "\\<bookmark_value\\>ཚིག་ཡིག་ འོག་ཡིག་དང་སྟེང་ཡིག་\\</bookmark_value\\>\\<bookmark_value\\>སྟེང་ཡིག་ཚིག་ཡིག་\\</bookmark_value\\>\\<bookmark_value\\>འོག་ཡིག་ཚིག་ཡིག་\\</bookmark_value\\>\\<bookmark_value\\>ཡིག་འབྲུ་ཚུ་ འོག་ཡིག་དང་སྟེང་ཡིག་\\</bookmark_value\\>"

#: subscript.xhp#hd_id3155174.1.help.text
msgid "\\<variable id=\\\"subscript\\\"\\>\\<link href=\\\"text/swriter/guide/subscript.xhp\\\" name=\\\"Making Text Superscript or Subscript\\\"\\>Making Text Superscript or Subscript\\</link\\>\\</variable\\>"
msgstr "\\<variable id=\\\"subscript\\\"\\>\\<link href=\\\"text/swriter/guide/subscript.xhp\\\" name=\\\"Making Text Superscript or Subscript\\\"\\>ཚིག་ཡིག་ སྟེང་ཡིག་ཡང་ན་འོག་ཡིག་བཟོ་དོ་\\</link\\>\\</variable\\>"

#: subscript.xhp#par_id3155917.2.help.text
msgid "Select the text that you want to make superscript or subscript."
msgstr "ཁྱོད་ཀྱིས་སྟེང་ཡིག་ཡང་ན་འོག་ཡིག་བཟོ་དགོ་མནོ་མི་ཚིག་ཡིག་འདི་སེལ་འཐུ་འབད།"

#: subscript.xhp#par_id3155865.28.help.text
msgid ""
