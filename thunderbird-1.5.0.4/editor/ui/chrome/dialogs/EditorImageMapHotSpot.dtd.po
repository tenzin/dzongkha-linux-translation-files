# Window title
#: windowTitle.label
msgid "Hotspot Properties"
msgstr "སྤྲོ་གནས་(ཧོཊི་སིཔོཊི)ཀྱི་རྒྱུ་དངོས་"

#: link.label
msgid "Hotspot Link"
msgstr "སྤྲོ་གནས་ཀྱི་འབྲེལ་ལམ་"

#: urlFieldset.label
msgid "Hotspot URL:"
msgstr "སྤྲོ་གནས་ཀྱི་ཡུ་ཨར་ཨེལ་:"

#: altFieldset.label
msgid "Hotspot Alt Text:"
msgstr "སྤྲོ་གནས་ཀྱི་ཚིག་ཡིག་ཚབ་:"

#: browseButton.label
msgid "Browse..."
msgstr "བརྡ་འཚོལ་་་་་"

#: targetFieldset.label
msgid "Target Setting"
msgstr "དམིགས་གཏད་སྒྲིག་སྟངས་"

#: target.label
msgid "Hotspot Target"
msgstr "སྤྲོ་གནས་ཀྱི་དམིགས་གཏད་"

# Targets
#: commonFieldset.label
msgid "Common Targets:"
msgstr "མཐུན་མོང་གི་དམིགས་གཏད་:"

#: none.value
msgid "(none)"
msgstr "(ཅི་མེད་)"

#: same.value
msgid "Same Frame"
msgstr "གཞི་ཁྲམ་གཅིག་པ་"

#: whole.value
msgid "Whole Page"
msgstr "ཤོག་ལེབ་ཧྲིལ་བུ་"

#: new.value
msgid "New Window"
msgstr "སྒོ་སྒྲིགགསརཔ་"

#: parent.value
msgid "Parent Frame"
msgstr "རྩ་ལག་གི་མིང་"

