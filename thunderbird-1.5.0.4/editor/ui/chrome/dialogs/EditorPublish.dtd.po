# Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "ཤོག་ལེབ་དཔེ་བསྐྲུན་འབད་"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "སྒྲིག་སྟངས་དཔེ་བསྐྲུན་འབད་"

#: publishTab.label
#: publishButton.label
msgid "Publish"
msgstr "དཔེ་བསྐྲུན་འབད་"

#: settingsTab.label
msgid "Settings"
msgstr "སྒྲིག་སྟངས་"

# Publish Tab Panel
#: siteList.label
#: siteName.label
msgid "Site Name:"
msgstr "ས་ཁོངས་ཀྱི་མིང་:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "ཁྱོད་ཀྱིས་དཔེ་བསྐྲུན་འབད་དགོ་མནོ་སའི་ས་ཁོངས་དེ་ གདམ་ཁ་རྐྱབས།"

#: newSiteButton.label
msgid "New Site"
msgstr "ས་ཁོངས་གསརཔ་"

#: siteDirList.label
msgid "Select or type a directory name in your publishing site:"
msgstr "ཁྱོད་རའི་དཔེ་བསྐྲུན་ས་ཁོངས་ནང་ སྣོད་ཐོའི་མིང་ཅིག་ སེལ་འཐུ་འབད་ ཡང་ན་ ཡིག་དཔརརྐྱབས་:"

#: siteDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory to publish to"
msgstr "དཔེ་བསྐྲུན་འབད་སའི་ ཐག་རིང་ཡན་ལག་སྣོད་ཐོ་དེ་གི་མིང་ གདམ་ཁ་རྐྱབས་ ཡང་ན་ བཙུགས།"

#: docDirList.label
msgid "Site subdirectory for this page:"
msgstr "ཤོག་ལེབ་འདི་གི་དོན་ལུ་ ས་ཁོངས་ཀྱི་ཡན་ལག་སྣོད་ཐོ་:"

#: publishImgCheckbox.label
msgid "Include images and other files"
msgstr "གཟུགས་བརྙན་དང་ ཡིག་སྣོད་གཞན་ཚུ་ གྲངས་སུ་བཙུགས་"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr "ཤོག་ལེབ་འདི་གིས་གཞི་བསྟུན་འབད་མི་ གཟུགས་བརྙན་དང་ ཡིག་སྣོད་གཞན་ཚུ་ དཔེ་བསྐྲུན་འབད་"

#: sameLocationRadio.label
msgid "Use same location as page"
msgstr "ཤོག་ལེབ་དང་གཅིག་པའི་གནས་ཁོངས་ལག་ལེན་འཐབ།"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "ཤོག་ལེབ་དང་གཅིག་པའི་གནས་ཁོངས་ལུ་ ཡིག་སྣོད་ཚུ་དཔེ་བསྐྲུན་འབད་"

#: useSubdirRadio.label
msgid "Use this site subdirectory:"
msgstr "ས་ཁོངས་འདི་གི་ཡན་ལག་སྣོད་འཛིན་ལག་ལེན་འཐབ་:"

#: useSubdir.tooltip
msgid "Choose or enter name of remote subdirectory where files will be published"
msgstr "ཡིག་སྣོད་ཚུ་དཔེ་བསྐྲུན་འབད་སའི་ ཐག་རིང་སྣོད་ཐོའི་མིང་ གདམ་ཁ་རྐྱབས་ ཡང་ན་ མིང་བཙུགས་"

#: chooseDirButton.label
msgid "Choose..."
msgstr "གདམ་ཁ་རྐྱབས་་་་"

#: pageTitle.label
msgid "Page Title:"
msgstr "ཤོག་ལེབ་ཀྱི་མགོ་མིང་:"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr "སྒོ་སྒྲིག་ནང་ན་དང་ དེབ་རྟགས་ནང་ན་ ཤོག་ལེབ་འདི་ངོེས་འཛིན་འབད་ནི་ལུ་ མགོ་མིང་ཅིག་བཙུགས།"

#: pageTitleExample.label
msgid "e.g.: &quot;My Web Page&quot;"
msgstr "དཔེར་ན་:&quot;My Web Page&quot;"

#: filename.label
msgid "Filename:"
msgstr "ཡིག་སྣོད་མིང་:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr "ཝེབ་ཤོག་ལེབ་ཀྱི་  '.html' བརྩིས་ཏེ་ ཡིག་སྣོད་འདི་གི་དོན་ལུ་ མིང་ཅིག་བཙུགས།"

#: filenameExample.label
msgid "e.g,: &quot;mypage.html&quot;"
msgstr "དཔེར་ན་:&quot;mypage.html&quot;"

#: relatedDocs.label
msgid "Other files to publish"
msgstr "དཔེ་བསྐྲུན་འབད་ནིའི་ཤོག་ལེབ་གཞན་"

#: setDefaultButton.label
msgid "Set as Default"
msgstr "སྔོན་སྒྲིག་སྦེ་ གཞི་སྒྲིག་འབད་"

#: setDefSiteAndDir.label
msgid "Set as Default Site and Directory"
msgstr "སྔོན་སྒྲིག་ས་ཁོངས་དང་ སྣོད་ཐོ་སྦེ་ གཞི་སྒྲིག་འབད་"

#: removeButton.label
msgid "Remove Site"
msgstr "ས་ཁོངས་རྩ་བསྐྲད་གཏང་"

# Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "ས་ཁོངས་ཚུ་དཔེ་བསྐྲུན་འབད་དོ་"

#: serverInfo.label
msgid "Server Information"
msgstr "སར་བར་གྱི་བརྡ་དོན་"

#: loginInfo.label
msgid "Login Information"
msgstr "ནང་བསྐྱོད་བརྡ་དོན་"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr "དཔེ་བསྐྲུན་ས་ཁོངས་འདི་་ངོས་འཛིན་འབད་མི་ སྐྱོན་མིང་ཅིག་ (དཔེར་ན་: 'MySite')"

#: siteUrl.label
msgid "Publishing address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr "དཔེ་བསྐྲུན་ཁ་བྱང་ (དཔེར་ན་.: 'ftp://ftp.myisp.com/myusername'):"

#: siteUrl.tooltip
msgid "The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr "ཁྱོད་རའི་ཨིན་ཊར་ནེཊི་ཞབས་ཏོག་བྱིན་མི་ ཡང་ན་ ཝེབ་ཧོསི་ཊིང་ཞབས་ཏོག་གིས་བྱིན་མི་ཁ་བྱང་ FTP:// or HTTP:// "

#: browseUrl.label
msgid "HTTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr "ཁྱོད་རའི་ཁྱིམ་གྱི་ཤོག་ལེབ་ཀྱི་ ཨེཆ་ཊི་ཊི་པི་ཁ་བྱང་ (དཔེར་ན་: 'http://www.myisp.com/myusername'):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr "ཁྱོད་རའི་ཁྱིམ་གྱི་སྣོད་ཐོའི་ཁ་བྱང་ HTTP://་(ཡིག་སྣོད་མིང་གྲངས་སུ་མ་བཙུགས)"

#: username.label
msgid "User name:"
msgstr "ལག་ལེན་པའི་མིང་:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr "ཁྱོད་རའི་ཨིན་ཊར་ཞབས་ཏོག་བྱིན་མི་ ཡང་ན་ ཝེབ་ཧོསི་ཊིང་ཞབས་ཏོག་ནང་ལུ་ ནང་བསྐྱོད་འབད་ནི་ལུ་ ལག་ལེན་འཐབ་མི་ ལག་ལེན་པའི་མིང་"

#: password.label
msgid "Password:"
msgstr "ཆོག་ཡིག་:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "ཁྱོད་ཀྱི་ལག་ལེན་པའི་མིང་དང་ འབྲེལ་བ་ཡོད་པའི་ཆོག་ཡིག་དེ་"

#: savePassword.label
msgid "Save Password"
msgstr "ཆོག་ཡིག་སྲུངས་"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr "ཆོག་ཡིག་འཛིན་སྐྱོང་པ་ལག་ལེན་འཐབ་པའི་ཐོག་ལས་ ཁྱོད་རའི་ཆོག་ཡིག་མཐའ་བཙན་སྦེ་བསྲུང་ནི་ལུ་ འདི་སེལ་འཐུ་འབད།"

