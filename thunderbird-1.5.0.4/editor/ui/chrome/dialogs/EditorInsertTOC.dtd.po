#: Window.title
msgid "Table of Contents"
msgstr "ནང་དོན་དཀར་ཆག་"

#: buildToc.label
msgid "Build table of contents from:"
msgstr "ནང་ལས་ནང་དོན་དཀར་ཆག་བཟོ་:"

#: style.label
msgid "Style:"
msgstr "བཟོ་རྣམ་:"

#: tag.label
msgid "Tag:"
msgstr "ངོ་རྟགས་:"

#: class.label
msgid "Class:"
msgstr "དབྱེ་རིགས་:"

#: header1.label
msgid "Level 1"
msgstr "གནས་རིམ་ ༡་"

#: header2.label
msgid "Level 2"
msgstr "གནས་རིམ་ ༢་"

#: header3.label
msgid "Level 3"
msgstr "གནས་རིམ་ ༣་"

#: header4.label
msgid "Level 4"
msgstr "གནས་རིམ་ ༤་"

#: header5.label
msgid "Level 5"
msgstr "གནས་རིམ་ ༥་"

#: header6.label
msgid "Level 6"
msgstr "གནས་རིམ་ ༦་"

#: makeReadOnly.label
msgid "Make the table of contents read-only"
msgstr "ནང་དོན་དཀར་ཆག་དེ་ལྷག་ནི་ཙམ་བཟོ་"

#: orderedList.label
msgid "Number all entries in the table of contents"
msgstr "ནང་དོན་དཀར་ཆག་ནང་གི་ ཐོ་བཀོད་ཆ་མཉམ་ལུ་ ཨང་གྲངས་བཏགས།"

