# Window title
#: windowTitle.label
msgid "Check Image and Link Locations"
msgstr "གཟུགས་བརྙན་དང་འབྲེལ་ལམ་གནས་ཁོངས་ལུ་ ཞིབ་དཔྱད་འབད།"

#: urlsNotFound.label
msgid "The following image or link locations were not found:"
msgstr "འོག་གིགཟུགས་བརྙན་དང་ འབྲེལ་ལམ་གནས་ཁོངས་ཚུ་ཐོབ་མ་ཚུགས་:"

#: notFoundKey.label
msgid "(&quot;X&quot; = file not found, &quot;?&quot; = timed-out attempting to connect.)"
msgstr "(&quot;X&quot; = ཡིག་སྣོད་མ་ཐོབ།  &quot;?&quot; = ངལ་མཚམས་ མཐུད་ནི་ལུ་རྩ་འགེངས་དོ་)"

#: changeUrlButton.label
msgid "Change Location of Selected Item"
msgstr "སེལ་འཐུ་འབད་མི་རྣམ་གྲངས་ཚུ་གི་གནས་ཁོངས་སོར་"

#: succeeded.label
msgid "Succeeded"
msgstr "མཐའ་འཁྱོལ་ཡོད་"

#: failed.label
msgid "Failed"
msgstr "མཐའ་མ་འཁྱོལ་"

#: closeButton.label
msgid "Close"
msgstr "ཁ་བསྡམས་"

