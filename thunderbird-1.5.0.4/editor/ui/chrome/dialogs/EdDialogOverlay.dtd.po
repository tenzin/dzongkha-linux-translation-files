#: AdvancedEditButton.label
#: AdvancedEditButton.accessKey
msgid "Advanced &Edit..."
msgstr "མཐོ་རིམ་ཞུན་དག་་་(&E)"

#: AdvancedEditButton.tooltip
msgid "Add or modify HTML attributes, style attributes, and JavaScript"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ཁྱད་ཆོས་དང་ བཟོ་རྣམ་ཁྱད་ཆོས་ ཇ་བ་སི་ཀིརིཔཊི་ ཁ་སྐོང་རྐྱབས་ ཡང་ན་ ལེགས་བཅོས་འབད།"

#: chooseButton.label
msgid "Choose File..."
msgstr "ཡིག་སྣོད་གདམ་ཁ་རྐྱབས་་་་"

#: chooseFile.accessKey
msgid "F"
msgstr "F"

#: chooseFileLink.accessKey
msgid "o"
msgstr "o"

#: makeUrlRelative.label
#: makeUrlRelative.accessKey
msgid "U&RL is relative to page location"
msgstr "ཡུ་ཨར་ཨེལ་དེ་ ཤོག་ལེབ་ཀྱི་གནས་ཁོངས་དང་འབྲེལ་བ་ཡོད་(&R)"

#: makeUrlRelative.tooltip
msgid "Change between relative and absolute URL. You must first save the page to change this."
msgstr "འབྲེལ་བའི་ཡུ་ཨར་ཨེལ་དང་ ཡང་དག་གི་ཡུ་ཨར་ཨེལ་གྱི་བར་ན་སོར། འདི་སོར་ནིའི་དོན་ལུ་ ཁྱོད་ཀྱིས་དང་པ་ར་ ཤོག་ལེབ་འདི་བསྲུང་དགོ།"

# Shared by Link and Image dialogs
#: LinkURLEditField.label
#: LinkURLEditField.accessKey
msgid "Enter a web page &location, a local file, or select a Named Anchor or Heading from the popup list:"
msgstr "ཝེབ་ཤོག་ལབ་ཀྱི་གནས་ཁོངས་ཅིག་ ཉེ་གནས་ཀྱི་ཡིག་སྣོད་ཅིག་བཙུགས་ ཡང་ན་ པོཔ་ཨཔ་ཐོ་ཡིག་ནང་ལས་ མིང་བཏགས་ཡོད་པའི་ཨེན་ཀོར་ཅིག་ ཡང་ན་ མགོ་རྒྱན་ཅིག་སེལ་འཐུ་འབད་:"

