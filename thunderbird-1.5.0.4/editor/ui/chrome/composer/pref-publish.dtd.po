# extracted from content/pref-publish.xul
# LOCALIZATION NOTE : FILE The Publish preferences screen
#: window.title
#: composerHeader.label
msgid "Publish"
msgstr "དཔེ་བསྐྲུན་འབད་"

#: maintainCheckbox.label
msgid "Maintain links"
msgstr "འབྲེལ་ལམ་རྒྱུན་སྐྱོང་འཐབ་"

#: keepCheck.label
msgid "Keep images with page"
msgstr "གཟུགས་བརྙན་ཚུ་ ཤོག་ལེབ་དང་གཅིག་ཁར་བཞག་"

#: adjustDesc.label
msgid ""
"_: Attempt to maintain approximate line length, linefeeds and white space\n"
"Adjust links to work from the page's location. (Links to local pages will work when \t\t\t      local versions exist.)           "
msgstr "ཤོག་ལེབ་ཀྱི་གནས་ཁོངས་ནང་ལས་ ལཱ་འབད་ནིའི་དོན་ལུ་ འབྲེལ་ལམ་ཚུ་ བདེ་སྒྲིག་འབད། (ཉེ་གནས་ཀྱི་ཤོག་ལེབ་ཚུ་གིས་ \t\t\t  ཉེ་གནས་ཐོན་རིམ་ཡོད་པའི་སྐབས་ ལཱ་འབད་འོང་)"

#: saveDesc.label
msgid ""
"_: Attempt to maintain approximate line length, linefeeds and white space\n"
"Save copies of images to the page's location.  Images will always \t\t\t\tappear in local versions of the page and when the page is published."
msgstr "གཟུགས་བརྙན་གྱི་འདྲ་བཤུས་ཚུ་ ཤོག་ལེབ་ཀྱི་གནས་ཁོངས་ནང་སྲུངས། ཤོག་ལེབ་དེ་དཔེ་བསྐྲུན་འབདཝ་ད་ གཟུགས་བརྙན་ཚུ་ ཨ་རྟག་ར་ ཉེ་གནས་ཀྱི་ཐོན་རིམ་ \t\t\t\tappear ནང་འཆརཝ་ཨིན།"

#: tip.label
msgid "Tip: Check both options if you will be using remote publishing."
msgstr "ཕན་བསླབ་: ཁྱོད་ཀྱིས་ཐག་རིང་དཔེ་བསྐྲུན་ལག་ལེན་འཐབ་ནི་ཨིན་པ་ཅིན་ གདམ་ཁ་གཉིས་ཆ་ར་ལུ་ ཨིན་རྟགས་བཀལ།"

#: defaultHeader.label
msgid "Default publishing location"
msgstr "སྔོན་སྒྲིག་དཔེ་བསྐྲུན་གནས་ཁོངས་"

#: publishFtp.label
#: publishhtp.label
msgid ""
"_: Edit box appears beside this label\n"
"If publishing to a FTP site, enter the HTTP address to browse to:"
msgstr "ཨེཕ་ཊི་པི་ས་ཁོངས་ལུ་ དཔེ་བསྐྲུན་འབད་ནི་ཨིན་པ་ཅིན་ བརྡ་འཚོལ་འབད་ནི་ལུ་ ཨེཆ་ཊི་ཊི་པི་ ཁ་བྱང་བཙུགས།"

