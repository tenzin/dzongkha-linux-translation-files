#: lHeader
msgid "Composer"
msgstr "རྩོམ་མཁན་"

#: rHeader
msgid "Settings for authoring of new Web pages"
msgstr "ཝེབ་ཤོག་ལེབ་གསརཔ་ རྩོམ་ནིའི་དོན་ལུ་ སྒྲིག་སྟངས་ཚུ་"

#: saving
msgid "Saving"
msgstr "སྲུང་དོ་"

#: AutoSaveCheck
msgid "Automatically save every"
msgstr "རང་བཞིན་གྱིས་རེ་ནང་སྲུངས་"

#: minText
msgid "minutes"
msgstr "སྐར་མ་"

#: exterLegend.label
msgid "External Editors"
msgstr "ཕྱིའི་ཞུན་དགཔ་ཚུ་"

#: htmlSource
msgid "HTML Source:"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ འབྱུང་ཁུངས་:"

#: imageeditor
msgid "Images:"
msgstr "གཟུགས་བརྙན་ཚུ་:"

#: chooseButton.label
msgid "Choose"
msgstr "གདམ་ཁ་རྐྱབས་"

#: recentFiles.title
msgid "Recent Pages Menu"
msgstr "འཕྲལ་ཁམས་ཀྱི་ཤོག་ལེབ་དཀར་ཆག་"

#: documentsInMenu.label
#: documentsInMenu.accesskey
msgid "Maximum &number of pages listed:"
msgstr "ཤོག་གྲངས་ཐོ་བཀོད་མང་མཐའ་:(&n)"

#: savingFiles.title
msgid "When Saving or Publishing Pages"
msgstr "སྲུང་བའི་སྐབས་ལུ་ ཡང་ན་ དཔེ་བསྐྲུན་གྱི་སྐབས་ལུ་"

#: preserveExisting.label
#: preserveExisting.accesskey
msgid "&Preserve original source formatting"
msgstr "ཐོག་མའི་འབྱུང་ཁུངས་རྩ་སྒྲིག་ ཉམས་སྲུང་འབད་(&P)"

#: preserveExisting.tooltip
msgid "Preserves line breaks and page's original formatting"
msgstr "གྱལ་རིམ་མཚམས་དང་ ཤོག་ལེབ་ཀྱི་འགོ་ཐོག་རྩ་སྒྲིག་ ཉམས་སྲུང་འབད་"

#: saveAssociatedFiles.label
#: saveAssociatedFiles.accesskey
msgid "&Save images and other associated files when saving pages"
msgstr "ཤོག་ལེབ་སྲུང་པའི་སྐབས་ལུ་ གཟུགས་བརྙན་དང་ འབྲེལ་ཡོད་ཡིག་སྣོད་གཞན་ཚུ་སྲུངས་(&S)"

#: showPublishDialog.label
#: showPublishDialog.accesskey
msgid "&Always show Publish dialog when publishing pages"
msgstr "ཨ་རྟག་ར་ ཤོག་ལེབ་དཔེ་བསྐྲུན་འབད་བའི་སྐབས་ལུ་ དཔེ་བསྐྲུན་གྱི་ཌའི་ལོག་སྟོན་(&A)"

#: composerEditing.label
msgid "Editing"
msgstr "ཞུན་དག་རྐྱབ་དོ་"

#: maintainStructure.label
#: maintainStructure.accesskey
msgid "&Maintain table layout when inserting or deleting cells"
msgstr "ནང་ཐིག་ཚུ་བཙུགས་པའི་སྐབས་དང་ བཏོན་གཏང་པའི་སྐབས་ལུ་ ཐིག་ཁྲམ་གྱི་སྒྲིག་བཀོད་ རྒྱུན་སྐྱོང་འཐབ་(&M)"

#: maintainStructure.tooltip
msgid "Preserves table's rectangular shape by automatically adding cells after inserting or deleting cells"
msgstr "ནང་ཐིག་ཚུ་བཙུགས་པའམ་ བཏོན་བཏང་པའི་ཤུལ་ལུ་ རང་བཞིན་གྱིས་ ནང་ཐིག་ཁ་སྐོང་རྐྱབ་ཐོག་ལས་ ཐིག་ཁྲམ་གྱི་བཟོ་རྣམ་ གྲུ་བཞི་ནར་མོ་ཉམས་སྲུང་འབད་"

#: useCSS.label
#: useCSS.accesskey
msgid "&Use CSS styles instead of HTML elements and attributes"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ཆ་ཤས་དང་ ཁྱད་ཆོས་ཚུ་གི་ཚབ་ལུ་ སི་ཨེསི་ཨེསི་བཟོ་རྣམ་ཚུ་ ལག་ལེན་འཐབ་(&U)"

#: crInPCreatesNewP.label
#: crInPCreatesNewP.accesskey
msgid "&Return in a paragraph always creates a new paragraph"
msgstr "དོན་མཚམས་ཅིག་ནང་ ལོག་ལྡེ་ཨེབ་པ་ཅིན་ ཨ་རྟག་ར་ དོན་མཚམས་གསརཔ་ཅིག་ གསར་བསྐྲུན་འབདཝ་ཨིན་ (&R)"

