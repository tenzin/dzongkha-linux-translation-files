#: SSLProtocolVersions
msgid "SSL Protocol Versions"
msgstr "ཨེསི་ཨེསི་ཨེལ་ གནས་སྤེལ་ལམ་ལུགས་ཐོན་རིམ་"

#: SSLWarnings
msgid "SSL Warnings"
msgstr "ཨེསེ་ཨེསི་ཨེལ་ཉེན་བརྡ་ཚུ་"

#: SSLClientAuthMethod
msgid "Client Certificate Selection"
msgstr "ཞབས་ཏོག་སྤྱོད་མིའི་ལག་ཁྱེར་སེལ་འཐུ།"

#: prefheader.title
msgid "Secure Sockets Layer (SSL)"
msgstr "མཐའ་བཙན་སོ་ཀེཊི་བང་རིམ་ (ཨེསི་ཨེསི་ཨེལ་)"

#: enable.ssl2
msgid "Enable SSL version 2"
msgstr "ཨེསི་ཨེསི་ཨེལ་ ཐོན་རིམ་ ༢ པ་ ལྕོགས་ཅན་བཟོ་"

#: enable.ssl3
msgid "Enable SSL version 3"
msgstr "སི་ཨེསི་ཨེལ་ ཐོན་རིམ་ ༣ པ་ ལྕོགས་ཅན་བཟོ་"

#: enable.tls
msgid "Enable TLS"
msgstr "ཊི་ཨེལ་ཨེསི་ ལྕོགས་ཅན་བཟོ་"

#: edit.sslciphers
msgid "Edit Ciphers..."
msgstr "གསང་ཡིག་ཚུ་ཞུན་དག་རྐྱབས་་་་"

#: edit.sslciphers.accesskey
msgid "E"
msgstr "E"

#: warn.description
msgid "&brandShortName; can alert you to the security status of the web page you are viewing. Set &brandShortName; to show a warning and ask permission before:"
msgstr "&brandShortName; གིས་ ཁྱོད་ཀྱིས་བལྟ་བའི་བསྒང་ཡོད་པའི་ ཝེབ་ཤོག་ལེབ་དེ་གི་ སྲུང་སྐྱོབ་གནས་ཚད་སྐོར་ དྲན་བརྡ་འབད་ཚུགས། ཉེན་བརྡ་སྟོན་ནི་དང་ གནང་བ་ལེན་ནིའི་དོན་ལུ་ &brandShortName; གཞི་སྒྲིག་འབད།"

#: warn.enteringsecure
msgid "Loading a page that supports encryption"
msgstr "གསང་བཟོ་ལུ་རྒྱབ་སྐྱོར་འབད་མི་ ཤོག་ལེབ་ཅིག་མངོན་གསལ་འབད་དོ།"

#: warn.enteringweak
msgid "Loading a page that uses low-grade encryption"
msgstr "སྤུས་ཚད་དམའ་བའི་གསང་བཟོ་ ལག་ལེན་འཐབ་མི་ ཤོག་ལེབ་ཅིག་མངོན་གསལ་འབད་དོ།"

#: warn.insecurepost
msgid "Sending form data from an unencrypted page to an unencrypted page"
msgstr "གསང་བཟོས་མེན་པའི་ཤོག་ལེབ་ཅིག་ནང་ལས་ གསང་བཟོས་མེན་པའི་ཤོགལེབ་ཅིག་ནང་ འབྲི་ཤོག་གི་བརྡ་དོན་གཏང་དོ།"

#: warn.leavingsecure
msgid "Leaving a page that supports encryption"
msgstr "གསང་བཟོ་ལུ་རྒྱབ་སྐྱོར་འབད་མི་ ཤོག་ལེབ་ཅིག་ བཞག་འགྱོ་དོ།"

#: warn.secureredirect
msgid "Redirecting from one encrypted page to another"
msgstr "གསང་བཟོས་ཤོག་ལེབ་གཅིག་ལས་ གཞན་མི་ལུ་ སླར་ལོག་གཏང་དོ།"

#: warn.secureredirecttoinsecure
msgid "Redirecting from an encrypted page to an unencrypted page"
msgstr "གསང་བཟོས་ཤོག་ལེབ་ཅིག་ལས་ གསང་བཟོས་མེན་པའི་ཤོག་ལེབ་ཅིག་ནང་ སླར་ལོག་གཏང་དོ།"

#: warn.viewmixed
msgid "Viewing a page with an encrypted/unencrypted mix"
msgstr "གསང་བཟོས་/གསང་བཟོས་མེན་པའི་ བསྲེ་སྦྱོར་ཅིག་གི་ཐོག་ལས་ ཤོག་ལེབ་ཅིག་ལུ་བལྟ་དོ།"

#: certselect.description
msgid "Decide how &brandShortName; selects a security certificate to present to web sites that require one:"
msgstr "སྲུང་སྐྱོབ་ལག་ཁྱེར་དགོ་མི་ ཝེབ་ས་ཁོངས་ཚུ་ལུ་སྤྲོད་ནིའི་དོན་ལུ་ &brandShortName;གིས་ སྲུང་སྐྱོབ་ལག་ཁྱེར་ཅིག་ ག་དེ་སྦེ་ སེལ་འཐུ་འབདཝ་ཨིན་ན་ གྲོས་ཐག་གཅད།"

#: certselect.auto
msgid "Select Automatically"
msgstr "རང་བཞིན་གྱིས་ སེལ་འཐུ་འབད་"

#: certselect.ask
msgid "Ask Every Time"
msgstr "ཨ་རྟག་ར་དྲིས་"

# Cipher pref window
#: cipher.title
msgid "SSL: Edit Ciphers"
msgstr "ཨེསི་ཨེསི་ཨེལ་: གསང་ཡིག་ཞུན་དག་རྐྱབས་"

#: cipher.ssl2.label
msgid "SSL2 Cipher Suites"
msgstr "ཨེསི་ཨེསི་ཨེལ་༢ ཀྱི་གསང་ཡིག་ཆ་ཚང་"

#: cipher.ssl3.label
msgid "SSL3/TLS Cipher Suites"
msgstr "ཨེསི་ཨེསི་ཨེལ་ ༣ གྱི་གསང་ཡིག་ཆ་ཚང་"

#: cipher.ssl3_extra.label
msgid "Extra SSL3/TLS Cipher Suites"
msgstr "ཨེསི་ཨེསི་ཨེལ་༣/ཊི་ཨེལ་ཨེསི་གི་ གསང་ཡིག་ཆ་ཚང་"

#: cipher.tab.ssl2
msgid "SSL2"
msgstr "ཨེསི་ཨེསི་ཨེལ་༢་"

#: cipher.tab.ssl3tls
msgid "SSL3/TLS"
msgstr "ཨེསི་ཨེསི་ཨེལ་༣/ཊི་ཨེལ་ཨེསི་"

#: cipher.tab.ssl3tls_extra
msgid "Extra SSL3/TLS"
msgstr "ཨེསི་ཨེསི་ཨེལ་༣/ཊི་ཨེལ་ཨེསི་ཐེབས་"

# SSL2 Ciphers
# SSL3/TLS ciphers
#: cipher.ssl2.rc4_128
#: cipher.ssl3.rsa_rc4_128_md5
msgid "128-bit RC4 encryption with RSA and an MD5 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེམ་ཌི་༥ མེཀ་ཅིག་དང་བཅས་པའི་ བིཊི་-༡༢༨ ཨར་སི་༤ གི་གསང་བཟོ།"

#: cipher.ssl2.rc2_128
msgid "128-bit RC2 encryption with RSA and an MD5 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེམ་ཌི་༥ མེཀ་ཅིག་དང་བཅས་པའི་ བིཊི་-༡༢༨ ཨར་སི་༢ ཀྱི་གསང་བཟོ།"

#: cipher.ssl2.des_ede3_192
msgid "168-bit Triple DES encryption with RSA and MD5 MAC "
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེམ་ཌི་༥ མེཀ་དང་བཅས་པའི་ བིཊི་-༡༦༨ ཌི་ཨི་ཨེསི་གསུམ་ཅན་གྱི་གསང་བཟོ།"

#: cipher.ssl2.des_64
msgid "56-bit DES encryption with RSA and an MD5 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེམ་ཌི་༥ མེཀ་ཅིག་དང་བཅས་པའི་ བིཊི་-༥༦ ཌི་ཨི་ཨེསི་གི་གསང་བཟོ།"

#: cipher.ssl2.rc4_40
#: cipher.ssl3.rsa_rc4_40_md5
msgid "40-bit RC4 encryption with RSA and an MD5 MAC (export)"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེམ་ཌི་༥ མེཀ་(ཕྱིར་འདྲེན)ཅིག་དང་བཅས་པའི་ བིཊི་-༤༠ ཨར་སི་༤གི་་གསང་བཟོ།"

#: cipher.ssl2.rc2_40
#: cipher.ssl3.rsa_rc2_40_md5
msgid "40-bit RC2 encryption with RSA and an MD5 MAC (export)"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེམ་ཌི་༥ མེཀ་(ཕྱིར་འདྲེན་)ཅིག་དང་བཅས་པའི་ བིཊི་-༤༠ ཨར་སི་༢ ཀྱི་གསང་བཟོ།"

#: cipher.ssl3.rsa_rc4_128_sha
msgid "128-bit RC4 encryption with RSA and a SHA1 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་ཅིག་དང་བཅས་པའི་ བིཊི་-༡༢༨ ཨར་སི་༤ གི་གསང་བཟོ།"

#: cipher.ssl3.rsa_fips_des_ede3_sha
msgid "168-bit Triple DES with RSA and a SHA1 MAC (FIPS)"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་(ཨེཕ་ཨའི་པི་ཨེསི་)ཅིག་དང་བཅས་པའི་ བིཊི་-༡༦༨ ཌི་ཨི་ཨེསི་གསུམ་ཅན།"

#: cipher.ssl3.rsa_des_ede3_sha
msgid "168-bit Triple DES with RSA and a SHA1 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་ཅིག་དང་བཅས་པའི་ བིཊི་-༡༦༨ ཌི་ཨི་ཨེསི་གསུམ་ཅན།"

#: cipher.ssl3.rsa_fips_des_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC (FIPS)"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་(ཨེཕ་ཨའི་པི་ཨེསི)ཅིག་དང་བཅས་པའི་ བིཊི་-༡༦༨ ཌི་ཨི་ཨེསི་ གསང་བཟོ།"

#: cipher.ssl3.rsa_des_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་ཅིག་དང་བཅས་པའི་ བིཊི་-༥༦ ཌི་ཨི་ཨེསི་ གསང་བཟོ།"

#: cipher.ssl3.rsa_1024_rc4_56_sha
msgid "56-bit RC4 encryption with RSA and a SHA1 MAC (export)"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་(ཕྱིར་འདྲེན་)ཅིག་དང་བཅས་པའི་ བིཊི་-༥༦ ཨར་སི་༤ གི་གསང་བཟོ།"

#: cipher.ssl3.rsa_1024_des_cbc_sha
msgid "56-bit DES encryption with RSA and a SHA1 MAC (export)"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་(ཕྱིར་འདྲེན་)ཅིག་དང་བཅས་པའི་ བིཊི་-༥༦ ཌི་ཨི་ཨེསི་གསང་བཟོ།"

# Extra SSL3/TLS ciphers
#: cipher.ssl3.dhe_rsa_aes_256_sha
msgid "256-bit AES encryption with RSA, DHE, and a SHA1 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཌི་ཨེཆ་ཨི་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་ཅིག་དང་བཅས་པའི་ བིཊི་-༢༥༦  ཨེ་ཨི་ཨེསི་གསང་བཟོ།"

#: cipher.ssl3.dhe_dss_aes_256_sha
msgid "256-bit AES encryption with DSA, DHE, and a SHA1 MAC"
msgstr "ཌི་ཨེསི་ཨེ་དང་ ཌི་ཨེཆ་ཨི་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་ཅིག་དང་བཅས་པའི་ བིཊི་-༢༥༦  ཨེ་ཨི་ཨེསི་གསང་བཟོ།"

#: cipher.ssl3.rsa_aes_256_sha
msgid "256-bit AES encryption with RSA and a SHA1 MAC"
msgstr "ཨར་ཨེསི་ཨེ་དང་ ཨེསི་ཨེཆ་ཨེ་༡ མེཀ་ཅིག་དང་བཅས་པའི་ བིཊི་-༢༥༦  ཨེ་ཨི་ཨེསི་གསང་བཟོ།"

#: cipher.ssl3.dhe_rsa_aes_128_sha
