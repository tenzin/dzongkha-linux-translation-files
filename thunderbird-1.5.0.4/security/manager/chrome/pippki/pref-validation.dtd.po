#: validation.title
msgid "Validation"
msgstr "ནུས་ལྡན་བཟོ་བ་"

#: validation.crl.label
msgid "CRL"
msgstr "སི་ཨར་ཨེལ་"

#: validation.crl.description
msgid "Use the CRL Manager to manage the Certificate Revocation Lists (CRL) that are stored in your certificate database."
msgstr "ཁྱོད་རའི་ལག་ཁྱེར་གནས་སྡུད་གཞི་རྟེན་ནང་བསགས་ཏེ་ཡོད་པའི་ ལག་ཁྱེར་ཆ་མེད་ཐོ་ཡིག་(སི་ཨར་ཨེལ་) ཚུ་ འཛིན་སྐྱོང་འཐབ་ནི་ལུ་ སི་ཨར་ཨེལ་འཛིན་སྐྱོང་པ་ ལག་ལེན་འཐབ།"

#: validation.managecrls.button
msgid "Manage CRLs..."
msgstr "སི་ཨར་ཨེལ་ཚུ་ འཛིན་སྐྱོང་འཐབ་་་"

#: validation.managecrls.accesskey
msgid "M"
msgstr "M"

#: validation.crlmanager.label
msgid "Manage CRLs"
msgstr "སི་ཨར་ཨེལ་ཚུ་ འཛིན་སྐྱོང་འཐབ་"

#: validation.crlname.label
msgid "Name"
msgstr "མིང་"

#: validation.crllastupdate.label
msgid "Last Update"
msgstr "མཐའ་མཇུག་གི་དུས་མཐུན་བཟོ་ཚེས་"

#: validation.crlnextupdate.label
msgid "Next Update"
msgstr "ཤུཤ་མའི་དུས་མཐུད་བཟོ་ཚེས་"

#: validation.crlautoupdateenabled.label
msgid "Auto Update"
msgstr "རང་བཞིན་དུས་མཐུད་བཟོ་བ་"

#: validation.crlautoupdatestatus.label
msgid "Auto Update Status"
msgstr "རང་བཞིན་་དུས་མཐུན་བཟོ་བའི་གནས་ཚད་"

#: validation.deletecrl.label
msgid "Delete"
msgstr "བཏོན་གཏང་"

#: validation.updatecrl.label
#: validation.crl.autoupdate.time.label1
msgid "Update"
msgstr "དུས་མཐུན་བཟོ་"

#: validation.advanced.label
#: edit.button
msgid "Settings"
msgstr "སྒྲིག་སྟངས་ཚུ་"

#: validation.crl.autoupdate.title
msgid "Automatic CRL Update Preferences"
msgstr "རང་བཞིན་གྱིས་ སི་ཨར་ཨེལ་ དུས་མཐུན་བཟོ་ནིའི་ དགའ་གདམ་ཚུ་"

#: validation.crl.autoupdate.enable.label
msgid "Enable Automatic Update for this CRL"
msgstr "སི་ཨར་ཨེལ་འདི་གི་དོན་ལུ་ རང་བཞིན་གྱི་དུས་མཐུན་བཟོ་ནི་ ལྕོགས་ཅན་བཟོ།"

#: validation.crl.autoupdate.time.label2
msgid "Day(s) before Next Update date"
msgstr "ཤུལ་མའི་དུས་མཐུན་བཟོ་ནིའི་ཚེས་གྲངས་ཀྱི་ཧེ་མ་ཉིན་གྲངས་"

#: validation.crl.autoupdate.freq.label1
msgid "Update every"
msgstr "རེ་ནང་དུས་མཐུན་བཟོ་"

#: validation.crl.autoupdate.freq.label2
msgid "Day(s)"
msgstr "ཉིན་གྲངས་"

#: validation.crl.autoupdate.url.label
msgid "CRL would be imported From:"
msgstr "སི་ཨར་ཨེལ་ ནང་འདྲེན་འབད་ས་:"

#: crl.import.status.title
msgid "CRL Import Status"
msgstr "སི་ཨར་ཨེལ་ནང་འདྲེན་གྱི་གནས་ཚད་"

#: crl.import.success.message
msgid "The Certificate Revocation List (CRL) was successfully imported."
msgstr "ལག་ཁྱེར་ཆ་མེད་གཏང་བའི་ཐོ་ཡིག་འདི་ མཐར་འཁྱོལ་སྦེ་ ནང་འདྲེན་འབད་ཡི།"

#: crl.issuer.label
msgid "CRL Issued By:"
msgstr "སི་ཨར་ཨེལ་སྤྲོད་མི་:"

#: crl.issuer.org.label
msgid "Organization: "
msgstr "ཚོགས་སྡེ་"

#: crl.issuer.orgunit.label
msgid "Unit: "
msgstr "ཆ་ཕྲན་:"

#: crl.import.nextupdate.label
msgid "Next Update On: "
msgstr "ཤུལ་མ་དུས་མཐུན་བཟོ་ནིའི་ཚེས་གྲངས་"

#: crl.autoupdate.fail.cnt.label
msgid "Previous Consecutive Update Failures: "
msgstr "ཧེ་མ་དུས་མཐུན་བཟོ་ནི་འཐུས་ཤོར་གྱི་རྒྱུན་རིམ་:"

#: crl.autoupdate.fail.reason.label
msgid "Details of Last Update Failure: "
msgstr "མཐའ་མཇུག་ལུ་དུས་མཐུན་བཟོ་ནི་་འཐུས་ཤོར་བྱུང་བའི་ཚེས་གྲངས་:"

#: validation.ocsp.label
msgid "OCSP"
msgstr "ཨོ་སི་ཨེསི་པི་"

#: validation.ocsp.description
msgid "&brandShortName; can use Online Certificate Status Protocol (OCSP) to verify certificates. Set &brandShortName; to use OCSP as follows:"
msgstr "&brandShortName; གིས་ ལག་ཁྱེར་ཚུ་ བདེན་སྦྱོར་འབད་ནི་ལུ་ གློག་ཐོག་ལག་ཁྱེར་གནས་ཚད་གནས་སྤེལ་ལམ་ལུགས་(ཨོ་སི་ཨེསི་པི་) ལག་ལེན་འཐབ་ཚུགས། ཨོ་སི་ཨེསི་པི་གཤམ་གསལ་ལྟར་དུ་ ལགལེན་འཐབ་ནི་ལུ་ &brandShortName; གཞི་སྒྲིག་འབད་:"

#: disableOCSP.label
msgid "Do not use OCSP for certificate validation"
msgstr "ལག་ཁྱེར་ནུས་ལྡན་བཟོ་ནི་ལུ་ ཨོ་སི་ཨེསི་པི་ ལག་ལེན་མ་འཐབ།"

#: certOCSP.label
msgid "Use OCSP to validate only certificates that specify an OCSP service URL"
msgstr "ཨོ་སི་ཨེསི་པི་ ཞབས་ཏོག་ཀྱི་ ཡུ་ཨར་ཨེལ་ གསལ་བཀོད་འབད་མི་ ལག་ཁྱེར་ཚུ་རྐྱངམ་གཅིག་ ནུས་ལྡན་བཟོ་ནི་ལུ་ ཨོ་སི་ཨེསི་པི་ ལག་ལེན་འཐབ།"

#: proxyOCSP.label
msgid "Use OCSP to validate all certificates using this URL and signer:"
msgstr "ཡུ་ཨར་ཨེལ་འདི་དང་ མིང་རྟགས་བཀོད་མི་ ལག་ལེན་འཐབ་པའི་ཐོག་ལས་ ལག་ཁྱེར་ཆ་མཉམ་ར་ ནུས་ལྡན་བཟོ་ནི་ལུ་ ཨོ་སི་ཨེསི་པི་ ལག་ལེན་འཐབ་:"

#: serviceURL.label
#: serviceURL.accesskey
msgid "&Service URL:"
msgstr "ཞབས་ཏོག་ཀྱི་ཡུ་ཨར་ཨེལ་:(&S)"

#: signingCA.label
#: signingCA.accesskey
msgid "&Response Signer:"
msgstr "ངོས་ལན་ལུ་མིང་རྟགས་བཀོད་མི་:(&R)"

