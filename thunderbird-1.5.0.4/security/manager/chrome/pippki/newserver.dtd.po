# Values for newserver.xul
#: newserver.title
msgid "Website Certified by an Unknown Authority"
msgstr "ཝེབ་ས་ཁོངས་དེ་ ངོ་མ་ཤེས་པའི་དབང་འཛིན་ཅིག་གིས་ ངོ་སྦྱོར་འབད་ནུག"

#: newserver.reasons
msgid "Possible reasons for this error:"
msgstr "འཛོལ་བ་འདི་གི་ སྲིད་པའི་རྒྱུ་མཚན་ཚུ་:"

#: newserver.reason1
msgid "- Your browser does not recognize the Certificate Authority that issued the site's certificate."
msgstr "- ཁྱོད་ཀྱི་བརའུ་ཟར་དེ་གིས་ ས་ཁོངས་དེ་གི་ལག་ཁྱེར་སྤྲོད་མི་ ལག་ཁྱེར་དབང་འཛིན་དེ་ ངོས་འཛིན་མི་འབད་བས།"

#: newserver.reason2
msgid "- The site's certificate is incomplete due to a server misconfiguration."
msgstr "- སར་བར་གྱི་རིམ་སྒྲིག་ཕྱི་འགྱུར་ལུ་བརྟེན་ཏེ་ ས་ཁོངས་ཀྱི་ལག་ཁྱེར་དེ་ ཆ་ཚང་མིན་འདུག།"

#: newserver.notify
msgid "Please notify the site's webmaster about this problem."
msgstr "དཀའ་ངལ་འདི་གི་སྐོར་ལས་ ས་ཁོངས་དེ་གི་ཝེབ་འགོ་འཛིན་པ་ལུ་ བརྡ་བསྐུལ་འབད་གནང་།"

#: newserver.examine
msgid "Examine Certificate..."
msgstr "ལག་ཁྱེར་ལུ་བརྟག་དཔྱད་འབད་་་"

#: newserver.remember
msgid "Accept this certificate permanently"
msgstr "ལག་ཁྱེར་འདི་རྟག་བརྟན་སྦེ་ དང་ལེན་འབད་"

#: newserver.session
msgid "Accept this certificate temporarily for this session"
msgstr "ལག་ཁྱེར་འདི་ གནས་སྐབས་ཅིག་ ལཱ་ཡུན་འདི་གི་དོན་ལུ་ དང་ལེན་འབད།"

#: newserver.refuse
msgid "Do not accept this certificate and do not connect to this Web site"
msgstr "ལག་ཁྱེར་འདི་ དང་ལེན་མ་འབད་ དེ་ལས་ ཝེབ་ས་ཁོངས་འདི་ལུ་མ་མཐུད།"

