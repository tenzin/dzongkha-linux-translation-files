#: viewLog.title
msgid "Filter Log"
msgstr "ཚགས་མའི་དྲན་དེབ་"

#: viewLogInfo.text
msgid "The Filter Log documents the filters that have been run for this account. Use the check box below to enable logging."
msgstr "ཚགས་མའི་དྲན་དེབ་དེ་གིས་ རྩིས་ཐོ་འདི་གི་དོན་ལུ་ གཡོག་བཀོལ་ཡོད་པའི་ཚགས་མ་ཚུ་ ཡིག་ཐོག་ལུ་བཀོདཔ་ཨིན། དྲན་དེབ་བཀོད་ཚུགས་ནི་ལུ་ འོག་གི་དཔྱད་སྒྲོམ་ལག་ལེན་འཐབ།"

#: clearLog.label
#: clearLog.accesskey
msgid "&Clear Log"
msgstr "དྲན་དེབ་བསལ་(&C)"

#: enableLog.label
#: enableLog.accesskey
msgid "&Enable the Filter Log"
msgstr "ཚགས་མའི་དྲན་དེབ་ལྕོགས་ཅན་བཟོ་(&E)"

#: closeLog.label
#: closeLog.accesskey
msgid "Cl&ose"
msgstr "ཁ་བསྡམས་(&o)"

