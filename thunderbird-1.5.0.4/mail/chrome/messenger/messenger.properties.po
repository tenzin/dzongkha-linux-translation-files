# The following are used by the messenger application
#
#: renameAccount
msgid "Rename Account..."
msgstr "རྩིས་ཐོ་ལུ་བསྐྱར་མིང་བཏགས་་་"

#: renameNewsAccount
msgid "Rename News Account..."
msgstr "གནས་ཚུལ་རྩིས་ཐོ་ལུ་བསྐྱར་མིང་བཏགས་་་་"

#: renameFolder
msgid "Rename Folder..."
msgstr "སྣོད་འཛིན་ལུ་བསྐྱར་མིང་བཏགས་་་་"

#: compactFolder
msgid "Compact This Folder"
msgstr "སྣོད་འཛིན་འདི་སྲབ་ཆུང་བཟོ་"

#: removeAccount
msgid "Delete Account..."
msgstr "རྩིས་ཐོ་བཏོན་གཏང་་་་"

#: removeNewsAccount
msgid "Delete News Account..."
msgstr "གནས་ཚུལ་རྩི་ཐོ་བཏོན་གཏང་་་་"

#: removeFolder
msgid "Delete Folder"
msgstr "སྣོད་འཛིན་བཏོན་གཏང་"

#: newFolderMenuItem
msgid "Folder..."
msgstr "སྣོད་འཛིན་་་་"

#: newSubfolderMenuItem
msgid "Subfolder..."
msgstr "ཡན་ལག་སྣོད་འཛིན་་་་"

#: newFolder
msgid "New Folder..."
msgstr "སྣོད་འཛིན་གསརཔ་་་་"

#: newSubfolder
msgid "New Subfolder..."
msgstr "ཡན་ལག་སྣོད་འཛིན་གསརཔ་་་་"

#: folderProperties
msgid "Folder Properties"
msgstr "སྣོད་འཛིན་གྱི་རྒྱུ་དངོས་ཚུ་"

#: getNextNMessages
msgid "Get Next %S News Messages"
msgstr "ཤུལ་མའི་%Sགི་གནས་ཚུལ་འཕྲིན་དོན་ཚུ་ལེན།"

#: advanceNextPrompt
msgid "Advance to next unread message in %S?"
msgstr " %Sནང་ལུ་ མ་ལྷག་པའི་འཕྲིན་དོན་ཤུལ་མམ་ལུ་ ཡར་བསྐྱོད་འབད་ནི་ཨིན་ན?"

#: titleNewsPreHost
msgid "on"
msgstr "ལུ་"

#: titleMailPreHost
msgid "for"
msgstr "དོན་ལུ་"

#: replyToSender
msgid "Reply to Sender"
msgstr "གཏང་མི་ལུ་ལན་གསལ་གཏང་།"

#: reply
msgid "Reply"
msgstr "ལན་གསལ་"

#: EMLFiles
msgid "Mail Files"
msgstr "ཡིག་འཕྲིན་ཡིག་སྣོད་ཚུ་"

#: OpenEMLFiles
msgid "Open Message"
msgstr "འཕྲིན་དོན་ཁ་ཕྱེ་"

# LOCALIZATION NOTES(defaultSaveMessageAsFileName): Do not translate ".eml"
# in the line below.  Also, the complete file name should 8.3
#: defaultSaveMessageAsFileName
msgid "message.eml"
msgstr "འཕྲིན་དོན་.eml"

#: SaveMailAs
msgid "Save Message As"
msgstr "འཕྲིན་དོན་དེ་སྦེ་སྲུངས་"

#: SaveAttachment
msgid "Save Attachment"
msgstr "མཉམ་སྦྲགས་སྲུངས་"

#: SaveAllAttachments
msgid "Save All Attachments"
msgstr "མཉམ་སྦྲགས་ཆ་མཉམ་སྲུངས་"

#: LoadingMessageToPrint
msgid "Loading message to print..."
msgstr "དཔར་བསྐྲུན་འབད་ནིའི་འཕྲིན་དོན་མངོན་གསལ་འབད་དོ་་་"

#: MessageLoaded
msgid "Message loaded..."
msgstr "འཕྲིན་དོན་མངོན་གསལ་འབད་ཡི་་་་"

#: PrintingMessage
msgid "Printing message..."
msgstr "འཕྲིན་དོན་དཔར་བསྐྲུན་འབད་དོ་་་་"

#: PrintPreviewMessage
msgid "Print Previewing message..."
msgstr "སྔོན་ལྟའི་འཕྲིན་དོན་དཔར་བསྐྲུན་འབད་དོ་་་་་"

#: PrintingCard
msgid "Printing card..."
msgstr "ཤོག་བྱང་དཔར་བསྐྲུན་འབད་དོ་་་"

#: PrintPreviewCard
msgid "Print Previewing card..."
msgstr "སྔོན་ལྟའི་ཤོག་བྱང་དཔར་བསྐྲུན་འབད་་་་"

#: PrintingAddrBook
msgid "Printing AddressBook..."
msgstr "ཁ་བྱང་ཀི་དེབ་དཔར་བསྐྲུན་འབད་དོ་་་"

#: PrintPreviewAddrBook
msgid "Print Previewing AddressBook..."
msgstr "སྔོན་ལྟའི་ཁ་བྱང་ཀི་དེབ་ དཔར་བསྐྲུན་འབད་་་་"

#: PrintingComplete
msgid "Done."
msgstr "འབད་ཚར་"

#: PreviewTitle
msgid "%S - %S"
msgstr "%S - %S"

#: LoadingMailMsgForPrint
msgid "(Loading content for Printing)"
msgstr "(དཔར་བསྐྲུན་གྱི་དོན་ལུ་ ནང་དོན་མངོན་གསལ་འབད་དོ)"

#: LoadingMailMsgForPrintPreview
msgid "(Loading content for Print Preview)"
msgstr "(དཔར་བསྐྲུན་སྔོན་ལྟའི་དོན་ལུ་ ནང་དོན་མངོན་གསལ་འབད་དོ)"

#: saveAttachmentFailed
msgid "Unable to save the attachment. Please check your file name and try again later."
msgstr "མཉམ་སྦྱར་བསྲུང་མ་ཚུགས། ཁྱོད་རའི་ཡིག་སྣོད་མིང་ ཞིབ་དཔྱད་འབད་ཞིནམ་ལས་ ཤུལ་ལས་ལོག་སྟེ་འབད་རྩོལ་བསྐྱེད་གནང་།"

#: saveMessageFailed
msgid "Unable to save the message. Please check your file name and try again later."
msgstr "འཕྲིན་དོན་བསྲུང་མ་ཚུགས། ཁྱོད་རའི་ཡིག་སྣོད་མིང་ ཞིབ་དཔྱད་འབད་ཞིནམ་ལས་ ཤུལ་ལས་ལོག་སྟེ་འབད་རྩོལ་བསྐྱེད་གནང་།"

#: notDownloaded
msgid "(Not Downloaded)"
msgstr "(མངོན་གསལ་མ་འབད་བས)"

#: fileExists
msgid "%S already exists. Do you want to replace it?"
msgstr "%Sཧེ་མ་ལས་ར་འདུག། དེ་གི་ཚབ་མ་བཙུགས་ནི་ཨིན་ན?"

#: downloadingNewsgroups
msgid "Downloading Newsgroups for Offline Use"
msgstr "གློག་ཐོག་མེན་པའི་ལག་ལེན་གྱི་དོན་ལུ་ གནས་ཚུལ་སྡེ་ཚན་ཚུ་ ཕབ་ལེན་འབད་དོ།"

#: downloadingMail
msgid "Downloading Mail for Offline Use"
msgstr "གློག་ཐོག་མེན་པའི་ལག་ལེན་གྱི་དོན་ལུ་ ཡིག་འཕྲིན་ཕབ་ལེན་འབད་དོ།"

#: sendingUnsent
msgid "Sending Unsent Messages"
msgstr "མ་བཏང་པར་ཡོད་མི་ འཕྲིན་དོན་ཚུ་གཏང་དོ།"

#: folderExists
msgid "A folder with that name already exists. Please enter a different name."
msgstr "མིང་དེ་འབད་མི་ སྣོད་འཛིན་ཅིག་ཧེ་མ་ལས་ར་འདུག། མིང་སོ་སོ་ཅིག་བཙུགས་གནང་།"

#: folderCreationFailed
msgid "The folder could not be created because the folder name you specified contains an unrecognized character. Please enter a different name and try again."
msgstr "སྣོད་འཛིན་དེ་གསར་བསྐྲུན་འབད་མ་ཚུགས། ག་ཅི་སྦེ་ཟེར་བ་ཅིན་ ཁྱོད་ཀྱིས་གསལ་བཀོད་འབད་མི་ སྣོད་འཛིན་གྱི་མིང་དེ་ནང་ ངོས་འཛིན་འབད་མ་ཚུགས་པའི་ཡིག་འབྲུ་ཅིག་འདུག། མིང་སོ་སོ་ཅིག་བཙུགས་ཞིནམ་ལས་ ལོག་སྟེ་འབད་རྩོལ་བསྐྱེད་གནང་།"

#: compactingFolder
msgid "Compacting folder %S..."
msgstr "སྣོད་འཛིན་%Sསྲབ་ཆུང་བཟོ་དོ་་་"

#: compactingDone
msgid "Done compacting folders"
msgstr "སྣོད་འཛིན་སྲབ་ཆུང་བཟོ་ནི་འབད་ཚར་"

#: autoCompactAllFolders
msgid "Do you wish to compact all local and offline folders to save disk space?"
msgstr "ཌིཀསི་ནང་ བར་སྟོང་བཟོ་ནིའི་དོན་ལུ་ ཉེ་གནས་ཀྱི་སྣོད་འཛིན་དང་ གློག་ཐོག་མེན་པའི་སྣོད་འཛིན་ཆ་མཉམ་ར་ སྲབ་ཆུང་བཟོ་ནི་ཨིན་ན?"

#: confirmFolderDeletionForFilter
msgid "Deleting the folder '%S' will disable its associated filter(s). Are you sure you want to delete the folder?"
msgstr "སྣོད་འཛིན་དེ་བཏོན་གཏང་པ་ཅིན་ དེ་དང་འབྲེལ་བའི་ཚགས་མ་ཚུ་ ལྕོགས་མིན་བཟོ་འོང་། ཁྱོད་ཀྱིས་སྣོད་འཛིན་དེ་ བཏོན་གཏང་ནི་ངེས་གཏན་ཨིན་ན?"

#: alertFilterChanged
msgid "Filters associated with this folder will be updated."
msgstr "སྣོད་འཛིན་འདི་དང་འབྲེལ་བ་ཡོད་པའི་ཚགས་མ་ཚུ་ དུས་མཐུན་བཟོ་ནི་ཨིན།"

#: filterDisabled
msgid "The folder '%S' could not be found, so filter(s) associated with this folder will be disabled. Verify that the folder exists, and that filters point to a valid destination folder."
msgstr "སྣོད་འཛིན་'%S'དེ་འཚོལ་མ་ཐོབ། དེ་གིས་སྦེ་ སྣོད་འཛིན་འདི་དང་འབྲེལ་བ་ཡོད་པའི་ཚགས་མ་ཚུ་ ལྕོགས་མིན་བཟོ་ནི་ཨིན། སྣོད་འཛིན་ཚུ་ཡོདཔ་དང་ ཚགས་མ་ཚུ་གིས་ ནུས་ཅན་གྱི་འགྲོ་ཡུལ་སྣོད་འཛིན་ཅིག་ལུ་ རྩེ་དཔག་པའི་བདེན་སྦྱོར་འབད།"

#: filterFolderDeniedLocked
msgid "The messages could not be filtered to folder '%S' because another operation is in progress."
msgstr "འཕྲིན་དོན་ཚུ་སྣོད་འཛིན་'%S'ལུ་ བཙགས་མ་ཚུགས། ག་ཅི་སྦེ་ཟེར་བ་ཅིན་ བཀོལ་སྤྱོད་གཞན་མི་ཅིག་ ལཱ་འབད་བའི་བསྒང་འདུག།"

#: parsingFolderFailed
msgid "Unable to open the folder %S because it is in use by some other operation. Please wait for that operation to finish and then select the folder again."
msgstr "་སྣོད་འཛིན་%Sཁ་ཕྱེ་མ་ཚུགས། ག་ཅི་སྦེ་ཟེར་བ་ཅིན་ འདི་བཀོལ་སྤྱོད་གཞན་མི་ཅིག་གིས་ ལག་ལེན་འཐབ་བའི་བསྒང་འདུག།"

#: deletingMsgsFailed
msgid "Unable to delete messages in folder %S because it is in use by some other operation. Please wait for that operation to finish and then try again."
msgstr "་སྣོད་འཛིན་%Sནང་གི་འཕྲིན་དོན་ཚུ་ བཏོན་གཏང་མ་ཚུགས། ག་ཅི་སྦེ་ཟེར་བ་ཅིན་ འདི་བཀོལ་སྤྱོད་གཞན་མི་ཅིག་གིས་ ལག་ལེན་འཐབ་བའི་བསྒང་འདུག། བཀོལ་སྤྱོད་དེ་རྫོགས་རྫོགས་ བསྒུག་སྡོད་ཞིནམ་ལས་ དེ་ལས་ལོག་སྟེ་འབད་རྩོལ་བསྐྱེད་གནང་།"

#: alertFilterCheckbox
msgid "Do not warn me again."
msgstr "ང་ལུ་ལོག་སྟེ་ ཉེན་བརྡ་མ་འབད།"

#: compactFolderDeniedLock
msgid "The folder '%S' cannot be compacted because another operation is in progress. Please try again later."
msgstr "་སྣོད་འཛིན་%Sདེ་ སྲབ་ཆུང་བཟོ་མ་ཚུགས། ག་ཅི་སྦེ་ཟེར་བ་ཅིན་ བཀོལ་སྤྱོད་གཞན་མི་ཅིག་གིས་ ལག་ལེན་འཐབ་བའི་བསྒང་འདུག། ཤུལ་ལས་ལོག་སྟེ་འབད་རྩོལ་བསྐྱེད་གནང་།"

#: compactFolderWriteFailed
msgid "The folder '%S' could not be compacted because writing to folder failed. Verify that you have enough disk space, and that you have write privileges to the file system, then try again."
msgstr "་སྣོད་འཛིན་%Sདེ་ སྲབ་ཆུང་བཟོ་མ་ཚུགས། ག་ཅི་སྦེ་ཟེར་བ་ཅིན་ སྣོད་འཛིན་ལུ་་ཡི་གུ་འབྲི་མ་བཏུབ། ཁྱོད་ཀྱི་ཌིཀསི་གུ་ བར་སྟོང་ལངམ་ཡོད་པའི་ཁར་ ཁྱོད་ཀྱིས་ཡིག་སྣོད་རིམ་ལུགས་དེ་ལུ་ ཡི་གུ་འབྲི་ནིའི་ཁེ་དབང་ཡོད་པའི་ བདེན་སྦྱོར་འབད་ཞིནམ་ལས་ ལོག་སྟེ་འབད་རྩོལ་བསྐྱེད་གནང་།"

#: filterFolderWriteFailed
msgid "The messages could not be filtered to folder '%S' because writing to folder failed. Verify that you have enough disk space, and that you have write privileges to the file system, then try again."
msgstr "འཕྲིན་དོན་ཚུ་ སྣོད་འཛིན་'%S'ལུ་ བཙགས་མ་ཚུགས། ག་ཅི་སྦེ་ཟེར་བ་ཅིན་ སྣོད་འཛིན་ལུ་ཡི་གུ་འབྲི་མ་བཏུབ། ཁྱོད་ཀྱི་ཌིཀསི་གུ་ བར་སྟོང་ལངམ་ཡོད་པའི་ཁར་ ཁྱོད་ཀྱིས་ཡིག་སྣོད་རིམ་ལུགས་དེ་ལུ་ ཡི་གུ་འབྲི་ནིའི་ཁེ་དབང་ཡོད་པའི་ བདེན་སྦྱོར་འབད་ཞིནམ་ལས་ ལོག་སྟེ་འབད་རྩོལ་བསྐྱེད་གནང་།"

#: copyMsgWriteFailed
msgid "The messages could not be moved or copied to folder '%S' because writing to folder failed. To gain disk space, from the File menu, first choose Empty Trash, and then choose Compact Folders, and then try again."
msgstr "འཕྲིན་དོན་ཚུ་ སྣོད་འཛིན་'%S'ལུ་ སྤོ་ནིའམ་འདྲ་བཤུས་བརྐྱབ་མ་ཚུགས། ག་ཅི་སྦེ་ཟེར་བ་ཅིན་ སྣོད་འཛིན་ལུ་ཡི་གུ་འབྲི་མ་བཏུབ། ཁྱོད་ཀྱི་ཌིཀསི་གུ་ བར་སྟོང་ལངམ་ཡོད་པའི་ཁར་ ཁྱོད་ཀྱིས་ཡིག་སྣོད་རིམ་ལུགས་དེ་ལུ་ ཡི་གུ་འབྲི་ནིའི་ཁེ་དབང་ཡོད་པའི་ བདེན་སྦྱོར་འབད་ཞིནམ་ལས་ ལོག་སྟེ་འབད་རྩོལ་བསྐྱེད་གནང་།"

#: cantMoveMsgWOBodyOffline
msgid "While working offline, you cannot move or copy messages that were not downloaded for offline use. From the Mail window, open the File menu, choose Offline, then choose Work Online, and then try again."
msgstr "གློག་ཐོག་མེན་སྦེ་ ལཱ་འབད་བའི་སྐབས་ གློག་ཐོག་མེན་པའི་ལག་ལེན་གྱི་དོན་ལུ་ ཕབ་ལེན་མ་འབད་མི་ འཕྲིན་དོན་ཚུ་ སྤོ་ནི་དང་འདྲ་བཤུས་བརྐྱབ་ནི་ཚུ་མི་བཏུབ། ཡིག་འཕྲིན་སྒོ་སྒྲིག་ནང་ལས་ སྣོད་འཛིན་དཀར་ཆག་ཁ་ཕྱེ་སྟེ་ གློག་ཐོག་མེན་པའི་ གདམ་ཁ་བརྐྱབས་ཏེ་ གློག་ཐོག་ལུ་ལཱ་འབད་ ཟེར་མི་གདམ་ཁ་བརྐྱབས་ཞིནམ་ལས་ ལོག་སྟེ་འབད་རྩོལ་བསྐྱེད་གནང་།"

#: operationFailedFolderBusy
msgid "The operation failed because an other operation is using the folder. Please wait for that operation to finish and then try again."
msgstr "བཀོལ་སྤྱོད་འདི་འབད་མ་བཏུབ། ག་ཅི་སྦེ་ཟེར་བ་ཅིན་ སྣོད་འཛིན་དེ་ བཀོལ་སྤྱོད་གཞན་ཅིག་གིས་ ལག་ལེན་འཐབ་པའི་བསྒང་འདུག། བཀོལ་སྤྱོད་དེ་རྫོགས་རྫོགས་བསྒུག་ཞིནམ་ལས་ ལོག་སྟེ་འབད་རྩོལ་བསྐྱེད་གནང་།"

#: folderRenameFailed
msgid "The folder could not be renamed. Perhaps the folder is being reparsed, or the new name is not a valid folder name."
msgstr "སྣོད་འཛིན་འདི་ལུ་ བསྐྱར་མིང་བཏགས་མ་ཚུགས། སྣོད་འཛིན་འདི་ལུ་ ལོག་སྟེ་མིང་དཔྱད་འབད་དོ་ཡོདཔ་འོང་ནི་དང་ ཡང་ཅིན་ མིང་གསརཔ་དེ་ སྣོད་འཛིན་གྱི་ནུས་ཅན་མིང་ཅིག་ མེནམ་འོང་ནི་མས།"

# LOCALIZATION NOTES(verboseFolderFormat): %1$S is folder name, %2$S is server name
#: verboseFolderFormat
msgid "%1$S on %2$S"
msgstr "%2$S གུ་ %1$S"

# LOCALIZATION NOTES(filterFolderTruncateFailed): %1$S is replaced by the folder name, %2$S is replaced by the brandShortName
#: filterFolderTruncateFailed
msgid "There was an error truncating the Inbox after filtering a message to folder '%1$S'. You may need to shutdown %2$S and delete INBOX.msf."
msgstr "འཕྲིན་དོན་ཅིག་ སྣོད་འཛིན་'%1$S' ལུ་ བཙགས་བཏང་པའི་ཤུལ་ལུ་ ནང་སྒྲོམ་དེ་ ཆུང་ཀུ་བཟོ་མ་བཏུབ། ཁྱོད་ཀྱིས་%2$S སྒོ་བསྡམས་ཞིནམ་ལས་ INBOX.msf བཏོན་གཏང་དགོཔ་འོང་།"

#: mailboxTooLarge
msgid "The folder %S is full, and can't hold any more messages. To make room for more messages, delete any old or unwanted mail and compact the folder."
msgstr "སྣོད་འཛིན་%Sདེ་གང་ཡོདཔ་ལས་ འཕྲིན་དོན་དེ་ལས་མངམ་མི་ཤོམ་པས། འཕྲིན་དོན་མངམ་ཤོམ་ནིའི་དོན་ལུ་ ཡིག་འཕྲིན་རྙིངམ་ ཡང་ན་ མ་དགོཔ་ཚུ་བཏོན་བཏང་ཞིནམ་ལས་ སྣོད་འཛིན་དེ་སྲབ་ཆུང་བཟོ།"

