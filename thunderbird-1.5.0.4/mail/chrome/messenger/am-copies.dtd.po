# extracted from am-copies.xul
#: copyAndFolderTitle.label
msgid "Copies &amp; Folders"
msgstr "འདྲ་བཤུས་ &amp; སྣོད་འཛིན་ཚུ་"

#: sendingPrefix.label
msgid "When sending messages, automatically: "
msgstr "འཕྲིན་དོན་ཚུ་ རང་བཞིན་གྱིས་གཏང་བའི་སྐབས་ལུ་:"

#: fccMailFolder.label
#: fccMailFolder.accesskey
msgid "&Place a copy in:"
msgstr "འདྲ་བཤུས་ཅིག་བཞག་:(&P)"

#: chooseFolderButton.label
msgid "Choose Folder..."
msgstr "སྣོད་འཛིན་གདམ་ཁ་རྐྱབས་་་"

#: bccAddress.label
#: bccAddress.accesskey
msgid ""
"_: do not translate \"Bcc\" in below line\n"
"&Bcc these email addresses:"
msgstr "གློག་འཕྲིན་ཁ་བྱང་འདི་ཚུ་ Bcc བཟོ་:"

#: specialFolderTitle.label
msgid "Special Folders"
msgstr "དམིགས་བསལ་སྣོད་འཛིན་ཚུ་"

#: draftMailFolder.label
#: keepDrafts.label
msgid "Keep message drafts in:"
msgstr "འཕྲིན་དོན་གྱི་ཟིན་འབྲི་ཚུ་བཞག་:"

#: templateMailFolder.label
#: keepTempltes.label
msgid "Keep message templates in:"
msgstr "འཕྲིན་དོན་གྱི་ ཊེམ་པེལེཊི་ཚུ་བཞག་:"

#: saveMessageDlg.label
#: saveMessageDlg.accesskey
msgid "&Show confirmation dialog when messages are saved"
msgstr "འཕྲིན་དོན་ཚུ་བསྲུང་བའི་སྐབས་ ངེས་དཔྱད་ཌའི་ལོག་སྟོན།(&S)"

#: sentFolderOn.label
#: sentFolderOn.accesskey
msgid ""
"_: OK to translate this, bug #57440\n"
"&quot;S&ent&quot; Folder on:"
msgstr "&quot;S&ent&quot;སྣོད་འཛིན་ཨཱོན་འབད་:"

#: sentInOtherFolder.label
#: sentInOtherFolder.accesskey
msgid "&Other:"
msgstr "གཞན་:(&O)"

#: draftsFolderOn.label
#: draftsFolderOn.accesskey
msgid ""
"_: OK to translate this, bug #57440\n"
"&quot;&Drafts&quot; Folder on:"
msgstr "&quot;&Drafts&quot; སྣོད་འཛིན་ཨཱོན་:"

#: draftInOtherFolder.label
#: draftInOtherFolder.accesskey
msgid "O&ther:"
msgstr "གཞན་:(&t)"

#: templatesFolderOn.label
#: templatesFolderOn.accesskey
msgid ""
"_: OK to translate this, bug #57440\n"
"&quot;Te&mplates&quot; Folder on:"
msgstr "&quot;Te&mplates&quot; སྣོད་འཛིན་ཨཱོན་:"

#: templateInOtherFolder.label
#: templateInOtherFolder.accesskey
msgid "Othe&r:"
msgstr "གཞན་:(&r)"

#: specialFoldersTitle.label
msgid "Drafts and Templates"
msgstr "ཟིན་འབྲི་དང་ཊེམ་པེལེཊི་ཚུ།"

