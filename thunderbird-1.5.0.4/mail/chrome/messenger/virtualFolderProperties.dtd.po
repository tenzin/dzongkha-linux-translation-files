#: virtualFolderProperties.title
msgid "New Saved Search Folder"
msgstr "བསྲུངས་ཏེ་ཡོད་པའི་ འཚོལ་ཞིབ་སྣོད་འཛིན་གསརཔ་"

#: name.label
#: name.accesskey
msgid "&Name:"
msgstr "མིང་:(&N)"

#: description.label
#: description.accesskey
msgid "&Create as a subfolder of:"
msgstr "གི་ཡན་ལག་སྣོད་འཛིན་ཅིག་སྦེ་ གསར་བསྐྲུན་འབད་(&C)"

#: searchCriteriaText.label
msgid "Modify the search criteria used for this saved search:"
msgstr "བསྲུངས་ཏེ་ཡོད་པའི་འཚོལ་ཞིབ་འདི་གི་དོན་ལུ་ འཚོལ་ཞིབ་ཁྱད་ཚད་དེ་ ལེགས་བཅོས་འབད་:"

#: searchCriteriaButton.label
#: searchCriteriaButton.accesskey
msgid "&Search Criteria..."
msgstr "འཚོལ་ཞིབ་ཁྱད་ཚད་་་་(&S)"

#: searchTermCaption.label
msgid "Configure the search criteria used for this saved search folder: "
msgstr "བསྲུངས་ཏེ་ཡོད་པའི་འཚོལ་ཞིབ་སྣོད་འཛིན་གྱི་དོན་ལུ་ ལག་ལེན་འཐབ་ཡོད་པའི་ འཚོལ་ཞིབ་ཁྱད་ཚད་རིམ་སྒྲིག་འབད་:"

#: folderSelectionCaption.label
msgid "Select the folders to search: "
msgstr "འཚོལ་ཞིབ་འབད་ནིའི་ སྣོད་འཛིན་ཚུ་སེལ་འཐུ་འབད་:"

#: chooseFoldersButton.label
#: chooseFoldersButton.accesskey
msgid "C&hoose..."
msgstr "གདམ་ཁ་རྐྱབས་་་་(&h)"

#: searchOnline.label
#: searchOnline.accesskey
msgid "Search &Online (Gives up-to-date results for IMAP and News folders but increases time to open the folder)"
msgstr "གློག་ཐོག་འཚོལ་ཞིབ་(IMAP དང་ གནས་ཚུལ་སྣོད་འཛིན་གྱི་དོན་ལུ་ དུས་མཐུན་གྱི་གྲུབ་འབྲས་བྱིནམ་ཨིན་ དེ་འབདཝ་ད་ སྣོད་འཛིན་ཁ་ཕྱེ་ནིའི་དུས་ཚོད་རིང་སུ་འགོརཝ་ཨིན)(&O)"

