#: junkLog.title
msgid "Junk Mail Log"
msgstr "མཁོ་མེད་ཡིག་འཕྲིན་དྲན་དེབ་"

#: junkLogInfo.text
msgid "The Junk Mail Log documents the junk mail control activity on this account.  Use the check box below to enable logging."
msgstr "མཁོ་མེད་ཡིག་འཕྲིན་དྲན་དེབ་དེ་གིས་ རྩིས་ཐོ་འདི་གུ་ མཁོ་མེད་ཡིག་འཕྲིན་ཚད་འཛིན་གྱི་བྱ་བ་ ཡིག་ཐོག་ལུ་བཀོདཔ་ཨིན། དྲན་དེབ་ནང་བཀོད་ཚུགས་ནི་ལུ་ འོག་གི་དཔྱད་སྒོམ་ལག་ལེན་འཐབ།"

#: clearLog.label
#: clearLog.accesskey
msgid "&Clear Log"
msgstr "དྲན་དེབ་བསལ་(&C)"

#: enableLog.label
#: enableLog.accesskey
msgid "&Enable the Junk Mail Log"
msgstr "མཁོ་མེད་ཡིག་འཕྲིན་དྲན་དེབ་ ལྕོགས་ཅན་བཟོ།(&E)"

#: closeLog.label
#: closeLog.accesskey
msgid "Cl&ose"
msgstr "ཁ་བསྡམས་(&o)"

