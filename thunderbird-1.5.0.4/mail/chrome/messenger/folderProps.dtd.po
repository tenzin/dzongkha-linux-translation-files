#: folderProps.windowtitle.label
msgid "Properties"
msgstr "རྒྱུ་དངོས་ཚུ་"

#: generalInfo.label
msgid "General Information"
msgstr "ཡོངས་ཁྱབ་བརྡ་དོན་"

#: folderCharsetTab.label
#: folderCharsetTab.accesskey
msgid "Default Character &Encoding:"
msgstr "སྔོན་སྒྲིག་ཡིག་འབྲུའི་ཨིན་ཀོ་ཌིང་:(&E)"

#: folderCharsetOverride.label
#: folderCharsetOverride.accesskey
msgid "&Apply default to all messages in the folder (individual message character encoding settings and auto-detection will be ignored)"
msgstr "སྣོད་འཛིན་ནང་ཡོད་མི་ འཕྲིན་དོན་ཆ་མཉམ་ལུ་ སྔོན་སྒྲིག་ལག་ལེན་འཐབ། (མི་ངོ་སོ་སོའི་འཕྲིན་དོན་གྱི་ཡིག་འབྲུའི་ཨིན་ཀོ་ཌིང་སྒྲིག་སྟངས་དང་ རང་བཞིན་ངོས་འཛིན་ཚུ་སྣང་མེད་བཞག་འོང་)(&A)"

#: retention.label
msgid "Retention Policy"
msgstr "འཛིན་བཞག་སྲིད་བྱུས།"

#: retentionUseDefault.label
#: retentionUseDefault.accesskey
msgid "Use server &defaults"
msgstr "སར་བར་སྔོན་སྒྲིག་ཚུ་ལག་ལེན་འཐབ་(&d)"

#: days.label
msgid "days"
msgstr "ཉིན་གྲངས་"

#: daysOld.label
msgid "days old"
msgstr "ཉིན་གྲངས་སོན་པའི་"

#: message.label
msgid "messages"
msgstr "འཕྲིན་དོན་"

#: retentionCleanup.label
msgid "To recover disk space, old messages can be permanently deleted"
msgstr "ཌིཀསི་ནང་བར་སྟོང་བཟོ་ནི་ལུ་ འཕྲིན་དོན་རྙིངམ་ཚུ་ རྩ་བརྟན་སྦེ་བཏོན་གཏང་བཏུབ།"

#: retentionKeepMsg.label
#: retentionKeepMsg.accesskey
msgid "Delete &messages more than"
msgstr "ལས་ལྷག་པའི་འཕྲིན་དོན་ཚུ་བཏོན་གཏང་(&m)"

#: retentionKeepAll.label
#: retentionKeepAll.accesskey
msgid "Don't delete &any messages"
msgstr "འཕྲིན་དོན་གཅིག་ཡང་བཏོན་མ་གཏང་།(&a)"

#: retentionKeepNew.label
#: retentionKeepNew.accesskey
msgid "De&lete all but the last"
msgstr "མཐའ་མཇུག་དེ་མ་གཏོགས་པའི་ འཕྲིན་དོན་ཆ་མཉམ་བཏོན་གཏང་(&l)"

#: retentionKeepUnread.label
#: retentionKeepUnread.accesskey
msgid "Always delete &read messages"
msgstr "ཨ་རྟག་ར་ ལྷག་ཚར་མི་འཕྲིན་དོན་ཚུ་བཏོན་གཏང་(&r)"

#: folderOfflineTab.label
msgid "Offline"
msgstr "གློག་ཐོག་མེནམ་"

#: folderCheckForNewMessages.label
#: folderCheckForNewMessages.accesskey
msgid "&Check this folder for new messages"
msgstr "འཕྲིན་དོན་གསར་པའི་དོན་ལུ་ སྣོད་འཛིན་འདི་ལུ་ ཞིབ་དཔྱད་རྐྱབས་(&C)"

#: offlineFolder.check.label
#: offlineFolder.check.accesskey
msgid "&Select this folder for offline use"
msgstr "གློག་ཐོག་མེན་པའི་ ལག་ལེན་གི་དོན་ལུ་ སྣོད་འཛིན་འདི་སེལ་འཐུ་འབད་(&S)"

#: offlineFolder.button.label
#: offlineFolder.button.accesskey
#: offlineNewsgroup.button.label
#: offlineNewsgroup.button.accesskey
msgid "&Download Now"
msgstr "ད་ལྟོ་ར་ ཕབ་ལེན་འབད་(&D)"

#: selectofflineNewsgroup.check.label
#: selectofflineNewsgroup.check.accesskey
msgid "Select this newsgr&oup for offline use"
msgstr "གློག་ཐོག་མེན་པའི་ ལག་ལེན་གྱི་དོན་ལུ་ གནས་ཚུལ་སྡེ་ཚན་འདི་ སེལ་འཐུ་འབད་(&o)"

#: UseDefaultofflineNewsgroup.check.label
msgid "Use the default from account settings "
msgstr "རྩིས་ཐོ་སྒྲིག་སྟངས་ནང་ལས་ སྔོན་སྒྲིག་ལག་ལེན་འཐབ་"

#: folderProps.information.label
msgid "Information:"
msgstr "བརྡ་དོན་:"

#: folderProps.name.label
#: folderProps.name.accesskey
msgid "&Name:"
msgstr "མིང་:(&N)"

#: folderSharingTab.label
msgid "Sharing"
msgstr "རུབ་སྤྱོད་"

#: privilegesDesc.label
msgid "Share this and other folders with network users and display and set access privileges"
msgstr "སྣོད་འཛིན་འདི་དང་གཞན་ཚུ་ ཡོངས་འབྲེལ་ལག་ལེན་པ་ཚུ་དང་གཅིག་ཁར་ རུབ་སྤྱོད་འབད་ཞིནམ་ལས་ ཁེ་དབང་ཚུ་ བཀྲམ་སྟོན་དང་གཞི་སྒྲིག་འབད།"

#: privileges.button.label
#: privileges.button.accesskey
msgid "&Privileges..."
msgstr "ཁེ་དབང་་་(&P)"

#: permissionsDesc.label
msgid "You have the following permissions:"
msgstr "ཁྱོད་ལུ་འོག་གི་གནང་བ་ཚུ་འདུག་:"

#: folderType.label
msgid "Folder Type:"
msgstr "སྣོད་འཛིན་གྱི་དབྱེ་བ་:"

#: folderQuotaTab.label
msgid "Quota"
msgstr "ཐོབ་སྐལ་"

#: folderQuotaRoot.label
msgid "Quota Root:"
msgstr "ཐོབ་སྐལ་གྱི་རྩ་བ་:"

#: folderQuotaUsage.label
msgid "Usage:"
msgstr "ལག་ལེན་:"

#: folderQuotaStatus.label
msgid "Status:"
msgstr "གནས་ཚད་:"

