#
# The following are used by the imap code to display progress/status/error messages
#
# Status - opening folder
## @name IMAP_STATUS_SELECTING_MAILBOX
## @loc None
#: 5000
msgid "Opening folder..."
msgstr "སྣོད་འཛིན་ཁ་ཕྱེ་དོ་་་"

# Status - create folder
## @name IMAP_STATUS_CREATING_MAILBOX
## @loc None
#: 5001
msgid "Creating folder..."
msgstr "ཧྣོད་འཛིན་གསར་བསྐྲུན་འབད་དོ་་་་"

# Status - deleting a folder
## @name IMAP_STATUS_DELETING_MAILBOX
## @loc None
# LOCALIZATION NOTE (Error 5002): The "%S" below should not be translated.
# Instead, insert "%S" in your translation where you wish to display the name
# of the folder being deleted.
#: 5002
msgid "Deleting folder %S..."
msgstr "སྣོད་འཛིན་%Sབཏོན་གཏང་དོ་་་་"

# Status - renaming mailbox
## @name IMAP_STATUS_RENAMING_MAILBOX
## @loc None
# LOCALIZATION NOTE (Error 5003): The "%S" below should not be translated.
# Instead, insert "%S" in your translation where you wish to display the name
# of the folder being renamed.
#: 5003
msgid "Renaming folder %S..."
msgstr "སྣོད་འཛིན་%S ལུ་ བསྐྱར་མིང་བཏགས་དོ་་་་"

# Status - looking for mailboxes
## @name IMAP_STATUS_LOOKING_FOR_MAILBOX
## @loc None
#: 5004
msgid "Looking for folders..."
msgstr "སྣོད་འཛིན་གྱི་དོན་ལུ་བལྟ་དོ་་་་"

# Status - subscribing to mailbox
## @name IMAP_STATUS_SUBSCRIBE_TO_MAILBOX
## @loc None
# LOCALIZATION NOTE (Error 5005): The "%S" below should not be translated.
# Instead, insert "%S" in your translation where you wish to display the name
# of the folder being subscribed to.
#: 5005
msgid "Subscribing to folder %S..."
msgstr "སྣོད་འཛིན་%S ལུ་ མཁོ་མངགས་འབད་དོ་་་་"

# Status - unsubscribing from mailbox
## @name IMAP_STATUS_UNSUBSCRIBE_MAILBOX
## @loc None
# LOCALIZATION NOTE (Error 5005): The "%S" below should not be translated.
# Instead, insert "%S" in your translation where you wish to display the name
# of the folder being unsubscribed from.
#: 5006
msgid "Unsubscribing from folder %S..."
msgstr "སྣོད་འཛིན་%Sལུ་ མཁོ་མངགས་བཤོལ་དོ་་་་"

# Status - searching imap folder
## @name IMAP_STATUS_SEARCH_MAILBOX
## @loc None
#: 5007
msgid "Searching folder..."
msgstr "སྣོད་འཛིན་འཚོལ་ཞིབ་འབད་དོ་་་་"

# Status - getting message info
## @name IMAP_STATUS_MSG_INFO
## @loc None
#: 5008
msgid "Getting message info..."
msgstr "འཕྲིན་དོན་གྱི་བརྡ་དོན་ལེན་དོ་་་་"

# Status - closing a folder
## @name IMAP_STATUS_CLOSE_MAILBOX
## @loc None
#: 5009
msgid "Closing folder..."
msgstr "སྣོད་འཛིན་ཁ་བསྡམས་དོ་་་་"

# Status - compacting a folder
## @name IMAP_STATUS_EXPUNGING_MAILBOX
## @loc None
#: 5010
msgid "Compacting folder..."
msgstr "སྣོད་འཛིན་སྲབ་ཆུང་བཟོ་དོ་་་"

# Status - logging out
## @name IMAP_STATUS_LOGGING_OUT
## @loc None
#: 5011
msgid "Logging out..."
msgstr "ཕྱིར་བསྐྱོད་འབད་དོ་་་་"

# Status - checking server capabilities
## @name IMAP_STATUS_CHECK_COMPAT
## @loc None
#: 5012
msgid "Checking mail server capabilities..."
msgstr "ཡིག་འཕྲིན་སར་བར་གྱི་ལྕོགས་གྲུབ་་ཞིབ་དཔྱད་རྐྱབ་དོ་་་་"

# Status - logging on
## @name IMAP_STATUS_SENDING_LOGIN
## @loc None
#: 5013
msgid "Sending login information..."
msgstr "ནང་བསྐྱོད་བརྡ་དོན་གཏང་དོ་་་་"

# Status - auth logon
## @name IMAP_STATUS_SENDING_AUTH_LOGIN
## @loc None
#: 5014
msgid "Sending authenticate login information..."
msgstr "བདེན་ཁུངས་ཅན་གྱི་ནང་བསྐྱོད་བརྡ་དོན་གཏང་དོ་་་་"

## @name IMAP_DOWNLOADING_MESSAGE
## @loc None
#: 5015
msgid "Downloading message..."
msgstr "འཕྲིན་དོན་ཚུ་ ཕབ་ལེན་འབད་དོ་་་་"

## @name IMAP_STATUS_GETTING_NAMESPACE
## @loc None
# LOCALIZATION NOTE (Error 5020): Do not translate the word "IMAP" below.
#: 5020
msgid "Checking IMAP Namespace..."
msgstr "IMAP མིང་གི་བར་སྟོང་ ཞིབ་དཔྱད་རྐྱབ་དོ་་་་"

## @name IMAP_UPGRADE_NO_PERSONAL_NAMESPACE
## @loc None
#: 5021
msgid "\"The mail server indicates that you may not have any personal mail folders. Please verify your subscriptions."
msgstr "ཡིག་འཕྲིན་སར་བར་གྱིས་ ཁྱོད་ལུ་སྒེར་དོན་ཡིག་འཕྲིན་གྱི་སྣོད་འཛིན་ཚུ་མེད་པའི་ བརྡ་སྟོནམ་ཨིན་མས། ཁྱོད་རའི་མཁོ་མངགས་ལུ་ བདེན་སྦྱོར་འབད།"

## @name IMAP_UPGRADE_PROMPT_USER_2
## @loc None
#: 5024
msgid "Any folders which are left unsubscribed will not appear in your folder lists, but can be subscribed to later by choosing File / Subscribe."
msgstr "མཁོ་མངགས་མ་འབད་བའི་སྣོད་འཛིན་ཚུ་ ཁྱོད་ཀྱི་སྣོད་འཛིན་ཐོ་ཡིག་ནང་མི་འཆར། འདི་འབདཝ་ད་ ཡིག་སྣོད་/མཁོ་མངགས་འབད་ ཟེར་མི་གདམ་ཁ་བརྐྱབས་པའི་ཐོག་ལས་ མཁོ་མངགས་འབད་ཚུགས།"

## @name IMAP_UPGRADE_CUSTOM
## @loc None
#: 5026
msgid "Please choose which folders you want subscribed in the Subscribe window..."
msgstr " ཁྱོད་ཀྱིས་མཁོ་མངགས་འབད་དགོ་མནོ་མི་ སྣོད་འཛིན་ཚུ་ མཁོ་མངགས་སྒོ་སྒྲིག་ནང་ གདམ་ཁ་བརྐྱབ་གནང་་་་"

## @name IMAP_UPGRADE_SUCCESSFUL
## @loc None
#: 5028
msgid "The upgrade was successful."
msgstr "ཡར་བསྐྱེད་འབད་ནི་དེ་ མཐར་འཁྱོལ་བྱི་ཡོད།"

## @name IMAP_GETTING_ACL_FOR_FOLDER
## @loc None
# LOCALIZATION NOTE (Error 5029): Do not translate the word "ACL" below.
#: 5029
msgid "Getting folder ACL..."
msgstr "སྣོད་འཛིན་ ACL ལེན་དོ་་་་"

## @name IMAP_GETTING_SERVER_INFO
## @loc None
#: 5030
msgid "Getting Server Configuration Info..."
msgstr "སར་བར་རིམ་སྒྲིག་གི་བརྡ་དོན་ལེན་དོ་་་་"

## @name IMAP_GETTING_MAILBOX_INFO
## @loc None
#: 5031
msgid "Getting Mailbox Configuration Info..."
msgstr "ཡིག་སྒྲོམ་རིམ་སྒྲིག་གི་བརྡ་དོན་ལེན་དོ་་་"

## @name IMAP_EMPTY_MIME_PART
## @loc None
#: 5032
msgid "This body part will be downloaded on demand."
msgstr "མ་གཟུགས་ཆ་ཤས་འདི་ མཁོ་འདོད་བཀོད་སྐབས་ ཕབ་ལེན་འབད་ནི་ཨིན།"

## @name IMAP_UNABLE_TO_SAVE_MESSAGE
## @loc None
# LOCALIZATION NOTE (Error 5033): Do not translate the word "IMAP" below.
#: 5033
msgid "IMAP Error: The message could not be saved due to an error."
msgstr "IMAP གི་འཛོལ་བ་:  འཛོལ་བ་ཅིག་ལུ་བརྟེན་ཏེ་ འཕྲིན་དོན་འདི་བསྲུང་མ་ཚུགས།"

## @name IMAP_NO_ONLINE_FOLDER
## @loc None
# LOCALIZATION NOTE (Error 5034): Do not translate the word "IMAP" below.
#: 5034
msgid "IMAP Error: The online folder information could not be retrieved."
msgstr "IMAP གི་འཛོལ་བ་: གློག་ཐོག་སྣོད་འཛིན་གྱི་བརྡ་དོན་དེ་ སླར་འདྲེན་འབད་མ་ཚུགས།"

## @name IMAP_LOGIN_FAILED
## @loc None
#: 5035
msgid "Login to server %S failed."
msgstr "སར་བར་%Sལུ་ ནང་བསྐྱོད་འབད་མ་ཚུགས།"

