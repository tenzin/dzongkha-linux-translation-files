# extracted from MsgSynchronize.xul and msgSelectOffline.xul
#: MsgSynchronize.label
msgid "Download and Sync Messages"
msgstr "འཕྲིན་དོན་ཚུ་ཕབ་ལེན་འབད་དེ་ མཉམ་འབྱུང་འབད་"

#: MsgSelect.label
msgid "Items for Offline Use"
msgstr "གློག་ཐོག་མེན་པའི་ལག་ལེན་གྱི་དོན་ལུ་རྣམ་གྲངས་ཚུ་"

#: MsgSyncDesc.label
msgid "If you have already selected mail folders or newsgroups for offline use, you can download and/or sync them now. Otherwise, use the &quot;Select&quot; button to choose mail folders and newsgroups for offline use."
msgstr "ཁྱོད་ཀྱིས་ཧེ་མ་ལས་ གློག་ཐོག་མེན་པའི་ལག་ལེན་གྱི་དོན་ལུ་ ཡིག་འཕྲིན་སྣོད་འཛིན་ ཡང་ན་ གནས་ཚུལ་སྡེ་ཚན་ཚུ་ སེལ་འཐུ་འབད་དེ་ཡོད་པ་ཅིན་ ད་ལྟོ་ཁྱོད་ཀྱིས་ ཕབ་ལེན་འབད་དང་ ཡང་ན་ མཉམ་འབྱུང་འབད་ཚུགས། དེ་མིན་པ་ཅིན་ གློག་ཐོག་མེན་པའི་ལག་ལེན་གྱི་དོན་ལུ་ ཡིག་འཕྲིན་སྣོད་འཛིན་དང་ གནས་ཚུལ་སྡེ་ཚན་ཚུ་ གདམ་ཁ་བརྐྱབ་ནི་ལུ་ &quot;Select&quot; ཨེབ་རྟ་དེ་ ལགལེན་འཐབ།"

#: MsgSyncDirections.label
msgid "Download and/or sync the following:"
msgstr "འོག་གི་ཚུ་ ཕབ་ལེན་འབད་/ ཡང་ན་ མཉམ་འབྱུང་འབད།"

#: syncTypeMail.label
#: syncTypeMail.accesskey
msgid "&Mail messages"
msgstr "གློག་འཕྲིན་འཕྲིན་དོན་ཚུ་(&M)"

#: syncTypeNews.label
#: syncTypeNews.accesskey
msgid "&Newsgroup messages"
msgstr "གནས་ཚུལ་སྡེ་ཚན་གྱི་འཕྲིན་དོན་ཚུ་(&N)"

#: sendMessage.label
#: sendMessage.accesskey
msgid "&Send Unsent messages"
msgstr "མ་བཏང་པར་ཡོད་པའི་འཕྲིན་དོན་ཚུ་གཏང་(&S)"

#: workOffline.label
#: workOffline.accesskey
msgid "&Work offline once download and/or sync is complete"
msgstr "ཕབ་ལེན་དང་ ཡང་ན་ མཉམ་འབྱུང་འབད་ཚར་ཞིནམ་ལས་ གློག་ཐོག་མེནམ་སྦེ་ ལཱ་འབད།(&W)"

#: selectButton.label
#: selectButton.accesskey
msgid "S&elect..."
msgstr "སེལ་འཐུ་འབད་་་(&e)"

#: MsgSelectDesc.label
msgid "Choose mail folders and newsgroups for offline use."
msgstr "གློག་ཐོག་མེན་པའི་ལག་ལེན་གྱི་དོན་ལས་ ཡིག་འཕྲིན་སྣོད་འཛིན་དང་ གནས་ཚུལ་སྡེ་ཚན་ཚུ་ གདམ་ཁ་རྐྱབས།"

#: MsgSelectInd.label
msgid "Download"
msgstr "ཕབ་ལེན་འབད་"

#: MsgSelectItems.label
msgid "Folders and Newsgroups"
msgstr "སྣོད་འཛིན་དང་གནས་ཚུལ་སྡེ་ཚན་ཚུ་"

