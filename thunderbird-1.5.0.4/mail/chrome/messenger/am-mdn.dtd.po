#: pane.title
msgid "Return Receipts"
msgstr "སླར་ལོག་འབྱོར་རྟགས་ཚུ་"

#: useGlobalPrefs.label
#: useGlobalPrefs.accesskey
msgid "&Use my global return receipt preferences for this account"
msgstr "རྩིས་ཐོ་འདི་གི་དོན་ལུ་ ངེད་ཀྱི་སྤྱི་ཁྱབ་སླར་ལོག་འབྱོར་རྟགས་ཀྱི་ དགའ་གདམ་ཚུ་ ལག་ལེན་འཐབ།(&U)"

#: useCustomPrefs.label
#: useCustomPrefs.accesskey
msgid "&Customize return receipts for this account"
msgstr "རྩིས་ཐོ་འདི་གི་དོན་ལུ་ སླར་ལོག་འབྱོར་རྟགས་ཚུ་ སྲོལ་སྒྲིག་འབད་(&C)"

#: requestReceipt.label
#: requestReceipt.accesskey
msgid "&When sending messages, always request a return receipt"
msgstr "འཕྲིན་དོན་ཚུ་གཏངམ་ད་ ཨ་རྟག་ར་ སླར་ལོག་འབྱོར་རྟགས་ཅིག་གི་ ཞུ་བ་འབད་(&W)"

#: receiptArrive.label
msgid "When a receipt arrives:"
msgstr "འབྱོར་རྟགས་ཅིག་ལྷོད་པའི་སྐབས་:"

#: leaveIt.label
#: leaveIt.accesskey
msgid "Leave it in my &Inbox"
msgstr "ངེད་ཀྱི་ནང་སྒྲོམ་ནང་བཞག་(&I)"

# LOCALIZATION NOTE moveToSent.label Translate: 'Sent' according to Netscape glossary
#: moveToSent.label
#: moveToSent.accesskey
msgid "&Move it to my &quot;Sent&quot; folder"
msgstr "ངེད་ཀྱི་ &quot;Sent&quot; སྣོད་འཛིན་ནང་སྤོ་(&M)"

#: requestMDN.label
msgid "When I receive a request for a return receipt:"
msgstr "ང་ལུ་སླར་ལོག་འབྱོར་རྟགས་ཀྱི་དོན་ལུ་ ཞུ་བ་ཅིག་ཐོབ་པའི་སྐབས་:"

#: returnSome.label
#: returnSome.accesskey
msgid "Allow r&eturn receipts for some messages"
msgstr "འཕྲིན་དོན་ལ་ལོ་ཅིག་གི་དོན་ལུ་ སླར་ལོག་འབྱོར་རྟགས་ཚུ་གཏང་བཅུག་(&e)"

#: never.label
#: never.accesskey
msgid "&Never send a return receipt"
msgstr "ནམ་ཡང་སླར་ལོག་འབྱོར་རྟགས་ཅིག་མ་གཏང་(&N)"

#: notInToCc.label
#: notInToCc.accesskey
msgid "If I'm not in the &To or Cc of the message:"
msgstr "གལ་སྲིད་ ང་འཕྲིན་དོན་གྱི་གཏང་ཡུལ་ ཡང་ན་ ཤོག་གནག་འདྲ་བཤུས་ནང་མེད་པ་ཅིན་:(&T)"

#: outsideDomain.label
#: outsideDomain.accesskey
msgid "If the &sender is outside my domain:"
msgstr "གལ་སྲིད་ གཏང་མི་དེ་ ངེད་ཀྱི་ཌོ་མེན་གྱི་ཕྱི་ཁར་ཨིན་པ་ཅིན་:(&s)"

#: otherCases.label
#: otherCases.accesskey
msgid "In all &other cases:"
msgstr "གནད་དོན་གཞན་ཆ་མཉམ་ནང་:(&o)"

#: askMe.label
msgid "Ask me"
msgstr "ང་ལུ་འདྲིས་"

#: alwaysSend.label
msgid "Always send"
msgstr "ཨ་རྟག་ར་གཏང་"

#: neverSend.label
msgid "Never send"
msgstr "ནམ་ཡང་མ་གཏང་"

