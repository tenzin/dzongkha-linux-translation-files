#   LOCALIZATION NOTE :
#   do not localize "\n".  use "\n" to make the text fit nicely in the dialog.
#
# Download Messages Prompt
#
#: downloadMessagesWindowTitle
msgid "Work Offline"
msgstr "གློག་ཐོག་མེན་པར་ ལཱ་འབད་"

#   LOCALIZATION NOTE :
#   do not localize "\n".  use "\n" to make the text fit nicely in the dialog.
#: downloadMessagesLabel
msgid ""
"Do you want to download messages \n"
"for offline use before you go offline?\n"
"\n"
msgstr ""
"ཁྱོད་ཀྱིས་གློག་ཐོག་ལུ་མེནམ་ མ་འགྱོ་བའི་ཧེ་མ་ \n"
" གློག་ཐོག་མེན་པའི་ལག་ལེེན་གྱི་དོན་ལུ་ འཕྲིན་དོན་ཚུ་ ཕབ་ལེན་འབད་ནི་ཨིན་ན?\n"
"\n"

#: downloadMessagesCheckboxLabel
msgid "Always ask me when I go offline"
msgstr "ང་གིས་གློག་ཐོག་ལུ་མེནམ་འགྱོཝ་ད་ ཨ་རྟག་ར་་ང་ལུ་དྲིས།"

#: downloadMessagesDownloadButtonLabel
msgid "Download"
msgstr "ཕབ་ལེན་འབད་"

#: downloadMessagesNoDownloadButtonLabel
msgid "Don't Download"
msgstr "ཕབ་ལེན་མ་འབད་"

#: downloadMessagesCancelButtonLabel
#: sendMessagesCancelButtonLabel
#: getMessagesOfflineCancelButtonLabel
#: sendMessagesOfflineCancelButtonLabel
msgid "Cancel"
msgstr "ཆ་མེད་"

#
# Send Messages Prompt
#
#: sendMessagesWindowTitle
msgid "Work Online"
msgstr "གློག་ཐོག་ལུ་མེན་པར་ ལཱ་འབད་"

#   LOCALIZATION NOTE :
#   do not localize "\n".  use "\n" to make the text fit nicely in the dialog.
#: sendMessagesLabel
msgid ""
"Would you like to send the messages \n"
"in your Unsent Messages folder?\n"
"\n"
msgstr ""
"ཁྱོད་ཀྱི་མ་བཏང་པར་ཡོད་པའི་ འཕྲིན་དོན་སྣོད་འཛིན་ནང་གི་\n"
"འཕྲིན་དོན་དེ་ཚུ་གཏང་ནི་ཨིན་ན?\n"
"\n"

#: sendMessagesCheckboxLabel
msgid "Always ask me when I go online"
msgstr "ང་གིས་གློག་ཐོག་ལུ་འགྱོཝ་ད་ ཨ་རྟག་ར་་ང་ལུ་དྲིས།"

#: sendMessagesSendButtonLabel
msgid "Send"
msgstr "གཏང་"

#: sendMessagesNoSendButtonLabel
msgid "Don't Send"
msgstr "མ་གཏང་"

#
# GetMessages Offline Prompt
#
#: getMessagesOfflineWindowTitle
msgid "Get Messages"
msgstr "འཕྲིན་དོན་ཚུ་ལེན་"

#   LOCALIZATION NOTE :
#   do not localize "\n".  use "\n" to make the text fit nicely in the dialog.
#: getMessagesOfflineLabel
msgid ""
"You are currently offline. Would you like \n"
"to go online to get your new messages?\n"
"\n"
msgstr ""
"ད་ལྟོ་ཁྱོད་ གློག་ཐོག་ལུ་མེན། ཁྱོད་རའི་འཕྲིན་དོན་གསརཔ་ཚུ་\n"
" ལེན་ནིའི་དོན་ལུ་ གློག་ཐོག་ལུ་འགྱོ་ནི་ཨིན་ན?\n"
"\n"

#: getMessagesOfflineGoButtonLabel
#: sendMessagesOfflineGoButtonLabel
msgid "Go online"
msgstr "གློག་ཐོག་ལུ་འགྱོ་"

#
# Send Messages Offline Prompt
#
#: sendMessagesOfflineWindowTitle
msgid "Send Messages"
msgstr "འཕྲིན་དོན་ཚུ་གཏང་"

#   LOCALIZATION NOTE :
#   do not localize "\n".  use "\n" to make the text fit nicely in the dialog.
#: sendMessagesOfflineLabel
msgid ""
"You are currently offline. Would you like \n"
"to go online and send your unsent messages?\n"
"\n"
msgstr ""
"ད་ལྟོ་ཁྱོད་ གློག་ཐོག་ལུ་མེན། གློང་ཐོག་ལུ་འགྱོ་ཞིནམ་ལས་\n"
" མ་བཏང་པར་ཡོད་པའི་འཕྲིན་དོན་ཚུ་ གཏང་ནི་ཨིན་ན?\n"
"\n"

