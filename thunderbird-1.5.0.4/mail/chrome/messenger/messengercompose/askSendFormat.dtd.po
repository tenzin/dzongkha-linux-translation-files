# LOCALIZATION NOTE askSendFormat.dtd UI for dialog that asks the user, which format to use for sending a message
#: windowTitle.label
msgid "HTML Mail Question"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ ཡིག་འཕྲིན་གྱི་དྲི་བ།"

#: recipient.label
msgid "Some of the recipients are not listed as being able to receive HTML mail."
msgstr "ལེན་མཁན་ལ་ལོ་ཅིག་ ཨེཆ་ཊི་ཨེམ་ཨེལ་ཡིག་འཕྲིན་ ལེན་ཚུགས་མི་སྦེ་ ཐོ་བཀོད་མ་འབད་བས།"

#: convertibleDefault.label
msgid "_: convertibleDefault.label\n"
msgstr ""

#: convertibleYes.label
msgid "Your message can be converted to plain text without losing information."
msgstr "ཁྱོད་ཀྱིས་འཕྲིན་དོན་དེ་ བརྡ་དོན་བརླག་སྟོར་མ་ཞུགས་པར་ ཡིག་རྐྱང་ལུ་གཞི་བསྒྱུར་འབད་ཚུགས།"

#: convertibleAltering.label
msgid "Your message can be converted to plain text without losing important information. However, the plain text version might look different from what you saw in the composer."
msgstr "ཁྱོད་ཀྱིས་འཕྲིན་དོན་དེ་ བརྡ་དོན་གལ་ཅན་བརླག་སྟོར་མ་ཞུགས་པར་ ཡིག་རྐྱང་ལུ་གཞི་བསྒྱུར་འབད་ཚུགས།"

#: convertibleNo.label
msgid "However, you used formatting (e.g. colors) that will not be converted to plain text."
msgstr "དེ་འབདཝ་ད་ ཁྱོད་ཀྱིས་ཡིག་རྐྱང་ལུ་ གཞི་བསྒྱུར་འབད་བཏུབ་པའི་རྩ་སྒྲིག་(དཔེར་ན་ཚོས་གཞི་ཚུ་) ལག་ལེན་འཐབ་ནུག།"

#: question.label
msgid "Would you like to convert the message to plain text or send it in HTML anyway?"
msgstr "ཁྱོད་ཀྱིས་འཕྲིན་དོན་དེ་ ཡིག་རྐྱང་ལུ་གཞི་བསྒྱུར་འབད་ནི་ཨིན་ན་ ཡང་ན་ ཨེཆ་ཊི་ཨེམ་ཨེལ་སྦེ་ གཏང་ནི་ཨིན་ན?"

#: plainTextAndHtml.label
msgid "Send in Plain Text and HTML"
msgstr "ཡིག་རྐྱང་དང་ ཨེཆ་ཊི་ཨེམ་ཨེལ་ནང་གཏང་།"

#: plainTextOnly.label
msgid "Send in Plain Text Only"
msgstr "ཡིག་རྐྱང་ནང་རྐྱང་གཅིག་གཏང་།"

#: htmlOnly.label
msgid "Send in HTML Only"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ནང་རྐྱངམ་གཅིག་གཏང་།"

#: plainTextAndHtmlRecommended.label
msgid "Send in Plain Text and HTML (recommended)"
msgstr "ཡིག་རྐྱང་དང་ཨེཆ་ཊི་ཨེམ་ཨེལ་ནང་གཏང་།(འོས་སྦྱོར་འབད་ཡོདཔ)"

#: plainTextOnlyRecommended.label
msgid "Send in Plain Text Only (recommended)"
msgstr "ཡིག་རྐྱང་ནང་རྐྱངམ་གཅིག་གཏང་།(འོས་སྦྱོར་འབད་ཡོདཔ་)"

#: htmlOnlyRecommended.label
msgid "Send in HTML Only (recommended)"
msgstr "ཨེཆ་ཨེམ་ཊི་ཨེལ་ནང་རྐྱངམ་གཅིག་གཏང་།(འོས་སྦྱོར་འབད་ཡོདཔ)"

#: recommended.label
msgid "(recommended)"
msgstr "(འོས་སྦྱོར་འབད་ཡོདཔ)"

#: send.label
msgid "Send"
msgstr "གཏང་"

#: cancel.label
msgid "Cancel"
msgstr "ཆ་མེད་"

#: recipients.label
msgid "Recipients..."
msgstr "ལེན་མཁན་་་"

#: help.label
msgid "Help"
msgstr "གྲོགས་རམ་"

