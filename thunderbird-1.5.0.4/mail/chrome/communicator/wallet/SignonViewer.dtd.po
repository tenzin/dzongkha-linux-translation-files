#: windowtitle.label
msgid "Password Manager"
msgstr "ཆོག་ཡིག་འཛིན་སྐྱོང་པ།"

#: windowalttitle.label
msgid "Form Manager: Sites"
msgstr "འབྲི་ཤོག་འཛིན་སྐྱོང་པ་:ས་ཁོངས་ཚུ།"

#: tab.signonsstored.label
msgid "Passwords Saved"
msgstr "བསྲུངས་ཡོད་པའི་ཆོག་ཡིག་ཚུ།"

#: tab.signonsnotstored.label
msgid "Passwords Never Saved"
msgstr "ནམ་ཡང་མ་བསྲུངས་པའི་ཆོག་ཡིག་ཚུ།"

#: tab.nopreview.label
msgid "Forms Never Previewed"
msgstr "ནམ་ཡང་སྔོན་ལྟ་མ་འབད་བའི་འབྲི་ཤོག་ཚུ།"

#: tab.nocapture.label
msgid "Forms Never Saved"
msgstr "ནམ་ཡང་མ་བསྲུངས་པའི་འབྲི་ཤོག་ཚུ།"

#: spiel.signonsstored.label
msgid "Password Manager has saved login information for the following sites:"
msgstr "ཆོག་ཡིག་འཛིན་སྐྱོང་པ་གིས་ འོག་གི་ས་ཁོངས་ཚུ་གི་དོན་ལུ་ ནང་བསྐྱོད་བརྡ་དོན་བསྲུངས་བཞག་ནུག་:"

#: spiel.signonsnotstored.label
msgid "Password Manager will never save login information for the following sites:"
msgstr "ཆོག་ཡིག་འཛིན་སྐྱོང་པ་གིས་ ནམ་ཡང་འོག་གི་ས་ཁོངས་ཚུ་གི་དོན་ལུ་ ནང་བསྐྱོད་བརྡ་དོན་མི་བསྲུང་་:"

#: spiel.nopreview.label
msgid "Form Manager will never preview forms from the following sites before pre-filling them for you:"
msgstr "འབྲི་ཤོག་འཛིན་སྐྱོང་པ་གིས་ ཁྱོད་ཀྱི་དོན་ལུ་འབྲི་ཤོག་ཚུ་མ་བཀང་བའི་ཧེ་མ་ ནམ་ཡང་འོག་གི་ས་ཁོངས་ཚུ་ནང་ལས་ འབྲི་ཤོག་ཚུ་སྔོན་ལྟ་མི་འབད་:"

#: spiel.nocapture.label
msgid "Form Manager will never ask to save forms from the following sites:"
msgstr "འབྲི་ཤོག་འཛིན་སྐྱོང་པ་གིས་  ནམ་ཡང་འོག་གི་ས་ཁོངས་ཚུ་ནང་ལས་ འབྲི་ཤོག་ཚུ་བསྲུང་ཟེར་མི་སླབ་:"

#: treehead.site.label
msgid "Site"
msgstr "ས་ཁོངས།"

#: treehead.username.label
msgid "Username"
msgstr "ལག་ལེན་པའི་མིང་།"

#: treehead.password.label
msgid "Password"
msgstr "ཆོག་ཡིག།"

#: remove.label
msgid "Remove"
msgstr "རྩ་བསྐྲད་གཏང་།"

#: removeall.label
msgid "Remove All"
msgstr "ཆ་མཉམ་རྩ་བསྐྲད་གཏང་།"

