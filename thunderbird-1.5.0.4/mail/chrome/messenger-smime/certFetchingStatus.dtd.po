# LOCALIZATION NOTE shown while obtaining certificates from a directory
#: title.label
msgid "Downloading Certificates"
msgstr "ལག་ཁྱེར་ཚུ་ཕབ་ལེན་འབད་དོ་"

#: info.message
msgid "Searching the directory for recipients' certificates. This may take a few minutes."
msgstr "ཐོབ་མཁན་གྱི་ལག་ཁྱེར་དོན་ལུ་ སྣོད་ཐོ་འཚོལ་ཞིབ་འབད་དོ། འདི་གི་དོན་ལུ་ སྐར་མ་དག་པ་ཅིན་འགོར་འོང་།"

#: stop.label
msgid "Stop Searching"
msgstr "འཚོལ་ཞིབ་འབད་ནི་བཀག།"

