# LOCALIZATION NOTE msgReadSecurityInfo.dtd UI for viewing security status when reading a received message
#: status.label
msgid "Message Security"
msgstr "འཕྲིན་དོན་སྲུང་སྐྱོབ་"

#: signatureCert.label
msgid "View Signature Certificate"
msgstr "མིང་རྟགས་ལག་ཁྱེར་སྟོན།"

#: encryptionCert.label
msgid "View Encryption Certificate"
msgstr "གསང་བཟོའི་ལག་ཁྱེར་སྟོན་"

#: signer.name
msgid "Signed by:"
msgstr "མིང་རྟགས་བཀོད་མི་:"

#: recipient.name
msgid "Encrypted for:"
msgstr "དོན་ལུ་གསང་བ་བཟོ་ཡོདཔ་:"

#: email.address
msgid "Email address:"
msgstr "གློག་འཕྲིན་ཁ་བྱང་:"

#: issuer.name
msgid "Certificate issued by:"
msgstr "ལག་ཁྱེར་སྤྲོད་མི་:"

