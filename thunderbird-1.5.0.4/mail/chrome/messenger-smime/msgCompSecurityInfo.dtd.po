# LOCALIZATION NOTE msgCompSecurityInfo.dtd UI for viewing security status when composing a message
#: title.label
msgid "Message Security"
msgstr "འཕྲིན་དོན་སྲུང་སྐྱོབ་"

#: subject.plaintextWarning
msgid "Please note: Subject lines of email messages are never encrypted."
msgstr "སེམས་ཁར་བཞག་: གློག་འཕྲིན་འཕྲིན་དོན་ཚུ་གི་དོན་ཚན་གྱལ་རིམ་ཚུ་ ནམ་ཡང་གསང་བ་ཟོ་ནི་མེད།"

#: status.heading
msgid "The contents of your message will be sent as follows:"
msgstr "ཁྱོད་ཀྱི་འཕྲིན་དོན་གྱི་ ནང་དོན་ཚུ་གཤམ་གསལ་ལྟར་དུ་ གཏང་ནི་ཨིན་:"

#: status.signed
msgid "Digitally signed:"
msgstr "ཌི་ཇི་ཊཱལ་ཐོག་ལས་ མིང་རྟགས་བཀོད་ཡོདཔ་:"

#: status.encrypted
msgid "Encrypted:"
msgstr "གསང་བཟོས་:"

#: status.certificates
msgid "Certificates:"
msgstr "ལག་ཁྱེར་ཚུ་:"

#: view.label
#: view.accesskey
msgid "&View"
msgstr "མཐོང་སྣང་(&V)"

#: tree.recipient
msgid "Recipient"
msgstr "ཐོབ་མཁན་"

#: tree.status
msgid "Status"
msgstr "གནས་ཚད་"

#: tree.issuedDate
msgid "Issued"
msgstr "སྤྲོད་ཡོདཔ་"

#: tree.expiresDate
msgid "Expires"
msgstr "དུས་ཡོལ་ཐལཝ་ཨིན་"

