# LOCALIZATION NOTE msgCompSMIMEOverlay.dtd UI for s/mime hooks in message composition
#: securityMenu.label
#: securityMenu.accesskey
msgid "S&ecurity"
msgstr "སྲུང་སྐྱོབ་(&e)"

#: menu_securityEncryptIfPossible.label
#: menu_securityEncryptIfPossible.accesskey
msgid "Encrypt if &Possible"
msgstr "ཚུགས་པ་ཅིན་ གསང་བ་བཟོ་(&P)"

#: menu_securityEncryptRequire.label
#: menu_securityEncryptRequire.accesskey
msgid "&Encrypt This Message"
msgstr "འཕྲིན་དོན་འདི་གསང་བ་བཟོ་(&E)"

#: menu_securityNoEncryption.label
#: menu_securityNoEncryption.accesskey
msgid "Do &Not Encrypt This Message"
msgstr "འཕྲིན་དོན་འདི་ གསང་བ་མ་བཟོ་(&N)"

#: menu_securitySign.label
#: menu_securitySign.accesskey
msgid "Digitally &Sign This Message"
msgstr "འཕྲིན་དོན་འདི་ལུ་ ཌི་ཇི་ཊཱལ་ཐོག་ལས་ མིང་རྟགས་བཀོད་(&S)"

#: menu_securityStatus.label
#: menu_securityStatus.accesskey
msgid "View Security &Info"
msgstr "སྲུང་སྐྱོབ་བརྡ་དོན་སྟོན་(&I)"

#: securityButton.label
msgid "Security"
msgstr "སྲུང་སྐྱོབ་"

#: securityButton.tooltip
msgid "View or change security settings"
msgstr "སྲུང་སྐྱོབ་ཀྱི་སྒྲིག་སྟངས་སྟོན་ ཡང་ན་ བསྒྱེར་བཅོས་འབད།"

#: menu_viewSecurityStatus.label
#: menu_viewSecurityStatus.accesskey
msgid "Message Security &Info"
msgstr "འཕྲིན་དོན་སྲུང་སྐྱོབ་བརྡ་དོན།(&I)"

