#: Installing_Component
msgid "Currently Installing:"
msgstr "ད་ལྟོ་གཞི་བཙུགས་འབད་དོ་:"

#: Success_Title
#: Success_Header
msgid "Install Complete"
msgstr "གཞི་བཙུགས་ཚར་ཡོད་"

#: Success_Message0
msgid "%s was successfully installed."
msgstr "%sདེ་མཐར་འཁྱོལ་ཅན་སྦེ་ གཞི་བཙུགས་འབད་ཡི།"

#: Success_Message1
msgid "Click Finish to complete Setup."
msgstr "གཞི་སྒྲིག་མཇུག་བསྡུ་ནི་ལུ་ རྫོགས་ གུ་ཨེབ་གཞང་འབད།"

#: Success_Launch
msgid "Launch %s now."
msgstr "ད་ལྟོ་ར་ %sགསར་བཙུགས་འབད།"

#: Check_Message
msgid "Download of $ProductName$ was successful. $ProductNameInternal$ must be closed to proceed with installation. Click OK to exit $ProductNameInternal$ automatically and to begin installation."
msgstr "$ProductName$ ཕབ་ལེན་འབད་ནི་དེ་ མཐར་འཁྱོལ་བྱུང་ཡོད། གཞི་བཙུགས་འཕྲོ་མཐུད་ནི་ལུ་$ProductNameInternal$ ཁ་བསྡམ་དགོ། $ProductNameInternal$ རང་བཞིན་གྱིས་ཕྱིར་འཐོན་འབད་དེ་ གཞི་བཙུགས་འགོ་བཙུག་ནི་ལུ་ བཏུབ་ གུ་ཨེབ་གཏང་འབད། "

#: Check_FullMessage
msgid "$ProductNameInternal$ must be closed to proceed with installation. Click OK to exit $ProductNameInternal$ automatically and to begin installation."
msgstr "གཞི་བཙུགས་འཕྲོ་མཐུད་ནི་ལུ་ $ProductNameInternal$ ཁ་བསྡོམ་དགོ། $ProductNameInternal$ རང་བཞིན་གྱིས་ཕྱིར་འཐོན་འབད་དེ་ གཞི་བཙུགས་འགོ་བཙུག་ནི་ལུ་ བཏུབ་ གུ་ཨེབ་གཏང་འབད། "

#: Check_Wait
msgid "Shutting down $ProductNameInternal$.  Please wait..."
msgstr " $ProductNameInternal$ སྒོ་བསྡམ་དོ། བསྒུག་སྡོད་གནང་་་"

#: Component_XPCOM
msgid "Cross Platform COM"
msgstr "མཉེན་རིམ་ཕན་ཚུན་འབྲེལ་བའི་ཀོམ།"

#: Component_Mail
msgid "$ProductName$"
msgstr "$ProductName$"

#: Component_Offline
msgid "Offline Support"
msgstr "གློག་ཐོག་མེན་པའི་རྒྱབ་སྐྱོར།"

