#: Welcome_Title
msgid "$ProductName$ Setup"
msgstr "$ProductName$ གི་གཞིསྒྲིག་"

#: Welcome_MessageWelcome
msgid "Welcome to %s!"
msgstr "%sལུ་འབྱོན་པ་ལེགས་སོ་ཡོད།"

#: Welcome_Message0
msgid "You are about to install %s."
msgstr "ཁྱོད་ཀྱིས་%sགཞི་བཙུགས་འབད་ནི་འབད་དོ།"

#: Welcome_Message1
msgid "It is strongly recommended that you exit all Windows programs before running this Setup program."
msgstr "གཞི་བཙུགས་ཀྱི་ལས་རིམ་འདི་ གཡོག་མ་བཀོལ་བའི་ཧེ་མ་ལས་ ཁྱོད་ཀྱིས་ཝིན་ཌོསི་གི་ལས་རིམ་ཆ་མཉམ་ར་ ཕྱིར་འཐོན་འབད་དགོ།"

#: Welcome_Message2
msgid "Click Cancel to quit Setup and then close any programs you have running.  Click Next to continue the Setup program."
msgstr "གཞི་སྒྲིག་ལས་སྤང་ནི་ལུ་ ཆ་མེད་ གུ་ཨེབ་གཏང་འབད་ཞིནམ་ལས་ ཁྱོད་ཀྱིས་ལགལེན་འཐབ་སྟེ་ཡོད་མི་ ལས་རིམ་ཚུ་ཁ་བསྡམས། གཞི་སྒྲིག་ལས་རིམ་འཕྲོ་མཐུད་ནི་ལུ་ ཤུལ་མམ་ གུ་ཨེབ་གཏང་འབད།"

#: Welcome_Message3
msgid "Click Next to continue installing %s."
msgstr "%sགཞི་བཙུགས་འབད་ནི་ལུ་ ཤུལ་མམ་ གུ་ཨེབ་གཏང་འབད།"

#: License_Title
msgid "Software License Agreement"
msgstr "མཉེན་ཆས་ཆོག་ཐམ་གྱི་གན་འཛིན།"

#: License_SubTitle
msgid "Terms and conditions for using this software."
msgstr "མཉེན་ཆས་འདི་ལག་ལེན་འཐབ་ནིའི་དོན་ལུ་ ཁ་ཚིག་དང་གནས་ཚིག་ཚུ།"

#: License_Message0
msgid "Please read the following license agreement. Use the scroll bar to view the rest of this agreement."
msgstr "འོག་གི་ཆོག་ཐམ་གན་འཛིན་དེ་ལྷག་གནང་། གན་འཛིན་འདི་ཆ་ཚང་བལྟ་ནི་ལུ་ བཤུད་ཕྲ་ལག་ལེན་འཐབ།"

#: License_Accept
msgid "I &Accept the terms of the License Agreement"
msgstr "ང་གིས་ཆོག་ཐམ་གྱི་གནས་ཚིག་ཚུ་ལུ་ དང་ལེན་འབདཝ་ཨིན།(&A)"

#: License_Decline
msgid "I do NO&T Accept the terms of the License Agreement"
msgstr "ང་གིས་ཆོག་ཐམ་གྱི་གནས་ཚིག་ཚུ་ལུ་ དང་ལེན་མི་འབད།(&A)"

#: Type_Title
msgid "Setup Type"
msgstr "གཞི་སྒྲིག་གི་དབྱེ་བ་"

#: Type_SubTitle
msgid "Choose setup options."
msgstr "གཞི་སྒྲིག་གི་གདམ་ཁ་སེལ།"

#: Type_Message0
msgid "Choose the type of Setup you prefer, then click Next."
msgstr "ཁྱོད་ཀྱིས་དགའ་མི་གཞི་སྒྲིག་དེ་ གདམ་ཁ་བརྐྱབས་ཞིནམ་ལས་ ཤུལ་མམ་ གུ་ཨེབ་གཏང་འབད།"

#: Standard_Short
msgid "&Standard"
msgstr "ཚད་ལྡན་(&S)"

#: Standard_Long
msgid "Thunderbird will be installed with the most common options."
msgstr "ཐཱན་ཌར་བཌི་དེ་ སྤྱི་མཐུན་གྱི་གདམ་ཁའི་ཐོག་ལུ་ གཞི་བཙུགས་འབད་ནི་ཨིན།"

#: Custom_Short
msgid "&Custom"
msgstr "སྲོལ་སྒྲིག་(&C)"

#: Custom_Long
msgid "You may choose individual options to be installed. Recommended for experienced users."
msgstr "ཁྱོད་ཀྱིས་གཞི་བཙུགས་འབད་ནིའི་དོ་ནལུ་ རང་སོའི་གདམ་ཁ་ཚུ་ གདམ་ཁ་བརྐྱབ་ཆོག། ཉམས་མྱོང་ཅན་གྱི་ལག་ལེན་པ་ཚུ་གི་དོ་ནལུ་འོས་སྦྱོར་འབདཝ་ཨིན།"

#: Select_Title
msgid "Select Components"
msgstr "ཆ་ཤས་ཚུ་སེལ་འཐུ་འབད།"

#: Select_SubTitle
msgid "Additional components that enhance $ProductName$."
msgstr "$ProductName$ ཡར་བསྐྱེད་གནང་མི་ ཆ་ཤས་ཁ་སྐོང་ཚུ།"

#: Select_Message0
msgid "Choose additional components you want to install, then click Next."
msgstr "ཁྱོད་ཀྱིས་གཞི་བཙུགས་འབད་དགོ་མནོ་མི་ ཆ་ཤས་ཁ་སྐོང་ཚུ་ གདམ་ཁ་བརྐྱབ་ཞིནམ་ལས་ ཤུལ་མམ་ གུ་ཨེབ་གཏང་འབད།"

#: Path_Title
msgid "Install Folder"
msgstr "སྣོད་འཛིན་གཞི་བཙུགས་འབད་"

#: Path_SubTitle
msgid "Where $ProductName$ will be installed."
msgstr "$ProductName$  ག་ཏེ་གཞི་བཙུགས་འབད་ནི་ཨིན་ན།"

#: Path_Message0
msgid "$ProductName$ will be installed into the following folder:"
msgstr "$ProductName$ དེ་ འོག་གི་སྣོད་འཛིན་ཚུ་ནང་ གཞི་བཙུགས་འབད་ནི་ཨིན་:"

#: Integration_Title
msgid "Set Up Shortcuts"
msgstr "མགྱོགས་ཐབས་གཞི་བཙུགས་འབད་"

#: Integration_SubTitle
msgid "Creates Program Icons"
msgstr "ལས་རིམ་ངོས་དཔར་ཚུ་ གསར་བསྐྲུན་འབདཝ་ཨིན།"

#: Integration_Message0
msgid "Create icons for $ProductShortName$:"
msgstr "$ProductShortName$ གི་དོན་ལུ་ ངོས་དཔར་ཚུ་ གསར་བསྐྲུན་འབད་:"

#: Integration_Desktop
msgid "On my Desktop"
msgstr "ངེད་ཀྱི་ཌེཀསི་ཊོཔ་གུ་"

#: Integration_StartMenu
msgid "In my Start Menu Programs folder"
msgstr "ངེད་ཀྱི་འགོ་བཙུགས་དཀར་ཆག་གི་ ལས་རིམ་སྣོད་འཛིན་ནང་"

#: Integration_QuickLaunch
msgid "In my Quick Launch bar"
msgstr "ངེད་ཀྱི་འཕྲལ་མགྱོགས་གཞི་བཙུགས་ཕྲ་རིང་ནང་"

#: SelectFolder_Title
msgid "$ProductName$ Setup - Select Program Folder"
msgstr "$ProductName$ གཞི་སྒྲིག་- ལས་རིམ་སྣོད་འཛིན་སེལ་འཐུ་འབད།"

#: SelectFolder_Message0
msgid "Setup will add program icons to the Program Folder listed below.  You may type a new folder name, or select one from the Existing Folders list.  Click Next to continue."
msgstr "གཞི་སྒྲིག་དེ་གིས་ འོག་ལུ་ཐོ་བཀོད་འབད་མི་ ལས་རིམ་སྣོད་འཛིན་ལུ་ ལས་རིམ་ངོས་དཔར་ཚུ་ ཁ་སྐོང་བརྐྱབ་འོང་།"

#: Advanced_Title
msgid "$ProductName$ Setup - Advanced Settings"
msgstr "$ProductName$ གཞི་སྒྲིག་- མོཐ་རིམ་ཅན་གྱི་སྒྲིག་སྟངས་"

#: Advanced_Message0
msgid "If your Internet connection requires a proxy server, enter your server name and port information.  If your Internet connection doesn't require a proxy server, leave the fields blank."
msgstr "ཁྱོད་ཀྱི་ཨིན་ཊར་ནེཊི་མཐུད་ལམ་ལུ་ པོ་རོེག་སི་སར་བར་ཅིག་དོག་པ་ཅིན་ ཁྱོད་རའི་སར་བར་མིང་དང་ འདྲེན་ལམ་བརྡ་དོན་བཙུགས། "

#: Install_Title
msgid "Summary"
msgstr "བཅུད་དོན་"

