#ResolvingHost=Looking up
#ConnectedTo=Connected to
#ConnectingTo=Connecting to
#SendingRequestTo=Sending request to
#TransferringDataFrom=Transferring data from
#: 3
msgid "Looking up %1$S..."
msgstr "%1$S ལུ་བལྟ་དོ་་་་"

#: 4
msgid "Connected to %1$S..."
msgstr "%1$S ལུ་མཐུད་ཅི་་་་"

#: 5
msgid "Sending request to %1$S..."
msgstr "%1$S ལུ་ ཞུ་བ་གཏང་དོ་་་་"

#: 6
msgid "Transferring data from %1$S..."
msgstr "%1$S ནང་ལས་ གནས་སྡུད་གནས་སོར་འབད་དོ་་་་"

#: 7
msgid "Connecting to %1$S..."
msgstr "%1$S ལུ་མཐུད་དོ་་་"

#: 8
msgid "Read %1$S"
msgstr "%1$S ལྷག་"

#: 9
msgid "Wrote %1$S"
msgstr "%1$S བྲིས་ཡོད་"

#: 10
msgid "Waiting for %1$S..."
msgstr "%1$S ལུ་བསྒུད་སྡོད་དོ་་་་"

#: 27
msgid "Beginning FTP transaction..."
msgstr "ཨེཕ་ཊི་པི་སྤྲོད་ལེན་འགོ་བཙུགས་དོ་་་"

#: 28
msgid "Finished FTP transaction"
msgstr "ཨེཕ་ཊི་པི་སྤྲོད་ལེན་མཇུག་བསྡུ་ཡོད་"

#: EnterUserPasswordForRealm
msgid "Enter username and password for \"%1$S\" at %2$S"
msgstr "\"%1$S\" at %2$S གི་དོན་ལུ་ ལག་ལེན་པའི་མིང་དང་ ཆོག་ཡིག་བཙུགས་"

#: EnterUserPasswordForProxy
msgid "Enter username and password for proxy \"%1$S\" at %2$S"
msgstr "པོརོག་སི་ \"%1$S\" གི་དོན་ལུ་  %2$S ལུ་ ལག་ལེན་པའི་མིང་དང་ ཆོག་ཡིག་བཙུགས་"

#: EnterUserPasswordFor
msgid "Enter username and password for %1$S"
msgstr "%1$Sགི་དོན་ལུ་ ལག་ལེན་པའི་མིང་དང་ ཆོག་ཡིག་བཙུགས་"

#: EnterPasswordFor
msgid "Enter password for %1$S on %2$S"
msgstr "%1$S གི་དོན་ལུ་ %2$S གུ་ ཆོག་ཡིག་བཙུགས་"

#: UnsupportedFTPServer
msgid "The FTP server %1$S is currently unsupported."
msgstr "ཨེཕ་ཊི་པི་སར་བར་ %1$S དེ་ལུ་ ད་ལྟོ་རྒྱབ་སྐྱོར་མིན་འདུག།"

#: RepostFormData
msgid "This web page is being redirected to a new location. Would you like to resend the form data you have typed to the new location?"
msgstr "ཝེབ་ཤོག་ལེབ་འདི་ གནས་ཁོངས་གསརཔ་ཅིག་ལུ་ ལོག་གཏང་དོ། ཁྱོད་ཀྱིས་ཡིག་དཔར་བརྐྱབས་མི་ འབྲི་ཤོག་གནས་སྡུད་དེ་ སླར་གཏང་ནི་ཨིན་ན?"

# Directory listing strings
#: DirTitle
msgid "Index of %1$S"
msgstr "%1$Sགི་ཟུར་ཐོ་"

#: DirGoUp
msgid "Up to higher level directory"
msgstr "མཐོ་རིམ་གནས་རིམ་གྱི་སྣོད་ཐོ་ཚུན་ཚོད་"

#Gopher Search Prompt
#: GopherPromptTitle
msgid "Search"
msgstr "འཚོལ་ཞིབ་"

#: GopherPromptText
msgid "Enter a search term:"
msgstr "འཚོལ་ཞིབ་འབད་ནིའི་ཐ་སྙད་ཅིག་བཙུགས་:"

#: PhishingAuth
msgid "You are about to visit \"%1$S\". This site may be attempting to trick you into thinking you are visiting a different site. Use extreme caution."
msgstr "ཁྱོད་ཀྱིས་ \"%1$S\"ལུ་ ལྟ་སྤྱོད་འབད་ནི་འབད་དོ། ས་ཁོངས་འདི་གིས་ ཁྱོད་ལུ་ས་ཁོངས་སོ་སོ་ཅིག་ལུ་ ལྟ་སྤྱོད་འབད་དོ་ཟེར་མནོ་བཅུག་ནི་ལུ་ གཡོ་སྒྱུ་གཏང་དོ་ཡོདཔ་འོང་། དྲན་པ་ཐེབས་བཏོན།"

#: PhishingAuthAccept
msgid "I understand and will be very careful"
msgstr "ང་གིས་ཧ་ཡོདཔ་ལས་ དྲན་པ་ཐེབས་བཏོན་འོང་།"

#: SuperfluousAuth
msgid ""
"You are about to log into the site \"%1$S\" with the username \"%2$S\", but the website does not require authentication. This may be an attempt to trick you.\n"
"\n"
"Is \"%1$S\" the site you want to visit?"
msgstr ""
"ཁྱོད་ཀྱིས་ས་ཁོངས་  \"%1$S\"ལུ་ ནང་བསྐྱོད་འབད་ནི་འབད་དོ་ ལག་ལེན་པའི་མིང་ \"%2$S\" གི་ཐོག་ལས། དེ་འབདཝ་ད་ ཝེབ་ས་ཁོངས་འདི་ལུ་ བདེན་བཤད་མི་དགོ་པས།  འདི་ཡང་ཁྱོད་ལུ་གཡོ་སྒྱུ་གཏང་ནིའི་དཔའ་བཅམ་འོང་ནི་མས། n\n"
"ཁྱོད་ཀྱིས་ལྟ་སྤྱོད་འབད་དགོ་མི་ས་ཁོངས་དེ་  \"%1$S\" ཨིན་ན?"

#: AutomaticAuth
msgid "You are about to log into the site \"%1$S\" with the username \"%2$S\"."
msgstr "ཁྱོད་ཀྱིས་ས་ཁོངས་  \"%1$S\"ལུ་ ནང་བསྐྱོད་འབད་ནི་འབད་དོ་ ལག་ལེན་པའི་མིང་ \"%2$S\"གི་ཐོག་ལས།"

