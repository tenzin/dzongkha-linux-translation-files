#--------------------------------------------------------------------
#   Install Actions
#--------------------------------------------------------------------
#: InstallFile
msgid "Installing: %s"
msgstr " %sགཞི་བཙུགས་འབད་དོ་"

#: InstallSharedFile
msgid "Installing Shared File: %s"
msgstr "རུབ་སྤྱོད་ཡིག་སྣོད་%sགཞི་བཙུགས་འབད་དོ་"

#: ReplaceFile
msgid "Replacing: %s"
msgstr " %sགི་ཚབ་བཙུགས་དོ་"

#: ReplaceSharedFile
msgid "Replacing Shared File: %s"
msgstr "རུབ་སྤྱོད་ཡིག་སྣོད་%sགི་ཚབ་བཙུགས་དོ་"

#: SkipFile
msgid "Skipping: %s"
msgstr "%sགོམ་འགྱོ་དོ་"

#: SkipSharedFile
msgid "Skipping Shared File: %s"
msgstr "རིབསྤྱོད་ཡིག་སྣོད་%sགོམ་འགྱོ་དོ་"

#: DeleteFile
msgid "Deleting file: %s"
msgstr "ཡིག་སྣོད་%sབཏོན་གཏང་དོ་"

#: DeleteComponent
msgid "Deleting component: %s"
msgstr "ཆ་ཤས་%sབཏོན་གཏང་དོ་"

#: Execute
msgid "Executing: %s"
msgstr "%sབསྟར་སྤྱོད་འབད་དོ་"

#: ExecuteWithArgs
msgid "Executing: %s with argument: %s"
msgstr "%sདེ་ གྲུབ་རྟགས་%sདང་གཅིག་ཁར་ བསྟར་སྤྱོད་འབད་དོ་"

#: CopyFile
msgid "Copy File: %s to %s"
msgstr "ཡིག་སྣོད་%sལས་%sལུ་འདྲ་བཤུས་རྐྱབ་དོ་"

#: ExecuteFile
msgid "Execute File: %s"
msgstr "ཡིག་སྣོད་%sབསྟར་སྤྱོད་འབད་"

#: ExecuteFileWithArgs
msgid "Execute File: %s with argument: %s"
msgstr "ཡིག་སྣོད་%sདེ་ གྲུབ་རྟགས་%sདང་གཅིག་ཁར་ བསྟར་སྤྱོད་འབད་"

#: MoveFile
msgid "Move File: %s to %s"
msgstr "ཡིག་སྣོད་%sལས་%sལུ་སྤོ་བཤུད་འབད་"

#: RenameFile
msgid "Rename File: %s to %s"
msgstr "ཡིག་སྣོད་%sལས་%sལུ་ བསྐྱར་མིང་རྟགས་"

#: CreateFolder
msgid "Create Folder: %s"
msgstr "སྣོད་འཛིན་%sགསར་བསྐྲུན་འབད་"

#: RemoveFolder
msgid "Remove Folder: %s"
msgstr "སྣོད་འཛིན་%s རྩ་བསྐྲད་གཏང་"

#: RenameFolder
msgid "Rename Folder: %s to %s"
msgstr "སྣོད་འཛིན་%sལས་%sལུ་ བསྐྱར་མིང་བཏགས་"

#: WindowsShortcut
msgid "Windows Shortcut: %s"
msgstr "ཝིན་ཌོསི་མགྱོགས་ཐབས་%s་"

#: MacAlias
msgid "Mac Alias: %s"
msgstr "མེཀ་གི་མིང་གཞན་%s་"

#: WindowsRegisterServer
msgid "Windows Register Server: %s"
msgstr "ཝིན་ཌོསི་གི་ཐོ་འགོད་སར་བར་:%s་"

#: UnknownFileOpCommand
msgid "Unknown file operation command!"
msgstr "མ་ཤེས་པའི་ཡིག་སྣོད་བཀོད་སྤྱོད་བརྡ་བཀོད་"

#: Patch
msgid "Patching: %s"
msgstr "%sལུ་ལྷན་པ་བཏབ་དོ་"

#: Uninstall
msgid "Uninstalling: %s"
msgstr "%sགཞི་བཙུགས་བཤོལ་དོ་"

#: RegSkin
msgid "Register Skin: %s"
msgstr "ཕྱི་ལྤགས་%s ཐོ་འགོད་འབད་"

#: RegLocale
msgid "Register Locale: %s"
msgstr "ལོ་ཀེལ་:%s ཐོ་འགོད་འབད་"

#: RegContent
msgid "Register Content: %s"
msgstr "ནང་དོན་:%s ཐོ་འགོད་འབད་"

#: RegPackage
msgid "Register Package: %s"
msgstr "ཐུམ་ཚན་:%s ཐོ་འགོད་འབད་"

#--------------------------------------------------------------------
#   Dialog Messages
#--------------------------------------------------------------------
#: ApplyNowSkin
msgid "Use this theme"
msgstr "བརྗོད་དོན་འདི་ ཐོ་འགོད་འབད་"

#: ApplyNowLocale
msgid "Use this locale"
msgstr "ལོ་ཀེལ་འདི་ ལག་ལེན་འཐབ་"

#: ConfirmSkin
msgid "Install the theme \"%1$S\" from %2$S?"
msgstr "བརྗོན་དོན་ %2$S ནང་ལས་ \"%1$S\" གཞི་བཙུགས་འབད་ནི་ཨིན་ན?"

#: ConfirmLocale
msgid "Install the locale \"%1$S\" from %2$S?"
msgstr "ལོ་ཀེལ་ %2$S ནང་ལས་ \"%1$S\" གཞི་བཙུགས་འབད་ནི་ཨིན་ན?"

#: OK
msgid "Install"
msgstr "གཞི་བཙུགས་འབད་"

#: progress.queued
msgid "Queued"
msgstr "གྱལ་རིམ་བསྒྲིགས་ཡོདཔ་"

#: progress.downloading
msgid "Downloading..."
msgstr "ཕབ་ལེན་འབད་དོ་་་"

#: progress.downloaded
msgid "Downloaded"
msgstr "ཕབ་ལེན་འབད་ཡོདཔ་"

#: progress.installing
msgid "Installing..."
msgstr "གཞི་བཙུགས་འབད་དོ་་་་"

#: Unsigned
msgid "Unsigned"
msgstr "མིང་རྟགས་མ་བཀོདཔ་"

#--------------------------------------------------------------------
#   Miscellaneous
#--------------------------------------------------------------------
#: ERROR
msgid "ERROR"
msgstr "འཛོལ་བ་"

#: error0
msgid "Success"
msgstr "མཐར་འཁྱོལ་"

#: error999
msgid "Restart to complete"
msgstr "མཇུག་བསྡུ་ནི་ལུ་ ལོག་སྟེ་འགོ་བཙུགས་"

#: error-202
msgid "Access denied"
msgstr "འཛུལ་སྤྱོད་ཉན་མ་བཏུབ་"

#: error-203
msgid ""
"Unexpected installation error\n"
"Review the JavaScript Console log for more details."
msgstr ""
"རེ་བ་མེད་པའི་ གཞི་བཙུགས་ཀྱི་འཛོལ་བ།\n"
"ཁ་གསལ་གྱི་དོན་ལུ་ ཇབ་སི་ཀིརིཔཊི་ མ་སྒྲོམ་དྲན་དེབ་ལུ་ བསྐྱར་ཞིབ་འབད།"

#: error-204
msgid "Install script not found"
msgstr "གཞི་བཙུགས་ཀྱི་ཡིག་ཚུགས་མ་ཐོབ་"

#: error-207
msgid "Not a valid install package"
msgstr "གཞི་བཙུགས་ཀྱི་ཐུམ་སྒྲིལ་ ནུས་ཅན་ཅིག་མེན་པས་"

#: error-208
msgid "Invalid argument"
msgstr "ནུས་མེད་གྲུབ་རྟགས་"

#: error-210
msgid "User cancelled"
msgstr "ལག་ལེན་པ་ཆ་མེད་བཏང་ཡོད་"

#: error-214
msgid "Required file does not exist"
msgstr "དགོ་པའི་ཡིག་སྣོད་དེ་མིན་འདུག་"

#: error-215
msgid "Read only"
msgstr "ལྷག་ནི་ཙམ་"

#: error-218
msgid "AppleSingle extraction error"
msgstr "ཨེ་པཱལ་སིང་གཱལ་ཕྱིར་འདོན་གྱི་འཛོལ་བ་"

#: error-219
msgid "Invalid path"
msgstr "ནུས་མེད་འགྲུལ་ལམ་"

#: error-225
msgid "EXTRACTION_FAILED"
msgstr "ཕྱིར་འདོན་མ་བཏུབ་"

#: error-227
msgid "Cancelled"
msgstr "ཆ་མེད་བཏང་ཡོད་"

#: error-228
msgid "Download error"
msgstr "ཕབ་ལེན་ལུ་འཛོལ་བ་"

#: error-229
msgid "Script error"
msgstr "ཡིག་ཚུགས་ཀྱི་འཛོེལ་བ་"

#: error-230
msgid "Already exists"
msgstr "ཧེ་མ་ལས་ར་འདུག་"

#: error-235
msgid "Out of space"
msgstr "བར་སྟོང་གིས་བདའ་མི་ཟུན་པས་"

#: error-239
msgid "Chrome registration failed"
msgstr "ཀོརོམ་ཐོ་འགོད་མ་བཏུབ་"

#: error-240
msgid "Unfinished install"
msgstr "མཇུག་མ་བསྡུ་བའི་གཞི་བཙུགས་"

#: error-260
msgid "Signing could not be verified."
msgstr "མིང་རྟགས་བདེན་སྦྱོར་འབད་མ་ཚུགས་"

#: error-261
msgid "Invalid file hash (possible download corruption)"
msgstr "ནུས་མེད་ཡིག་སྣོད་ཨང་རྟགས་(ཕབ་ལེབ་གྱི་ངན་ལྷད་འོང་སྲིིད་)"

#: error-262
msgid "Unknown or invalid file hash type"
msgstr "ཡིག་སྣོད་ཨང་རྟགས་དབྱེ་བ་ མ་ཤེས་པའམ་ནུས་མེད།"

#: error-299
msgid "Out of memory"
msgstr "དྲན་ཚད་ཀྱིས་བདའ་མ་ཟུནམ་"

# there are other error codes, either rare or obsolete,
# that are not worth translating at this time.
#: unknown.error
msgid "Unexpected error %S"
msgstr "རེ་བ་མེད་པའི་འཛོལ་བ་%S།"

