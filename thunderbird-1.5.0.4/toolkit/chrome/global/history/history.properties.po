#: deleteHost
msgid "Delete all from %S"
msgstr "%Sནང་ལས་ཆ་མཉམ་བཏོན་གཏང་"

#: deleteDomain
msgid "Delete entire domain %S"
msgstr "%Sནང་ལས་ཌོ་མེན་ཧྲིལ་བུ་བཏོན་གཏང་"

#: deleteHostNoSelection
msgid "Delete host"
msgstr "ཧོསཊི་བཏོན་གཏང་"

#: deleteDomainNoSelection
msgid "Delete domain"
msgstr "ཌོ་མེན་བཏོན་གཏང་"

#: finduri-AgeInDays-is-0
msgid "Today"
msgstr "ད་རིས་"

#: finduri-AgeInDays-is-1
msgid "Yesterday"
msgstr "ཁ་རྩང་"

#: finduri-AgeInDays-is
msgid "%S days ago"
msgstr "ཉིན་གྲངས་%Sགི་ཧེ་མ་"

#: finduri-AgeInDays-isgreater
msgid "Older than %S days"
msgstr "ཉིན་གྲངས་་%Sལས་རྒས་མི་"

#: finduri-Hostname-is-
msgid "(no host)"
msgstr "(ཧོསཊི་མིན་འདུག)"

#: load-js-data-url-error
msgid "For security reasons, javascript or data urls cannot be loaded from the history window or sidebar."
msgstr "སྲུང་སྐྱོབ་ཀྱི་རྒྱུ་མཚན་ལུ་བརྟེན་ཏེ་ ཇ་བ་སི་ཀིརིཔཊི་ ཡང་ན་ གནས་སྡུད་ཀྱི་ཡུ་ཨར་ཨེལ་ཚུ་ ལོ་རྒྱུས་ ཡང་ན་ ཟུར་ཁའི་ཕྲ་རིང་ནང་ལས་ མངོན་གསལ་འབད་མི་ཚུགས་པས།"

