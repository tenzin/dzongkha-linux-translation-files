#: props.box.label
msgid "About this cookie"
msgstr "ཀུ་ཀི་འདི་གི་སྐོར་ལས་"

#: props.name.label
msgid "Name:"
msgstr "མིང་:"

#: props.value.label
msgid "Content:"
msgstr "ནང་དོན་:"

#: props.domain.label
msgid "Host:"
msgstr "ཧོསཊི་:"

#: props.path.label
msgid "Path:"
msgstr "འགྲུལ་ལམ་:"

#: props.secure.label
msgid "Send For:"
msgstr "དོན་ལུ་གཏང་:"

#: props.expires.label
msgid "Expires:"
msgstr "དུས་ཡོལ་འགྱོཝ་ཨིན་:"

#: props.policy.label
msgid "Policy:"
msgstr "སྲིད་བྱུས་:"

#: button.allow.label
#: button.allow.accesskey
msgid "&Allow"
msgstr "ཆོག་(&A)"

#: button.session.label
#: button.session.accesskey
msgid "Allow for &Session"
msgstr "ཡཱ་ཡུན་གྱི་དོ་ནལུ་ཆོག་(&S)"

#: button.deny.label
#: button.deny.accesskey
msgid "&Deny"
msgstr "མ་ཉན་"

#: dialog.title
msgid "Confirm setting cookie"
msgstr "ཀུ་ཀི་སྒྲིག་སྟངས་ལུ་ངེས་དཔྱད་འབད་"

#: dialog.remember.label
#: dialog.remember.accesskey
msgid "&Use my choice for all cookies from this site"
msgstr "ས་ཁོངས་འདི་ནང་ལས་ ཀུ་ཀི་ཆ་མཉམ་གྱི་དོན་ལུ་ ངེད་གདམ་ཁ་ལག་ལེན་འཐབ་(&U)"

