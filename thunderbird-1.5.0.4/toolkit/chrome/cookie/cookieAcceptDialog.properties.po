#: hostColon
msgid "Host:"
msgstr "ཧོསཊི་:"

#: domainColon
msgid "Domain:"
msgstr "ཌོ་མེན་:"

#: forSecureOnly
msgid "Encrypted connections only"
msgstr "གསང་བཟོས་མཐུད་ལམ་རྐྱངམ་གཅིག་"

#: forAnyConnection
msgid "Any type of connection"
msgstr "མཐུད་ལམ་ག་ཅི་ཨིན་རུང་"

#: atEndOfSession
msgid "at end of session"
msgstr "ལཱ་ཡུན་གྱི་མཇུག་ལུ་"

#: showDetails
msgid "Show Details"
msgstr "རྒྱས་བཤད་སྟོན་"

#: hideDetails
msgid "Hide Details"
msgstr "རྒྱས་བཤད་སྦ་བཞག་"

#: detailsAccessKey
msgid "T"
msgstr "T"

#: permissionToSetACookie
msgid "The site %S wants to set a cookie."
msgstr "ས་ཁོངས་ %S གིས་ ཀུ་ཀི་ཅིག་ གཞི་སྒྲིག་འབད་དགོ་མནོཝ་མས།"

#: permissionToSetSecondCookie
msgid "The site %S wants to set a second cookie."
msgstr "ས་ཁོངས་ %S གིས་ ཀུ་ཀི་གཉིས་པ་ཅིག་ གཞི་སྒྲིག་འབད་དགོ་མནོཝ་མས།"

#  LOCALIZATION NOTE (PermissionToSetAnotherCookie): First %S: sitename, second %S: number of cookies already present for that site
#: permissionToSetAnotherCookie
msgid ""
"The site %S wants to set another cookie.\n"
"You already have %S cookies from this site."
msgstr ""
"ས་ཁོངས་ %S གིས་ ཀུ་ཀི་གཞན་མི་ གཞི་སྒྲིག་འབད་དགོ་མནོཝ་མས།\n"
"ས་ཁོངས་འདི་ནང་ལས་ ཁྱོད་ལུ་ཧེ་མ་ལས་ར་ ཀུ་ཀིསི་%Sའདུག།"

#: permissionToModifyCookie
msgid "The site %S wants to modify an existing cookie."
msgstr "ས་ཁོངས་ %S གིས་ ཡོད་བཞིན་པའི་ཀུ་ཀི་ཅིག་ ལེགས་བཅོས་འབད་དགོ་མནོཝ་མས།"

#: permissionToAcceptImage
msgid "The site %S wants to load an image."
msgstr "ས་ཁོངས་ %S གིས་ གཟུགས་བརྙན་ཅིག་ མངོན་གསལ་འབད་དགོ་མནོཝ་མས།"

#: rememberThisDecision
msgid "Remember this decision"
msgstr "གྲོས་ཆོད་འདི་སེམས་ཁར་བཞག།"

