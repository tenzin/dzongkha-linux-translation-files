#: aboutWindowTitle
msgid "About %S"
msgstr "%Sགི་སྐོར་ལས་"

#: aboutWindowCloseButton
msgid "Close"
msgstr "ཁ་བསྡམས་"

#: aboutWindowVersionString
msgid "version %S"
msgstr "ཐོནརིམ་%S་"

#: aboutExtension
msgid "About %S..."
msgstr "%Sགི་སྐོར་ལས་་་"

#: updatingMessage
msgid "Looking for updates to %S..."
msgstr "%S ལུ་ དུས་མཐུན་གྱི་དོན་ལུ་བལྟ་དོ་"

#: updateAvailableMessage
msgid "A new version of %S (%S) is available."
msgstr "%S (%S)གི་ཐོནརིམ་གསརཔ་ཅིག་ཡོད།"

#: updateCompatibilityMessage
msgid "A compatibility update has been applied to %S."
msgstr "མཐུན་འགྱུར་གྱི་དུས་མཐུན་དེ་%Sལུ་འཇུག་སྤྱོད་འབད་ཡི།"

#: updateNoUpdateMessage
msgid "No updates were found for %S."
msgstr "%Sགི་དོན་ལུ་ དུས་མཐུན་མ་ཐོབ།"

#: updateErrorMessage
msgid "An error occurred while trying to find updates for %S."
msgstr "%Sགི་དོན་ལུ་ དུས་མཐུན་འཚོལ་བའི་སྐབས་ འཛོལ་བ་ཅིག་བྱུང་ཡི།"

#: updateDisabledMessage
msgid "Updates are disabled for %S."
msgstr "%Sགི་དོན་ལུ་ དུས་མཐུན་ཚུ་ ལྕོགས་མིན་བཟོ་ནུག།"

#: updateAppManagedMessage
msgid "Updates to %S are performed when %S is updated."
msgstr "%Sགི་དུས་མཐུན་ཚུ་ %Sདུས་མཐུན་བཟོཝ་ད་ ལག་ལེན་འཐབ་ཨིན།"

#: updateReadOnlyMessage
msgid "Update not supported (install location is read only)."
msgstr "དུས་མཐུན་ཚུ་ལུ་རྒྱབ་སྐྱོར་མིན་འདུག། (གཞི་བཙུགས་གནས་ཁོངས་དེ་ལྷག་ནི་ཙམ་ཅིག་ཨིན་མས་)"

#: updateNotManagedMessage
msgid "Update not supported (install location is not managed by %S)."
msgstr "དུས་མཐུན་ལུ་རྒྱབ་སྐྱོར་མིན་འདུག།(གཞི་བཙུགས་གནས་ཁོངས་དེ་%Sགིས་འཛིན་སྐྱོང་མ་འཐབ་པས)"

#: restartBeforeEnableTitle
msgid "Enable Extension"
msgstr "རྒྱ་བསྐྱེད་ལྕོགས་ཅན་བཟོ་"

#: restartBeforeDisableTitle
msgid "Disable Extension"
msgstr "རྒྱ་བསྐྱེད་ལྕོགས་མིན་བཟོ་"

#: restartBeforeEnableMessage
msgid "%S will be enabled when %S is restarted."
msgstr "%Sལོག་སྟེ་འགོ་བཙུགསཔ་ད་ %Sལྕོགས་ཅན་བཟོ་འོང་།"

#: restartBeforeDisableMessage
msgid "%S will be disabled when %S is restarted."
msgstr "%Sལོག་སྟེ་འགོ་བཙུགསཔ་ད་ %Sལྕོགས་མིན་བཟོ་འོང་།"

#: restartBeforeUninstallTitle
msgid "Uninstall"
msgstr "གཞི་བཙུགས་བཤོལ་"

#: restartBeforeUninstallMessage
msgid "%S will be uninstalled when %S is restarted."
msgstr "%Sལོག་སྟེ་འགོ་བཙུགསཔ་ད་ %Sགཞི་བཙུགས་བཤོལ་འོང་།"

#: restartBeforeInstallMessage
msgid "%S will be installed when %S is restarted."
msgstr "%Sལོག་སྟེ་འགོ་བཙུགསཔ་ད་ %Sགཞི་བཙུགས་འབད་འོང་།"

#: restartBeforeUpgradeMessage
msgid "%S will be upgraded when %S is restarted."
msgstr "%Sལོག་སྟེ་འགོ་བཙུགསཔ་ད་ %Sདུས་མཐུན་བཟོ་འོང་།"

#: incompatibleUpdateMessage
msgid "%S is checking for a compatibility update to %S."
msgstr "%Sགིས་ %Sགི་དོན་ལུ་ མཐུན་འགྱུར་གྱི་དུས་མཐུན་ཅིག་ཡོད་མེད་ ཞིབ་དཔྱད་འབད་དོ།"

#: queryUninstallExtensionMessage
msgid "If you uninstall %S, the functionality it offers will no longer be available. Do you want to uninstall %S?"
msgstr "ཁྱོད་ཀྱིས་%S གཞི་བཙུགས་བཤོལ་བ་ཅིན་  དེ་གི་ལས་འགན་ཚུ་མི་འཐོབ། ཁྱོད་ཀྱིས་%Sགཞི་བཙུགས་བཤོལ་ནི་ཨིན་ན?"

#: queryUninstallThemeMessage
msgid "Do you want to uninstall %S?"
msgstr "ཁྱོད་ཀྱིས་%S གཞི་བཙུགས་བཤོལ་ནི་ཨིན་ན?"

#: queryUninstallTitle
msgid "Uninstall %S"
msgstr "%Sགཞི་བཙུགས་བཤོལ་"

#: installSuccess
msgid "Install completed successfully"
msgstr "གཞི་བཙུགས་ཆ་ཚང་མཐར་འཁྱོལ་བྱུང་ཡོད།"

#: installWaiting
msgid "Waiting..."
msgstr "བསྒུག་དོ་་་་"

#: installInstalling
msgid "Installing..."
msgstr "གཞི་བཙུགས་འབད་དོ་་་་"

#: droppedInWarning
msgid "The following items were found in your Extensions folder. Do you want to install them?"
msgstr "འོག་གི་རྣམ་གྲངས་ཚུ་ ཁྱོད་ཀྱི་རྒྱ་བསྐྱེད་སྣོད་འཛིན་ནང་འདུག། དེ་ཚུ་གཞི་བཙུགས་འབད་ནི་ཨིན་ན?"

#: disabledBySafeMode
msgid "%S is disabled by safe mode."
msgstr "ཉེན་སྲུང་ཐབས་ལམ་ཐོག་ལས་ %Sལྕོགས་མིན་བཟོ་ཡི།"

