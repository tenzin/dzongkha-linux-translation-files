#### Master Password
#: password_not_set
msgid "(not set)"
msgstr "(གཞི་སྒྲིག་མ་འབདཝ་)"

#: failed_pw_change
msgid "Unable to change Master Password."
msgstr "ཆོག་ཡིག་ཨམ་སོར་མ་ཚུགས།"

#: incorrect_pw
msgid "You did not enter the correct current Master Password. Please try again."
msgstr "ཁྱོད་ཀྱིས་ད་ལྟོའི་ཆོག་ཡིག་ཨམ་ངེས་བདེན་དེ་ མ་བཙུགས་པས། ལོག་སྟེ་འབད་རྩོལ་བསྐྱེད་གནང་།"

#: pw_change_ok
msgid "Master Password successfully changed."
msgstr "ཆོག་ཡིག་ཨམ་ མཐར་འཁྱོལ་སྦེ་སོར་ཡི།"

#: pw_erased_ok
msgid "You have deleted your Master Password."
msgstr "ཁྱོད་ཀྱིས་ཁྱོད་རའི་ ཆོག་ཡིག་ཨམ་ བཏོན་བཏང་ནུག།"

#: pw_not_wanted
msgid "Warning! You have decided not to use a Master Password."
msgstr "ཉེན་བརྡ་! ཁྱོད་ཀྱིས་ཆོག་ཡིག་ཨམ་ ལག་ལེན་མི་འཐབ་ནི་སྦེ་ གྲོས་ཐག་བཅད་ནུག།"

#: pw_empty_warning
msgid "Your stored web and email passwords, form data, and private keys will not be protected."
msgstr "ཁྱོད་ཀྱིས་གསོག་འཇོག་སྦེ་བཞག་ཡོད་པའི་ ཝེབ་དང་གློག་འཕྲིན་ཆོག་ཡིག་ཚུ་དང་ འབྲི་ཤོག་གནས་སྡུད་ སྒེར་གྱི་ལྡེ་མིག་ཚུ་ལུ་ ཉེན་སྐྱོབ་མི་འབད།"

#: pw_change2empty_in_fips_mode
msgid "You are currently in FIPS mode. FIPS requires a non-empty Master Password."
msgstr "ད་ལྟོ་ཁྱོད་ ཨེཕ་ཨའི་པི་ཨེསི་ ཐབས་ལམ་ནང་འདུག། ཨེཕ་ཨའི་པི་ཨེསི་ལུ་ སྟོངམ་མེན་པའི་ ཆོག་ཡིག་ཨམ་ཅིག་དགོ།"

#: pw_change_success_title
msgid "Password Change Succeeded"
msgstr "ཆོག་ཡིག་ཨམ་སོར་ནི་ མཐར་འཁྱོལ་བྱུང་ཡོད།"

#: pw_change_failed_title
msgid "Password Change Failed"
msgstr "ཆོག་ཡིག་སོར་ནི་མ་བཏུབ་"

#: pw_remove_button
msgid "Remove"
msgstr "རྩ་བསྐྲད་གཏང་"

