#: security.title
msgid "Security Preferences"
msgstr "སྲུང་སྐྱོབ་ཀྱི་དགའ་གདམ་ཚུ་"

#: security.label
msgid "Privacy and Security"
msgstr "སྒེར་དབང་དང་སྲུང་སྐྱོབ་"

#: security.description
msgid "Settings for Privacy and Security"
msgstr "སྒེར་དབང་དང་སྲུང་སྐྱོབ་ཀྱི་དོན་ལུ་སྒྲིག་སྟངས་"

#: resetsettings.label
msgid "Reset Settings"
msgstr "སྒྲིག་སྟངས་སླར་སྒྲིག་འབད་"

#: resetpreferences.label
msgid "Reset Preferences..."
msgstr "དགའ་གདམ་ཚུ་སླར་སྒྲིག་འབད་་་་་"

#: managecerts.label
#: managecerts.accesskey
msgid "&Manage Certificates"
msgstr "ལག་ཁྱེར་ཚུ་འཛིན་སྐྱོད་འཐབ་(&M)"

#: managecerts.text
msgid "Use the Certificate Manager to manage your personal certificates, as well as those of other people and certificate authorities."
msgstr "ཁྱོད་རའི་རང་དོན་ལག་ཁྱེར་དང་ མི་གཞན་དང་ ལ་ག་ཁྱེར་དབང་འཛིན་གཞན་ཚུ་གི་ ལག་ཁྱེར་ཚུ་འཛིན་སྐྱོང་འཐབ་ནི་ལུ་ ལག་ཁྱེར་འཛིན་སྐྱོང་པ་ལག་ལེན་འཐབ།"

#: managecerts.button
msgid "Manage Certificates..."
msgstr "ལག་ཁྱེར་ཚུ་འཛིན་སྐྱོང་འཐབ་་་་"

#: managedevices.label
#: managedevices.accesskey
msgid "Manage &Security Devices"
msgstr "སྲུང་སྐྱོབ་ཐབས་འཕྲུལ་ཚུ་ འཛིན་སྐྱོང་འཐབ་"

#: managedevices.text
msgid "Use this button to manage your security devices, such as smart cards."
msgstr "ཁྱོད་རའི་སྲུང་སྐྱོབ་ཐབས་འཕྲུལ་ དཔེར་ན་ སི་མརཊི་ཀརཌི་ བཟུམ་ཚུ་འཛིན་སྐྱོང་འཐབ་ནི་ལུ་ ཨེབ་རྟ་འདི་ལག་ལེན་འཐབ།"

#: managedevices.button
msgid "Manage Security Devices..."
msgstr "སྲུང་སྐྱོབ་ཐབས་འཕྲུལ་ཚུ་འཛིན་སྐྱོང་འཐབ་་་་"

#: ssl.label
msgid "SSL"
msgstr "ཨེསི་ཨེསི་ཨེལ་"

#: certs.title
#: certs.label
msgid "Certificates"
msgstr "ལག་ཁྱེར་ཚུ་"

