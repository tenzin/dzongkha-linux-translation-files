#: reportWizard.title
msgid "Report A Broken Web Site"
msgstr "ཝེབ་ས་ཁོངས་ཆད་པ་ཅིག་སྙན་ཞུ་འབད།"

#: privacyNotice.label
#: reportForm.privacyPolicy.title
msgid "Privacy Policy"
msgstr "སྒེར་གཙང་སྲིད་བྱུས།"

#: reportWizardPrivacy.description
msgid "This tool allows you to tell the Evangelism Team about web sites that do not work properly in &brandShortName;, or shut &brandShortName; out. This is your way to help us ensure the best possible experience for &brandShortName; users. Please take a moment to read the following to learn how we use the information we collect should you choose to participate."
msgstr "ལག་ཆས་དེ་གིས་ཁྱོད་ལུ་&brandShortName; ནང་ལཱ་ཚུལ་ལྡན་འབད་ཝེབ་ས་ཁོངས་ཚུའི་སྐོར་ལས་ཨིན་བེནཇི་ལིཟིམ་དེ་ལུ་སླབ་བཅུགཔ་ ཡང་ན་&brandShortName; སྒོ་བསྡམས་བཅུགཔ་ཨིན། འ་ནི་དེ་ཁྱོད་ཀྱིས་ང་བཅས་ལུ་ &brandShortName; ལག་ལེན་པ་ཚུའི་དོན་ལུ་ཉམས་མྱོང་ཚུགསཔ་དྲག་ཤོས་ངེས་ཏིག་གྲོགས་རམ་ལུ་ཐབས་ལམ་ཅིག་ཨིན། འོག་གི་ང་བཅས་ཀྱིས་ག་དེ་ལག་ལེན་འཐབ་ནིའི་བརྡ་དོན་ང་བཅས་ཀྱིས་བསྡུ་གསོག་་འབད་ནི་ཁྱོད་ལུ་བཅའ་མར་བཏོག་ནི་ལུ་གདམ་ནིཧ་་གོ་ནི་ལུ་དུས་ཡུན་དུམ་གྲ་ཅིག་ལྷག་གནང་།"

#: reportWizardMustAgree.description
msgid "To continue you need to agree to the privacy policy above."
msgstr "འཕྲོ་མཐུད་ནི་ལུ་ཁྱོད་ཀྱིས་ལྟག་གི་སྒེར་གཙང་སྲིད་བྱུས་ལུ་ཁས་ལེན་འབད་དགོཔ་ཨིན།"

#: dontShowPrivacyStatement.label
#: dontShowPrivacyStatement.accesskey
msgid "&I have read and accept the above privacy policy."
msgstr "ང་གིས་ལྷག་ཞིནམ་ལས་ལྟག་གི་སྒེར་གཙང་སྲིད་བྱུས་དེ་ལུ་དང་ལེན་འབད་འོང་།"

#: reportForm.label
msgid "Fill out Report"
msgstr "སྙན་ཞུ་བཀང་།"

#: reportForm.description
msgid "Tell us a little about the problem you encountered:"
msgstr "ཁྱོད་ལུ་གདོང་ཐུག་འབྱུང་ཡོད་མི་དཀའ་ངལ་དེའི་སྐོར་ལས་ང་བཅས་ལུ་དུམ་གྲ་ཅིག་སླབ:"

#: reportForm.url.title
msgid "Web site URL:"
msgstr "ཝེབ་ས་ཁོངས་ཡུ་ཨར་ཨེལ:"

# No ":" at the end
#: reportForm.behind_login.title
#: reportForm.behind_login.accesskey
msgid "Web site is &password-protected"
msgstr "ཝེབ་ས་ཁོངས་དེ་ཆོག་ཡིག་ཉོན་སྐྱོབ་འབད་ཡོད།"

#: reportForm.problem_type.title
#: reportForm.problem_type.accesskey
msgid "Problem &Type:"
msgstr "དཀའ་ངལ་དབྱེ་བ:(&T)"

#: reportForm.problem_type.chooseOne.title
msgid "Choose One..."
msgstr "གཅིག་གདམ..."

# DO *NOT* Add/change/modify (except localization) without consulting with r.m.o server admin first!
#: reportForm.problem_type.item1.title
msgid "Browser not supported"
msgstr "བརའུ་ཟར་རྒྱབ་སྐྱོར་མ་འབདཝ།"

#: reportForm.problem_type.item2.title
msgid "Can't log in"
msgstr "ནང་བསྐྱོད་འབད་མི་བཏུབ།"

#: reportForm.problem_type.item3.title
msgid "Plugin not shown"
msgstr "པ་ལག་ཡིན་མ་སྟོནམ།"

#: reportForm.problem_type.item4.title
msgid "Other content missing"
msgstr "ནང་དོན་གཞན་བརླག་སྟོར་འབྱུང་ཡོདཔ།"

#: reportForm.problem_type.item5.title
msgid "Behavior wrong"
msgstr "སྤྱོད་ལམ་ཕྱི་འགྱུར།"

#: reportForm.problem_type.item6.title
msgid "Appearance wrong"
msgstr "འབྱུང་སྣང་ཕྱི་འགྱུར།"

#: reportForm.problem_type.item7.title
msgid "Other problem"
msgstr "དཀའ་ངལ་གཞན།"

#: reportForm.describe.title
#: reportForm.describe.accesskey
msgid "&Describe Problem:"
msgstr "དཀའ་ངལ་འགྲེལ་བཤད་འབད:(&D)"

