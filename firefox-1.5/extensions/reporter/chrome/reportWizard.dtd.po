msgid "Platform:"
msgstr "མཉེན་རིམ:"

#: reportForm.product.title
msgid "Product:"
msgstr "ཐོན་སྐྱེན:"

#: reportForm.gecko.title
msgid "Gecko:"
msgstr "གེཀ་ཀོ:"

#: reportForm.useragent.title
msgid "Useragent:"
msgstr "ལག་ལེན་པའི་ལས་ཚབ:"

#: reportForm.language.title
msgid "Language:"
msgstr "ཁ་སྐད:"

# Make sure this has "(Optional)" at the end.  This is important for the user to know
#: reportForm.email.title
#: reportForm.email.accesskey
msgid "&Email (Optional):"
msgstr "གློག་འཕྲིན (གདམ་ཁ་ཅན):(&E)"

#: reportForm.privacyPolicy.tooltip
msgid "View Privacy Policy"
msgstr "སྒེར་གཙང་སྲིད་བྱུས་སྔོན་ལྟ།"

#: reportResults.showDetail.title
#: reportResults.showDetail.accesskey
msgid "&Show Details"
msgstr "རྒྱས་བཤད་ཚུ་སྟོན།(&S)"

#: sendReport.label
msgid "Sending Report"
msgstr "སྙན་ཞུ་གཏང་དོ།"

#: sendReport.description
msgid "Sending Report to server..."
msgstr "སར་བར་ལུ་སྙན་ཞུ་གཏང་དོ..."

#: finish.label
msgid "Report Sent"
msgstr "སྙན་ཞུ་བཏང་ཡོད།"

