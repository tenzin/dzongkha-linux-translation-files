# Map Expat error codes to error strings
#: 1
msgid "out of memory"
msgstr "དྲན་ཚད་ཀྱི་ཕྱི་ཁ།"

#: 2
msgid "syntax error"
msgstr "ཚིག་སྦྱོར་གྱི་འཛོལ་བ།"

#: 3
msgid "no element found"
msgstr "ཆ་ཤས་འཐོབ་མ་ཚུགས་པས།"

#: 4
msgid "not well-formed"
msgstr "ལེགས་སྒྲིག་ཅན་མིན་འདུག"

#: 5
msgid "unclosed token"
msgstr "ཁ་མ་བསྡམས་པའི་ངོ་རྟགས།"

#: 6
msgid "partial character"
msgstr "ཡིག་འབྲུའི་ཆ་ཤས།"

#: 7
msgid "mismatched tag"
msgstr "མཐུན་སྒྲིག་མེད་པའི་  ངོ་རྟགས།"

#: 8
msgid "duplicate attribute"
msgstr "ཁྱད་ཆོས་རྫུན་མ།"

#: 9
msgid "junk after document element"
msgstr "ཡིག་ཆའི་ཆ་ཤས་ཀྱི་ཤུལ་ལས་  མཁོ་མེད།"

#: 10
msgid "illegal parameter entity reference"
msgstr "ཁྲིམས་འགལ་ཚད་བཟུང་ངོ་བོ་གི་གཞི་བསྟུན།"

#: 11
msgid "undefined entity"
msgstr "ངེས་འཛིན་མ་འབད་བའི་ངོ་བོ།"

#: 12
msgid "recursive entity reference"
msgstr "ལོག་རིམ་ངོ་བོའི་གཞི་བསྟུན།"

#: 13
msgid "asynchronous entity"
msgstr "གནད་སྡུད་རྒྱུན་སྤེལ་གྱི་ངོ་བོ།"

#: 14
msgid "reference to invalid character number"
msgstr "ནུས་ཅན་མེད་པའི་ཡིག་འབྲུའི་ཨང་གྲངས་ལུ་  གཞི་བསྟུན།"

#: 15
msgid "reference to binary entity"
msgstr "ཟུང་ལྡན་ངོ་བོ་ལུ་གཞི་བསྟུན།"

#: 16
msgid "reference to external entity in attribute"
msgstr "ཁྱད་ནང་ཡོད་པའི་ ཕྱིའི་ངོ་བོ་ལུ་གཞི་བསྟུན།"

#: 17
msgid "xml declaration not at start of external entity"
msgstr "ཨེགསི་ཨེམ་ཨེལ་གསལ་བསྒྲགས་འདི་  ཕྱིའི་ངོ་བོ་གི་འགོ་ལུ་མིན་འདུག"

#: 18
msgid "unknown encoding"
msgstr "མ་ཤས་པའི་ཨིན་ཀོ་ཌིང་།"

#: 19
msgid "encoding specified in XML declaration is incorrect"
msgstr "ཨེགསི་ཨེམ་ཨེལ་གསལ་བསྒྲགས་ནང་ལུ་  གསལ་བཀོད་འབད་ཡོད་པའི་ ཨིན་ཀོ་ཌིང་འདི་ བདེན་མེད་ཨིན།"

#: 20
msgid "unclosed CDATA section"
msgstr "ཁ་མ་བསྡམས་པའི་CDATA དབྱེ་ཚན།"

#: 21
msgid "error in processing external entity reference"
msgstr "ཕྱིའི་ངོ་བོ་གི་གཞི་བསྟུན་འདི་བཟོ་སྦྱོར་འབད་བའི་བསྒང་  འཛོལ་བ།"

#: 22
msgid "document is not standalone"
msgstr "ཡིག་ཆ་འདི་རང་དབང་མེན།"

#: 23
msgid "unexpected parser state"
msgstr "རེ་བ་མེད་པའི་ མིང་དཔྱདཔ་གནས་ལུགས།"

#: 24
msgid "entity declared in parameter entity"
msgstr "ཚད་བཟུང་ངོ་བོ་ནང་ལུ་  ངོ་བོ་འདི་གསལ་བསྒྲགས་འབད་ཡོད།"

#: 27
msgid "prefix not bound to a namespace"
msgstr "སྔོན་ཚིག་འདི་མིང་གི་ས་སྟོང་ཅིག་ལུ་ གནས་དེ་མེད།"

#: 29
msgid "reserved prefix (xml) must not be undeclared or bound to another namespace URI"
msgstr "ཟུར་སོག་འབད་ཡོད་པའི་ སྔོན་ཚིག་(xml) འདི་  གསར་བསྒྲགས་འབད་ནི་དང་  ཡང་ན་  ཡུ་ཨར་ཨའི་མིང་གི་ས་སྟོང་གཞན་མི་ལུ་  གནས་ནི་མི་འོང་།"

#: 30
msgid "reserved prefix (xmlns) must not be declared or undeclared"
msgstr "ཟུར་སོག་འབད་ཡོད་པའི་སྔོན་ཚིག་(xmlns)འདི་  གསལ་བསྒྲགས་འབད་ནི་མི་འོང་  ཡང་ན་ གསལ་བསྒྲགས་མ་འབད་བར་ཡང་བཞག་ནི་མི་འོང་།"

#: 31
msgid "prefix must not be bound to one of the reserved namespace URIs"
msgstr "སྔོན་ཚིག་འདི་  ཟུར་སོག་འབད་ཡོད་པའི་མིང་གི་ས་སྟོང་ཡུ་ཨར་ཨའི་གང་རུང་ཅིག་ལུ་  གནས་ནི་མི་འོང་།"

# %1$S is replaced by the Expat error string, may be followed by Expected (see below)
# %2$S is replaced by URL
# %3$d is replaced by line number
# %4$d is replaced by column number
#: XMLParsingError
msgid ""
"XML Parsing Error: %1$S\n"
"Location: %2$S\n"
"Line Number %3$d, Column %4$d:"
msgstr ""
"ཨེགསི་ཨེམ་ཨེལ་མིང་དཔྱད་འབདཝ་ད་གི་འཛོལ་བ: %1$S\n"
"གནས་ཁོངས: %2$S\n"
"གྲལ་ཐིག་ཨང་ %3$d, ཀེར་ཐིག་ %4$d:"

# %S is replaced by a tag name.
# This gets appended to the error string if the error is mismatched tag.
#: Expected
msgid ". Expected: </%S>."
msgstr ". རེ་བ་བསྐྱེད་ཡོདཔ: </%S>."

