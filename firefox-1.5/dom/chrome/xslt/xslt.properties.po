#: 1
msgid "Parsing an XSLT stylesheet failed."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་  སི་ཊ་ཡེལ་ཤིཊི་འདི་མིང་དཔྱད་འབད་ནི་ལུ་ འཐུས་ཤོར་འབྱུང་ནུག"

#: 2
msgid "Parsing an XPath expression failed."
msgstr "ཨེགསི་ འགྲུལ་ལམ་གསལ་བརྗོད་འདི་ མིང་དཔྱད་འབད་ནི་ལུ་  འཐུས་ཤོར་འབྱུང་ནུག"

#: 4
msgid "XSLT transformation failed."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ བཟོ་བསྒྱུར་འདི་འཐུས་ཤོར་འབྱུང་ནུག"

#: 5
msgid "Invalid XSLT/XPath function."
msgstr "ནུས་ཅན་མེད་པའི་ XSLT/XPath ལས་འགན།"

#: 6
msgid "XSLT Stylesheet (possibly) contains a recursion."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་  སི་ཊ་ཡེལ་ཤིཊི་ (སྲིད་པར་) སླར་འབྱུང་ཅིག་གནས་དེ་ཡོད།"

#: 7
msgid "Attribute value illegal in XSLT 1.0."
msgstr "ཁྱད་ཆོས་གནད་གོང་འདི་  XSLT 1.0. ནང་ལུ་ཁྲིམས་ལས་འགལ་བ་ཨིན།"

#: 8
msgid "An XPath expression was expected to return a NodeSet."
msgstr "ཨེགསི་འགྲུལ་ལམ་གསལ་བརྗོད་འདི་  མཐུད་མཚམས་གཞི་སྒྲིག་ཅིག་སླར་ལོག་འབད་ནི་ལུ་ རེ་བ་བསྐྱེད་ནུག"

#: 9
msgid "XSLT transformation was terminated by <xsl:message>."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ བཟོ་བསྒྱུར་འདི་ <xsl:message>གིས་ རྩ་གྲོལ་གཏང་ནུག"

#: 10
msgid "A network error occured loading an XSLT stylesheet:"
msgstr "ཡོངས་འབྲེལ་འཛོལ་བ་འདི་  ཨེགསི་ཨེསི་ཨེལ་ཊི་  སི་ཊ་ཡེལ་ཤིཊི་མངོན་གསལ་འབད་དེ་འབྱུང་ཡོད།"

#: 11
msgid "An XSLT stylesheet does not have an XML mimetype:"
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་  སི་ཊ་ཡེལ་ཤིཊི་དེ་ལུ་  ཨེགསི་ཨེལ་ཨེལ་ མ་ཡིམ་དབྱེ་བ་འདི་མེད:"

#: 12
msgid "An XSLT stylesheet directly or indirectly imports or includes itself:"
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་  སི་ཊ་ཡེལ་ཤིཊི་དེ་གིས་  ཐད་ཀར་དུ་ཡང་ན་ ཐད་ཀོར་འབད་  ཁོ་རང་ནང་འདྲེན་འབད་ནི་ཡང་ན་ གྲངས་སུ་བཙུགསཔ་ཨིན།"

#: 13
msgid "An XPath function was called with the wrong number of arguments."
msgstr "ཨེགསི་འགྲུལ་ལམ་འདི་  སྒྲུབ་རྟགས་ཚུ་གི་  ཨང་གྲངས་ཕྱི་འགྱུར་དང་བཅས་ འབོ་དོ་ཡོདཔ་ཨིན།"

#: 14
msgid "An unknown XPath extension function was called."
msgstr "མ་ཤེས་པའི་ཨེགསི་འགྲུལ་ལམ་རྒྱ་བསྐྱེད་ལས་འགན་འདི་  འབོ་དེ་ཡོད།"

#: 15
msgid "XPath parse failure: ')' expected:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འབད་ནི་འདི་ འཐུས་ཤོར་འབྱུང་ཡོདཔ: ')' འདི་རེ་བ་བསྐྱེད་ནུག:"

#: 16
msgid "XPath parse failure: invalid axis:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འབད་ནི་འདི་  འཐུས་ཤོར་འབྱུང་ཡོདཔ: ནུས་ཅན་མེད་པའི་ ཚད་ཐིག:"

#: 17
msgid "XPath parse failure: Name or Nodetype test expected:"
msgstr "ཨེགསི་འགྲུལ་ལམ་ མིང་དཔྱད་འབད་ནི་འདི་འཐུས་ཤོར་འབྱུང་ཡོདཔ: མིང་ཡང་ན་ མཐུད་མཚམས་དབྱེ་བའི་བརྟག་ཞིབ་འདི་རེ་བ་བསྐྱེད་ནུག:"

#: 18
msgid "XPath parse failure: ']' expected:"
msgstr "ཨེགསི་འགྲུལ་ལམ་ མིང་དཔྱད་འབད་ནི་འདི་ འཐུས་ཤོར་འབྱུང་ཡོདཔ: ']' འདི་རེ་བ་བསྐྱེད་ནུག:"

#: 19
msgid "XPath parse failure: invalid variable name:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འབད་ནི་འདི་ འཐུས་ཤོར་འབྱུང་ཡོདཔ:  ནུས་ཅན་མེད་པའི་འགྱུར་ཅན་མིང་:"

#: 20
msgid "XPath parse failure: unexpected end of expression:"
msgstr "ཨེགསི་འགྲུལ་ལམ་ མིང་དཔྱད་འབད་ནི་འདི་ འཐུས་ཤོར་འབྱུང་ཡོདཔ:  རེ་བ་མེད་པའི་གསལ་བརྗོད་ཀྱི་མཇུག:"

#: 21
msgid "XPath parse failure: operator expected:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འབད་ནི་འདི་  འཐུས་ཤོར་འབྱུང་ཡོདཔ:  བཀོལ་སྤྱོདཔ་འདི་རེ་བ་བསྐྱེད་ནུག:"

#: 22
msgid "XPath parse failure: unclosed literal:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འབད་ནི་འདི་ འཐུས་ཤོར་འབྱུང་ཡོདཔ: ཁ་བསྡམས་མེད་པའི་ཐད་དོན:"

#: 23
msgid "XPath parse failure: ':' unexpected:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འབད་ནི་འདི་  འཐུས་ཤོར་འབྱུང་ཡོདཔ: ':' རེ་བ་མེདཔ:"

#: 24
msgid "XPath parse failure: '!' unexpected, negation is not():"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འབད་ནི་འདི་  འཐུས་ཤོར་འབྱུང་ཡོདཔ: རེ་བ་མེད་པའི་ '!' བཀག་ཆ་འདི་ () མེན:"

#: 25
msgid "XPath parse failure: illegal character found:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འབད་ནི་འདི་  འཐུས་ཤོར་འབྱུང་ཡོདཔ:  ཁྲིམས་འགལ་ཡིག་འབྲུ་འདི་འཐོབ་ནུག:"

#: 26
msgid "XPath parse failure: binary operator expected:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་འབད་ནི་འདི་  འཐུས་ཤོར་འབྱུང་ཡོགཔ:  བཀོལ་སྤྱོད་ཟུང་ལྡན་འདི་རེ་བ་བསྐྱེད་ནུག:"

#: 27
msgid "An XSLT stylesheet load was blocked for security reasons."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་  སི་ཊ་ཡེལ་ཤིཊི་གི་མངོན་གསལ་འདི་  སྲུང་སྐྱོབ་རྒྱུ་མཚན་ཚུ་གིས་འགགས་ནུག"

#: LoadingError
msgid "Error loading stylesheet: %S"
msgstr "སི་ཊ་ཡེལ་ཤིཊི་མངོན་གསལ་འབདཝ་ད་ འཛོལ་བ: %S"

#: TransformError
msgid "Error during XSLT transformation: %S"
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ བཟོ་བསྒྱུར་འབདཝ་ད་འཛོལ་བ: %S"

