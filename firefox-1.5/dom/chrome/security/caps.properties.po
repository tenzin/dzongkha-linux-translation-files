#: Yes
msgid "Allow"
msgstr "བཅུག་ནི"

#: No
msgid "Deny"
msgstr "མི་བཅུག་ནི།"

#: Titleline
msgid "Internet Security"
msgstr "ཨིན་ཊར་ནེཊི་སྲུང་སྐྱོབ།"

#: CheckMessage
msgid "Remember this decision"
msgstr "གྲོས་ཆད་འདི་སེམས་ཁར་བཞག"

#: EnableCapabilityQuery
msgid ""
"A script from \"%S\" is requesting enhanced abilities that are UNSAFE and could be used to compromise your machine or data:\n"
"\n"
"%S\n"
"\n"
"Allow these abilities only if you trust this source to be free of viruses or malicious programs."
msgstr ""
"\"%S\"་ལས་ཨིན་པའི་ཡིག་ཚུགས་དེ་གིས་  ཁྱོད་ཀྱི་གློག་འཕྲུལ་ཡང་ན་གནད་སྡུད་འདི་ ནང་འགྲིགས་འབད་ནི་ལུ་  ལག་ལེན་འཐབ་བཏུབ་མི་དང་  UNSAFEའབད་ཡོད་པའི་  གོང་སྤེལ་གྱི་ལྕོགས་གྲུབ་འདི་ ཞུ་བ་འབད་དོ:\n"
"\n"
"%S\n"
"\n"
"  ཁྱོད་ཀྱིས་  འབྱུང་ཁུངས་འདི་  གནོད་ཅན་གྱི་ལས་རིམ་ཚུ་དང་  གནོད་འབུབ་ཚུ་མེད་ཟེར་བློ་གཏད་པ་ཅིན་རྐྱངམ་ཅིག་  ལྕོགས་གྲུབ་དེ་ཚུ་  བཅུག་ད།"

#: EnableCapabilityDenied
msgid "A script from \"%S\" was denied %S privileges."
msgstr "\"%S\"ལས་ཚུགས་ཡིག་འདི་ %S ཁེ་དབང་ཚུ་གིས་ཉེན་མ་བཏུབ་པས།"

#: CheckLoadURIError
msgid "Security Error: Content at %S may not load or link to %S."
msgstr "སྲུང་སྐྱོབ་ཀྱི་འཛོལ་བ:  %Sལུ་ཡོད་པའི་ནང་དོན་འདི་  %Sལུ་ ཡང་ན་མངོན་གསལ་མི་འབད་ ཡང་ན་ འབྲེལ་ལམ་མུ་འབད་འོང་།"

#: CheckSameOriginError
msgid "Security Error: Content at %S may not load data from %S."
msgstr "སྲུང་སྐྱོབ་ཀྱི་འཛོལ་བ:  %Sལུ་ཡོད་པའི་ ནང་དོན་དེ་གིས་  %Sལས་  གནད་སྡུད་འདི་མངོན་གསལ་མི་འབད་འོང་།"

#: GetPropertyDenied
msgid "Permission denied to get property %S.%S"
msgstr "རྒྱུ་དངོས་  %S ལེན་ནི་དོན་ལུ་  གནང་བ་བྱིན་མ་བཏུབ་པས།%S"

#: SetPropertyDenied
msgid "Permission denied to set property %S.%S"
msgstr "རྒྱུ་དངོས་%S འདི་གཞི་སྒྲིག་འབད་ནི་དོན་ལུ་  གནང་བ་འདི་བྱིན་མ་བཏུབ་པས།%S"

#: CallMethodDenied
msgid "Permission denied to call method %S.%S"
msgstr "ཐབས་ལམ་ %S འདི་འབོ་ནི་ལུ་  གནང་བ་འདི་བྱིན་མ་བཏུབ་པས།%S"

#: CreateWrapperDenied
msgid "Permission denied to create wrapper for object of class %S"
msgstr "དབྱེ་རིགས་ %S གི་དངོས་པོའི་དོན་ལུ་  ལོག་མཚམས་བཟོ་ནི་འདི་གསར་བསྐྲུན་འབད་ནི་ལུ་  གནང་བ་འདི་བྱིན་མ་བཏུབ་པས།"

#: ExtensionCapability
msgid "Unknown: %S"
msgstr "མ་ཤསཔ:  %S"

#
# The following descriptions are shown in the EnableCapabilityQuery dialog
#
#: capdesc.UniversalBrowserRead
msgid "Read private data from any site or window"
msgstr "སྒེར་དོན་གནད་སྡུད་འདི་  ས་ཁོངས་ཡང་ན་སྒོ་སྒྲིག་གང་རུང་ཅིག་ལས་  ལྷག་ད།"

#: capdesc.UniversalBrowserWrite
msgid "Modify any open window"
msgstr "ཁ་ཕྱེ་བའི་སྒོ་སྒྲིག་གང་རུང་ཅིག་ལེགས་བཅོས་འབད།"

#: capdesc.UniversalXPConnect
msgid "Run or install software on your machine"
msgstr "ཁྱོད་རའི་གློག་འཕྲུལ་གུ་  མཉེན་ཆས་འདི་གཡོག་བཀོལ་ནི་ ཡང་ན་ གཞི་བཙུགས་འབད།"

#: capdesc.UniversalFileRead
msgid "Read and upload local files"
msgstr "ཉེ་གནས་ཡིག་སྣོད་ཚུ་  ལྷག་ཞིནམ་ལས་སྐྱེལ་བཙུགས་འབད།"

#: capdesc.CapabilityPreferencesAccess
msgid "By-pass core security settings"
msgstr "ཟུར་ལམ་ནང་སྙིང་སྲུང་སྐྱོབ་སྒྲིག་སྟངས་ཚུ།"

#: capdesc.UniversalPreferencesRead
msgid "Read program settings"
msgstr "ལས་རིམ་སྒྲིག་སྟངས་ཚུ་ལྷག"

#: capdesc.UniversalPreferencesWrite
msgid "Modify program settings"
msgstr "ལས་རིམ་སྒྲིག་སྟངས་ཚུ་ ལེགས་བཅོས་འབད།"

