#: intro.label
msgid "You have chosen to open"
msgstr "ཁྱོད་ཀྱིས་ཁ་ཕྱེ་ནི་ལུ་གདམ་ཁ་བརྐྱབ་ནུག"

#: from.label
msgid "from:"
msgstr "ལས:"

#: actionQuestion.label
msgid "What should &brandShortName; do with this file?"
msgstr "ཡིག་སྣོད་འདི་དང་ཅིག་ཁར་ &brandShortName; གིས་ག་ཅི་འབད་ནི?"

#: openWith.label
#: openWith.accesskey
msgid "&Open with"
msgstr "དང་བཅས་ ཁ་ཕྱེ།(&O)"

#: other.label
msgid "Other..."
msgstr "གཞན..."

#: save.label
#: save.accesskey
msgid "&Save to Disk"
msgstr "ཌིཀསི་ལུ་སྲུང་།(&S)"

#: rememberChoice.label
#: rememberChoice.accesskey
msgid "Do this &automatically for files like this from now on."
msgstr "ད་ལས་ཕར་  ཡིག་སྣོད་ཀྱི་དོན་ལུ་ འདི་རང་བཞིན་གྱིས་འབད།(&a)"

#: settingsChange.label
msgid "Settings can be changed in the Downloads section of Tools, Options."
msgstr "སྒྲིག་སྟངས་ཚུ་  ལག་ཆས་ཚུ་གི་ཕབ་ལེན་དབྱེ་ཚན་  གདམ་ཁ་ཚུ་ནང་བསྒྱུར་བཅོས་འབད་བཏུབ་ཨིན།"

#: settingsChangeMac.label
msgid "Settings can be changed in the Downloads section of &brandShortName;, Preferences."
msgstr "སྒྲིག་སྟངས་ཚུ་  &brandShortName; གི་ཕབ་ལེན་དབྱེ་ཚན་ཚུ་  དགའ་གདམ་ཚུ་ནང་བསྒྱུར་བཅོས་འབད་བཏུབ་ཨིན།"

#: settingsChangeUnix.label
msgid "Settings can be changed in the Downloads section of Edit, Preferences."
msgstr "སྒྲིག་སྟངས་ཚུ་  ཞུན་དག་གི་ཕབ་ལེན་དབྱེ་ཚན་  དགའ་གདམ་ཚུ་ནང་ བསྒྱུར་བཅོས་འབད་བཏུབ་ཨིན།"

#: whichIsA.label
msgid "which is a:"
msgstr "འདི་ཡང་:"

#: chooseHandlerMac.label
#: chooseHandlerMac.accesskey
msgid "&Choose..."
msgstr "གདམ་ཁ་བརྐྱབ...(&C)"

#: chooseHandler.label
#: chooseHandler.accesskey
msgid "&Browse..."
msgstr "བརྡ་འཚོལ...(&B)"

