#: changeAction.title
msgid "Change Action"
msgstr "བྱ་བ་འདི་བསྒྱུར་བཅོས་འབད་"

#: whenDownloading.label
msgid "When downloading files of type:"
msgstr "དབྱེ་བ་གི་ཡིག་སྣོད་ཚུ་  ཕབ་ལེན་འབདཝ་ད:"

#: always.label
msgid "automatically:"
msgstr "རང་བཞིན་གྱིས:"

#: openDefault.label
msgid "Open them in the Default application:"
msgstr "དེ་ཚུ་ སྔོན་སྒྲིག་འཇུག་སྤྱོད་ནང་ལུ་ཁ་ཕྱེ:"

#: openApplication.label
msgid "Open them in this application:"
msgstr "དེ་ཚུ་  འཇུག་སྤྱོད་འདི་གི་ནང་ལུ་ཁ་ཕྱེ:"

#: changeApp.label
#: changeApp.accesskey
msgid "&Change..."
msgstr "བསྒྱུར་བཅོས་འབད...(&C)"

#: customAppPath.label
msgid "Click 'Change ...' to choose an application."
msgstr "འཇུག་སྤྱོད་འདི་གདམ་ཁ་བརྐྱབ་ནི་ལུ་  བསྒྱུར་བཅོས་ འདི་ཨེབ་གཏང་..."

#: saveToDisk.label
msgid "Save to Disk"
msgstr "ཌིཀསི་ལུ་སྲུང་བཞག་འབད།"

#: changeAppWindowTitle.label
msgid "Choose Application..."
msgstr "འཇུག་སྤྱོད་འདི་གདམ་ཁ་བརྐྱབ..."

