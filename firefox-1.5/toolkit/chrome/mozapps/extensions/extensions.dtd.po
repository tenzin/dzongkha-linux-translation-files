#: cmd.options.label
#: cmd.options.accesskey
msgid "&Options"
msgstr "གདམ་ཁ་ཚུ།(&O)"

#: cmd.options.tooltip
msgid "Set Options for the selected Extension"
msgstr "སེལ་འཐུ་གྲུབ་ཡོད་པའི་  རྒྱ་བསྐྱེད་ཀྱི་དོན་ལུ་ གདམ་ཁ་ཚུ་ གཞི་སྒྲིག་འབད།"

#: cmd.optionsUnix.label
#: cmd.optionsUnix.accesskey
msgid "P&references"
msgstr "དགའ་གདམ་ཚུ།(&r)"

#: cmd.optionsUnix.tooltip
