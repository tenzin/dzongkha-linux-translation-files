# Command Bar items
#: cmd.uninstall.label
#: cmd.uninstall.accesskey
msgid "Un&install"
msgstr "གཞི་བཙུགས་བཤོལ།(&i)"

#: cmd.uninstall.tooltip
msgid "Uninstalls the selected Extension"
msgstr "སེལ་འཐུ་གྲུབ་ཡོད་པའི་རྒྱ་བསྐྱེད་དེ་ གཞི་བཙུགས་བཤོལཝ་ཨིན།"

#: cmd.update.label
#: cmd.update.accesskey
msgid "Find &Updates"
msgstr "དུས་མཐུན་ཚུ་འཚོལ།(&U)"

#: cmd.update.tooltip
msgid "Finds Updates to your Extensions"
msgstr "ཁྱོད་རའི་རྒྱ་བསྐྱེད་ཚུ་ནང་ལུ་ དུས་མཐུན་ཚུ་འཚོལཝ་ཨིན།"

#: cmd.useTheme.label
#: cmd.useTheme.accesskey
msgid "Use &Theme"
msgstr "བརྗོད་དོན་ལག་ལེན་འཐབ།(&T)"

#: cmd.useTheme.tooltip
msgid "Changes &brandShortName;'s Theme"
msgstr "&brandShortName;གི་བརྗོད་དོན་དེ་སོརཝ་ཨིན།"

# the following command bar items are used by thunderbird only
#: cmd.install.label
#: cmd.install.accesskey
msgid "I&nstall"
msgstr "གཞི་བཙུགས།(&n)"

#: cmd.install.tooltip
msgid "Install an Extension"
msgstr "རྒྱ་བསྐྱེད་ཅིག་གཞི་བཙུགས་འབད།"

