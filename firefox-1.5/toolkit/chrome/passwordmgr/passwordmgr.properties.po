#: rememberValue
msgid "Use Password Manager to remember this value."
msgstr "གནས་གོང་དེ་སེམས་ཁར་བཞག་ནི་ལུ་ཆོག་ཡིག་འཛིན་སྐྱོང་པ་ལག་ལེན་འཐབ།"

#: rememberPassword
msgid "Use Password Manager to remember this password."
msgstr "ཆོག་ཡིག་དེ་སེམས་ཁར་བཞག་ནི་ལུ་ཆོག་ཡིག་འཛིན་སྐྱོང་པ་ལག་ལེན་འཐབ།"

#: savePasswordTitle
msgid "Confirm"
msgstr "ངེས་དཔྱད་འབད་ནི།"

#: savePasswordText
msgid "Do you want %S to remember this password?"
msgstr "ཁྱོད་ཀྱིས་ %ཨེསི་ལུ་ཆོག་ཡིག་དེ་སེམས་ཁར་བཞག་ནི་ཨིན་ན?"

#: rememberButtonText
msgid "&Remember"
msgstr "སེམས་ཁར་བཞག (&R)"

#: notNowButtonText
msgid "&Not Now"
msgstr "ད་ལྟོ་མེན།(&N)"

#: neverForSiteButtonText
msgid "Ne&ver for This Site"
msgstr "ས་ཁོངས་དེའི་དོན་ལུ་ནམ་ཡང་མེན།(&v)"

#: close
msgid "Close"
msgstr "ཁ་བསྡམས།"

#: passwordChangeTitle
msgid "Confirm Password Change"
msgstr "ཆོག་ཡིག་བསྒྱུར་བཅོས་ངེས་དཔྱད་འབད་ནི།"

#: passwordChangeText
msgid "Would you like to have Password Manager change the stored password for %S?"
msgstr "ཁྱོད་ཀྱིས་ཆོག་ཡིག་འཛིན་སྐྱོང་པ་ལུ་ %ཨེསི་གི་དོན་ལུ་གསོག་འཇོག་འབད་ཡོད་མི་ཆོག་ཡིག་དེ་བསྒྱུར་བཅོས་འབད་ནི་ཨིན་ན?"

#: userSelectText
msgid "Please confirm which user you are changing the password for"
msgstr "ཁྱོད་ཀྱིས་ལག་ལེན་པ་ག་འདི་ཆོག་ཡིག་གི་དོན་ལུ་བསྒྱུར་བཅོས་འབད་ནི་ཨིན་ན་ངེས་དཔྱད་འབད་གནང་།"

#: hidePasswords
msgid "Hide Passwords"
msgstr "ཆོག་ཡིག་ཚུ་གསང་།"

#: showPasswords
msgid "Show Passwords"
msgstr "ཆོག་ཡིག་ཚུ་སྟོན།"

#: noMasterPasswordPrompt
msgid "Are you sure you wish to show your passwords?"
msgstr "ཁྱོད་ཀྱིས་ངེས་ཏིག་ཨིན་ན་ཁྱོད་རའི་ཆོག་ཡིག་ཚུ་སྟོན་ནི་ཨིན་ན?"

