#: searchText
msgid "Web Search for \"%S\""
msgstr "\"%S\"གི་དོན་ལུ་ཝེབ་འཚོལ་ཞིབ་"

#: SavePageTitle
msgid "Save Web Page"
msgstr "ཝེབ་ཤོག་ལེབ་སྲུངས་"

#: SaveImageTitle
msgid "Save Image"
msgstr "གཟུགས་བརྙན་སྲུངས་"

#: SaveLinkTitle
msgid "Save As"
msgstr "དེ་སྦེ་སྲུངས་"

#: DefaultSaveFileName
msgid "index"
msgstr "ཟུར་ཐོ་"

#: WebPageCompleteFilter
msgid "Web Page, complete"
msgstr "ཝེབ་ཤོག་ལེབ་ ཆ་ཚང་"

#: WebPageHTMLOnlyFilter
msgid "Web Page, HTML only"
msgstr "ཝེབ་ཤོག་ལེབ་ ཨེཆ་ཊི་ཨེམ་ཨེབ་རྐྱངམ་གཅིག་"

#: WebPageXHTMLOnlyFilter
msgid "Web Page, XHTML only"
msgstr "ཝེབ་ཤོག་ལེབ་ ཨེསི་ཨེཆ་ཊི་ཨེམ་ཨེབ་རྐྱངམ་གཅིག་"

#: WebPageXMLOnlyFilter
msgid "Web Page, XML only"
msgstr "ཝེབ་ཤོག་ལེབ་ ཨེགསི་ཨེམ་ཨེལ་རྐྱངམ་གཅིག་"

# LOCALIZATION NOTE (filesFolder):
#    This is the name of the folder that is created parallel to a HTML file
#    when it is saved "With Images". The %S section is replaced with the
#    leaf name of the file being saved (minus extension).
#: filesFolder
msgid "%S_files"
msgstr "ཡིག་སྣོད་%S(_f)"

#: saveLinkErrorMsg
msgid "The link could not be saved.  The web page might have been removed or had its name changed."
msgstr "འབྲེལ་ལམ་འདི་བསྲུང་མ་ཚུགས།་ ཝེབ་ཤོག་དེ་རྩ་བསྐྲད་བཏང་དཔ་འོང་ནི་དང་ ཡང་ན་ དེ་གི་མིང་སོར་དཔ་འོང་ནི་མས།"

#: saveLinkErrorTitle
msgid "Save Link"
msgstr "འབྲེལ་ལམ་སྲུངས་"

#: unknownDescriptionFilesFilter
msgid "%S File"
msgstr "ཡིག་སྣོད་%S"

#: unknownDescriptionFilesPluralFilter
msgid "%S Files"
msgstr "ཡིག་སྣོད་%Sཚུ་"

