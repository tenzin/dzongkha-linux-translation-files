# LOCALIZATION NOTE FILE
# --do not localize the extensions, only the titles
#: allTitle
msgid "All Files"
msgstr "ཡིག་སྣོད་ཆ་མཉམ་"

#: allFilter
msgid "*"
msgstr "*"

#: htmlTitle
msgid "HTML Files"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ ཡིག་སྣོད་ཚུ་"

#: htmlFilter
msgid "*.html; *.htm; *.shtml; *.xhtml"
msgstr "*.html; *.htm; *.shtml; *.xhtml"

#: textTitle
msgid "Text Files"
msgstr "ཚིག་ཡིག་ཡིག་སྣོད་ཚུ་"

#: textFilter
msgid "*.txt; *.text"
msgstr "*.txt; *.text"

#: imageTitle
msgid "Image Files"
msgstr "གཟུགས་བརྙན་ཡིག་སྣོད་ཚུ་"

#: imageFilter
msgid "*.jpg; *.jpeg; *.gif; *.png; *.bmp; *.xbm; *.ico"
msgstr "*.jpg; *.jpeg; *.gif; *.png; *.bmp; *.xbm; *.ico"

#: xmlTitle
msgid "XML Files"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ཡིག་སྣོད་ཚུ་"

#: xmlFilter
msgid "*.xml"
msgstr "*.xml"

#: xulTitle
msgid "XUL Files"
msgstr "ཨེགསི་ཡུ་ཨེལ་ཡིག་སྣོད་ཚུ་"

#: xulFilter
msgid "*.xul"
msgstr "*.xul"

#: appsTitle
msgid "Applications"
msgstr "གློག་རིམ་ཚུ་"

#: dirTextInputLabel
#: promptNewDirMessage
msgid "Directory name:"
msgstr "སྣོད་ཐོའི་མིང་:"

#: confirmTitle
msgid "Confirm"
msgstr "ངེས་དཔྱད་འབད་"

#: confirmFileReplacing
msgid ""
"%S already exists.\n"
"Do you want to replace it?"
msgstr ""
"%Sཧེ་མ་ལས་འདུག།\n"
"དེ་གི་ཚབ་བཙུགས་ནི་ཨིན་ན?"

#: openButtonLabel
msgid "Open"
msgstr "ཁ་ཕྱེ་"

#: saveButtonLabel
msgid "Save"
msgstr "སྲུངས་"

#: selectFolderButtonLabel
msgid "Select"
msgstr "སེལ་འཐུ་འབད་"

#: noButtonLabel
msgid "No"
msgstr "མེན་"

#: errorOpenFileDoesntExistTitle
#: errorOpeningFileTitle
msgid "Error opening %S"
msgstr "%Sཁ་ཕྱེ་ནི་ལུ་འཚོལ་བ་"

#: errorOpenFileDoesntExistMessage
msgid "File %S doesn't exist"
msgstr "ཡིག་སྣོད་%Sམིན་འདུག།"

#: errorDirDoesntExistTitle
msgid "Error accessing %S"
msgstr "%Sལུ་འཛུལ་སྤྱོད་འབད་ནི་ལུ་འཛོལ་བ་"

#: errorDirDoesntExistMessage
msgid "Directory %S doesn't exist"
msgstr "སྣོད་ཐོ་%Sམིན་འདུག།"

#: errorDirNotReadableMessage
msgid "Directory %S is not readable or does not exist."
msgstr "སྣོད་ཐོ་%S དེ་ ལྷག་མི་བཏུབ་པའམ་ མེདཔ་འོང་ནི་མས།"

#: openWithoutPermissionMessage_file
msgid "File %S is not readable"
msgstr "ཡིག་སྣོད་%S ལྷག་མི་བཏུབ་པས་"

#: errorSavingFileTitle
msgid "Error saving %S"
msgstr "བསྲུང་ནི་ལུ་འཛོལ་བ་"

#: saveParentIsFileMessage
msgid "%S is a file, can't save %S"
msgstr "%S དེ་ཡིག་སྣོད་ཅིག་ཨིན་པས། %S དེ་བསྲང་མི་ཚུགས།"

#: saveParentDoesntExistMessage
msgid "Path %S doesn't exist, can't save %S"
msgstr "འགྲུལ་ལམ་%Sམིན་འདུག། %S བསྲུང་མི་ཚུགས།"

#: saveWithoutPermissionMessage_file
msgid "File %S is not writable."
msgstr "ཡིག་སྣོད་%Sདེ་ འབྲི་བཏུབ་མེན་པས།"

#: saveWithoutPermissionMessage_dir
msgid "Cannot create file. Directory %S is not writable."
msgstr "ཡིག་སྣོད་གསར་བསྐྲུན་མི་ཚུགས། སྣོད་ཐོ་%Sདེ་འབྲི་བཏུབ་མེན་པས།"

#: errorNewDirDoesExistTitle
#: errorCreateNewDirTitle
msgid "Error creating %S"
msgstr "%S གསར་བསྐྲུན་འབད་ནི་ལུ་འཛོལ་བ་"

#: errorNewDirDoesExistMessage
msgid "A file named %S already exists, directory cannot be created."
msgstr "ཡིང་སྣོད་མིང་%Sཟེར་ བཏགས་བཏགཔ་ཅིག་ ཧེ་མ་ལས་འདུག། སྣོད་ཐོ་གསར་བསྐྲུན་འབད་མི་ཚུགས།"

#: errorCreateNewDirMessage
msgid "Directory %S could not be created"
msgstr "སྣོད་ཐོ་%S གསར་བསྐྲུན་འབད་མ་ཚུགས།"

#: errorCreateNewDirIsFileMessage
msgid "Directory cannot be created, %S is a file"
msgstr "སྣོད་ཐོ་གསར་བསྐྲུན་འབད་མི་ཚུགས། %S དེ་ཡིག་སྣོད་ཅིག་ཨིན།"

#: errorCreateNewDirPermissionMessage
msgid "Directory cannot be created, %S not writable"
msgstr "སྣོད་ཐོ་གསར་བསྐྲིན་འབད་མི་ཚུགས། %S འབྲི་བཏུབ་མེན་པས།"

#: promptNewDirTitle
msgid "Create new directory"
msgstr "སྣོད་ཐོ་གསར་བསྐྲུན་འབད་"

#: errorPathProblemTitle
msgid "Unknown Error"
msgstr "མ་ཤེས་པའི་འཛོལ་བ་"

#: errorPathProblemMessage
msgid "An unknown error occured (path %S)"
msgstr "མ་ཤེས་པའི་འཛོལ་ཅིག་བྱུང་ནུག།(འགྲུལ་ལམ་%S)"

