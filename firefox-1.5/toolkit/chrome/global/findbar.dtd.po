# LOCALIZATION NOTE : FILE This file contains the entities needed to
# LOCALIZATION NOTE : FILE use the Find Bar.
# entities split out from browser.dtd
#: find.label
msgid "Find:"
msgstr "འཚོལ་:"

#: findNext.label
#: findNext.accesskey
msgid "Find &Next"
msgstr "ཤུལ་མམ་འཚོལ་(&N)"

#: findNext.tooltip
msgid "Find the next occurrence of the phrase"
msgstr "ཚིག་ཚན་གྱི་ཤུལ་མའི་འབྱུང་བ་འཚོལ་"

#: findPrevious.label
#: findPrevious.accesskey
msgid "Find &Previous"
msgstr "ཧེ་མམ་འཚོལ་"

#: findPrevious.tooltip
msgid "Find the previous occurrence of the phrase"
msgstr "ཚིག་ཚན་གྱི་ཧེ་མའི་འབྱུང་བ་འཚོལ་"

#: findCloseButton.tooltip
msgid "Close Find bar"
msgstr "འཚོལ་ནིའི་ཕྲ་རིང་ཁ་བསྡམས།"

#: highlight.label
#: highlight.accesskey
msgid "Highlight &all"
msgstr "ཆ་མཉམ་ལུ་གཙོ་དམིགས་འབད་(&al)"

#: highlight.tooltip
msgid "Highlight all occurrences of the phrase (Ctrl+Enter)"
msgstr "ཚིག་ཚན་གྱི་་འབྱུང་བ་ཆ་མཉམ་ལུ་ གཙོ་དམིགས་འབད་(ཚད་འཛིན་+ལོག་ལྡེ་)"

#: highlight.macTooltip
msgid "Highlight all occurrences of the phrase (Cmd+Return)"
msgstr "ཚིག་ཚན་གྱི་་འབྱུང་བ་ཆ་མཉམ་ལུ་ གཙོ་དམིགས་འབད་(སི་ཨེམ་ཌི་+ལོག་ལྡེ་)"

#: caseSensitiveCheckbox.label
#: caseSensitiveCheckbox.accesskey
msgid "Mat&ch case"
msgstr "ཡི་གུའི་མཐུན་སྒྲིག(&c)"

