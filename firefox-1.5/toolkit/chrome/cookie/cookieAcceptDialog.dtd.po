#: props.box.label
msgid "About this cookie"
msgstr "ཀུ་ཀི་འདིའི་སྐོར་ལས།"

#: props.name.label
msgid "Name:"
msgstr "མིང་:"

#: props.value.label
msgid "Content:"
msgstr "ནང་དོན:"

#: props.domain.label
msgid "Host:"
msgstr "ཧོསཊི:"

#: props.path.label
msgid "Path:"
msgstr "འགྲུལ་ལམ:"

#: props.secure.label
msgid "Send For:"
msgstr "དོན་ལུ་གཏང་:"

#: props.expires.label
msgid "Expires:"
msgstr "དུས་ཡོལཝ:"

#: props.policy.label
msgid "Policy:"
msgstr "སྲིད་བྱུས:"

#: button.allow.label
#: button.allow.accesskey
msgid "&Allow"
msgstr "འབད་བཅུག  (&A)"

#: button.session.label
#: button.session.accesskey
msgid "Allow for &Session"
msgstr "ལཱ་ཡུན་དོན་ལུ་འབད་བཅུག (&S)"

#: button.deny.label
#: button.deny.accesskey
msgid "&Deny"
msgstr "ཉན་མི་བཏུབ་པས།(&D)"

#: dialog.title
msgid "Confirm setting cookie"
msgstr "ཀུ་ཀི་སྒྲིག་སྟངས་ངེས་དཔྱད་འབད་ནི།"

#: dialog.remember.label
#: dialog.remember.accesskey
msgid "&Use my choice for all cookies from this site"
msgstr "ས་ཁོངས་འདི་ལས་ཀུ་ཀི་ཚུ་ཆ་མཉམ་གྱི་དོན་ལུ་ངེའི་གདམ་ཁ་ལག་ལེན་འཐབ།(&U)"

