#: NEXT
msgid "Next >"
msgstr "ཤུལ་མམ་ >"

#: BACK
msgid "< Back"
msgstr "< རྒྱབ་ལོག་"

#: CANCEL
#: CANCEL_LABEL
msgid "Cancel"
msgstr "ཆ་མེད་"

#: ACCEPT
msgid "Accept"
msgstr "དང་ལེན་འབད་"

#: DECLINE
msgid "Decline"
msgstr "དང་ལེན་མ་འབད་"

#: INSTALL
msgid "Install"
msgstr "གཞི་བཙུགས་འབད་"

#: PAUSE
msgid "Pause"
msgstr "ཐེམ་"

#: RESUME
msgid "Resume"
msgstr "བསྐྱར་ལོག་"

#: DEFAULT_TITLE
msgid "Mozilla Installer"
msgstr "མོ་ཛིལ་ལ་གཞི་བཙུགས་པ་"

#: DEST_DIR
msgid "Destination Directory"
msgstr "འགྲོ་ཡུལ་སྣོད་ཐོ་"

#: BROWSE
msgid "Change..."
msgstr "སོར་་་"

#: SELECT_DIR
msgid "Select a directory"
msgstr "སྣོད་ཐོ་ཅིག་སེལ་འཐུ་འབད་"

#: DOESNT_EXIST
msgid "Directory %s doesn't exist.  Create it?"
msgstr "སྣོད་ཐོ་ %sགནས་ཏེ་མིན་འདུག། གསར་བསྐྲུན་འབད་ནི་ཨིན་ན?"

#: YES_LABEL
msgid "Yes"
msgstr "ཨིན་"

#: NO_LABEL
msgid "No"
msgstr "མེན་"

#: OK_LABEL
msgid "OK"
msgstr "བཏུབ་"

#: DELETE_LABEL
msgid "Delete Directory"
msgstr "སྣོད་ཐོ་བཏོན་གཏང་"

#: ERROR
msgid "Error [%d]: %s"
msgstr "འཛོལ་བ་ [%d]: %s"

#: FATAL_ERROR
msgid "Fatal error [%d]: %s"
msgstr "ཚབས་ཆེན་འཛོལ་བ་[%d]: %s"

#: DESCRIPTION
msgid "Description"
msgstr "འགྲེལ་བཤད་"

#: WILL_INSTALL
msgid "Setup will install the following components:"
msgstr "གཞི་སྒྲིག་གིས་གཤམ་གསལ་གྱི་ཆ་ཤས་ཚུ་: "

#: TO_LOCATION
msgid "to the following location:"
msgstr "འོག་གི་གནས་ཁོངས་ནང་ གཞི་བཙུགས་འབད་འོང་:"

#: PREPARING
msgid "Preparing %s..."
msgstr "%sགྲ་སྒྲིག་འབད་དོ་་་"

#: EXTRACTING
msgid "Extracting %s..."
msgstr "%sཕྱིར་འདོན་འབད་དོ་་་"

#: INSTALLING_XPI
msgid "Installing %s..."
msgstr "%s གཞི་བཙུགས་འབད་དོ་་་"

#: PROCESSING_FILE
msgid "Processing file %d of %d..."
msgstr "ཡིག་སྣོད་%dལས་ %dལས་སྦྱོར་འབད་དོ་་་"

#: NO_PERMS
msgid "Choose another directory because you do not have permission to install to: %s"
msgstr "ཁྱོད་ཀྱིས་%sལུ་གཞི་བཙུགས་འབད་ནིའི་གནང་བ་མེདཔ་ལས་ སྣོད་ཐོ་གཞན་ཅིག་ གདམ་ཁ་རྐྱབས།"

#: DL_SETTINGS
msgid "Download Settings"
msgstr "སྒྲིག་སྟངས་ཚུ་ཕབ་ལེན་འབད་"

#: SAVE_MODULES
msgid "Save installer modules upon download"
msgstr "གཞི་བཙུགས་པའི་ཚད་གཞི་ཚུ་ ཕབ་ལེན་འབད་བའི་སྐབས་བསྲུང་བཞག།"

#: PROXY_SETTINGS
msgid "Proxy Settings..."
msgstr "པོརོག་སི་སྒྲིག་སྟངས་་་"

#: PS_LABEL0
msgid "Proxy Host [required]:"
msgstr "པོརོག་སི་ཧོསིཊི་[ངེས་པར་དགོཔ་]:"

#: PS_LABEL1
msgid "Proxy Port [required]:"
msgstr "པོརོག་སི་འདྲེན་ལམ་[ངེས་པར་དགོཔ་]:"

#: PS_LABEL2
msgid "Proxy Username [optional]:"
msgstr "པོརོག་སི་ལག་ལེན་པའི་མིང་[གདམ་ཁ་ཅན་]:"

#: PS_LABEL3
msgid "Proxy Password [optional]:"
msgstr "པོརོག་སི་ཆོག་ཡིག [གདམ་ཁ་ཅན་]:"

#: ERROR_TITLE
msgid "Error!"
msgstr "འཛོལ་བ་!"

#: DS_AVAIL
msgid "Disk Space Available = %ld KB"
msgstr "ཌིཀསི་ནང་བར་སྟོང་ཡོད་པ་=ཀེ་བི་%ld"

#: DS_REQD
msgid "Disk Space Required = %ld KB"
msgstr "ཌིཀསི་ནང་བར་སྟོང་ངེས་པར་དགོཔ་=ཀེ་བི་%ld"

#: NO_DISK_SPACE
msgid "Please select a directory on a disk with enough space to install or free some disk space on the selected disk."
msgstr "གཞི་བཙུགས་འབད་ནི་ལུ་ བར་སྟོང་ལངམ་ཡོད་པའི་ཌིཀསི་ཅིག་ལུ་ སྣོད་ཐོ་ཅིག་སེལ་འཐུ་འབད་ ཡང་ན་ སེལ་འཐུ་འབད་མི་ཌིཀསི་དེ་གུ་ བར་སྟོང་དུམ་གྲ་ཅིག་བཏོན།"

#: CXN_DROPPED
msgid "A network connection failure occured.  Please check your network connection and press the 'Resume' button.  Alternatively, press the 'Cancel' button to quit the installer."
msgstr "ཡོངས་འབྲེལ་མཐུད་ལམ་ལུ་ འཐུས་ཤོར་ཅིག་བྱུང་ཡི། ཁྱོད་རའི་ཡོངས་འབྲེལ་མཐུད་ལམ་ལུ་ ཞིབ་དཔྱད་འབད་ཞིནམ་ལས་'བསྐྱར་ལོག་' ཟེར་མི་ཨེབ་རྟ་གུ་མནན། གཞི་བཙུགས་པ་བཤོལ་ནི་ལུ་་'ཆ་མེད་གཏང་'་ཟེར་མི་ཨེབ་རྟ་གུ་མནན།"

#: CRC_CHECK
msgid "Some files failed to download correctly. Retrying."
msgstr "ཡིག་སྣོད་ལ་ལོ་ཅིག་ ཚུལ་མཐུན་སྦེ་ཕབལེན་འབད་མ་ཚུགས། སླར་བརྩོན་འབད་དོ།"

#: CRC_FAILED
msgid "Installation has failed due to multiple CRC failures."
msgstr "སི་ཨར་སི་མང་རབས་ཅིག་ འཐུས་ཤོར་བྱུང་མི་ལུ་བརྟེན་ཏེ་ གཞི་བཙུགས་འབད་ནི་ལུ་ འཐུས་ཤོར་བྱུང་ཡི།"

#: DOWNLOADING
msgid "Downloading:"
msgstr "ཕབལེན་འབད་དོ་:"

#: FROM
msgid "From:"
msgstr "ལས་:"

#: TO
msgid "To:"
msgstr "ལུ་:"

#: STATUS
msgid "Status:"
msgstr "གནས་ཚད་:"

#: DL_STATUS_STR
msgid "%d KB  of  %d KB (at %d KB/sec)"
msgstr "ཀེ་བི་%dལས་ ཀེ་བི་%d(སྐར་ཆ་རེ་ལུ་ཀེ་བི་ %d)"

#: USAGE_MSG
msgid "Usage: %s [options]%s\t[options] can be any of the following combination:%s\t\t-h:\t\tThis help.%s\t\t-ma:\t\tRun setup in Auto mode: show progress UI,%s\t\t\t\tbut assume defaults without user intervention.%s\t\t-ms:\t\tRun setup in Silent mode: show no UI and have%s\t\t\t\tno user intervention.%s\t\t-ira:\t\tIgnore the [RunAppX] sections%s"
msgstr "བེད་སྤྱོད་:  %s [options]%s\t[options] དེ་འོག་གི་མཉམ་མཐུད་གང་རུང་ཅིག་གིས་བཏུབ། :%s\t\t-h:\t\tThis help.%s\t\t-ma:\t\tRun རང་བཞིན་ཐབས་ལམ་ནང་ གཞི་སྒྲིག་འབད་ཡོདཔ་: ཡར་འཕེལ་ཡུ་ཨའི་སྟོན། %s\t\t\t\tbut ལག་ལེན་པའི་བར་དཀྲོགས་མེད་པར་ སྔོན་སྒྲིག་ཚུ་ཡོད་པར་ཆ་བཞག། %s\t\t-ms:\t\tRun སྒྲ་མེད་ཐབས་ལམ་ནང་གཞི་སྒྲིག་འབད་ཡོདཔ། ཡུ་ཨའི་མ་སྟོན་པར་ %s\t\t\t\tno ལག་ལེན་པའི་བར་དཀྲོགས་ལེན། %s\t\t-ira:\t\tIgnore the [RunAppX] དབྱེ་ཚན་%s"

#: UNKNOWN
msgid "Unknown"
msgstr "མ་ཤེསཔ་"

#------------------------------------------------------------------------------
#   Error strings
#------------------------------------------------------------------------------
#: -601
msgid "Out of memory"
msgstr "དྲན་ཚད་ཀྱིས་བདའ་མ་ཟུནམ།"

#: -602
msgid "Invalid param"
msgstr "ནུས་མེད་ཚད་བཟུང་"

#: -603
msgid "Invalid member variable"
msgstr "ནུས་མེད་འཐུས་མིའི་འགྱུར་བ།"

#: -604
msgid "Invalid pointer"
msgstr "ནུས་མེད་དཔག་བྱེད།"

#: -605
msgid "Parse key has no value"
msgstr "མིང་དཔྱད་ལྡེ་མིག་ལུ་ གནས་གོང་མིན་འདུག།"

#: -606
msgid "Failed to read readme"
msgstr "ང་ལྷག་ཟེར་མི་ ལྷག་མ་ཚུགས།"

#: -607
msgid "Failed to read license"
msgstr "ཆོག་ཐམ་ལྷག་མ་ཚུགས།"

#: -608
msgid "Out of bounds in Component/Setup Type list"
msgstr "ཆ་ཤས་/གཞི་སྒྲིག་གི་དབྱེ་བའི་ཐོ་ཡིག་གི་ ཚད་ཀྱི་ཕྱི་ཁར།"

#: -609
msgid "Mismatched ref counts"
msgstr "མཐུན་མིན་བྱུང་ཡོད་པའི་གཞི་བསྟུན་གྱི་གྲངས།"

#: -610
msgid "No components in the INI file"
msgstr "ཨེའི་ཨེན་ཨའི་ཡིག་སྣོད་ནང་ ཆ་ཤས་མིན་འདུག།"

#: -611
msgid "No setup types in the INI file"
msgstr "ཨེའི་ཨེན་ཨའི་ཡིག་སྣོད་ནང་ གཞི་སྒྲིག་གི་དབྱེ་བ་ཚུ་མིན་འདུག།"

#: -612
msgid "URL at this index already exists"
msgstr "ཟུར་ཐོ་འདི་ནང་ ཡུ་ཨར་ཨེལ་ ཧེ་མ་ལས་འདུག།"

#: -613
msgid "Couldn't create the directory"
msgstr "སྣོད་ཐོ་གསར་བསྐྲུན་འབད་མ་ཚུགས།"

#: -614
msgid "FTP URL malformed"
msgstr "ཨེཕ་ཊི་པི་ ཡུར་ཨར་ཨེལ་ བཟོ་ཉེས་གྱུར་ནུག།"

#: -615
msgid "Download failed"
msgstr "ཕབ་ལེན་འཐུས་ཤོར་བྱུང་ཡོད།"

#: -616
msgid "Extraction of XPCOM failed"
msgstr "ཨེགསི་པི་ཀོམ་ ཕྱིར་འདོན་འབདམ་ཚུགས།"

#: -617
msgid "Failed to fork a process"
msgstr "ལས་སྦྱོར་ཅིག་ ཁ་སྤེལ་མ་ཚུགས།"

#: -618
msgid "Couldn't open xpistub library"
msgstr "xpistub དཔེ་མཛོད་ ཁ་ཕྱེམ་ཚུགས།"

#: -619
msgid "Couldn't get symbol in library"
msgstr "དཔེ་མཛོད་ནང་ བརྡ་མཚོན་ཐོབ་མ་ཚུགས།"

#: -620
msgid "A xpistub call failed"
msgstr "xpistub ཅིག་ལུ་ ཞུ་འབོད་མ་བཏུབ།"

#: -621
msgid "An Installer module %s (.xpi) failed to install"
msgstr "གཞི་བཙུགས་པའི་ཐབས་ལམ་%s (.xpi)ཅིག་ གཞི་བཙུགས་འབད་མ་ཚུགས།"

#: -622
msgid "Copy of an installer module (.xpi) failed"
msgstr "གཞི་བཙུགས་པའི་ཐབས་ལམ་ ཅིག་(.xpi)འདྲ་བཤུས་རྐྱབ་མ་ཚུགས།"

#: -623
msgid "Destination directory doesn't exist"
msgstr "འགྲོ་ཡུལ་གྱི་སྣོད་ཐོ་གནས་ཏེ་མིན་འདུག།"

#: -624
msgid "Can't make destination directory.  Please try another directory."
msgstr "འགྲོ་ཡུལ་སྣོད་ཐོ་ བཟོ་མི་ཚུགས་པས། སྣོད་ཐོ་གཞན་མི་ཅིག་ལུ་ འབད་རྩོལ་བསྐྱེད་གནང་།"

#: -625
msgid "A previous installation exists."
msgstr "ཧེ་མའི་གཞི་བཙུགས་ཅིག་ གནས་ཏེ་འདུག།"

#: -626
msgid "Insufficient permission"
msgstr "གནང་བ་ལངམ་མིན་འདུག།"

#: -627
msgid "Insufficient disk space"
msgstr "ཌིཀསི་ནང་བར་སྟོང་ལངམ་མིན་འདུག།"

#: -628
msgid "Multiple CRC Failure"
msgstr "སྣ་མང་གི་ སི་ཨར་སི་ མང་འཐུས་ཤོར།"

