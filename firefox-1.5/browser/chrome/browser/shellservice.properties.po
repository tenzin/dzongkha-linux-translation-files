#: optionsLabel
msgid "%S &Options"
msgstr "གདམ་ཁ་%S །(&O)"

#: setDefaultBrowserTitle
msgid "Default Browser"
msgstr "སྔོན་སྒྲིག་བཱརའུ་ཟར།"

#: setDefaultBrowserMessage
msgid "%S is not currently set as your default browser. Would you like to make it your default browser?"
msgstr "ད་ལྟོ་%Sདེ་ ཁྱོད་ཀྱི་སྔོན་སྒྲིག་བཱརའུ་ཟར་སྦེ་ གཞི་སྒྲིག་མ་འབད་བས། འདི་ཁྱོད་ཀྱི་སྔོན་སྒྲིག་ བཱརའུ་ཟར་སྦེ་བཟོ་ནི་ཨིན་ན?"

#: setDefaultBrowserDontAsk
msgid "Always perform this check when starting %S."
msgstr "ཨ་རྟག་ར་ %Sའགོ་བཙུགས་སྐབས་ ཞིབ་དཔྱད་འདི་ ལག་ལེན་འཐབ།"

#: alreadyDefaultBrowser
msgid "%S is already set as your default browser."
msgstr "%Sདེ་ཧེ་མ་ལས་ ཁྱོད་ཀྱི་སྔོན་སྒྲིག་བཱརའུ་ཟར་སྦེ་ གཞི་སྒྲིག་འབད་ནུག།"

#: desktopBackgroundLeafNameWin
msgid "Desktop Background.bmp"
msgstr "ཌེཀསི་ཊོཔ་གི་རྒྱབ་གཞི།"

#: DesktopBackgroundNoPreview
msgid "(no preview available)"
msgstr "(སྔོན་ལྟ་མིན་འདུག)"

#: DesktopBackgroundDownloading
msgid "Saving Picture..."
msgstr "པར་སྲུང་དོ་་་་"

#: DesktopBackgroundSet
msgid "Set Desktop Background"
msgstr "ཌེཀསི་ཊོཔ་རྒྱབ་གཞི་ གཞི་སྒྲིག་འབད།"

