#: minimizeWindow.key
msgid "m"
msgstr "m"

#: minimizeWindow.label
msgid "Minimize"
msgstr "ཆུང་ཀུ་བཟོ།"

#: bringAllToFront.label
msgid "Bring All to Front"
msgstr "ཆ་མཉམ་གདོང་ཁར་འབག་ཤོག།"

#: zoomWindow.label
msgid "Zoom"
msgstr "རྒྱས་ཟུམ།"

#: windowMenu.label
msgid "Window"
msgstr "སྒོ་སྒྲིག།"

# LOCALIZATION NOTE some localizations of Windows (ex:french, german) use "?"
# for the help button in the menubar but Gnome does not.
#: helpMenu.label
#: helpMenu.accesskey
#: helpMenuWin.label
#: helpMenuWin.accesskey
msgid "&Help"
msgstr "གྲོགས་རམ།(&H)"

#: aboutCmd.label
#: aboutCmd.accesskey
msgid "&About &brandFullName;"
msgstr "&brandFullName; གི་སྐོར་ལས།(&A)"

#: helpContents.label
#: helpContents.accesskey
msgid "&Help Contents"
msgstr "གྲོགས་རམ་གྱི་ནང་དོན།(&H)"

#: helpContentsMac.label
msgid "&brandShortName; Help"
msgstr "&brandShortName; གྲོགས་རམ།"

#: helpForIEUsers.label
#: helpForIEUsers.accesskey
msgid "For &Internet Explorer Users"
msgstr "ཨིན་ཊར་ནེཊི་དཔྱད་ཞིབ་བྱེད་མཁན་ ལག་ལེན་པ་ཚུ་གི་དོན་ལུ།(&I)"

#: openHelp.commandkey
msgid "VK_F1"
msgstr "ཝི་ཀེ་ཨེཕ་༡།(_F)"

#: openHelpMac.commandkey
msgid "VK_HELP"
msgstr "ཝི་ཀེ་གྲོགས་རམ།(_H)"

# LOCALIZATION NOTE openHelpMac2.commandkey and openHelpMac2.modifiers
# are all the necessary keys to hit OS X's open-help
# keyboard (visible) shortcut (that's Cmd+? for most locales)
#: openHelpMac2.commandkey
msgid "/"
msgstr "/"

#: openHelpMac2.modifiers
msgid "accel,shift"
msgstr "ཨེཀ་སེལ། སོར་ལྡེ།"

#: openHelpMac2.frontendCommandkey
msgid "?"
msgstr "?"

#: openHelpMac2.frontendModifiers
msgid "accel"
msgstr "ཨེཀ་སེལ།"

#: helpReleaseNotes.label
#: helpReleaseNotes.accesskey
msgid "Release &Notes"
msgstr "དྲན་འཛིན་ཚུ་འཛིན་འགྲོལ་འབད།(&N)"

#: updateCmd.label
#: updateCmd.accesskey
msgid "Check f&or Updates..."
msgstr "དུས་མཐུན་ཡོད་མེད་ཞིབ་དཔྱད་འབད་...(&o)"

