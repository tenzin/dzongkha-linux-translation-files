msgstr "བརྡ་འཚོལ་གྱི་ལོ་རྒྱུས་ད་ལྟོ་བསལ་གཏང་།(&C)"

#: itemFormData.label
msgid "Saved Forms"
msgstr "བསྲུངས་ཡོད་པའི་འབྲི་ཤོག་ཚུ་"

#: savedFormData.intro
msgid "Information entered in forms and the Search Bar is saved to make filling out forms and searching faster."
msgstr "བརྡ་དོན་ཚུ་འབྲི་ཤོག་ནང་བཙུགས་ཡོདཔ་དང་ འབྲི་ཤོག་བཀང་ནི་དང་ འཚོལ་ཞིབ་འབད་ནི་ མགྱོགས་ནིའི་དོན་ལུ་ འཚོལ་ཞིབ་ཕྲ་རིང་དེ་ བསྲུངས་ཏེ་བཞག་ཡོད།"

#: enableFormFill.label
#: enableFormFill.accesskey
msgid "&Save information I enter in forms and the Search Bar."
msgstr "ང་གིས་འབྲི་ཤོག་དང་ འཚོལ་ཞིབ་ཕྲ་རིང་ནང་བཙུགས་མི་ བརྡ་དོན་དེ་སྲུངས།(&S)"

#: clearFormData.label
#: clearFormData.accesskey
msgid "Clear Saved &Form Data Now"
msgstr "བསྲུངས་ཡོད་པའི་འབྲི་ཤོག་གི་གནད་སྡུད་་ད་ལྟོ་རང་བསལ་གཏང་།(&F)"

#: itemPasswords.label
msgid "Passwords"
msgstr "ཆོག་ཡིག་ཚུ་"

#: savedPasswords.intro
msgid "&brandShortName; can remember login information for web pages so that you do not need to re-enter your login details every time you visit."
msgstr "&brandShortName; གིས་ ཝེབ་ཤོག་ལེབ་ཚུ་གི་དོན་ལུ་ ནང་བསྐྱོད་བརྡ་དོན་ཚུ་སེམས་ཁར་བཞག་དོ་ཡོདཔ་ལས་ ཤུལ་ལས་ཁྱོད་ཀྱིས་ལྟ་སྤྱོད་འབད་རེ་འབད་ཚེ་ ཁྱོད་རའི་ནང་བསྐྱོད་བརྡ་དོན་ཚུ་ ལོག་བཙུགས་མི་དགོ།"

#: enablePasswords.label
#: enablePasswords.accesskey
msgid "Remem&ber Passwords"
msgstr "ཆོག་ཡིག་སེམས་ཁར་བཞག(&b)"

#: masterPassword.intro
msgid "When set, the Master Password protects all your passwords - but you must enter it once per session."
msgstr "གཞི་སྒྲིག་འབད་ཚརཝ་ད་ ཆོག་ཡིག་ཨམ་དེ་གིས་ ཁྱོད་ཀྱི་ཆོག་ཡིག་ག་ར་ ཉེན་སྐྱོབ་འབདཝ་ཨིན། དེ་འབདཝ་ད་ ཁྱོད་ཀྱིས་ཚར་རེ་ནང་ཚར་རེ་བཙུགས་དགོ།"

#: setMasterPassword.label
#: setMasterPassword.accesskey
msgid "&Master Password"
msgstr "ཆོག་ཡིག་ཨམ་(&M)"

#: removeMasterPassword.label
#: removeMasterPassword.accesskey
msgid "&Remove Master Password..."
msgstr "ཆོག་ཡིག་ཨམ་རྩ་བསྐྲད་གཏང་་་་་(&R)"

#: viewPasswords.label
#: viewPasswords.accesskey
msgid "&View Saved Passwords"
msgstr "བསྲུངས་ཏེ་ཡོད་པའི་ཆོག་ཡིག་ཚུ་སྟོན།(&V)"

#: itemDownloads.label
msgid "Download History"
msgstr "ལོ་རྒྱུས་ཕབ་ལེན་འབད་"

#: downloadManager.intro
msgid "The Downloads window keeps track of recently downloaded files."
msgstr "ཕབ་ལེན་སྒོ་སྒྲིག་དེ་གིས་ ད་རིས་ཁམས་ ཕབ་ལེན་འབད་མི་ཡིག་སྣོད་ཚུ་གི་རྗེས་བཤུལ་ བདག་འཛིན་འཐབ་བཞགཔ་ཨིན།"

#: downloadsRetentionPolicy.label
#: downloadsRetentionPolicy.accesskey
msgid "R&emove files from the Download Manager:"
msgstr "ཕབ་ལེན་འཛིན་སྐྱོང་པའི་ནང་ལས་ ཡིག་སྣོད་ཚུ་རྩ་བསྐྲད་གཏང་།(&e)"

#: downloadsRemoveImmediately.label
msgid "Upon successful download"
msgstr "ཕབ་ལེན་མཐར་འཁྱོལ་གྱི་སྐབས་ལུ་"

#: downloadsRemoveExit.label
msgid "When &brandShortName; exits"
msgstr " &brandShortName; ཡོད་པའི་སྐབས་ལུ་"

#: downloadsRemoveManually.label
msgid "Manually"
msgstr "ལག་ཐོག་ལས་"

#: viewDownloads.label
#: viewDownloads.accesskey
msgid "View Download Histor&y"
msgstr "ཕབ་ལེན་གྱི་ལོ་རྒྱུས་སྟོན།(&y)"

#: clearDownloads.label
#: clearDownloads.accesskey
msgid "Cle&ar Download History Now"
msgstr "ཕབ་ལེན་གྱི་ལོ་རྒྱུས་ ད་ལྟོ་ར་བསལ་གཏང་།(&a)"

#: itemCookies.label
msgid "Cookies"
msgstr "ཀུ་ཀིསི་"

#: cookies.intro
msgid "Cookies are pieces of information stored by sites on your computer. They are used to remember login information and other data."
msgstr "ཀུ་ཀིསི་ཟེར་མི་ཚུ་ ཝེབ་ས་ཁོངས་ཚུ་གིས་ ཁྱོད་ཀྱི་གློག་རིག་གུ་ གསོག་འཇོག་འབད་བཞག་མི་ བརྡ་དོན་ཚུ་ཨིན། དེ་ཚུ་ནང་བསྐྱོད་ཀྱི་བརྡ་དོན་དང་ གནས་སྡུད་གཏན་ཚུ་ སེམ་ཁར་བཞག་ནི་ལུ་ ལག་ལེན་འཐབ་ཨིན།"

#: enableCookies.label
#: enableCookies.accesskey
msgid "A&llow sites to set Cookies"
msgstr "ས་ཁོངས་ཚུ་ལུ་ ཀུ་ཀིསི་གཞི་སྒྲིག་འབད་བཅུག(&l)"

#: exceptions.label
msgid "Exceptions"
msgstr "དམིགས་བསལ་ཚུ་"

#: cookieExceptions.accesskey
msgid "x"
msgstr "x"

#: enableCookiesForOriginating.label
#: enableCookiesForOriginating.accesskey
msgid "for the ori&ginating site only"
msgstr "འགོ་ཐོག་བྱུང་མི་ས་ཁོངས་ཀྱི་དོན་ལུ་རྐྱངམ་གཅིག་(&g)"

#: enableCookiesNotRemoved.label
#: enableCookiesNotRemoved.accesskey
msgid "&unless I have removed cookies set by the site"
msgstr "ས་ཁོངས་ཀྱིས་ གཞི་སྒྲིག་འབད་མི་ཀུ་ཀིསི་ཚུ་ ང་གིས་རྩ་བསྐྲད་བཏང་ཡོད་ན་མགཏོགས་་དེ་མིན་(&u)"

#: keepCookies.label
#: keepCookies.accesskey
msgid "&Keep Cookies:"
msgstr "ཀུ་ཀིསི་བཞག་:(&K)"

#: acceptNormally.label
msgid "until they expire"
msgstr "དུས་ཡོལ་མ་འགྱོ་ཚུན་"

#: acceptForSession.label
msgid "until I close &brandShortName;"
msgstr "ང་གིས་ &brandShortName; ཁ་མ་བསྡམས་ཚུན་"

#: askAboutCookies.label
msgid "ask me every time"
msgstr "ཨ་རྟག་ར་ ང་ལུ་དྲིས་"

#: viewCookies.label
#: viewCookies.accesskey
msgid "Vie&w Cookies"
msgstr "ཀུ་ཀིསི་སྟོན་(&w)"

#: clearCookies.label
#: clearCookies.accesskey
msgid "Clear C&ookies Now"
msgstr "ཀུ་ཀིསི་ད་ལྟོ་ར་བསལ་གཏང་(&o)"

#: itemCache.label
msgid "Cache"
msgstr "འདྲ་མཛོད་"

#: cache.intro
msgid "Pages you view are stored in the cache for faster viewing later on."
msgstr "ཁྱོད་ཀྱི་བལྟ་མི་ཤོག་ལེབ་ཚུ་ ཤུལ་ལས་མགྱོགས་པར་བལྟ་ཚུགས་པའི་དོན་ལུ་ འདྲ་མཛོད་ནང་གསོག་འཇོག་སྦེ་བཞག་ཡོད།"

#: cacheUseUpTo.label
#: cacheUseUpTo.accesskey
msgid "Use u&p to:"
msgstr "ཚུན་ལག་ལེན་འཐབ་:(&p)"

#: cacheSize.label
msgid "MB of disk space for the cache."
msgstr "འདྲ་མཛོད་ཀྱི་དོན་ལུ་ ཌིཀསི་གི་བར་སྟོང་ མེ་ག་བའིཊི་(ཨེམ་བི་)"

#: clearCache.label
#: clearCache.accesskey
msgid "Clear Cache &Now"
msgstr "འདྲ་མཛོད་ད་ལྟོ་ར་བསལ་གཏང་(&N)"

#: sanitizeSettings.label
#: sanitizeSettings.accesskey
msgid "Se&ttings..."
msgstr "སྒྲིགས་སྟངས་་་་(&t)"

#: sanitize.intro
msgid "The Clear Private Data tool can be used to erase your private data using a keyboard shortcut or when &brandShortName; closes."
msgstr "ཁྱོད་རའི་སྒེར་གྱི་གནས་སྡུད་ ཀྲེག་བཏང་ནིའི་དོན་ལུ་ ལྡེ་སྒྲོམ་མགྱོགས་ཐབས་ཅིག་ གཡོག་བཀོལ་ཐོག་ལས་ ཡང་ན་ &brandShortName; ཁ་བསྡམས་པའི་སྐབས་ སྒེར་གྱི་གནས་སྡུད་བསལ་གཏང་ལག་ཆས་དེ་ ལག་ལེན་འཐབ་བཏུབ།"

