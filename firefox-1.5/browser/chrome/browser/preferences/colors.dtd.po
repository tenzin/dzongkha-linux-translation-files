#: colorsDialog.title
msgid "Colors"
msgstr "ཚོས་གཞི་"

#: window.width
msgid ""
"_: Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger.\n"
"38em"
msgstr "ཨི་ཨེམ་༣༨"

#: allowPagesToUse.label
#: allowPagesToUse.accesskey
msgid "&Allow pages to choose their own colors, instead of my selections above"
msgstr "ཤོག་ལེབ་ཚུ་གིས་ ང་གིས་གོང་ལུ་སེལ་འཐུ་འབད་མི་ཚུ་གི་ཚབ་ལུ་ རང་སོའི་ཚོས་གཞི་གདམ་ཁ་བརྐྱབ་བཅུག(&A)"

#: color
msgid "Text and Background"
msgstr "ཚིག་ཡིག་དང་རྒྱབ་གཞི།"

#: textColor.label
#: textColor.accesskey
msgid "&Text:"
msgstr "ཚིག་ཡིག་:(&T)"

#: backgroundColor.label
#: backgroundColor.accesskey
msgid "&Background:"
msgstr "རྒྱབ་གཞི་:(&B)"

#: useSystemColors.label
#: useSystemColors.accesskey
msgid "U&se system colors"
msgstr "རིམ་ལུགས་ཚོས་གཞི་ལག་ལེན་འཐབ་(&s)"

#: underlineLinks.label
#: underlineLinks.accesskey
msgid "&Underline links"
msgstr "འབྲེལ་ལམ་ཚུ་ལུ་འོག་ཐིག་རྐྱབས།(&U)"

#: links
msgid "Link Colors"
msgstr "ཚོས་གཞི་ཚུ་འབྲེལ་མཐུད་འབད།"

#: linkColor.label
#: linkColor.accesskey
msgid "Unvisited &Links:"
msgstr "ལྟ་སྤྱོད་མ་འབད་བའི་འབྲེལ་ལམ་ཚུ་:(&L)"

#: visitedLinkColor.label
#: visitedLinkColor.accesskey
msgid "&Visited Links:"
msgstr "ལྟ་སྤྱོད་འབད་ཡོད་པའི་འབྲེལ་ལམ་ཚུ་:(&V)"

