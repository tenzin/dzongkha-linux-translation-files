#: askOnSave.label
msgid "Download Folder"
msgstr "ཕབ་ལེན་སྣོད་འཛིན་"

#: alwaysAsk.label
#: alwaysAsk.accesskey
msgid "&Ask me where to save every file"
msgstr "ཡིག་སྣོད་རེ་རེ་བཞིན་དུ་ ག་ཏེ་བཞག་ནི་ཨིན་ན་ང་ལུ་དྲིས།(&A)"

#: neverAsk.label
#: neverAsk.accesskey
msgid "&Save all files to this folder:"
msgstr "ཡིག་སྣོད་ཆ་མཉམ་སྣོད་འཛིན་འདི་ནང་སྲུངས་:(&S)"

#: desktop.label
msgid "Desktop"
msgstr "ཌེཀསི་ཊོཔ་"

#: downloads.label
msgid "My Downloads"
msgstr "ངེད་ཀྱི་ཕབ་ལེན་ཚུ་"

#: chooseFolderWin.label
#: chooseFolderWin.accesskey
msgid "&Browse..."
msgstr "བརྡ་འཚོལ་་་་(&B)"

#: chooseFolderMac.label
#: chooseFolderMac.accesskey
msgid "C&hoose..."
msgstr "གདམ་ཁ་རྐྱབས་་་(&h)"

#: downloadManagerWindow.label
msgid "Download Manager"
msgstr "ཕབ་ལེན་འཛིན་སྐྱོང་པ་"

#: showWhenStarting.label
#: showWhenStarting.accesskey
msgid "Sh&ow Download Manager when a download begins"
msgstr "ཕབ་ལེན་འགོ་བཙུགསཔ་ད་ ཕབ་ལེན་འཛིན་སྐྱོང་པ་སྟོན།(&o)"

#: closeWhenDone.label
#: closeWhenDone.accesskey
msgid "&Close the Download Manager when all downloads are complete"
msgstr "ཕབ་ལེན་ཆ་མཉམ་མཇུག་བསྡུཝ་ད་ ཕབ་ལེན་འཛིན་སྐྱོང་པ་ཁ་བསྡམས།(&C)"

#: fileTypes.label
msgid "Download Actions"
msgstr "ཕབ་ལེན་གྱི་བྱ་བ།"

#: configureActions.label
#: configureActions.accesskey
msgid "&View &amp; Edit Actions..."
msgstr "&View &amp; གིས་བྱ་བ་ཞུན་དག་རྐྱབ་ཨིན་་་་"

#: fileTypesDescription.label
msgid "&brandShortName; can automatically download or open files of certain types."
msgstr "&brandShortName;གིས་ ཡིག་སྣོད་ཀྱི་དབྱེ་བ་ལ་ལོ་ཅིག་ རང་བཞིན་གྱིས་ཕབ་ལེན་དང་ ཁ་ཕྱེ་ཚུགས།"

