#: div.cookiesonsystem.label
msgid "View and remove cookies that are stored on your computer."
msgstr "ཁྱོད་ཀྱི་གློག་རིག་གུ་གསོག་འཇོག་འབད་ཡོད་པའི་ཀུ་ཀིསི་ཚུ་སྟོན་ཞིནམ་ལས་རྩ་བསྐྲད་གཏང།"

#: treehead.cookiename.label
msgid "Cookie Name"
msgstr "ཀུ་ཀི་མིང་།"

#: treehead.cookiedomain.label
msgid "Site"
msgstr "ས་ཁོངས།"

#: treehead.infoselected.label
msgid "Information about the selected Cookie"
msgstr "སེལ་འཐུ་འབད་མི་ ཀུ་ཀི་སྐོར་ལས་བརྡ་དོན།"

#: button.removecookie.label
msgid "Remove Cookie"
msgstr "ཀུ་ཀི་རྩ་བསྐྲད་གཏང་།"

#: button.removeallcookies.label
msgid "Remove All Cookies"
msgstr "ཀུ་ཀི་ཆ་མཉམ་རྩ་བསྐྲད་གཏང་།"

#: props.name.label
msgid "Name:"
msgstr "མིང་:"

#: props.value.label
msgid "Content:"
msgstr "ནང་དོན།"

#: props.domain.label
msgid "Host:"
msgstr "ཧོསིཊི།"

#: props.path.label
msgid "Path:"
msgstr "འགྲུལ་ལམ་:"

#: props.secure.label
msgid "Send For:"
msgstr "གཏང་ཡུལ་:"

#: props.expires.label
msgid "Expires:"
msgstr "དུས་ཡོལ་ཐལ་བ།"

#: window.title
msgid "Stored Cookies"
msgstr "བསགས་ཏེ་ཡོད་པའི་ཀུ་ཀིསི།"

#: checkbox.label
msgid "Don't allow sites that set removed cookies to set future cookies"
msgstr "རྩ་བསྐྲད་བཏང་མི་ཀུ་ཀིསི་ཚུ་ མ་འོངས་པའི་ཀུ་ཀིསི་སྦེ་ གཞི་སྒྲིག་འབད་མི་ས་ཁོངས་ཚུ་ འོང་མ་བཅུག།"

