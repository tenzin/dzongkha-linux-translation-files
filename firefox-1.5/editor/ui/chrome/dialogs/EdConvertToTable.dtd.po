# Window title
#: windowTitle.label
msgid "Convert To Table"
msgstr "ཐིག་ཁྲམ་ལུ་གཞི་བསྒྱུར་་འབད།"

#: instructions1.label
msgid "Composer creates a new table row for each paragraph in the selection."
msgstr "རྩོམ་མཁན་གྱིས་སེལ་འཐུ་ནང་དོན་མཚམས་རེ་རེ་དོན་ལུ་ཐིག་ཁྲམ་གྲལ་ཐིག་གསརཔ་ཅིག་གསར་བསྐྲུན་འབདཝ་ཨིན།"

#: instructions2.label
msgid "Choose the character used to separate the selection into columns:"
msgstr "ཀེར་ཐིག་ཚུ་ནང་སེལ་འཐུ་དེ་དབྱེ་བྱེད་ལུ་ལག་ལེན་འཐབ་ཡོད་མི་ཡིག་འབྲུ་དེ་གདམ།"

#: commaRadio.label
msgid "Comma"
msgstr "ལྷོད་རྟགས།"

#: spaceRadio.label
msgid "Space"
msgstr "བར་སྟོང་།"

#: otherRadio.label
msgid "Other Character:"
msgstr "ཡིག་འབྲུ་གཞན:"

#: deleteCharCheck.label
msgid "Delete separator character"
msgstr "ཡིག་འབྲུ་དབྱེ་བྱེད་བཏོན་གཏང་།"

#: collapseSpaces.label
msgid "Ignore extra spaces"
msgstr "བར་སྟོང་ཐེབས་ཚུ་སྣང་མེད་བཞག"

#: collapseSpaces.tooltip
msgid "Convert adjacent spaces to one separator"
msgstr "ཉེའདབས་བར་སྟོང་ཚུ་དབྱེ་བྱེད་གཅིག་ལུ་གཞི་བསྒྱུར་འབད།"

