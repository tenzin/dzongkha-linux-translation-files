# Window title
#: windowTitle.label
msgid "Page Colors and Background"
msgstr "ཤོག་ལེབ་ཚོས་གཞི་དང་རྒྱབ་གཞི།"

#: pageColors.label
msgid "Page Colors"
msgstr "ཤོག་ལེབ་ཚོས་གཞི་ཚུ།"

#: defaultColorsRadio.label
#: defaultColorsRadio.accessKey
msgid "Reader's default colors (&Don't set colors in page)"
msgstr "ལྷག་མི་གི་སྔོན་སྒྲིག་ཚོས་གཞི་ཚུ།(ཤོག་ལེབ་ནང་ཚོས་གཞི་ཚུ་གཞི་སྒྲིག་མ་འབད)(&D)"

#: defaultColorsRadio.tooltip
msgid "Use the color settings from the viewer (reader's) browser only"
msgstr "ཚོས་གཞི་སྒྲིག་སྟངས་དེ་མཐོང་བྱེད་(ལྷག་མི་)གི་བརའུ་ཟར་རྐྱངམ་ཅིག་ལས་ལག་ལེན་འཐབ།"

#: customColorsRadio.label
#: customColorsRadio.accessKey
msgid "Use &custom colors:"
msgstr "སྲོལ་སྒྲིག་ཚོས་གཞི་ཚུ་ལག་ལེན་འཐབ:(&c)"

#: customColorsRadio.tooltip
msgid "These color settings override the viewer's browser settings"
msgstr "ཚོས་གཞི་སྒྲིག་སྟངས་ཚུ་གིས་མཐོང་བྱེད་ཀྱི་བརའུ་ཟར་སྒྲིག་སྟངས་དེ་མེདཔ་གཏངམ་ཨིན།"

#: normalText.label
#: normalText.accessKey
msgid "&Normal text"
msgstr "སྤྱིར་བཏང་ཚིག་ཡིག (&N)"

#: linkText.label
#: linkText.accessKey
msgid "&Link text"
msgstr "འབྲེལ་ལམ་ཚིག་ཡིག (&L)"

#: activeLinkText.label
#: activeLinkText.accessKey
msgid "&Active link text"
msgstr "འབྲེལ་ལམ་ཚིག་ཡིག་ཤུགས་ལྡན་བཟོ།(&A)"

#: visitedLinkText.label
#: visitedLinkText.accessKey
msgid "&Visited link text"
msgstr "བལྟ་སྤྱོད་འབད་ཡོད་མི་འབྲེལ་ལམ་ཚིག་ཡིག (&V)"

#: background.label
#: background.accessKey
msgid "&Background:"
msgstr "རྒྱབ་གཞི:(&B)"

#: colon.character
msgid ":"
msgstr ":"

#: backgroundImage.label
#: backgroundImage.accessKey
msgid "Background I&mage:"
msgstr "རྒྱབ་གཞི་གཟུགས་བརྙན:(&m)"

#: backgroundImage.tooltip
msgid "Use an image file as the background for your page"
msgstr "ཁྱོད་རའི་ཤོག་ལེབ་དོན་ལུ་གཟུགས་བརྙན་ཡིག་སྣོད་ཅིག་རྒྱབ་གཞི་སྦེ་ལག་ལེན་འཐབ།"

