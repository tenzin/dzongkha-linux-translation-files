# Window title
#: windowTitle.label
msgid "Publish Page"
msgstr "ཤོག་ལེབ་དཔེ་བསྐྲུན།"

#: windowTitleSettings.label
msgid "Publish Settings"
msgstr "དཔེ་བསྐྲུན་སྒྲིག་སྟངས་ཚུ།"

#: publishTab.label
#: publishButton.label
msgid "Publish"
msgstr "དཔེ་བསྐྲུན།"

#: settingsTab.label
msgid "Settings"
msgstr "སྒྲིག་སྟངས་ཚུ།"

# Publish Tab Panel
#: siteList.label
#: siteName.label
msgid "Site Name:"
msgstr "ས་ཁོངས་མིང་:"

#: siteList.tooltip
msgid "Choose the site that you want to publish to"
msgstr "ཁྱོད་ཀྱིས་དཔེ་བསྐྲུན་འབད་ནི་ཨིན་མི་ས་ཁོངས་གདམ།"

#: newSiteButton.label
msgid "New Site"
msgstr "ས་ཁོངས་གསརཔ།"

#: siteDirList.label
msgid "Select or type a directory name in your publishing site:"
msgstr "ཁྱོད་རའི་དཔེ་བསྐྲུན་འབད་ནི་ས་ཁོངས་ནང་སྣོད་ཐོ་མིང་ཅིག་ཡིག་དཔར་རྐྱབས་ཡང་སེལ་འཐུ་འབད:"

#: siteDirList.tooltip
msgid "Choose or enter the name of the remote subdirectory to publish to"
msgstr "ཐག་རིང་གི་སྣོད་ཐོ་འོགམ་ལུ་དཔེ་བསྐྲུན་ལུ་མིང་བཙུགས་ཡང་ན་གདམ།"

#: docDirList.label
msgid "Site subdirectory for this page:"
msgstr "ཤོག་ལེབ་དེའི་དོན་ལུ་སྣོད་ཐོ་འོགམ་ས་ཁོངས:"

#: publishImgCheckbox.label
msgid "Include images and other files"
msgstr "གཟུགས་བརྙན་ཚུ་དང་གཏན་ཡིག་སྣོད་ཚུ་གྲངས་སུ་བཙུགས།"

#: publishImgCheckbox.tooltip
msgid "Publish images and other files referenced by this page"
msgstr "ཤོག་ལེབ་དེ་གིས་གཞི་བསྟུན་འབད་ཡོད་མི་གཟུགས་བརྙན་ཚུ་དང་གཞན་ཡིག་སྣོད་ཚུ་དཔེ་བསྐྲུན་འབད།"

#: sameLocationRadio.label
msgid "Use same location as page"
msgstr "ཤོག་ལེབ་སྦེ་གནས་ཁོངས་ཅོག་འཐདཔ་ལག་ལེན་འཐབ།"

#: sameLocationRadio.tooltip
msgid "Publish files to the same location as the page"
msgstr "ཤོག་ལེབ་བཟུམ་སྦེ་གནས་ཁོངས་ཅོག་ཐདཔ་ལུ་ཡིག་སྣོད་ཚུ་དཔེ་བསྐྲུན་འབད།"

#: useSubdirRadio.label
msgid "Use this site subdirectory:"
msgstr "སྣོད་ཐོ་འོག་ས་ཁོངས་དེ་ལག་ལེན་འཐབ:"

#: useSubdir.tooltip
msgid "Choose or enter name of remote subdirectory where files will be published"
msgstr "ཡིག་སྣོད་ཚུ་དཔེ་བསྐྲུན་འབད་ནི་ཨིན་མི་ཐག་རིང་གི་སྣོད་ཐོ་འོགམ་གི་མིང་་བཙུགས་ཡང་ན་གདམ།"

#: chooseDirButton.label
msgid "Choose..."
msgstr "གདམ..."

#: pageTitle.label
msgid "Page Title:"
msgstr "ཤོག་ལེབ་མགོ་མིང་:"

#: pageTitle.tooltip
msgid "Enter a title to identify the page in the window and in bookmarks"
msgstr "ཝིན་ཌོ་ནང་ཤོག་ལེབ་ངོས་འཛིན་ལུ་དང་དེབ་རྟགས་ཚུ་ནང་མགོ་མིང་ཅིག་བཙུགས།"

#: pageTitleExample.label
msgid "e.g.: &quot;My Web Page&quot;"
msgstr "དཔེར་ན་: quot;My Web Page&quot;(&q)(&q)"

#: filename.label
msgid "Filename:"
msgstr "ཡིག་སྣོད་མིང་:"

#: filename.tooltip
msgid "Enter a name for this file, including '.html' for a web page"
msgstr "ཡིག་སྣོད་དེའི་དོན་ལུ་ ཝེབ་ཤོག་ལེབ་ཅིག་དོན་ལུ་ '.html'གྲངས་སུ་རྩིས་མིང་ བཙུགས།"

#: filenameExample.label
msgid "e.g,: &quot;mypage.html&quot;"
msgstr "དཔེར་ན་: &quot;mypage.html&quot;(q)(&q)"

#: relatedDocs.label
msgid "Other files to publish"
msgstr "གཞན་ཡིག་སྣོད་ཚུ་དཔེ་བསྐྲུན་ལུ།"

#: setDefaultButton.label
msgid "Set as Default"
msgstr "སྔོན་སྒྲིག་བཟུམ་སྦེ་གཞི་སྒྲིག་འབད།"

#: setDefSiteAndDir.label
msgid "Set as Default Site and Directory"
msgstr "སྔོན་སྒྲིག་ས་ཁོངས་དང་སྣོད་ཐོ་བཟུམ་སྦེ་གཞི་སྒྲིག་འབད།"

#: removeButton.label
msgid "Remove Site"
msgstr "ས་ཁོངས་བསྐྲད་གཏང་།"

# Settings Tab Panel
#: publishSites.label
msgid "Publishing Sites"
msgstr "ས་སྒོ་ཚུ་དེབ་བསྐྲུན།"

#: serverInfo.label
msgid "Server Information"
msgstr "སར་བར་བརྡ་དོན།"

#: loginInfo.label
msgid "Login Information"
msgstr "ནང་བསྐྱོད་བརྡ་དོན།"

#: siteName.tooltip
msgid "A nickname that identifies this publishing site (e.g.: 'MySite')"
msgstr "དེབ་བསྐྲུན་ས་ཁོངས་དེ་ངོས་འཛིན་མི་སྐྱོན་མིང་ཅིག(དཔེར་ན: 'MySite'་)"

#: siteUrl.label
msgid "Publishing address (e.g.: 'ftp://ftp.myisp.com/myusername'):"
msgstr "ཁ་བྱང་དེབ་བསྐྲུན (དཔེར་ན་: 'ftp://ftp.myisp.com/myusername'):"

#: siteUrl.tooltip
msgid "The FTP:// or HTTP:// address provided by your ISP or web hosting service"
msgstr "ཨེཕ་ཊི་པི་:// ཡང་ན་ཨེཆ་ཊི་ཊི་པི་:// ཁ་བྱང་དེ་ཁྱོད་རའི་ཨའི་ཨེསི་པི་ཡང་ན་ཝེབ་འགོ་འདྲེན་འཐབ་ནི་ཞབས་ཏོག་གིས་བྱིན་ཡོད་མི།"

#: browseUrl.label
msgid "HTTP address of your homepage (e.g.: 'http://www.myisp.com/myusername'):"
msgstr "ཁྱོད་རའི་ཁྱིམ་ཤོག་ལེབ་ཀྱི་ཨེཆ་ཊི་ཊི་པི་ཁ་བྱང་་།(དཔེར་ན: 'http://www.myisp.com/myusername'):"

#: browseUrl.tooltip
msgid "The HTTP:// address of your home directory (don't include filename)"
msgstr "ཨེཆ་ཊི་ཊི་པི་དེ་:// ཁྱོད་རའི་ཁྱིམ་སྣོད་ཐོ་གི་ཁ་བྱང་ (ཡིག་སྣོད་མིང་གྲངས་སུ་མ་རྩིས)"

#: username.label
msgid "User name:"
msgstr "ལག་ལེན་པའི་མིང་:"

#: username.tooltip
msgid "The user name you use to log in to your ISP or web hosting service"
msgstr "ཁྱོད་ཀྱིས་ཁྱོད་རའི་ཨའི་ཨེསི་པི་ནང་ལུ་ཡང་ན་ཝེབ་འགོ་འདྲེན་འཐབ་ནི་ཞབས་ཏོག་་ལུ་ནང་བསྐྱོད་འབད་ནི་ལག་ལེན་པའི་མིང་།"

#: password.label
msgid "Password:"
msgstr "ཆོག་ཡིག:"

#: password.tooltip
msgid "The password associated with your user name"
msgstr "ཆོག་ཡིག་དེ་ཁྱོད་ཀྱི་ལག་ལེན་པའི་མིང་གཅིག་ཁར་་འབྲེལ་བ་ཡོདཔ།"

#: savePassword.label
msgid "Save Password"
msgstr "ཆོག་ཡིག་སྲུངས་བཞག་འབད།"

#: savePassword.tooltip
msgid "Select this to save your password securely using Password Manager"
msgstr "ཆོག་ཡིག་འཛིན་སྐྱོང་པ་ལག་ལེན་འཐབ་ཁྱོད་རའི་ཆོག་ཡིག་མཐའ་བཙན་སྲུངས་བཞག་ལུ་དེ་སེལ་འཐུ་འབད།"

