# Window title
#: siteUrl.label
msgid "Site URL:"
msgstr "ས་ཁོངས་ཡུ་ཨར་ཨེལ:"

#: docSubdir.label
msgid "Page subdirectory:"
msgstr "ཤོག་ལེབ་སྣོད་ཐོ་འོགམ:"

#: otherSubdir.label
msgid "Image subdirectory:"
msgstr "གཟུགས་བརྙན་སྣོད་ཐོ་འོགམ:"

#: status.label
msgid "Publishing..."
msgstr "དེབ་བསྐྲུན..."

#: fileList.label
msgid "Publishing Status"
msgstr "དེབ་བསྐྲུན་གནས་ཚད།"

#: statusHeader.label
msgid "Status"
msgstr "གནས་ཚད།"

#: filenameHeader.label
msgid "Filename"
msgstr "ཡིག་སྣོད་མིང་།"

#: succeeded.label
msgid "Succeeded"
msgstr "མཐར་འཁྱོལ་ཡོདཔ།"

#: failed.label
msgid "Failed"
msgstr "འཐུས་ཤོར་ཡོདཔ།"

#: troubleshooting.label
#: troubleshooting.accessKey
msgid "&Troubleshooting"
msgstr "དཀའ་ངལ་སེལ་ནི།(&T)"

#: keepOpen
msgid "Keep this window open after publishing is complete."
msgstr "དེབ་བསྐྲུན་དེ་མཇུག་བསྡུ་པའི་ཤུལ་ལས་ཝིན་ཌོ་དེ་ཁ་ཕྱེ་བཞག"

#: closeButton.label
msgid "Close"
msgstr "ཁ་བསྡམས།"

