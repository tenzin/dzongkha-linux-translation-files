#: WindowTitle.label
msgid "Advanced Property Editor"
msgstr "བརྔ་གཡར་རྒྱུ་དངོས་ཞུན་དགཔ།"

#: AttName.label
msgid "Attribute: "
msgstr "ཁྱད་ཆོས: "

#: AttValue.label
msgid "Value: "
msgstr "གནས་གོང་: "

#: PropertyName.label
msgid "Property: "
msgstr "རྒྱུ་དངོས: "

#: currentattributesfor.label
msgid "Current attributes for: "
msgstr "དོན་ལུ་ད་ལྟོའི་ཁྱད་ཆོས་ཚུ: "

#: tree.attributeHeader.label
msgid "Attribute"
msgstr "ཁྱད་ཆོས།"

#: tree.propertyHeader.label
msgid "Property"
msgstr "རྒྱུ་དངོས།"

#: tree.valueHeader.label
msgid "Value"
msgstr "གནས་གོང་།"

#: tabHTML.label
msgid "HTML Attributes"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ཁྱད་ཆོས་ཚུ།"

#: tabCSS.label
msgid "Inline Style"
msgstr "ཨིན་ལའིན་བཟོ་རྣམ།"

#: tabJSE.label
msgid "JavaScript Events"
msgstr "ཇ་བ་ཡིག་ཙུགས་འབྱུང་ལས་ཚུ།"

#: editAttribute.label
msgid "Click on an item above to edit its value"
msgstr "དེའི་གནས་གོང་ཞུན་དག་འབད་ནི་ལུ་ལྟག་གི་རྣམ་གྲངས་ཅིག་གུ་ཨེབ་གཏང་འབད།"

#: removeAttribute.label
msgid "Remove"
msgstr "རྩ་བསྐྲད་གཏང་།"

