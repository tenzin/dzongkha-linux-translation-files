# Window title
#: windowTitle.label
msgid "List Properties"
msgstr "རྒྱུ་དངོས་ཚུའི་ཐོ་ཡིག"

#: ListType.label
msgid "List Type"
msgstr "དབྱེ་བ་ཐོ་ཡིག"

#: bulletStyle.label
msgid "Bullet Style:"
msgstr "འགོ་ཚག་བཟོ་རྣམ:"

#: startingNumber.label
#: startingNumber.accessKey
msgid "&Start at:"
msgstr "འགོ་བཙུགས་ལུ:(&S)"

#: none.value
msgid "None"
msgstr "ཅི་མེད།"

#: bulletList.value
msgid "Bullet (Unnumbered) List"
msgstr "འགོ་ཚག་ (ཨང་མ་བཏགས་མི) ཐོ་ཡིག"

#: numberList.value
msgid "Numbered List"
msgstr "ཨང་བཏགས་ཡོད་མི་ཐོ་ཡིག"

#: definitionList.value
msgid "Definition List"
msgstr "ངེས་ཚིག་ཐོ་ཡིག"

#: changeListMsg.label
msgid "When changing list type:"
msgstr "ཐོ་ཡིག་དབྱེ་བ་བསྒྱུར་བཅོས་འབད་བའི་སྐབས:"

#: changeEntireListRadio.label
#: changeEntireListRadio.accessKey
msgid "&Change entire list"
msgstr "ཐོ་ཡིག་ཧྲིལ་བུ་བསྒྱུར་བཅོས་འབད།(&C)"

#: changeSelectedRadio.label
#: changeSelectedRadio.accessKey
msgid "Change just selected &items"
msgstr "སེལ་འཐུ་འབད་ཡོད་མི་རྣམ་གྲང་ཚུ་རྐྱངམ་བསྒྱུར་བཅོས་འབད།"

