# extracted from EdReplace.xul
#: replaceDialog.title
msgid "Find and Replace"
msgstr "འཚོལ་འབད་ཚབ་བཙུགས།"

#: findField.label
#: findField.accesskey
msgid "Fi&nd text:"
msgstr "ཚིག་ཡིག་འཚོལ:(&n)"

#: replaceField.label
#: replaceField.accesskey
msgid "R&eplace with:"
msgstr "ཚབ་བཙུགས་གཅིག་ཁར:(&e)"

#: caseSensitiveCheckbox.label
#: caseSensitiveCheckbox.accesskey
msgid "Mat&ch upper/lower case"
msgstr "ཡིག་ཚུགས/མགྱོགས་ཡིག་མཐུན་སྒྲིག་འབད།"

#: wrapCheckbox.label
#: wrapCheckbox.accesskey
msgid "&Wrap around"
msgstr "མཐའ་སྐོར་ལོག་མཚམས།(&W)"

#: backwardsCheckbox.label
#: backwardsCheckbox.accesskey
msgid "Search &backwards"
msgstr "འཚོལ་ཞིབ་རྒྱབ་བསྐྱོད།(&b)"

#: findNextButton.label
#: findNextButton.accesskey
msgid "&Find Next"
msgstr "འཚོལ་གཞན།(&F)"

#: replaceButton.label
#: replaceButton.accesskey
msgid "&Replace"
msgstr "ཚབ་བཙུགས།(&R)"

#: replaceAndFindButton.label
#: replaceAndFindButton.accesskey
msgid "Replace an&d Find"
msgstr "ཚབ་བཙུགས་འབད་འཚོལ།(&d)"

#: replaceAllButton.label
#: replaceAllButton.accesskey
msgid "Replace &All"
msgstr "ཚབ་བཙུགས་ཆ་མཉམ།(&A)"

#: closeButton.label
msgid "Close"
msgstr "ཁ་བསྡམས།"

#: notFoundWarning.label
msgid "The text you entered was not found."
msgstr "ཁྱོད་ཀྱིས་བཙུགས་ཡོད་མི་ཚིག་ཡིག་དེ་འཚོལ་མ་ཐོ།བ"

