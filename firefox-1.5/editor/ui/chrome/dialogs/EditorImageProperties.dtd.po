# These strings are for use specifically in the editor's image and form image dialogs.
# Window title
#: windowTitle.label
msgid "Image Properties"
msgstr "གཟུགས་བརྙན་རྒྱུ་དངོས་ཚུ།"

#: pixelsPopup.value
msgid "pixels"
msgstr "པིག་སེལ་ཚུ།"

#: percentPopup.value
msgid "percent"
msgstr "བརྒྱ་ཆ།"

# These are in the Location tab panel
#: locationEditField.label
#: locationEditField.accessKey
msgid "Image &Location:"
msgstr "གཟུགས་བརྙན་གནས་ཁོངས:(&L)"

#: locationEditField.tooltip
msgid "Type the image's filename or location"
msgstr "གཟུགས་བརྙན་དེའི་ཡིག་སྣོད་མིང་ཡང་ན་གནས་ཁོངས་ཡིག་དཔར་རྐྱབས།"

#: title.label
#: title.accessKey
msgid "&Tooltip:"
msgstr "ལག་ཆས་ཕན་བསླབ:(&T)"

#: title.tooltip
msgid "The html 'title' attribute that displays as a tooltip"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་'མགོ་མིང་'ཁྱད་ཆོས་དེ་ལག་ཆས་ཕན་བསླབ་སྦེ་བཀྲམ་བཏོན་འབདཝ་ཨིན།"

#: altText.label
#: altText.accessKey
msgid "&Alternate text:"
msgstr "ཐབས་གཞན་ཚིག་ཡིག:(&A)"

#: altTextEditField.tooltip
msgid "Type text to display in place of the image"
msgstr "གཟུགས་བརྙན་གྱི་ཚབ་མ་ལུ་ཚིག་ཡིག་བཀྲམ་བཏོན་འབད་ནི་ལུ་ཡིག་དཔར་རྐྱབས།"

#: noAltText.label
#: noAltText.accessKey
msgid "&Don't use alternate text"
msgstr "ཐབས་གཞན་ཚིག་ཡིག་ལག་ལེན་མ་འཐབ།(&D)"

#: previewBox.label
msgid "Image Preview"
msgstr "གཟུགས་བརྙན་སྔོན་ལྟ།"

#: MoreFewerButton.tooltip
msgid "Display more or fewer properties to edit"
msgstr "་ཞུན་དག་འབད་ནི་ལུ་རྒྱུ་དངོས་ཚུ་ལྷགཔ་ཡང་ན་དག་པ་ཅིག་བཀྲམ་བཏོན་འབད།"

# These controls are in the Dimensions tab panel
# actualSize.label should be same as actualSizeRadio.label + ":"
#: actualSize.label
msgid "Actual Size:"
msgstr "ཚད་ངོ་མ:"

#: actualSizeRadio.label
#: actualSizeRadio.accessKey
msgid "&Actual Size"
msgstr "ཚད་ངོ་མ།(&A)"

#: actualSizeRadio.tooltip
msgid "Revert to the image's actual size"
msgstr "གཟུགས་བརྙན་གྱི་ཚད་ངོ་མ་དེ་ལུ་རྒྱབ་ལོག"

#: customSizeRadio.label
#: customSizeRadio.accessKey
msgid "Custom &Size"
msgstr "སྲོལ་སྒྲིག་ཚད།(&S)"

#: customSizeRadio.tooltip
msgid "Change the image's size as displayed in the page"
msgstr "ཤོག་ལེབ་ནང་བཀྲམ་བཏོན་འབད་ཡོད་མི་བཟུམ་གཟུགས་བརྙན་གྱི་ཚད་དེ་བསྒྱུར་བཅོས་འབད།"

#: heightEditField.label
#: heightEditField.accessKey
msgid "Hei&ght:"
msgstr "མཐོ་ཚད:(&g)"

#: widthEditField.label
#: widthEditField.accessKey
msgid "&Width:"
msgstr "རྒྱ་ཚད:(&W)"

#: constrainCheckbox.label
#: constrainCheckbox.accessKey
msgid "&Constrain"
msgstr "དམ་ནི།(&C)"

#: constrainCheckbox.tooltip
msgid "Maintain the image's aspect ratio"
msgstr "གཟུགས་བརྙན་གྱི་རྣམ་པའི་དཔྱ་ཚད་དེ་རྒྱུན་སྐྱོང་འབད།"

# These controls are in the Image Map box of the expanded area
#: imagemapBox.label
msgid "Image Map"
msgstr "གཟུགས་བརྙན་ས་ཁྲ།"

#: removeImageMapButton.label
#: removeImageMapButton.accessKey
msgid "&Remove"
msgstr "རྩ་བསྐྲད་གཏང་།(&R)"

#: editImageMapButton.label
msgid "Edit..."
msgstr "ཞུན་དག..."

#: editImageMapButton.tooltip
msgid "Create clickable hotspots for this image"
msgstr "གཟུགས་བརྙན་དེའི་དོན་ལུ་ཨེབ་གཏང་འབད་བཏུབ་པའི་འཁྱམས་གནས་གསར་བསྐྲུན་འབད།"

# These are the options for image alignment
#: alignment.label
msgid "Align Text to Image"
msgstr "ཚིག་ཡིག་གཟུགས་བརྙན་ལུ་ཕྲང་།"

#: bottomPopup.value
msgid "At the bottom"
msgstr "གཤམ་ལུ།"

#: topPopup.value
msgid "At the top"
msgstr "མགོ་ལུ།"

#: centerPopup.value
msgid "In the center"
msgstr "དབུས་ནང་ལུ།"

#: wrapRightPopup.value
msgid "Wrap to the right"
msgstr "གཡས་ལུ་ལོག་མཚམས་བཟོ་ནི།"

#: wrapLeftPopup.value
msgid "Wrap to the left"
msgstr "གཡོན་ལུ་ལོག་མཚམས་བཟོ་ནི།"

# These controls are in the Spacing Box
#: spacingBox.label
msgid "Spacing"
msgstr "བར་སྟོང་བཞག་ཐངས།"

#: spacingBox.tooltip
msgid "Type a number for the amount of space around image"
msgstr "གཟུགས་བརྙན་མཐའ་སྐོར་བར་སྟོང་གི་དངུལ་བསྡོམས་དོན་ལུ་ཨང་ཡིག་དཔར་རྐྱབས།"

#: leftRightEditField.label
#: leftRightEditField.accessKey
msgid "&Left and Right:"
msgstr "གཡོན་དང་གཡས:(&L)"

#: topBottomEditField.label
#: topBottomEditField.accessKey
msgid "&Top and Bottom:"
msgstr "མགོ་དང་གཤམ:(&T)"

#: borderEditField.label
#: borderEditField.accessKey
msgid "Solid &Border:"
msgstr "རགས་པ་མཐའ་མཚམས:(&B)"

# These controls are in the Link Box
#: showImageLinkBorder.label
#: showImageLinkBorder.accessKey
msgid "Show &border around linked image"
msgstr "འབྲེལ་མཐུད་ཡོད་པའི་གཟུགས་བརྙན་མཐའ་སྐོར་ལུ་མཐའ་མཚམས་སྟོན།(&b)"

# These controls may be added some day; currently not used
#: makePageBackgroundCheckbox.label
msgid "Make Page Background"
msgstr "ཤོག་ལེབ་ཀྱི་རྒྱབ་གཞི་བཟོ།"

#: makePageBackgroundCheckbox.tooltip
msgid "Use image as background for your page"
msgstr "ཁྱོད་རའི་ཤོག་ལེབ་ཀྱི་དོན་ལུ་རྒྱབ་གཞི་སྦེ་གཟུགས་བརྙན་ལག་ལེན་འཐབ།"

#: editImageButton.label
msgid "Edit Image"
msgstr "གཟུགས་བརྙན་ཞུན་དག་འབད།"

#: editImageButton.tooltip
msgid "Edit image file in another program"
msgstr "གཟུགས་བརྙན་ཡིག་སྣོད་གཞན་ལས་རིམ་ནང་ཞུན་དག་འབད།"

# These tabs are currently used in the image input dialog
#: imageInputTab.label
msgid "Form"
msgstr "འབྲི་ཤོག"

#: imageLocationTab.label
msgid "Location"
msgstr "གནས་ཁོངས།"

#: imageDimensionsTab.label
msgid "Dimensions"
msgstr "མཚོན་གྲངས་ཚུ།"

#: imageAppearanceTab.label
msgid "Appearance"
msgstr "འབྱུང་སྣང་།"

#: imageLinkTab.label
msgid "Link"
msgstr "འབྲེལ་ལམ།"

