#: SavePassword
msgid "Use Password Manager to save this password"
msgstr "ཆོག་ཡིག་དེ་སྲུངས་བཞག་ནི་ལུ་ཆོག་ཡིག་འཛིན་སྐྱོང་པ་ལག་ལེན་འཐབ།"

#: CorrectSpelling
msgid "(correct spelling)"
msgstr "(ཡིག་སྡེབ་ནོར་བཅོས་འབད)"

#: NoSuggestedWords
msgid "(no suggested words)"
msgstr "(བསམ་འཆར་མ་བཀོད་པའི་མིང་ཚིག་ཚུ)"

#: NoMisspelledWord
msgid "No misspelled words"
msgstr "སྡེབ་འཛོལ་བའི་མིང་ཚིག་ཚུ་མེད།"

#: CheckSpellingDone
msgid "Completed spell checking."
msgstr "ཡིག་སྡེབ་བརྟག་ཞིབ་མཇུག་བསྡ་ཡོདཔ།"

#: CheckSpelling
msgid "Check Spelling"
msgstr "ཡིག་སྡེབ་བརྟག་ཞིབ་འབད།"

#: LoadingDone
msgid "Done loading page"
msgstr "ཤོག་ལེབ་མངོན་གསལ་འབད་ཚར་ནུག"

#: InputError
msgid "Error"
msgstr "འཛོལ་བ།"

#: Alert
msgid "Alert"
msgstr "དྲན་བརྡ།"

#: CantEditFramesetMsg
msgid "Composer cannot edit HTML framesets, or pages with inline frames. For framesets, try editing the page for each frame separately. For pages with iframes, save a copy of the page and remove the <iframe> tag."
msgstr "རྩོམ་མཁན་གྱིས་ཨེཆ་ཊི་ཨེམ་ཨེལ་གཞི་ཁྲམ་ཚན་ཚུ་་ཡང་ན་ཨིན་ལའིན་གཞི་ཁྲམ་ཚུ་དང་གཅིག་ཁར་་ཤོག་ལེབ་ཚུ་ཞུན་དག་འབད་མི་བཏུབ། གཞི་ཁྲམ་ཚན་ཚུའི་དོན་ལུ་ གཞི་ཁྲམ་དབྱེ་བྱེད་རེ་རེ་དོན་ལུ་ཤོག་ལེབ་དེ་ཞུན་དག་འབད་ན་འབད་རྩོལ་བསྐྱེད། ཨའི་གཞི་ཁྲམ་ཚུ་དང་གཅིག་ཁར་གྱི་ཤོག་ལེབ་ཚུའི་དོན་ལུ་ ཤོག་ལེབ་ཀྱི་འདྲ་བཤུས་ཅིག་སྲུངས་བཞག་འབད་དེ་<iframe> ངོ་རྟགས་དེ་རྩ་བསྐྲད་གཏང་།"

#: CantEditMimeTypeMsg
msgid "This type of page can't be edited."
msgstr "ཤོག་ལེབ་དེ་བཟུམ་་ཞུན་དག་འབད་མི་བཏུབ།"

#: CantEditDocumentMsg
msgid "This page can't be edited for an unknown reason."
msgstr "རྒྱ་མཚན་མ་ཤེསཔ་དོན་ལུ་ཤོག་ལེབ་དེ་ཞུན་དག་མི་འབད།"

#: HTMLFiles
msgid "HTML Files"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ཡིག་སྣོད་ཚུ།"

#: IMGFiles
msgid "Image Files"
msgstr "གཟུགས་བརྙན་ཡིག་སྣོད་ཚུ།"

#: TextFiles
msgid "Text Files"
msgstr "ཚིག་ཡིག་ ཡིག་སྣོད་ཚུ།"

#: AllFiles
msgid "All Files"
msgstr "ཡིག་སྣོད་ཚུཆ་མཉམ།"

#: BeforeClosing
msgid "before closing"
msgstr "ཁ་བསྡམ་པའི་ཧེ་མ།"

#: BeforePreview
msgid "before viewing in Navigator"
msgstr "འགྲུལ་བསྐྱོདཔ་ནང་སྟོན་པའི་ཧེ་མ།"

#: BeforeValidate
msgid "before validating the document"
msgstr "ཡིག་ཆ་དེ་ནུས་ཅན་བཟོ་བའི་ཧེ་མ།"

# LOCALIZATION NOTE (SaveFilePrompt, PublishPrompt): Don't translate %title% and %reason% (this is the reason for asking user to close, such as "before closing")
#: SaveFilePrompt
#: PublishPrompt
msgid "Save changes to \"%title%\" %reason%?"
msgstr " \"%མགོ་མངག་%\" %རྒྱུ་མཚན་%ལུ་བསྒྱུར་བཅོས་ཚུ་སྲུངས་བཞག་འབད་ནི་ཨིན་ན?"

#: SaveFileFailed
msgid "Saving file failed!"
msgstr "ཡིག་སྣོད་སྲུངས་བཞག་འབད་མི་འཐུས་ཤོར་བྱུང་ཡོདཔ!"

# Publishing error strings:
# LOCALIZATION NOTE Don't translate %dir% or %file% in the Publishing error strings:
#: FileNotFound
msgid "%file% not found."
msgstr "%file% འཚོལ་མ་ཐོབ།"

#: SubdirDoesNotExist
msgid "The subdirectory \"%dir%\" doesn't exist on this site or the filename \"%file%\" is already in use by another subdirectory."
msgstr "སྣོད་ཐོ་འོག་མ་ \"%dir%\" ས་ཁོངས་དེ་གུ་མེདཔ་ཡང་ན་ཡིག་སྣོད་མིང་\"%file%\" དེ་ཧེ་མ་ལས་གཞན་སྣོད་ཐོ་འོག་མ་གིས་ལག་ལེན་འཐབ་ཡོདཔ་ཨིན།"

#: FilenameIsSubdir
msgid "The filename \"%file%\" is already in use by another subdirectory."
msgstr "ཡིག་སྣོད་མིང་ \"%file%\" དེ་ཧེ་མ་ལས་གཞན་སྣོ་ཐོ་གིས་ལག་ལེན་འཐབ་ཡོདཔ་ཨིན།"

#: ServerNotAvailable
msgid "The server is not available. Check your connection and try again later."
msgstr "སར་བར་དེ་ཐོབ་མི་ཚུགས་པས། ཁྱོད་རའི་མཐུད་ལམ་བརྟག་ཞིབ་དེ་ལོག་ཤུལ་ས་འབད་རྩོལ་བསྐྱེད།"

#: Offline
msgid "You are currently offline. Click the icon near the lower-right corner of any window to go online."
msgstr "ཁྱོད་ད་ལྟོ་ཨོཕ་ལ་ཡིན་ཨིན་མས། གློག་ཐོག་འགྱོ་ནི་ལུ་ཝིན་ཌོ་གང་རུང་གི་གཡས་ཕྱོགས་འོག་གི་སྒྱིག་ཁུག་་སྦོ་ལོག་ཁར་གྱི་ངོས་དཔར་དེ་ཨེབ་གཏང་འབད།"

#: DiskFull
msgid "There is not enough disk space available to save the file \"%file%.\""
msgstr "\"%file%.\"ཡིག་སྣོད་སྲུངས་བཞག་འབད་ནི་ལུ་ཌིཀསི་བར་སྟོང་ལངམ་མིན་འདུག"

#: NameTooLong
msgid "The filename or subdirectory name is too long."
msgstr "ཡིག་སྣོད་མིང་ཡང་ན་སྣོད་ཐོ་འོག་མ་མིང་དེ་གནམ་མེད་ས་མེད་རིང་མས།"

#: AccessDenied
msgid "You do not have permission to publish to this location."
msgstr "ཁྱོད་ལུ་གནས་ཁོངས་དེ་ཁར་ལུ་དཔེ་བསྐྲུན་འབད་ནི་གནང་བ་མེད།"

#: UnknownPublishError
msgid "Unknown publishing error occurred."
msgstr "མ་ཤེསཔ་དཔེ་བསྐྲུན་འཛོལ་བ་འབྱུང་ཡོདཔ།"

#: PublishFailed
msgid "Publishing failed."
msgstr "དཔེ་བསྐྲུན་འཐུས་ཤོར་བྱུང་ཡོདཔ།"

#: PublishCompleted
msgid "Publishing completed."
msgstr "དཔེ་བསྐྲུན་མཇུག་བསྡུ་ཡོདཔ།"

#: AllFilesPublished
msgid "All files published"
msgstr "ཡིག་སྣོད་ཆ་མཉམ་དཔེ་བསྐྲུན་འབད་ཡོདཔ།"

# LOCALIZATION NOTE Don't translate %x% or %total%
#: FailedFileMsg
msgid "%x% of %total% files failed to publish."
msgstr "%x% of %total% ཡིག་སྣོད་ཚུ་དཔེ་བསྐྲུན་འབད་ནི་འཐུས་ཤེར་བྱུང་ཡོདཔ།"

# End-Publishing error strings
#: Prompt
msgid "Prompt"
msgstr "ནུས་སྤེལ།"

# LOCALIZATION NOTE (PromptFTPUsernamePassword): Don't translate %host%
#: PromptFTPUsernamePassword
msgid "Enter username and password for FTP server at %host%"
msgstr "%host%ལུ་སར་བར་ཨེཆ་ཊི་པི་དོན་ལུ་ལག་ལེན་པ་མིང་དང་ཆོག་ཡིག་བཙུགས།"

#: RevertCaption
msgid "Revert To Last Saved"
msgstr "མཇུག་སྲུངས་བཞག་ཡོད་མི་ལུ་རྒྱབ་ལོག"

#: Revert
msgid "Revert"
msgstr "རྒྱབ་ལོག"

#: SendPageReason
msgid "before sending this page"
msgstr "ཤོག་ལེབ་དེ་གཏང་བའི་ཧེ་མ།"

#: Send
msgid "Send"
msgstr "གཏང་།"

## LOCALIZATION NOTE (PublishProgressCaption, PublishToSite, AbandonChanges): Don't translate %title%
#: PublishProgressCaption
msgid "Publishing: %title%"
msgstr "དཔེ་བསྐྲུན་འབད་དོ: %title%"

#: PublishToSite
msgid "Publishing to Site: %title%"
msgstr "ས་ཁོངས་ལུ་དཔེ་བསྐྲུན་འབད་དོ: %title%"

#: AbandonChanges
msgid "Abandon unsaved changes to \"%title%\" and reload page?"
msgstr "སྲུངས་བཞག་མ་འབད་མི་བསྒྱུར་བཅོས་ཚུ་ \"%title%\" ལུ་འདོར་བ་དང་ཤོག་ལེབ་བསྐྱར་ལོག་མངོན་གསལ་འབད་ནི་ཨིན་ན?"

#: DocumentTitle
msgid "Page Title"
msgstr "ཤོག་ལེབ་མགོ་མིང་།"

#: NeedDocTitle
msgid "Please enter a title for the current page."
msgstr "ད་ལྟོའི་ཤོག་ལེབ་དོན་ལུ་མགོ་མིང་ཅིག་བཙུགས་གནང་།"

#: DocTitleHelp
msgid "This identifies the page in the window title and bookmarks."
msgstr "དེ་གིས་ཝིན་ཌོ་མགོ་མིང་དང་དེབ་རྟགས་ཚུ་ནང་ངོས་འཛིན་འབདཝ་ཨིན།"

#: CancelPublishTitle
msgid "Cancel publishing?"
msgstr "དཔེ་བསྐྲུན་ཆ་མེད་གཏང་ནི?"

## LOCALIZATION NOTE: "Continue" in this sentence must match the text for
## the CancelPublishContinue key below
#: CancelPublishMessage
msgid "Cancelling while publishing is in progress may result in your file(s) being incompletely transferred. Would you like to Continue or Cancel?"
msgstr "དཔེ་བསྐྲུན་དེ་ཡར་འཕེལ་འགྱོ་བའི་བསྒང་ཆ་མེད་གཏང་མི་དེ་གིས་ཁྱོད་ཀྱི་ཡིག་སྣོད་(ཨེསི་)རྩ་བ་ལས་གནས་སོར་གཏང་ནི་ནང་་གནོད་པ་འབྱུང་འོང་། ཁྱོད་ཀྱིས་འཕྲོ་མཐུད་ནི་ཡང་ན་ཆ་མེད་གཏང་ནི་ཨིན་ན?"

