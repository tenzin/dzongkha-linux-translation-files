# Smiley Menu items
#: insertSmiley.label
#: insertSmiley.accesskey
msgid "&Smiley"
msgstr "སི་མའི་ལི།(&S)"

#: smiley1Cmd.label
#: smiley1Cmd.accesskey
msgid "&Smile"
msgstr "བགའ་འཛུམ་བཏོན།(&S)"

#: smiley1Cmd.tooltip
#: SmileButton.tooltip
msgid "Insert a smiley face"
msgstr "སི་མའི་ལི་གི་གདོང་ཅིག་བཙུགས།"

#: smiley2Cmd.label
#: smiley2Cmd.accesskey
msgid "&Frown"
msgstr "དཔྱལཝ་ཨོ་ཀྲོ་བསུམ་ནི།(&F)"

#: smiley2Cmd.tooltip
msgid "Insert a frown face"
msgstr "དཔྱལཝ་ཨོ་ཀྲོ་བསུམ་མིའི་གདོང་ཅིག་བཙུགས།"

#: smiley3Cmd.label
#: smiley3Cmd.accesskey
msgid "&Wink"
msgstr "མིག་འཁྲིབ་རྐྱབ་མི།(&W)"

#: smiley3Cmd.tooltip
msgid "Insert a wink face"
msgstr "མིག་འཁྲིབ་རྐྱབ་མི་གདོང་ཅིག་བཙུགས།"

#: smiley4Cmd.label
#: smiley4Cmd.accesskey
msgid "&Tongue-out"
msgstr "ལྕེ་ཕྱིར་བཏོན་ནི།(&T)"

#: smiley4Cmd.tooltip
msgid "Insert a tongue-out face"
msgstr "ལྕེ་ཕྱིར་བཏོན་མི་གདོང་ཅིག་བཙུགས།"

#: smiley5Cmd.label
#: smiley5Cmd.accesskey
msgid "&Laughing"
msgstr "བགའ་མི།(&L)"

#: smiley5Cmd.tooltip
msgid "Insert a laughing face"
msgstr "བགའ་མི་གདོང་ཅིག་བཙུགས།"

#: smiley6Cmd.label
#: smiley6Cmd.accesskey
msgid "&Embarrassed"
msgstr "ངོ་ཚ་ཅན།(&E)"

#: smiley6Cmd.tooltip
msgid "Insert an embarrassed face"
msgstr "ངོ་ཚ་ཅན་གྱི་གདོང་ཅིག་བཙུགས།"

#: smiley7Cmd.label
#: smiley7Cmd.accesskey
msgid "&Undecided"
msgstr "ཐག་མ་བཅད་པ།(&U)"

#: smiley7Cmd.tooltip
msgid "Insert an undecided face"
msgstr "ཐག་མ་བཅད་པའི་གདོང་ཅིག་བཙུགས།"

#: smiley8Cmd.label
#: smiley8Cmd.accesskey
msgid "Sur&prise"
msgstr "ཧ་ལས་པ།(&p)"

#: smiley8Cmd.tooltip
msgid "Insert a surprised face"
msgstr "ཧ་ལས་པའི་གདོང་ཅིག་བཙུགས།"

#: smiley9Cmd.label
#: smiley9Cmd.accesskey
msgid "&Kiss"
msgstr "འུ་བཀལ་ནི།(&K)"

#: smiley9Cmd.tooltip
msgid "Insert a kiss face"
msgstr "འུ་བཀལ་མིའིགདོང་ཅིག་བཙུགས།"

#: smiley10Cmd.label
#: smiley10Cmd.accesskey
msgid "&Yell"
msgstr "སྐད་ངན་རྐྱབས་ནི།(&Y)"

#: smiley10Cmd.tooltip
msgid "Insert a yelling face"
msgstr "སྐད་ངན་རྐྱབས་མིའི་གདོང་ཅིག་བཙུགས།"

#: smiley11Cmd.label
#: smiley11Cmd.accesskey
msgid "&Cool"
msgstr "ཞི་བ།(&C)"

#: smiley11Cmd.tooltip
msgid "Insert a cool face"
msgstr "ཞི་བའི་གདོང་ཅིག་བཙུགས།"

#: smiley12Cmd.label
#: smiley12Cmd.accesskey
msgid "&Money-Mouth"
msgstr "མ་ནི་མཱའུཐ།(&M)"

#: smiley12Cmd.tooltip
msgid "Insert a money-mouth face"
msgstr "མ་ནི་མཱའུཐ་གདོང་ཅིག་བཙུགས།"

#: smiley13Cmd.label
#: smiley13Cmd.accesskey
msgid "F&oot-in-Mouth"
msgstr "ཕུཊི་ཨིན་མཱའུཐ།(&o)"

#: smiley13Cmd.tooltip
msgid "Insert a foot-in-mouth face"
msgstr "ཕུཊི་ཨིན་མཱའུཐ་གདོང་ཅིག་བཙུགས།"

#: smiley14Cmd.label
#: smiley14Cmd.accesskey
msgid "&Innocent"
msgstr "མནོ་མེད།(&I)"

#: smiley14Cmd.tooltip
msgid "Insert an innocent face"
msgstr "མནོ་མེད་གདོང་ཅིག་བཙུགས།"

#: smiley15Cmd.label
#: smiley15Cmd.accesskey
msgid "C&ry"
msgstr "སྔུ་ནི།(&r)"

#: smiley15Cmd.tooltip
msgid "Insert a crying face"
msgstr "སྔུ་ནི་གདོང་ཅིག་བཙུགས།"

#: smiley16Cmd.label
#: smiley16Cmd.accesskey
msgid "Lips-&are-Sealed"
msgstr "མཆུ་བཙུམས་ནུག་(&a)"

#: smiley16Cmd.tooltip
msgid "Insert a lips-are-sealed face"
msgstr "མཆུ་བཙུམས་མའི་གདོང་ཅིག་བཙུགས།"

