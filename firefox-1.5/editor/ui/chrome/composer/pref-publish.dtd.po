# extracted from content/pref-publish.xul
# LOCALIZATION NOTE : FILE The Publish preferences screen
#: window.title
#: composerHeader.label
msgid "Publish"
msgstr "དཔེ་བསྐྲུན།"

#: maintainCheckbox.label
msgid "Maintain links"
msgstr "འབྲེལ་ལམ་ཚུ་རྒྱུན་སྐྱོང་འབད།"

#: keepCheck.label
msgid "Keep images with page"
msgstr "གཟུགས་བརྙན་ཚུ་ཤོག་ལེབ་གཅིག་ཁར་བཞག"

#: adjustDesc.label
msgid ""
"_: Attempt to maintain approximate line length, linefeeds and white space\n"
"Adjust links to work from the page's location. (Links to local pages will work when \t\t\t      local versions exist.)           "
msgstr "ཤོག་ལེབ་ཀྱི་གནས་ཁོངས་ལས་ལཱ་འབད་ནི་ལུ་འབྲེལ་ལམ་ཚུ་བདེ་སྒྲིག་འབད། (ཉེ་གནས་ཐོན་རིམ་ཚུ་ཡོད་པའི་སྐབས \t\t\t    ཉེ་གནས་ཤོག་ལེབ་ཚུ་ལུ་འབྲེལ་ལམ་ཚུ་ལཱ་འབད་འོང་།  )    "

#: saveDesc.label
msgid ""
"_: Attempt to maintain approximate line length, linefeeds and white space\n"
"Save copies of images to the page's location.  Images will always \t\t\t\tappear in local versions of the page and when the page is published."
msgstr "ཤོག་ལེབ་ཀྱི་གནས་ཁོངས་ལུ་གཟུགས་བརྙན་ཚུ་གི་འདྲ་བཤུས་ཚུ་སྲུངས་བཞགཔ་ཨིན། གཟུགས་བརྙན་ཚུ་ཨ་རྟག་རང་\t\t\t\t ཤོག་ལེབ་དེ་དཔེ་བསྐྲུན་འབད་ཡོདཔ་ད་དང་ཤོག་ལེབ་ཀྱི་ཉེ་གནས་ཐོན་རིམ་ཚུ་ནང་འབྱུངཙ་ཨིན།"

#: tip.label
msgid "Tip: Check both options if you will be using remote publishing."
msgstr "ཕན་བསླབ: ཁྱོད་ཀྱིས་ཐག་རིང་གི་དེབ་བསྐྲུན་ལག་ལེན་འཐབ་ནི་ཨིན་པ་ཅིན་གདམ་ཁ་ཚུ་གཉིས་ཆ་རང་་ཞིབ་དཔྱོད་འབད།"

#: defaultHeader.label
msgid "Default publishing location"
msgstr "སྔོན་སྒྲིག་དེབ་བསྐྲུན་གནས་ཁོངས།"

#: publishFtp.label
#: publishhtp.label
msgid ""
"_: Edit box appears beside this label\n"
"If publishing to a FTP site, enter the HTTP address to browse to:"
msgstr "ཨེཕ་ཊི་པི་ས་ཁོངས་ལུ་དེབ་བསྐྲུན་འབད་ནི་ཨིན་པ་ཅིན་ ཨེཆ་ཊི་ཊི་པི་ཁ་བྱང་དེ་ལུ་བརྡ་འཚོལ་ལུ་བཙུགས:"

