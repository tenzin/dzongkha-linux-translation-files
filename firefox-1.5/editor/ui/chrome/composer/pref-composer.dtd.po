#: lHeader
msgid "Composer"
msgstr "རྩོམ་མཁན།"

#: rHeader
msgid "Settings for authoring of new Web pages"
msgstr "ཝེབ་ཤོག་ལེབ་གསརཔ་ཚུ་བཟོ་ནའི་སྒྲིག་སྟངས།"

#: saving
msgid "Saving"
msgstr "སྲུངས་བཞག་འབད་དོ།"

#: AutoSaveCheck
msgid "Automatically save every"
msgstr "རང་བཞིན་གྱི་རེ་རེ་སྲུངས་བཞག"

#: minText
msgid "minutes"
msgstr "སྐར་མ་ཚུ།"

#: exterLegend.label
msgid "External Editors"
msgstr "ཕྱིའི་ཞུན་དགཔ་ཚུ།"

#: htmlSource
msgid "HTML Source:"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་འབྱུང་ཁུངས:"

#: imageeditor
msgid "Images:"
msgstr "གཟུགས་བརྙན་ཚུ:"

#: chooseButton.label
msgid "Choose"
msgstr "གདམ།"

#: recentFiles.title
msgid "Recent Pages Menu"
msgstr "འཕྲལ་གྱི་ཤོག་ལེབ་ཚུའི་དཀར་ཆག"

#: documentsInMenu.label
#: documentsInMenu.accesskey
msgid "Maximum &number of pages listed:"
msgstr "ཤོག་ལེབ་ཚུ་ཐོ་བཀོད་ཡོད་པའི་ཨང་མང་མཐའ།(&n)"

#: savingFiles.title
msgid "When Saving or Publishing Pages"
msgstr "ཤོག་ལེབ་ཚུ་དེབ་བསྐྲུན་ཡང་ན་སྲུངས་བཞག་འབད་བའི་སྐབས།"

#: preserveExisting.label
#: preserveExisting.accesskey
msgid "&Preserve original source formatting"
msgstr "རྩ་སྒྲིག་འབྱུང་ཁུངས་འདྲ་དང་པམ་ཉམས་སྲུང་འབད།(&P)"

#: preserveExisting.tooltip
msgid "Preserves line breaks and page's original formatting"
msgstr "གྱལ་མཚམས་ཚུ་དང་ཤོག་ལེབ་ཀྱི་རྩ་སྒྲིག་འདྲ་དང་བམ་ཉམས་སྲུངམ་ཨིན།"

#: saveAssociatedFiles.label
#: saveAssociatedFiles.accesskey
msgid "&Save images and other associated files when saving pages"
msgstr "གཟུགས་བརྙན་ཚུ་དང་ཤོག་ལེབ་ཚུ་སྲུངས་བཞག་པའི་སྐབས་གཞན་འབྲེལ་བའི་ཡིག་སྣོད་ཚུ་སྲུངས་བཞག་འབད།(&S)"

#: showPublishDialog.label
#: showPublishDialog.accesskey
msgid "&Always show Publish dialog when publishing pages"
msgstr "ཤོག་ལེབ་ཚུ་དེབ་བསྐྲུན་སྐབས་དཔེ་བསྐྲུན་ཌའི་ལོག་ཨ་རྟག་ར་སྟོན།"

#: composerEditing.label
msgid "Editing"
msgstr "ཞུན་དག་འབད་དོ།"

#: maintainStructure.label
#: maintainStructure.accesskey
msgid "&Maintain table layout when inserting or deleting cells"
msgstr "ནང་ཐིག་ཚུ་བཏོན་གཏངམ་ད་ཡང་ན་བཙུགས་པའི་སྐབས་ཐིག་ཁྲམ་སྒྲིག་བཀོད་རྒྱུན་སྐྱོང་འབད།(&M)"

#: maintainStructure.tooltip
msgid "Preserves table's rectangular shape by automatically adding cells after inserting or deleting cells"
msgstr "ནང་ཐིག་ཚུ་བཏོན་གཏང་ཡང་ནབཙུགས་པའི་ཤུལ་ལས་ནང་ཐིག་ཚུ་རང་བཞིན་གྱིས་འབད་ཐིག་ཁྲམ་གྱི་གྲུ་བཞི་ནར་་མོ་དབྱིབས་ཉམས་སྲུངམ་ཨིན།"

#: useCSS.label
#: useCSS.accesskey
msgid "&Use CSS styles instead of HTML elements and attributes"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ཆ་ཤས་ཚུ་དང་ཁྱད་ཆོས་ཚུ་གི་ཚབ་མ་སི་ཨེསི་ཨེསི་བཟོ་རྣམ་ཚུ་ལག་ལེན་འཐབ།(&U)"

#: crInPCreatesNewP.label
#: crInPCreatesNewP.accesskey
msgid "&Return in a paragraph always creates a new paragraph"
msgstr "དོན་མཚམས་ནང་སླར་ལོག་ནི་དེ་གིས་ཨ་རྟག་ར་དོན་མཚམས་གསརཔ་ཅིག་གསར་བསྐྲུན་འབདཝ་ཨིན།"

