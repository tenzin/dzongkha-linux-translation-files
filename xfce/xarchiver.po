#: ../src/add_dialog.c:176
msgid "Generate a solid archive"
msgstr "ཡིག་མཛོད་རག་པ་ཅིག་བཟོ་བཏོན་འབད།"

#: ../src/add_dialog.c:180
msgid "In a solid archive the files are grouped together featuring a better compression ratio."
msgstr "ཡིག་མཛོད་རགས་པའི་ནང་  ཨེབ་བཙུགས་དཔྱ་ཚད་ལེགས་ཤོམ་ཅིག་གི་རྣམ་པའི་ཐོག་ལས་  ཡིག་སྣོད་ཚུ་གཅིག་ཁར་ཡོད།"

#: ../src/add_dialog.c:185
msgid "Remove files after adding"
msgstr "ཡིག་སྣོད་ཚུ་ཁ་སྐོང་འབད་ཚར་བའི་ཤེལ་ལུ་རྩ་བསྐྲད་གཏང་།"

#: ../src/add_dialog.c:194
msgid "Do not add absolute paths"
msgstr "ཡང་དག་འགྲུལ་ལམ་ ཁ་སྐོང་མ་བརྐྱབ་"

#: ../src/add_dialog.c:196
msgid "Do not add file paths"
msgstr "ཡིག་སྣོད་འགྲུལ་ལམ་ཚུ་ཁ་སྐོང་མ་འབད།"

#: ../src/add_dialog.c:201
msgid "The files path doesn't include the user home directory."
msgstr "ཡིག་སྣོད་འགྲུལ་ལམ་གྱིས་ ལག་ལེན་པའི་ཁྱིམ་གྱི་སྣོད་ཐོ་འདི་ གྲངས་སུ་མི་བཙུགས།"

#: ../src/add_dialog.c:203
msgid "Store just the name of a file without its directory names."
msgstr "སྣོད་ཐོའི་མིང་ཚུ་མེད་པར་  ཡིག་སྣོད་ཅིག་གི་མིང་གསོག་འཇོག་འབད།"

#: ../src/add_dialog.c:205
msgid "Freshen an existing entry in the archive"
msgstr "ཡིག་མཛོད་ནང་ལུ་  གནས་ཏེ་ཡོད་པའི་ཐོ་བཀོད་འདི་གསརཔ་བཟོ།"

#: ../src/add_dialog.c:209
msgid "This option affects the archive only if it has been modified more recently than the version already in the archive; unlike the update option it will not add files that are not already in the archive."
msgstr "ཡིག་མཛོད་ནང་ཧེ་མ་ལས་ཡོད་པའི་འཐོན་རིམ་ལས་  འཕྲལ་སྐབས་ཅིག་ལུ་ལེགས་བཅོས་འབད་དེ་ཡོད་པ་ཅིན་རྐྱངམ་ཅིག་  གདམ་ཁ་འདི་གིས་ཡིག་མཛོད་ལུ་གནོདཔ་ཨིན་; དུས་མཐུན་གདམ་ཁ་མ་ཡིན་པ་གཞན་འབད་བ་ཅིན་  ཡིག་མཛོད་ནང་ཧེ་མ་ལས་ཡོད་པའི་ཡིག་སྣོད་ཚུ་ཁ་སྐོང་མི་བརྐྱབ།"

#: ../src/add_dialog.c:213
msgid "Update an existing entry in the archive"
msgstr "གནས་ཏེ་ཡོད་པའི་ཐོ་བཀོད་ཚུ་  ཡིག་མཛོད་ནང་དུས་མཐུན་བཟོ།"

#: ../src/add_dialog.c:217
msgid "This option will add any new files and update any files which have been modified since the archive was last created/modified."
msgstr "གདམ་ཁ་འདི་གིས་  ཡིག་མཛོད་འདི་མཇུག་མམ་གསར་བསྐྲུན་འབད་དེ་/ལེགས་བཅོས་འབད་དེ་ཡོདཔ་ལས་  ལེགས་བཅོས་འབད་ཡོད་པའི་  ཡིག་སྣོད་གསརཔ་ཚུ་དང་དུས་མཐུན་ཡིག་སྣོད་གང་རུང་ཅིག་ཁ་སྐོང་བརྐྱབ་ཨིན།"

#: ../src/add_dialog.c:226
msgid "0 = no compression, 5 is default, 9 = best compression but slowest"
msgstr "༠ = ཨེབ་བཙུགས་མེད་, ༥་ འདི་སྔོན་སྒྲིག་ཨིན་, ༩ = ཨེབ་བཙུགས་ལེགས་ཤོམ་དེ་འབདཝ་ད་ལྷོད་མི་ཨིན།"

#: ../src/add_dialog.c:233
msgid "0 = no compression, 6 is default, 9 = best compression but slowest"
msgstr "༠ = ཨེབ་བཙུགས་མེད་, ༦ འདི་སྔོན་སྒྲིག་ཨིན་, ༩ = ཨེབ་བཙུགས་ལེགས་ཤོམ་དེ་འབདཝ་ད་ལྷོད་མི།"

#: ../src/add_dialog.c:240
msgid "0 = no compression, 3 is default, 5 = best compression but slowest"
msgstr "༠ =ཨེབ་བཙུགས་མེད་, ༣་  འདི་སྔོན་སྒྲིག་ཨིན་, ༥ = ཨེབ་བཙུགས་ལེགས་ཤོམ་  དེ་འབདཝ་ད་  ལྷོད་མི་ཨིན།"

#: ../src/add_dialog.c:247
msgid "0 = no compression, 1 is default, 4 = fastest but least compression"
msgstr "༠= ཨེབ་བཙུགས་མེད་, ༡་ འདི་སྔོན་སྒྲིག་ཨིན་, ༤ = མགྱོགས་དྲགས་  དེ་འབདཝ་ད་ ཨེབ་བཙུགས་ཉུང་ཤོས་ཨིན།"

#: ../src/add_dialog.c:254
msgid "5 = default compression, 7 = max compression"
msgstr "༥ = སྔོན་སྒྲིག་ཨེབ་བཙུགས་, ༧ = ཨེབ་བཙུགས་མང་མཐའ།"

