#: ../panel-plugin/xmms_plugin.c:756
msgid "Properties"
msgstr "རྒྱུ་དངོས་"

#: ../panel-plugin/xmms_plugin.c:768
msgid "Xfce4 XMMS Plugin Options"
msgstr "ཨེགསི་ཨེཕ་སི་ཨི་༤་ ཨེགསི་ཨེམ་ཨེམ་ཨེསི་པ་ལག་ཨིན་གདམ་ཁ་ཚུ་"

#. put labels into the left column of our table
#: ../panel-plugin/xmms_plugin.c:787
msgid "Font Size"
msgstr "ཡིག་གཟུགས་ཚད་"

#: ../panel-plugin/xmms_plugin.c:788
msgid "Scroll Speed"
msgstr "མགྱོགས་ཚད་བཤུད་སྒྲིལ་"

#: ../panel-plugin/xmms_plugin.c:789
msgid "Scroll Stepwidth"
msgstr "གོམ་པའི་རྒྱ་ཚད་བཤུད་སྒྲིལ་འབད་"

#: ../panel-plugin/xmms_plugin.c:790
msgid "Scroll Delay"
msgstr "བཤུད་སྒྲིལ་ཕྱིར་འགྱོངས་"

#. add check buttons for the scrolled title, progressbar and volume bar
#: ../panel-plugin/xmms_plugin.c:802
msgid "Show scrolled song title"
msgstr "བཤུད་སྒྲིལ་འབད་དེ་ཡོད་པའི་ཞབས་ཁྲའི་མགོ་མིང་འདི་སྟོན་"

#: ../panel-plugin/xmms_plugin.c:803
msgid "Show track position"
msgstr "གླུ་རིམ་གནས་ས་སྟོན་"

#: ../panel-plugin/xmms_plugin.c:804
msgid "Show volume level"
msgstr "སྐད་ཤུགས་གནས་གོང་སྟོན་"

#. add check button for simple title option
#: ../panel-plugin/xmms_plugin.c:806
msgid "Simple song title format"
msgstr "ཞབས་ཁྲའི་མགོ་མིང་རྩ་སྒྲིག་འཇམ་སམ་"

#. add check button for hor_vol_if_vertical option
#: ../panel-plugin/xmms_plugin.c:808
msgid "Horizontal volume bar on vertical panels"
msgstr "ཀེར་ཕྲང་པེ་ནཱལསི་གུ་ ཐད་སྙོམས་སྐད་ཤུགས་ཕྲ་རིང་"

#. add check button for "Use BMP" option
#: ../panel-plugin/xmms_plugin.c:811
msgid "Use BMP (Beep Media Player)"
msgstr "བི་ཨེམ་པི་ལག་ལེན་འཐབ་ (བིཔ་བརྡ་ལམ་གཏང་འཕྲུལ་)"

#. add check button for quit xmms option
#: ../panel-plugin/xmms_plugin.c:813
msgid "Quit XMMS/BMP when plugin terminates"
msgstr "ཨེགསི་ཨེམ་ཧེམ་ཨེསི་/བི་ཨེམ་པི་དེ་ཚུ་  པ་ལག་ཨིན་བཏོན་བཀོཝ་ད་སྤང་"

#: ../panel-plugin/xfce4-xmms-plugin.desktop.in.in.h:1
msgid "XMMS plugin for Xfce 4.4 panel"
msgstr "ཨེགསི་ཨེཕ་སི་ཨི་ ༤.༤པེ་ནཱལ་དོན་ལུ་ཨེགསི་ཨེམ་ཨེམ་ཨེསི་པ་ལག་ཨིན་"

#: ../panel-plugin/xfce4-xmms-plugin.desktop.in.in.h:2
msgid "Xfce4 XMMS Plugin"
msgstr "ཨེགསི་ཨེཕ་སི་ཨི་༤་ ཨེགསི་ཨེམ་ཨེམ་ཨེསི་པ་ལག་ཨིན་"

