#: ../panel-plugin/xfce4-mount-plugin.desktop.in.in.h:1
msgid "Mount devices"
msgstr "ཐབས་འཕྲུལ་ཚུ་སྦྱར་རྩེགས་འབད་"

#: ../panel-plugin/mount-plugin.c:686
msgid "This is only useful and recommended if you specify \"sync\" as part of the \"unmount\" command string."
msgstr "ཁྱོད་ཀྱིས་ \"sync\" འདི་ \"unmount\" བརྡ་བཀོད་ཡིག་རྒྱུན་གྱི་ཆ་ཤས་ཅིག་སྦེ་གསལ་བཀོད་འབད་བ་ཅིན་རྐྱངམ་ཅིག་  འདི་གལ་ཅན་དང་འོས་སྦྱོར་འབདཝ་ཨིན།"

#: ../panel-plugin/mount-plugin.c:691
msgid "Show _message after unmount"
msgstr "སྦྱར་རྩེགས་མ་འབད་ཤུལ་ལས་ འཕྲིན་དོན་སྟོན་ (_m)"

#: ../panel-plugin/mount-plugin.c:703
msgid "You can specify a distinct icon to be displayed in the panel."
msgstr "པེ་ནཱལ་ནང་ལུ་བཀྲམ་སྟོན་འབད་ནི་ལུ་  ཁྱོད་ཀྱིས་ཁྱད་རྟགས་ཅན་གྱི་ངོས་དཔར་གཅིག་གསལ་བཀོད་འབད་ཚུགས།"

#: ../panel-plugin/mount-plugin.c:710
msgid "Icon:"
msgstr "ངོས་དཔར:"

#: ../panel-plugin/mount-plugin.c:714
msgid "Select an image"
msgstr "གཟུགས་བརྙན་ཅིག་སེལ་འཐུ་འབད་"

#: ../panel-plugin/mount-plugin.c:721
msgid "_General"
msgstr "ཡོངས་ཁྱབ་ (_G)"

#: ../panel-plugin/mount-plugin.c:736
#, c-format
msgid ""
"This command will be executed after mounting the device with the mount point of the device as argument.\n"
"If you are unsure what to insert, try \"thunar \\%m\".\n"
"'\\%d' can be used to specify the device, '\\%m' for the mountpoint."
msgstr ""
"ཐབས་འཕྲུལ་འདི་སྒྲུབ་རྟགས་སྦེ་ཨིན་པའི་སྦྱར་རྩེགས་དང་ཅིག་ཁར་  ཐབས་འཕྲུལ་འདི་སྦྱར་རྩེགས་འབད་བའི་ཤུལ་ལུ་  བརྡ་བཀོད་འདི་ལག་ལེན་འཐབ་ནི་ཨིན།\n"
"ག་ཅི་བཙུགས་ནི་ཨིན་ན་ཁྱོད་ངེས་གཏན་མེད་པ་ཅིན་ \"thunar \\%m\" འབད་རྩོལ་བསྐྱེད་གནང་།\n"
"'\\%d' འདི་ ཐབས་འཕྲུལ་ '\\%m' སྦྱར་རྩེགས་འབད་སའི་དོན་ལུ་ གསལ་བཀོད་འབད་ནི་ལུ་ལག་ལེན་འཐབ་བཏུབ་ཨིན།"

#: ../panel-plugin/mount-plugin.c:746
msgid "_Execute after mounting:"
msgstr "སྦྱར་རྩེགས་འབད་བའི་ཤུལ་ལུ་ལག་ལེན་འཐབ: (_E)"

#: ../panel-plugin/mount-plugin.c:769
msgid "WARNING: These options are for experts only! If you do not know what they may be good for, keep your hands off!"
msgstr "WARNING: གདམ་ཁ་དེ་ཚུ་ཁྱད་འཕགས་ཚུ་གི་དོན་ལུ་རྐྱངམ་ཅིག་ཨིན! ཁྱོད་ཀྱིས་དེ་ཚུ་ག་ཅི་གི་དོན་ལུ་ལེགས་ཤོམ་ཡོདཔ་ཨིན་ན་མ་ཤེས་པ་ཅིན་  ཁྱོད་རང་མ་དཀྲོགས་པར་བཞག!"

#: ../panel-plugin/mount-plugin.c:774
msgid "_Custom commands"
msgstr "སྲོལ་སྒྲིག་བརྡ་བཀོད་ཚུ་ (_C)"

#: ../panel-plugin/mount-plugin.c:793
#, c-format
msgid ""
"Most users will only want to prepend \"sudo\" to both commands or prepend \"sync \\%d &&\" to the \"unmount \\%d\" command.\n"
"'\\%d' is used to specify the device, '\\%m' for the mountpoint."
msgstr ""
"ལག་ལེན་པ་མང་ཤོས་ཅིག་གི་རང་ \"sudo\" འདི་བརྡ་བཀོད་གཉིས་ཆ་རང་ལུ་པིརི་པེནཌི་འབད་དགོ་ནི་དང་  ཡང་ན་\"sync \\%d &&\" འདི་ \"unmount \\%d\" བརྡ་བཀོད་ལུ་འབདཝ་ཨིན།\n"
"'\\%d' འདི་ཐབས་འཕྲུལ་'\\%m' སྦྱར་རྩེགས་འབད་ས་འདི་གསལ་བཀོད་འབད་ནི་ལུ་ལག་ལེན་འཐབ་ཨིན།"

#: ../panel-plugin/mount-plugin.c:802
msgid "_Mount command:"
msgstr "བརྡ་བཀོད་སྦྱར་རྩེགས་འབད: (_M)"

#: ../panel-plugin/mount-plugin.c:808
msgid "_Unmount command:"
msgstr "བརྡ་བཀོད་སྦྱར་རྩེགས་མ་འབད: (_U)"

#: ../panel-plugin/mount-plugin.c:834
msgid "_Commands"
msgstr "བརྡ་བཀོད་ཚུ་ (_C)"

#: ../panel-plugin/mount-plugin.c:849
msgid "Activate this option to also display network file systems like NFS, SMBFS, SHFS and SSHFS."
msgstr "NFS, SMBFS, SHFS དང་ SSHFSབཟུམ་མའི་ཡོངས་འབྲེལ་རིམ་ལུགས་ཚུ་བཀྲམ་སྟོན་འབད་ནི་ལུ་ཡང་  གདམ་ཁ་འདི་ཤུགས་ལྡན་བཟོ།"

#: ../panel-plugin/mount-plugin.c:854
msgid "Display _network file systems"
msgstr "ཡོངས་འབྲེལ་ཡིག་སྣོད་རིམ་ལུགས་ཚུ་བཀྲམ་སྟོན་འབད་ (_n)"

#: ../panel-plugin/mount-plugin.c:867
msgid "Activate this option to also eject a CD-drive after unmounting and to insert before mounting."
msgstr "སྦྱར་རྩེགས་འབད་བའི་ཤུལ་ལུ་དང་སྦྱར་རྩེགས་མ་འབད་བའི་ཧེ་མར་བཙུགས་པའི་བསྒང་  སི་ཌི་-འདྲེན་འཕྲུལ་གཅིག་བཏོན་ནི་ལུ་ཡང་  གདམ་ཁ་འདི་ཤུགས་ལྡན་བཟོ།"

#: ../panel-plugin/mount-plugin.c:872
msgid "_Eject CD-drives"
msgstr "སི་ཊི་-འདྲེན་འཕྲུལ་ཚུ་བཏོན་ (_E)"

#: ../panel-plugin/mount-plugin.c:885
msgid ""
"Exclude the following file systems from the menu.\n"
"The list is separated by simple spaces.\n"
"It is up to you to specify correct devices or mount points."
msgstr ""
"དཀར་ཆག་ལས་འོག་གི་ཡིག་སྣོད་རིམ་ལུགས་འདི་གྲངས་ལས་བཏོན་གཏང་།\n"
"ཐོ་ཡིག་འདི་བར་སྟོང་འཇམ་སམ་འདི་གིས་ཁ་འཕྱལཝ་ཨིན།\n"
"ཐབས་འཕྲུལ་བདེན་པ་གསལ་བཀོད་འབད་ནི་ཨིན་ན་  ཡང་ན་  སྦྱར་རྩེགས་འབད་ས་གསལ་བཀོད་འབད་ནི་ཨིན་ན་འདི་ཁྱོད་ཀྱི་ལག་པར་ཨིན།"

#: ../panel-plugin/mount-plugin.c:895
msgid "E_xclude specified file systems"
msgstr "གསལ་བཀོད་འབད་དེ་ཡོད་པའི་ཡིག་སྣོད་རིམ་ལུགས་འདི་གྲངས་ལས་བཏོན་གཏང་ (_x)"

#: ../panel-plugin/mount-plugin.c:910
msgid "_File sytems"
msgstr "ཡིག་སྣོད་རིམ་ལུགས་ (_F)"

#: ../panel-plugin/xfce4-mount-plugin.desktop.in.in.h:2
msgid "Shows all mountable devices and (un)mounts them on request."
msgstr "སྦྱར་རྩེགས་འབད་བཏུབ་པའི་ཐབས་འཕྲུལ་ཚུ་ཆ་ཚང་སྟོནམ་ཨིནམ་དང་ དེ་ཚུ་(un)ཞུ་བ་འབདཝ་ད་སྦྱར་རྩེགས་འབད་བཤོལ་འབདཝ་ཨིན།"

