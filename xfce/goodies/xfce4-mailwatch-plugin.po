msgstr "འ་ནི་ཡིག་སྒྲོམ་གྱི་དབྱེ་བ་འདི་ལུ་ རིམ་སྒྲིག་གི་སྒྲིག་སྟངས་ག་ནི་ཡང་མི་དགོ།"

#: ../libmailwatch-core/mailwatch.c:647
msgid "Mailbox _Name:"
msgstr "ཡིག་སྒྲོམ་གྱི་མིང་:(_N)"

#: ../libmailwatch-core/mailwatch.c:664
msgid "Mailwatch"
msgstr "མེལ་ཝཅི་"

#: ../libmailwatch-core/mailwatch.c:665
msgid "Mailbox name required."
msgstr "ཡིག་སྒྲོམ་གི་མིང་དགོཔ་ཨིན།"

#: ../libmailwatch-core/mailwatch.c:666
msgid "Please enter a name for the mailbox."
msgstr "ཡིག་སྒྲོམ་འདི་གི་དོན་ལུ་ མིང་ཅིག་ཐོ་བཀོད་འབད་གནང་།"

#: ../libmailwatch-core/mailwatch.c:709
#, c-format
msgid "Edit Mailbox: %s"
msgstr "%s: ཡིག་སྒྲོམ་ཞུན་དག་འབད་"

#: ../libmailwatch-core/mailwatch.c:772
msgid "Select Mailbox Type"
msgstr "ཡིག་སྒྲོམ་དབྱེ་བ་སེལ་འཐུ་འབད་"

#: ../libmailwatch-core/mailwatch.c:781
msgid "Select a mailbox type.  A description of the type will appear below."
msgstr "ཡིག་སྒྲོམ་དབྱེ་བ་ཅིག་སེལ་འཐུ་འབད། དབྱེ་བ་འདི་གི་འགྲེལ་བཤད་ཅིག་འོག་ལུ་འབྱུང་འོང་།"

#: ../libmailwatch-core/mailwatch.c:837
msgid "Add New Mailbox"
msgstr "ཡིག་སྒྲོམ་གསརཔ་ཁ་སྐོང་རྐྱབས་"

#: ../libmailwatch-core/mailwatch.c:900
msgid "Remove Mailbox"
msgstr "ཡིག་སྒྲོམ་རྩ་བསྐྲད་གཏང་"

#: ../libmailwatch-core/mailwatch.c:901
msgid "Are you sure?"
msgstr "ཁྱོད་ངེས་གཏན་ཨིན་ན?"

#: ../libmailwatch-core/mailwatch.c:902
msgid "Removing a mailbox will discard all settings, and cannot be undone."
msgstr "ཡིག་སྒྲོམ་རྩ་བསྐྲད་གཏང་ནི་འདི་གིས་ སྒྲིག་སྟངས་ཚུ་ཆ་མཉམ་བཏོན་བཀོག་འོང་ དེ་ལས་ འབད་བཤོལ་འབད་མི་བཏུབ།"

#: ../libmailwatch-core/mailwatch.c:973
msgid "Mailboxes"
msgstr "ཡིག་སྒྲོམ་ཚུ་"

#: ../panel-plugin/mailwatch.desktop.in.in.h:1
msgid "Check mail from multiple mailboxes"
msgstr "སྣ་མང་ཡིག་སྒྲོམ་ཚུ་ལས་ ཡིག་འཕྲིན་ཞིབ་དཔྱད་འབད་"

