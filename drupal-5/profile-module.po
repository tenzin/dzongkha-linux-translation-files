msgstr "རྩོམ་པ་པོའི་བརྡ་དོན།"

#: modules/profile/profile.module:116
msgid "Link to full user profile"
msgstr "གསལ་སྡུད་ལག་ལེན་པ་ཆ་ཚང་ལུ་འབྲེལ་མཐུད་འབད།"

#: modules/profile/profile.module:118
msgid "Profile fields to display"
msgstr "བཀྲམ་སྟོན་འབད་ནི་ཨིན་པའི་གསལ་སྡུད་ས་སྒོ་ཚུ།"

#: modules/profile/profile.module:121
msgid "Select which profile fields you wish to display in the block. Only fields designated as public in the <a href=\"@profile-admin\">profile field configuration</a> are available."
msgstr "སྡེབ་ཚན་ནང་ལུ་  གསལ་སྡུད་ས་སྒོ་ཚུ་ག་འདི་བཀྲམ་སྟོན་འབད་ནི་ཨིན་ན་འདི་  ཁྱོད་ཀྱིས་སེལ་འཐུ་འབད།  <a href=\"@profile-admin\">གསལ་སྡུད་ས་སྒོའི་རིམ་སྒྲིག་</a>ནང་ལུ་  མི་མང་འབད་ངོས་འཛིན་འབད་དེ་ཡོད་མི་གི་  ས་སྒོ་ཚུ་རྐྱངམ་ཅིག་  འཐོབ་ཚུགསཔ་ཡོད།"

#: modules/profile/profile.module:152
msgid "View full user profile"
msgstr "གསལ་སྡུད་ལག་ལེན་པ་ཆ་ཚང་སྟོན།"

#: modules/profile/profile.module:157
msgid "About %name"
msgstr "%nameསྐོར་ལས།"

#: modules/profile/profile.module:204
msgid "edit %title"
msgstr "%titleའདི་ཞུན་དག་འབད།"

#: modules/profile/profile.module:222
msgid "add new %type"
msgstr " %typeགསརཔ་འདི་ཁ་སྐོང་བརྐྱབ།"

#: modules/profile/profile.module:227
msgid "Field settings"
msgstr "ས་སྒོ་སྒྲིག་སྟངས་ཚུ།"

#: modules/profile/profile.module:233
msgid "The category the new field should be part of. Categories are used to group fields logically. An example category is \"Personal information\"."
msgstr "དབྱེ་རིམ་དང་ས་སྒོ་གསརཔ་འདི་ཚུ་ ཁ་བྲལ་དགོ།  དབྱེ་རིམ་ཚུ་  ས་སྒོ་ཚུ་གཏན་ཚིག་ཅན་གྱི་ཐོག་ལུ་འབད་སྡེ་ཚན་བཟོ་ནི་དོན་ལུ་ ལག་ལེན་འཐབ་ཨིན།  དབྱེ་རིམ་གྱི་དཔེ་འདི་  \"Personal information\"ཨིན།"

#: modules/profile/profile.module:239
msgid "The title of the new field. The title will be shown to the user. An example title is \"Favorite color\"."
msgstr "ས་སྒོ་གསརཔ་གི་མགོ་མིང་།  མགོ་མིང་འདི་ལག་ལེན་པ་ལུ་སྟོནམ་འོང་།  དེ་གི་དཔེ་འདི་  \"Favorite color\" ཨིན།"

#: modules/profile/profile.module:243
msgid "Form name"
msgstr "འབྲི་ཤོག་མིང་།"

#: modules/profile/profile.module:245
msgid ""
"The name of the field. The form name is not shown to the user but used internally in the HTML code and URLs.\n"
"Unless you know what you are doing, it is highly recommended that you prefix the form name with <code>profile_</code> to avoid name clashes with other fields. Spaces or any other special characters except dash (-) and underscore (_) are not allowed. An example name is \"profile_favorite_color\" or perhaps just \"profile_color\"."
msgstr ""
"ས་སྒོ་གི་མིང་།  འབྲི་ཤོག་གི་མིང་འདི་ལག་ལེན་པ་ལུ་མ་སྟོན་མས་  དེ་འབདཝ་ད་  ནང་ན་ལས་  ཨེཆ་ཊི་ཨེམ་ཨེལ་ཨང་རྟགས་དང་ ཡུ་ཨར་ཨེལ་ཨེསི་ནང་ལུ་ལག་ལེན་འཐབ་ཨིན།\n"
"ཁྱོད་ཀྱིས་ཁྱོད་རང་ག་ཅི་འབདཝ་ཨིན་མ་ཤེས་ཚུན་  ས་སྒོ་གཞན་མི་ཚུ་དང་ཅིག་ཁར་མིང་ཁ་འཐབ་བརྐྱབ་ནི་འདི་བཀག་ནིའི་དོན་ལུ་  ཁྱོད་ཀྱིས་  འབྲི་ཤོག་མིང་འདི་  <code>གསལ་སྡུད་_</code> དང་བཅས་སྔོན་ཚིག་བཟོ་དགོ་ཟེར་  འོས་སྦྱོར་འབད་ཡོད།"

#: modules/profile/profile.module:250
msgid "Explanation"
msgstr "གསལ་བཤད།"

#: modules/profile/profile.module:252
msgid "An optional explanation to go with the new field. The explanation will be shown to the user."
msgstr "ས་སྒོ་གསརཔ་དང་ཅིག་ཁར་འགྱོ་ནི་ལུ་  གདམ་ཁ་ཅན་གྱི་གསལ་བཤད།  གསལ་བཤད་འདི་ལག་ལེན་པ་ལུ་ སྟོན་འོང་།"

#: modules/profile/profile.module:256
msgid "Selection options"
msgstr "སེལ་འཐུ་གི་གདམ་ཁ་ཚུ།"

#: modules/profile/profile.module:258
msgid "A list of all options. Put each option on a separate line. Example options are \"red\", \"blue\", \"green\", etc."
msgstr "གདམ་ཁ་ཚུ་ཆ་མཉམ་གྱི་ཐོ་ཡིག།  གདམ་ཁ་རེ་ལུ་  གྲལ་ཐིག་སོ་སོ་ཅིག་བཀོད།  དཔེ་གི་གདམ་ཁ་ཚུ་  \"red\", \"blue\", \"green\", ལ་སོགས་པ་ཚུ་ཨིན།"

#: modules/profile/profile.module:265
msgid "The weights define the order in which the form fields are shown. Lighter fields \"float up\" towards the top of the category."
msgstr "འབྲི་ཤོག་གི་ས་སྒོ་ཚུ་སྟོན་ཡོད་པའི་ནང་  ལྗིད་ཚད་དེ་གིས་ རིམ་པ་འདི་ངོས་འཛིན་འབདཝ་ཨིན།  དབྱེ་རིམ་གྱི་མགོ་ལུ་  ཡང་བའི་ས་སྒོ་ \"float up\" ཚུ།"

#: modules/profile/profile.module:268
msgid "Visibility"
msgstr "མཐོང་གསལ།"

#: modules/profile/profile.module:270
msgid "Hidden profile field, only accessible by administrators, modules and themes."
msgstr "སྦ་དེ་ཡོད་པའི་ས་སྒོ་འདི་  བདག་སྐྱོང་པ་ཚུ་དང་  ཚད་གཞི་  དེ་ལས་  བརྗོད་དོན་ཚུ་གིས་རྐྱངམ་ཅིག་ འཛུལ་སྤྱོད་འབད་ཚུགས།"

#: modules/profile/profile.module:270
msgid "Private field, content only available to privileged users."
msgstr "སྒེར་དོན་ས་སྒོ་གི་ནང་དོན་རྐྱངམ་ཅིག་  ཁེ་དབང་ཅན་གྱི་ལག་ལེན་ལུ་འཐོབ་ཚུགས།"

#: modules/profile/profile.module:270
msgid "Public field, content shown on profile page but not used on member list pages."
msgstr "མི་མང་ས་སྒོའི་ནང་དོན་འདི་  གསལ་སྡུད་ཤོག་ལེབ་གུ་སྟོན་ཡོད་  དེ་འབདཝ་ད་  འཐུས་མི་ཐོ་ཡིག་ཚུ་གི་ཤོག་ལེབ་གུ་ལག་ལེན་འཐམ་མི་མེད།"

#: modules/profile/profile.module:270
msgid "Public field, content shown on profile page and on member list pages."
msgstr "མི་མང་ས་སྒོའི་ནང་དོན་འདི་  གསལ་སྡུད་ཤོག་ལེབ་དང་ འཐུས་མི་ཐོ་ཡིག་ཤོག་ལེབ་གུ་སྟོན་ཡོད།"

#: modules/profile/profile.module:274;281
msgid "Page title"
msgstr "ཤོག་ལེབ་ཀྱི་མགོ་མིང་།"

#: modules/profile/profile.module:276
msgid "To enable browsing this field by value, enter a title for the resulting page. The word <code>%value</code> will be substituted with the corresponding value. An example page title is \"People whose favorite color is %value\". This is only applicable for a public field."
msgstr "གནས་གོང་གིས་ས་སྒོ་གི་བརྡ་འཚོལ་འབད་ནི་འདི་ལྕོགས་ཅན་བཟོ་ནི་ལུ་  ཤོག་ལེབ་གྲུབ་འབྲས་ཀྱི་དོན་ལུ་  མགོ་མིང་ཅིག་ཐོ་བཀོད་འབད།  <code>%value</code>མིང་ཚིག་འདི་  ཆ་མཉམ་པའི་གནས་གོང་དང་ཅིག་ཁར་ ཚབ་བཙུགས་འོང་།  ཤོག་ལེབ་མགོ་མིང་གི་དཔེ་འདི་   \"People whose favorite color is %value\"ཨིན།  འདི་ མི་མང་ས་སྒོ་ཅིག་ལུ་རྐྱངམ་ཅིག་འཇུག་སྤྱོད་འབད་བཏུབ་ཨིན།"

#: modules/profile/profile.module:283
msgid "To enable browsing this field by value, enter a title for the resulting page. An example page title is \"People who are employed\". This is only applicable for a public field."
msgstr "ས་སྒོ་འདི་  གནས་གོང་དེ་གིས་ བརྡ་འཚོལ་འབད་ནི་འདི་ལྕོགས་ཅན་བཟོ་ནི་ལུ་  ཤོག་ལེབ་གྲུབ་འབྲས་ཀྱི་དོན་ལུ་  མགོ་མིང་ཅིག་ཐོ་བཀོད་འབད།  ཤོག་ལེབ་མགོ་མིང་གི་དཔེ་འདི་  \"People who are employed\" ཨིན།  འདི་  མི་མང་ས་སྒོ་ཅིག་ལུ་མཇུག་སྤྱོད་འབད་བཏུབ་ཨིན།"

#: modules/profile/profile.module:287
msgid "Form will auto-complete while user is typing."
msgstr "ལག་ལེན་པ་འདི་ཡིག་དཔར་བརྐྱབ་ད་  རྣམ་པ་དེ་རང་བཞིན་-མཇུག་བསྡུ་ཨིནམ་འོང་།"

#: modules/profile/profile.module:291
msgid "The user must enter a value."
msgstr "ལག་ལེན་པ་འདི་གིས་ གནས་གོང་ཅིག་ཐོ་བཀོད་འབད་དགོ།"

#: modules/profile/profile.module:295
msgid "Visible in user registration form."
msgstr "ལག་ལེན་པའི་ཐོ་འགོད་འབྲི་ཤོག་ནང་ལུ་མཐོང་ཚུགས།"

#: modules/profile/profile.module:299
msgid "Save field"
msgstr "ས་སྒོ་འདི་སྲུངས།"

#: modules/profile/profile.module:310
msgid "The specified form name contains one or more illegal characters. Spaces or any other special characters expect dash (-) and underscore (_) are not allowed."
msgstr "གསལ་བཀོད་འབད་དེ་ཡོད་པའི་འབྲི་ཤོག་དེ་ལུ་  ཁྲིམས་འགལ་ཡིག་འབྲུ་གཅིག་ཡང་ན་མངམ་གནས་དེ་ཡོདཔ་ཨིན།  བར་སྟོང་ཡང་ན་  དམིགས་བསལ་གྱི་ཡིག་འབྲུ་ཚུ་  སྦྲེལ་རྟགས་(-) དང་ འོག་ཐིག་ (_) དེ་ཚུ་ མི་ཆོག"

#: modules/profile/profile.module:314
msgid "The specified form name is reserved for use by Drupal."
msgstr "གསལ་བཀོད་འབད་དེ་ཡོད་པའི་འབྲི་ཤོག་མིང་འདི་  ལག་ལེན་འཐབ་ནི་དོན་ལུ་ ཌུ་པཱལ་གྱིས་ཟུར་གསོག་འབད་ཡོད།"

#: modules/profile/profile.module:321
msgid "The specified category name is reserved for use by Drupal."
msgstr "གསལ་བཀོད་འབད་དེ་ཡོད་པའི་དབྱེ་རིམ་མིང་འདི་  ལག་ལེན་འཐབ་ནི་དོན་ལུ་  ཌུ་པཱལ་གྱིས་ཟུར་གསོག་འབད་ཡོད།"

#: modules/profile/profile.module:333
