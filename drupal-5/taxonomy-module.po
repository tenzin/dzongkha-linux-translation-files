#: modules/legacy/legacy.module:42;47
#: modules/taxonomy/taxonomy.info:0
msgid "Taxonomy"
msgstr "དབྱེ་བསལ་བྱ་རིམ་"

#: modules/taxonomy/taxonomy.module:67
msgid "Create vocabularies and terms to categorize your content."
msgstr "ཁྱོད་རའི་ནང་དོན་གྱི་དབྱེ་རིམ་བཟོ་ནི་ལུ་  མིང་ཚིག་དང་ཐ་སྙད་ཚུ་གསར་བསྐྲུན་འབད།"

#: modules/taxonomy/taxonomy.module:77
msgid "Add vocabulary"
msgstr "མིང་ཚིག་ཁ་སྐོང་རྐྱབས་"

#: modules/taxonomy/taxonomy.module:84
msgid "Edit vocabulary"
msgstr "མིང་ཚིག་ཞུན་དག་རྐྱབས་"

#: modules/taxonomy/taxonomy.module:90
msgid "Edit term"
msgstr "ཐ་སྙད་ཞུན་དག་རྐྱབས་"

#: modules/taxonomy/taxonomy.module:96
msgid "Taxonomy term"
msgstr "དབྱེ་བསལ་བྱ་རིམ་གྱི་ཐ་སྙད་"

#: modules/taxonomy/taxonomy.module:102
msgid "Autocomplete taxonomy"
msgstr "རང་བཞིན་ཡོངས་བསྒྲུབས་དབྱེ་བསལ་བྱ་རིམ་"

#: modules/taxonomy/taxonomy.module:111
msgid "List terms"
msgstr "ཐ་སྙད་ཚུ་ཐོ་བཀོད་འབད་"

#: modules/taxonomy/taxonomy.module:123
msgid "Add term"
msgstr "ཐ་སྙད་ཁ་སྐོང་རྐྱབས་"

#: modules/taxonomy/taxonomy.module:148
msgid "edit vocabulary"
msgstr "མིང་ཚིག་གི་ཐོ་ཞུན་དག་རྐྱབས།"

#: modules/taxonomy/taxonomy.module:149
msgid "list terms"
msgstr "ཐ་སྙད་ཚུ་ཐོ་བཀོད་འབད།"

#: modules/taxonomy/taxonomy.module:150
msgid "add terms"
msgstr "ཐ་སྙད་ཚུ་ཁ་སྐོང་རྐྱབས།"

#: modules/taxonomy/taxonomy.module:154
msgid "No categories available."
msgstr "དབྱེ་རིམ་ཚུ་ཐོབ་མི་ཚུགས།"

#: modules/taxonomy/taxonomy.module:189
msgid "No terms available."
msgstr "ཐ་སྙད་ཚུ་ཐོབ་མི་ཚུགས།"

#: modules/taxonomy/taxonomy.module:207
msgid "Vocabulary name"
msgstr "མིང་ཚིག་གི་ཐོའི་མིང་།"

#: modules/taxonomy/taxonomy.module:210
msgid "The name for this vocabulary. Example: \"Topic\"."
msgstr "མིང་ཚིག་འདི་གི་དོན་ལུ་མིང་། དཔེར་ན་: \"Topic\"."

#: modules/taxonomy/taxonomy.module:216
msgid "Description of the vocabulary; can be used by modules."
msgstr "མིང་ཚིག་གི་ཐོའི་འགྲེལ་བཤད། ཚད་གཞི་ཚུ་གིས་ལག་ལེན་འཐབ་ཚུགས།"

#: modules/taxonomy/taxonomy.module:219
msgid "Help text"
msgstr "གྲོགས་རམ་ཚིག་ཡིག།"

#: modules/taxonomy/taxonomy.module:221
msgid "Instructions to present to the user when choosing a term."
msgstr "ཐ་སྙད་གདམ་ཁ་བརྐྱབ་པའི་སྐབས་ལག་ལེན་པ་ལུ་སྤྲོད་ནིའི་བསླབ་སྟོན་ཚུ།"

#: modules/taxonomy/taxonomy.module:224
msgid "Types"
msgstr "དབྱེ་བ་ཚུ།"

#: modules/taxonomy/taxonomy.module:227
msgid "A list of node types you want to associate with this vocabulary."
msgstr "མིང་ཚིག་གི་ཐོ་འདི་དང་གཅིག་ཁར་ཁྱོད་ཀྱིས་འབྲེལ་བ་འབད་ནི་ཨིན་མི་ནོཌི་དབྱེ་བ་ཚུའི་ཐོ་ཡིག།"

#: modules/taxonomy/taxonomy.module:231
msgid "Hierarchy"
msgstr "སྡེ་རིམ།"

#: modules/taxonomy/taxonomy.module:233
msgid "Single"
msgstr "རྐྱང་པ།"

#: modules/taxonomy/taxonomy.module:233
msgid "Multiple"
msgstr "སྣ་མང་།"

#: modules/taxonomy/taxonomy.module:234
msgid "Allows <a href=\"@help-url\">a tree-like hierarchy</a> between terms of this vocabulary."
msgstr "མིང་ཚིག་ཐོ་ཡིག་འདི་གི་ཐ་སྙད་ཚུའི་བར་ན་<a href=\"@help-url\">སྡེ་རིམ་བཟུམ་-རྩ་འབྲེལ་</a> འབད་བཅུགཔ་ཨིན།"

#: modules/taxonomy/taxonomy.module:237;382
msgid "Related terms"
msgstr "འབྲེལ་བ་ཡོད་པའི་ཐ་སྙད་ཚུ།"

#: modules/taxonomy/taxonomy.module:239
msgid "Allows <a href=\"@help-url\">related terms</a> in this vocabulary."
msgstr "མིང་ཚིག་གི་ཐོ་འདི་ནང་ <a href=\"@help-url\">འབྲེལ་བ་ཡོད་པའི་ཐ་སྙད་ཚུ་</a> འབད་བཅུགཔ་ཨིན།"

#: modules/taxonomy/taxonomy.module:242
msgid "Free tagging"
msgstr "རང་དབང་གི་ངོ་རྟགས་སྦྱར་ནི།"

#: modules/taxonomy/taxonomy.module:244
msgid "Content is categorized by typing terms instead of choosing from a list."
msgstr "ནང་དོན་འདི་ ཐོ་ཡིག་ལས་གདམ་ཁ་བརྐྱབ་ནིའི་ཚབ་ལུ་ ཐ་སྙད་ཚུ་ཡིག་དཔར་བརྐྱབ་ཐོག་ལས་དབྱེ་རིམ་བཟོཝ་ཨིན།"

#: modules/taxonomy/taxonomy.module:247
msgid "Multiple select"
msgstr "སྣ་མང་སེལ་འཐུ།"

#: modules/taxonomy/taxonomy.module:249
msgid "Allows nodes to have more than one term from this vocabulary (always true for free tagging)."
msgstr "ནོཌོསི་ལུ་མིང་ཚིག་གི་ཐོ་འདི་ལས་ཐ་སྙད་གཅིག་ལས་ལྷག་སྟེ་བཞག་བཅུགཔ་ཨིན་  (རང་དབང་ངོ་རྟགས་སྦྱར་ནིའི་དོན་ལུ་ཨ་རྟག་རང་བདེན་)."

#: modules/taxonomy/taxonomy.module:254
msgid "If enabled, every node <strong>must</strong> have at least one term in this vocabulary."
msgstr "ལྕོགས་ཅན་ཨིན་པ་ཅིན་ <strong>མིང་ཚིག་གི་ཐོ་འདི་ནང་</strong> ནོཌོ་རེ་རེ་ལུ་ཐ་སྙད་གཅིག་ཙམ་ངེས་པར་འོང་དགོ།"

#: modules/taxonomy/taxonomy.module:259
msgid "In listings, the heavier vocabularies will sink and the lighter vocabularies will be positioned nearer the top."
msgstr "ཐོ་ཡིག་ཚུ་ནང་ ལྗིད་ཅན་མིང་ཚིག་གི་ཐོ་ཚུ་འཛུལ་ནི་དང་ལྗིད་ཡང་བའི་མིང་ཚིག་གི་ཐོ་ཚུ་མགོའི་སྦོ་ལོག་ཁར་བཞག་འོང་།"

#: modules/taxonomy/taxonomy.module:279
msgid "Created new vocabulary %name."
msgstr "གསར་བསྐྲུན་འབད་ཡོད་པའི་མིང་ཚིག་གི་ཐོ་གསརཔ་%name."

#: modules/taxonomy/taxonomy.module:282
msgid "Updated vocabulary %name."
msgstr "དུས་མཐུན་བཟོ་ཡོད་པའི་མིང་ཚིག་གི་ཐོ་ %name།"

#: modules/taxonomy/taxonomy.module:342
msgid "Are you sure you want to delete the vocabulary %title?"
msgstr "ཁྱོད་ཀྱིས་མིང་ཚིག་གི་ཐོ་ %title རྩ་བསྐྲད་གཏང་ནི་འདི་ངེས་ཏིག་ཨིན་ན?"

#: modules/taxonomy/taxonomy.module:345
msgid "Deleting a vocabulary will delete all the terms in it. This action cannot be undone."
msgstr "མིང་ཚིག་གི་ཐོ་བཏོན་གཏང་ནི་འདི་གིས་དེ་ནང་ཐ་སྙད་ཚུ་ཆ་མཉམ་བཏོན་གཏང་འོང་། བྱ་བ་འདི་འབད་བཤོལ་འབད་མི་བཏུབ།"

#: modules/taxonomy/taxonomy.module:352
msgid "Deleted vocabulary %name."
msgstr "བཏོན་གཏང་ཡོད་པའི་མིང་ཚིག་གི་ཐོ་%name."

#: modules/taxonomy/taxonomy.module:359
msgid "Term name"
msgstr "ཐ་སྙད་ཀྱི་མིང་།"

#: modules/taxonomy/taxonomy.module:359
msgid "The name for this term. Example: \"Linux\"."
msgstr "ཐ་སྙད་འདི་གི་དོན་ལུ་མིང་།  དཔེར་ན་: \"Linux\""

#: modules/taxonomy/taxonomy.module:361
msgid "A description of the term."
msgstr "ཐ་སྙད་ཀྱི་འགྲེལ་བཤད།"

#: modules/taxonomy/taxonomy.module:374
msgid "Parent term"
msgstr "རྩ་ལག་ཐ་སྙད།"

#: modules/taxonomy/taxonomy.module:377
msgid "Parents"
msgstr "རྩ་ལག་ཚུ།"

#: modules/taxonomy/taxonomy.module:377
msgid "Parent terms"
msgstr "རྩ་ལག་ཐ་སྙད་ཚུ།"

#: modules/taxonomy/taxonomy.module:385
msgid "Synonyms"
msgstr "མིང་གི་རྣམ་གྲངས་ཚུ།"

#: modules/taxonomy/taxonomy.module:385
msgid "<a href=\"@help-url\">Synonyms</a> of this term, one synonym per line."
msgstr "<a href=\"@help-url\">ཐ་སྙད་འདི་གི་མིང་གི་རྣམ་གྲངས་ཚུ་</a> གྲལ་ཐིག་རེ་ལུ་མིང་གི་རྣམ་གྲངས་གཅིག།"

#: modules/taxonomy/taxonomy.module:386
msgid "In listings, the heavier terms will sink and the lighter terms will be positioned nearer the top."
msgstr "ཐོ་ཡིག་ཚུ་ནང་ ལྗིད་ཅན་ཐ་སྙད་ཚུ་འཛུལ་ནི་དང་ལྗིད་ཡང་བའི་ཐ་སྙད་ཚུ་མཀོའི་སྦོ་ལོག་ཁར་བཞག་འོང་།"

#: modules/taxonomy/taxonomy.module:407
msgid "Created new term %term."
msgstr "གསར་བསྐྲུན་འབད་ཡོད་པའི་ཐ་སྙད་གསརཔ་ %term."

#: modules/taxonomy/taxonomy.module:410
msgid "The term %term has been updated."
msgstr "ཐ་སྙད་ %term འདི་དུས་མཐུན་བཟོ་ཡོདཔ།"

#: modules/taxonomy/taxonomy.module:521
msgid "Are you sure you want to delete the term %title?"
msgstr "ཁྱོད་ཀྱིས་ཐ་སྙད་ %title བཏོན་གཏང་ནི་འདི་ངེས་ཏིག་ཨིན་ན?"

#: modules/taxonomy/taxonomy.module:524
msgid "Deleting a term will delete all its children if there are any. This action cannot be undone."
msgstr "ཐ་སྙད་བཏོན་གཏང་ནི་འདི་གིས་ དེའི་ཆ་ལག་ཚུ་གང་རུང་ཡོད་པ་ཅིན་ཆ་མཉམ་བཏོན་གཏང་འོང་། བྱ་བ་འདི་འབད་བཤོལ་འབད་མི་བཏུབ།"

#: modules/taxonomy/taxonomy.module:531
msgid "Deleted term %name."
msgstr "བཏོན་གཏང་ཡོད་པའི་ཐ་སྙད་ %name."

#: modules/taxonomy/taxonomy.module:639
msgid "A comma-separated list of terms describing this content. Example: funny, bungee jumping, \"Company, Inc.\"."
msgstr "ལྷོད་རྟགས་-ནང་དོན་འདི་འགྲེལ་བཤད་བརྐྱབ་ནིའི་  ཐ་སྙད་ཚུ་གི་ཐོ་ཡིག་སོ་སོ་འཕྱལ་འཕྱལཝ། དཔེར་ན་: funny, bungee jumping, \"Company, Inc.\"."

#: modules/taxonomy/taxonomy.module:719
msgid "The %name vocabulary can not be modified in this way."
msgstr "%name མིང་ཚིག་གི་ཐོ་འདི་ཐབས་ལམ་འདི་བཟུམ་སྦེ་ལེགས་བཅོས་འབད་མི་བཏུབ།"

#: modules/taxonomy/taxonomy.module:1203
msgid "There are currently no posts in this category."
msgstr "ད་ལྟོ་དབྱེ་རིམ་འདི་ནང་འགྲེམ་སྐྱེལ་ཚུ་མེད།"

#: modules/taxonomy/taxonomy.module:1377
msgid "The taxonomy module is one of the most popular features because users often want to create categories to organize content by type. It can automatically classify new content, which is very useful for organizing content on-the-fly. A simple example would be organizing a list of music reviews by musical genre."
msgstr "དབྱེ་བསལ་བྱ་རིམ་ཚད་གཞི་འདི་ཡོངས་གྲགས་ཅན་གྱི་ཁྱད་རྣམ་ཚུ་ལས་གཅིག་ཨིན་ ག་ཅི་སྦེ་ཟེར་བ་ཅིན་ལག་ལེན་པ་ཚུ་གིས་འཕྲལ་འཕྲལ་རང་ དབྱེ་བ་གི་ཐོག་ལས་ནང་དོན་འགོ་འདྲེན་འཐབ་ནི་ལུ་དབྱེ་རིམ་ཚུ་གསར་བསྐྲུན་འབད་དགོཔ་ཨིན། དེ་ནང་དོན་བར་འབྱུང་སྦེ་འགོ་འདྲེན་འཐབ་ནིའི་དོན་ལུ་ན་མེད་ས་མེད་ཕན་ཐོགས་ཅན་ཨིན་མི་ ནང་དོན་གསརཔ་རང་བཞིན་གྱིས་དབྱེ་བ་བཟོ་ཚུགས། དཔེར་བརྗོད་འཇམ་སམ་འདི་སྙན་ཆའི་རིགས་ཀྱི་ཐོག་ལས་སྙན་ཆའི་སྔོན་ལྟ་ཚུའི་ཐོ་ཡིག་འགོ་འདྲེན་འཐབ་ནི་འདི་ཨིནམ་འོང་།"

#: modules/taxonomy/taxonomy.module:1378
msgid "Taxonomy is also the study of classification. The taxonomy module allows you to define vocabularies (sets of categories) which are used to classify content. The module supports hierarchical classification and association between terms, allowing for truly flexible information retrieval and classification. The taxonomy module allows multiple lists of categories for classification (controlled vocabularies) and offers the possibility of creating thesauri (controlled vocabularies that indicate the relationship of terms) and taxonomies (controlled vocabularies where relationships are indicated hierarchically). To view and manage the terms of each vocabulary, click on the associated <em>list terms</em> link. To delete a vocabulary and all its terms, choose <em>edit vocabulary.</em>"
msgstr "ཡང་དབྱེ་བསལ་བྱ་རིམ་འདི་དབྱེ་བ་བཟོ་ནིའི་སློབ་སྦྱོང་ཅིག་ཨིན། དབྱེ་བསལ་བྱ་རིམ་ཚད་གཞི་འདི་གིས་ཁྱོད་ལུ་ནང་དོན་དབྱེ་བ་བཟོ་ནི་ལུ་ལག་ལེན་འཐབ་མི་ མིང་ཚིག་གི་ཐོ་(དབྱེ་རིམ་ཚུའི་ཆ་ཚན་)ངེས་འཛིན་འབད་བཅུགཔ་ཨིན། ཚད་གཞི་འདི་གིས་ དབྱེ་བ་བཟོ་ནི་དང་ངེས་བདེན་སྦེ་བྱ་སྟབས་བདེ་ཏོག་ཏོ་གི་བརྡ་དོན་སླར་འདྲེན་གྱི་དོན་ལས་འབད་བཅུག་མི་ ཐ་སྙད་ཚུའི་བར་ན་ཚོགས་པ་དང་སྡེ་རིམ་གྱི་དབྱེ་བ་བཟོ་ནི་ལུ་རྒྱབ་བསྐྱོར་འབདཝ་ཨིན། དབྱེ་བསལ་བྱ་རིམ་ཚད་གཞི་འདི་གིས་(མཐུན་འབྲེལ་ཚུ་སྡེ་རིམ་སྦེ་བརྡ་སྟོན་ཡོད་མི་ཚད་འཛིན་འབད་ཡོད་པའི་མིང་ཚིག་གི་ཐོ་ཚུ་)དབྱེ་བསར་བྱ་རིམ་ཚུ་དང་ (ཐ་སྙད་ཚུའི་མཐུན་འབྲེལ་བརྡ་སྟོན་མི་ཚད་འཛིན་འབད་ཡོད་པའི་མིང་ཚིག་གི་ཐོ་ཚུ་)མངོན་བརྗོད་གསར་བསྐྲུན་འབད་ནི་གི་གོ་སྐབས་བྱིན་མི་དང་(ཚད་འཛིན་འབད་ཡོད་པའི་མིང་ཚིག་གི་ཐོ་ཚུ་)དབྱེ་བ་བཟོ་ནིའི་དོན་ལས་དབྱེ་རིམ་ཚུའི་སྣ་མང་ཐོ་ཡིག་ཚུ་འབད་བཅགཔ་ཨིན། མིང་ཚིག་གི་ཐོ་རེ་རེའི་ཐ་སྙད་ཚུ་འཛིན་སྐྱོང་དང་སྟོན་ནི་ལུ་ <em>འབྲེལ་བའི་ཐོ་ཡིག་ཐ་སྙད་ཚུའི་འབྲེལ་ལམ་གུ་ཨེབ་གཏང་།</em> མིང་ཚིག་གི་ཐོ་དང་དེའི་ཐ་སྙད་ཚུ་ཆ་མཉམ་བཏོན་གཏང་ནི་ལུ་ <em>མིང་ཚིག་གི་ཐོ་ཞུན་དག་རྐྱབས་ གདམ་ཁ་རྐྱབས།</em>"

#: modules/taxonomy/taxonomy.module:1379
msgid "A controlled vocabulary is a set of terms to use for describing content (known as descriptors in indexing lingo). Drupal allows you to describe each piece of content (blog, story, etc.) using one or many of these terms. For simple implementations, you might create a set of categories without subcategories, similar to Slashdot's sections. For more complex implementations, you might create a hierarchical list of categories."
msgstr "ཚད་འཛིན་འབད་ཡོད་པའི་མིང་ཚིག་གི་ཐོ་འདི་(ཟུར་སྦྲགས་འབད་ནིའི་ཁ་སྐད་ནང་འགྲེལ་བཤད་པ་ཟེར་སླབ་མི་)ནང་དོན་ནང་འགྲེལ་བཤད་འབད་ནིའི་དོན་ལས་ལག་ལེན་འཐབ་ནི་ལུ་ཐ་སྙད་ཚུ་གི་ཆ་ཚན་ཅིག་ཨིན། ཌུ་པཱལ་འདི་གིས་ཁྱོད་ལུ་(ཝེབ་དྲན་དེབ་ སྲུང་ ལ་སོགས་པའི་) ཐ་སྙད་འདི་ཚུ་ལས་གཅིག་ ཡང་ན་ ལེ་ཤ་ལག་ལེན་འཐབ་ཐོག་ལས་ ནང་དོན་གི་པིསི་རེ་རེ་འགྲེལ་བཤད་འབད་བཅུགཔ་ཨིན། འཇམ་སམ་སྦེ་ལག་ལེན་འཐབ་ནིའི་དོན་ལས་ ཡང་ཅིན་ཁྱོད་ཀྱིས་གཡོ་ཤད་ཚག་ཚུའི་དབྱེ་ཚན་བཟུམ་སྦེ་ ཡན་ལག་དབྱེ་རིམ་མེད་པར་དབྱེ་རིམ་ཚུའི་ཆ་ཚན་གསར་བསྐྲུན་འབད། ཧེང་བཀལ་གོ་དཀའ་བ་ལག་ལེན་འཐབ་ནིའི་དོན་ལས་ ཡང་ཅིན་ཁྱོད་ཀྱིས་དབྱེ་རིམ་ཚུའི་སྡེ་རིམ་གྱི་ཐོ་ཡིག་གསར་བསྐྲུན་འབད།"

#: modules/taxonomy/taxonomy.module:1380
msgid "For more information please read the configuration and customization handbook <a href=\"@taxonomy\">Taxonomy page</a>."
msgstr "བརྡ་དོན་ཧེང་སྐལ་གྱི་དོན་ལུ་རིམ་སྒྲིག་དང་སྲོལ་སྒྲིག་འབད་ནིའི་ལག་དེབ་<a href=\"@taxonomy\">དབྱེ་བསལ་བྱ་རིམ་གྱི་ཤོག་ལེབ་ལྷག་གནང་།</a>."

#: modules/taxonomy/taxonomy.module:1383
msgid "The taxonomy module allows you to classify content into categories and subcategories; it allows multiple lists of categories for classification (controlled vocabularies) and offers the possibility of creating thesauri (controlled vocabularies that indicate the relationship of terms), taxonomies (controlled vocabularies where relationships are indicated hierarchically), and free vocabularies where terms, or tags, are defined during content creation. To view and manage the terms of each vocabulary, click on the associated <em>list terms</em> link. To delete a vocabulary and all its terms, choose \"edit vocabulary\"."
msgstr "དབྱེ་བསལ་བྱ་རིམ་འདི་གིས་ཁྱོད་ལུ་དབྱེ་རིམ་ཚུ་དང་ཡན་ལག་དབྱེ་རིམ་ཚུ་ནང་ལུ་ནང་དོན་དབྱེ་བ་བཟོ་བཅུགཔ་ཨིན། དེ་གིས་ ཐ་སྙད་ཚུ་ ཡང་ན་ ངོ་རྟགས་ཚུ་ནང་དོན་གསར་བསྐྲུན་གྱི་སྐབས་སུ་ངེས་འཛིན་འབད་ཡོད་མི་རང་དབང་གི་མིང་ཚིག་གི་ཐོ་དང་ དབྱེ་བསལ་བྱ་རིམ་ཚུ་ (མཐུན་འབྲེལ་ཚུ་སྡེ་རིམ་སྦེ་བརྡ་སྟོན་ཡོད་མི་ཚད་འཛིན་འབད་ཡོད་པའི་མིང་ཚིག་གི་ཐོ་ཚུ་) (ཐ་སྙད་ཚུ་གི་མཐུན་འབྲེལ་བརྡ་སྟོན་མི་ཚད་འཛིན་འབད་ཡོད་པའི་མིང་ཚིག་གི་ཐོ་ཚུ་)མངོན་བརྗོད་གསར་བསྐྲུན་འབད་ནི་གི་གོ་སྐབས་བྱིན་མི་དང་ (ཚད་འཛིན་འབད་ཡོད་པའི་མིང་ཚིག་གི་ཐོ་ཚུ་)དབྱེ་བ་བཟོ་ནིའི་དོན་ལས་དབྱེ་རིམ་གྱི་སྣ་མང་ཐོ་ཡིག་ཚུ་འབད་བཅུགཔ་ཨིན། མིང་ཚིག་གི་ཐོ་རེ་རེའི་ཐ་སྙད་ཚུ་འཛིན་སྐྱོང་དང་སྟོན་ནི་ལུ་  <em>འབྲེལ་བའི་ཐོ་ཡིག་ཐ་སྙད་ཚུའི་འབྲེལ་ལམ་</em> ཨེབ་གཏང་། མིང་ཚིག་གི་ཐོ་དང་དེའི་ཐ་སྙད་ཚུ་ཆ་མཉམ་བཏོན་གཏང་ནི་ལུ་ \"edit vocabulary\" གདམ་ཁ་རྐྱབས།"

#: modules/taxonomy/taxonomy.module:1385
msgid "When you create a controlled vocabulary you are creating a set of terms to use for describing content (known as descriptors in indexing lingo). Drupal allows you to describe each piece of content (blog, story, etc.) using one or many of these terms. For simple implementations, you might create a set of categories without subcategories, similar to Slashdot.org's or Kuro5hin.org's sections. For more complex implementations, you might create a hierarchical list of categories."
msgstr "ཁྱོད་ཀྱིས་ཚད་འཛིན་འབད་ཡོད་པའི་མིང་ཚིག་གི་ཐོ་གསར་བསྐྲུན་འབདཝ་ད་ (ཟུར་སྦྲགས་འབད་ནིའི་ཁ་སྐད་ནང་འགྲེལ་བཤད་པ་ཟེར་སླབ་མི་) ནང་དོན་འགྲེལ་བཤད་བརྐྱབ་ནིའི་དོན་ལས་ལག་ལེན་འཐབ་ནི་ལུ་ཁྱོད་ཀྱིས་ཐ་སྙད་ཚུ་གི་ཆ་ཚན་གསར་བསྐྲུན་འབདཝ་ཨིན། ཌུ་པཱལ་གྱིས་ཁྱོད་ལུ་ (ཝེབ་དྲན་དེབ་དང་སྲུང་ལ་སོགས་པའི་) ཐ་སྙད་ཚུ་གཅིག་ཡང་ན་ལེ་ཤ་ལག་ལེན་འཐབ་ཐོག་ལས་ ནང་དོན་གྱི་པིསི་རེ་རེ་འགྲེལ་བཤད་འབད་བཅུགཔ་ཨིན། འཇམ་སམ་སྦེ་ལག་ལེན་འཐབ་ནི་ལུ་ ཡང་ན་ཁྱོད་ཀྱིས་ Slashdot.org's ཡང་ན་ Kuro5hin.org's དབྱེ་ཚན་ཚུ་ཆ་འདྲཝ་སྦེ་ ཡན་ལག་དབྱེ་རིམ་ཚུ་མེད་པར་དབྱེ་རིམ་ཚུའི་ཆ་ཚན་གསར་བསྐྲུན་འབད། ཧེང་བཀལ་གོ་དཀའ་བ་ལག་ལེན་འཐབ་ནི་ལུ་ ཡང་ན་ཁྱོད་ཀྱིས་དབྱེ་རིམ་ཚུའི་སྡེ་རིམ་གྱི་ཐོ་ཡིག་གསར་བསྐྲུན་འབད།"

