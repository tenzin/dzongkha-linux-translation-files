#: modules/user/user.module:239
msgid "You must enter a username."
msgstr "ཁྱོད་ཀྱི་ལག་ལེན་པའི་  མིང་ཅིག་ ཐོ་བཀོད་དགོ"

#: modules/user/user.module:240
msgid "The username cannot begin with a space."
msgstr "ལག་ལེན་པའི་མིང་དེ་  བར་སྟོང་དང་གཅིག་ཁར་  འགོ་བཙུགས་མི་བཏུབ།"

#: modules/user/user.module:241
msgid "The username cannot end with a space."
msgstr "ལག་ལེན་པའི་མིང་དེ་  བར་སྟོང་དང་གཅིག་ཁར་ མཇུག་བསྡུ་མི་བཏུབ།"

#: modules/user/user.module:242
msgid "The username cannot contain multiple spaces in a row."
msgstr "ལག་ལེན་པའི་མིང་དེ་  གྱལ་ནང་ལུ་  སྣ་མང་བར་སྟོང་  ཤོངམ་འབད་མེད།"

#: modules/user/user.module:243;254
msgid "The username contains an illegal character."
msgstr "ལག་ལེན་པའི་མིང་འདི་ནང་ལུ་  ཁྲིམས་འགལ་གྱི་ ཡིག་འབྲུ་ཅིག་འདུག"

#: modules/user/user.module:256
msgid "The username is not a valid authentication ID."
msgstr "ལག་ལེན་པའི་མིང་དེ་  ནུས་ཅན་ བདེན་བཤད་ཀྱི་ ཨའི་ཌི་ཅིག་མེན་པས།"

#: modules/user/user.module:257
msgid "The username %name is too long: it must be less than 56 characters."
msgstr "ལག་ལེན་པའི་མིང་  %name དེ་  གནམ་མེད་ས་མེད་རིངམོ་འདུག:  འདི་ཡིག་འབྲུ་ ༥༦་ ལས་ཉུངམ་འོང་ནི་མས།"

#: modules/user/user.module:261
msgid "You must enter an e-mail address."
msgstr "ཁྱོད་ཀྱི་གློག་འཕྲིན་གྱི་  ཁ་བྱང་  ཐོ་བཀོད་འབད་དགོ"

#: modules/user/user.module:263
msgid "The e-mail address %mail is not valid."
msgstr "གློག་འཕྲིན་ཁ་བྱང་  %mail་དེ་  ནུས་ཅན་མེན་པས།"

#: modules/user/user.module:278
msgid "The uploaded file was not an image."
msgstr "སྐྱེལ་བཙུགས་འབད་ཡོད་པའི་  ཡིག་སྣོད་དེ་  གཟུགས་བརྙན་མེན་པས།"

#: modules/user/user.module:284
msgid "The uploaded image is too large; the maximum file size is %size kB."
msgstr "སྐྱེལ་བཙུགས་འབད་ཡོད་པའི་  གཟུགས་བརྙན་དེ་  ཧ་ཅང་གིས་སྦོམ་འདུག   མང་མཐའ་ཡིག་སྣོད་ཀྱི་ཚད་འདི་  %size ་ཀྱི་ཀེ་བི་ཨིན།"

#: modules/user/user.module:287
msgid "The uploaded image is too large; the maximum dimensions are %dimensions pixels."
msgstr "སྐྱེལ་བཙུགས་འབད་ཡོད་པའི་  གཟུགས་བརྙན་དེ་  ཧ་ཅང་གིས་སྦོམ་འདུག་  མང་མཐའི་རྒྱ་འཁོན་འདི་  %dimensions གི་  པིག་སེལ་ཚུ་ཨིན། "

#: modules/user/user.module:295
msgid "Failed to upload the picture image; the %directory directory doesn't exist."
msgstr "པར་གྱི་གཟུགས་བརྙན་དེ་  སྐྱེལ་བཙུགས་འབད་ནི་ལུ་  འཐུས་ཤོར་བྱུང་ཡོད་   %directory  སྣོད་ཐོ་དེ་མིན་འདུག"

