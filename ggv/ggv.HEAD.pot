#: ../ggv.schemas.in.h:18
msgid "Percentage of the viewable area to scroll"
msgstr "བཤུད་སྒྲིལ་འབད་ནི་ལུ་མཐོང་ཚུགས་པའི་མངའ་ཁོངས་ཀྱི་བརྒྱ་ཆ།"

#: ../ggv.schemas.in.h:19
msgid "PostScript interpreter command"
msgstr "པོསིཊི་སིཀིརིཔཊི་ ཁ་སྒྱུར་མི་བརྡ་བཀོད།"

#: ../ggv.schemas.in.h:20
msgid "Printing command"
msgstr "དཔར་བསྐྲུན་བརྡ་བཀོད།"

#: ../ggv.schemas.in.h:22
msgid "Save window geometry"
msgstr "སྒོ་སྒྲིག་ཐིག་རྩིས་སྲུངས།"

#: ../ggv.schemas.in.h:23
msgid "Show menubar"
msgstr "དཀར་ཆག་ཕྲ་རིང་སྟོན།"

#: ../ggv.schemas.in.h:24
msgid "Show previous visible area when scrolling"
msgstr "བཤུད་སྒྲིལ་འབདཝ་ད་ལུ་ཧེ་གི་མཐོང་ཚུགས་པའི་མངའ་ཁོངས་སྟོན།"

#: ../ggv.schemas.in.h:25
msgid "Show side panel"
msgstr "ཟུར་ཁའི་པེ་ནཱལ་སྟོན།"

#: ../ggv.schemas.in.h:26
msgid "Show statusbar"
msgstr "གནས་ཚད་ཕྲ་རིང་སྟོན།"

#: ../ggv.schemas.in.h:27
msgid "Show toolbar"
msgstr "ལག་ཆས་ཕྲ་རིང་སྟོན།"

#: ../ggv.schemas.in.h:28
msgid "The command line used to convert PDF files to PostScript."
msgstr "པི་ཌི་ཨེཕ་ཡིག་སྣོད་ཚུ་ པོསིཊི་སིཀིརིཔཊི་ལུ་གཞི་བསྒྱུར་འབད་ནིའི་དོན་ལུ་ལག་ལེན་འཐབ་ཡོད་པའི་བརྡ་བཀོད་གྲལ་ཐིག"

#: ../ggv.schemas.in.h:29
msgid "The command line used to create PostScript DSC files from PDF files."
msgstr "པི་ཌི་ཨེཕ་ཡིག་སྣོད་ཚུ་ལས་པོསིཊི་སིཀིརིཔཊི་ ཌི་ཨེསི་སི་ཡིག་སྣོད་ཚུ་གསར་བསྐྲུན་འབད་ནི་ལུ་ལག་ལེན་འཐབ་ཡོད་པའི་བརྡ་བཀོད་གྲལ་ཐིག"

#: ../ggv.schemas.in.h:30
msgid "The command used to unpack files compressed with bzip2."
msgstr "བི་ཛིཔ་༢ དང་གཅིག་ཁར་ཨེབ་བཙུགས་འབད་ཡོད་པའི་ཡིག་སྣོད་ཚུ་བཤུབ་གཏང་ནི་ལུ་ལག་ལེན་འཐབ་ཡོད་པའི་བརྡ་བཀོད།"

#: ../ggv.schemas.in.h:31
msgid "The command used to unpack files compressed with gzip."
msgstr "ཇི་ཛིཔ་དང་གཅིག་ཁར་ཨེབ་བཙུགས་འབད་ཡོད་པའི་ཡིག་སྣོད་ཚུ་བཤུབ་གཏང་ནི་ལུ་ལག་ལེན་འཐབ་ཡོད་པའི་བརྡ་བཀོད།"

#: ../ggv.schemas.in.h:32
msgid "The default page size to use when no page size is specified by the document or when the user wishes to override the size specified in the document."
msgstr "ཡིག་ཆ་གིས་ཤོག་ལེབ་ཚད་གསལ་བཀོད་འབད་དེ་མེདཔ་ད་ལུ་ལག་ལེན་འཐབ་ནི་ སྔོན་སྒྲིག་ཤོག་ལེབ་ཚད་དང་ཡང་ན་ ཡིག་ཆ་ནང་ལུ་གསལ་བཀོད་འབད་ཡོད་པའི་ཚད་འདི་ལག་ལེན་པ་གིས་ཟུར་བཞག་འབད་ནི་རེ་འདོད་བསྐྱེདཔ་ད།"

#: ../ggv.schemas.in.h:33
msgid "The default zoom factor for the document. The value should belong to the interval [0.05, 30.0]."
msgstr "ཡིག་ཆའི་དོན་ལུ་སྔོན་སྒྲིག་རྒྱས་ཟུམ་ཆ་རྐྱེན། གནས་གོང་འདི་བར་མཚམས་[0.05, 30.0]ལུ་བང་དགོསཔ་ཨིན།"

#: ../ggv.schemas.in.h:34
msgid "The height of the viewer's window."
msgstr "ལྟ་མཁན་གྱི་སྒོ་སྒྲིག་མཐོ་ཚད།"

