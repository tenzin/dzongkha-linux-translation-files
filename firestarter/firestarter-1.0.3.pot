#: ../firestarter.desktop.in.h:1
msgid "Desktop Firewall Tool"
msgstr "ཌེཀསི་ཊོཔ་ཕ་ཡེར་ཝཱལ་ལག་ཆས།"

#: ../firestarter.desktop.in.h:2
#: ../src/firestarter.c:297
msgid "Firestarter"
msgstr "ཕ་ཡེར་ཨིས་ཊ་ཊར།"

#: ../firestarter.schemas.in.h:1
msgid "Apply changes made to policy immediately, the alternative is for the user to confirm the changes in advance."
msgstr "སྲིད་བྱུས་ལུ་བསྒྱུར་བཅོས་འབད་ཡོད་མི་ཚུ་དེ་འཕྲོ་ལས་འཇུག་སྤྱོད་འབད་ ཐབས་གཞན་འདི་ལག་ལེན་པ་གིས་མཐོ་རིམ་ནང་གི་བསྒྱུར་བཅོས་ངེས་དཔྱད་འབད་ནི་དོན་ལུ་ཨིན།"

#: ../firestarter.schemas.in.h:2
msgid "Apply the ICMP settings, as configured in the wizard."
msgstr "ཝི་ཛཌ་ནང་ལུ་རིམ་སྒྲིག་འབད་ཡོདཔ་བཞིན་དུ་ ཨའི་སི་ཨེམ་པི་གཞི་སྒྲིག་འཇུག་སྤྱོད་འབད།"

#: ../firestarter.schemas.in.h:3
msgid "Apply the settings of Type of Service, as configured in the wizard."
msgstr "ཝི་ཛཌ་ནང་ལུ་རིམ་སྒྲིག་འབད་ཡོདཔ་བཞིན་དུ་ ཞབས་ཏོག་གི་དབྱེ་བ་གི་གཞི་སྒྲིག་འཇུག་སྤྱོད་འབད།"

#: ../firestarter.schemas.in.h:4
#: ../src/preferences.glade.h:29
msgid "Block broadcasts from external network"
msgstr "ཕྱིའི་ཡོངས་འབྲེལ་ལས་རྒྱང་བསྒྲགས་འབད་མི་ཚུ་བཀག་ནི།"

#: ../firestarter.schemas.in.h:5
msgid "Block broadcasts from external network."
msgstr "ཕྱིའི་ཡོངས་འབྲེལ་ལས་རྒྱང་བསྒྲགས་འབད་མི་ཚུ་བཀག་ནི།"

#: ../firestarter.schemas.in.h:6
#: ../src/preferences.glade.h:30
msgid "Block broadcasts from internal network"
msgstr "ནང་འཁོདི་ཡོངས་འབྲེལ་ལས་རྒྱང་བསྒྲགས་འབད་མི་ཚུ་བཀག་ནི།"

#: ../firestarter.schemas.in.h:7
msgid "Block broadcasts from internal network."
msgstr "ནང་འཁོདི་ཡོངས་འབྲེལ་ལས་རྒྱང་བསྒྲགས་འབད་མི་ཚུ་བཀག་ནི།"

#: ../firestarter.schemas.in.h:8
msgid "Block potentially spoofed traffic claiming to come from IANA reserved address space on the external interface."
msgstr "ཨའི་ཨེ་ཨེན་ཨེ་ཟུར་གསོག་འབད་ཡོད་མི་ལས་ཀྱི་ཨིས་པོཕ་ཊེ་ཕིག་བཀག ཕྱིའི་དངོས་འདྲ་བའི་ནང་བར་སྟོང་སེལ།།"

#: ../firestarter.schemas.in.h:9
msgid "Block traffic from reserved address space on the public interface"
msgstr "མི་མང་གི་དངོས་འདྲ་བའི་བར་སྟོང་སེལ་ཡོད་མི་གི་ཟུར་གསོག་འབད་མི་ནང་ལས་ཀྱི་འགྲུལ་ལམ་བཀག"

#: ../firestarter.schemas.in.h:10
msgid "Deny packets"
msgstr "འཕྲིན་ཚན་ལུ་མ་ཉནམཔས།"

#: ../firestarter.schemas.in.h:11
msgid "Drop denied packets without sending an error packet back."
msgstr "འཛོལ་བ་ཅན་གྱི་འཕྲིན་ཚན་ལོག་མ་གཏང་པར་ ཉན་མ་བཏུབ་པའི་འཕྲིན་ཚན་བཀོག་བཞག"

#: ../firestarter.schemas.in.h:12
msgid "Enable DHCP server for connection sharing"
msgstr "ཌི་ཨེཆ་སི་པི་སར་བར་གྱི་དོན་ལུ་མཐུད་ལམ་རུབ་སྤྱོད་ཡོདཔ་བཟོ།"

#: ../firestarter.schemas.in.h:13
msgid "Enable NAT"
msgstr "ཨེན་ཨེ་ཊི་ལྕོགས་ཅན་བཟོ་ནི།"

#: ../firestarter.schemas.in.h:14
msgid "Enable Network Address Translation, or Internet connection sharing."
msgstr "ཡོངས་འབྲེལ་ཁ་བྱང་སྐད་བསྒྱུར་ལྕོགས་ཅི་བཟོ་ ཡངན་ ཨིན་ཊར་ནེཊི་མཐུད་ལམ་འཇུག་སྤྱོད་ཡོདཔ་བཟོ་ནི།"

#: ../firestarter.schemas.in.h:15
msgid "Enable the configuration of ICMP"
msgstr "ཨའི་ཨི་ཨེམ་པི་གི་རིམ་སྒྲིག་ས་ལྕོགས་ཅན་བཟོ་ནི།"

#: ../firestarter.schemas.in.h:16
msgid "Enable the configuration of tos"
msgstr "ཊོས་ཀྱིི་རིམ་སྒྲིག་ས་ལྕོགས་ཅན་བཟོ་ནིག"

#: ../firestarter.schemas.in.h:17
#: ../src/preferences.glade.h:41
msgid "Enable tray icon"
msgstr "ཤོག་སྣོད་ངོས་དཔར་ལྕོགས་ཅན་བཟོ་ནི།"

#: ../firestarter.schemas.in.h:18
msgid "Filter out hits not targeted at the firewall"
msgstr "ཕ་ཡར་ཝལ་གུ་དམིགས་གཏད་མ་བསྐྱེད་པའི་ཧེང་བཀལ་བཙག་གཏང་།"

#: ../firestarter.schemas.in.h:19
msgid "Filter out hits that have an destination IP that does not match the firewall host."
msgstr "ཕ་ཡར་ཝལ་ཧོསཊི་དང་མ་མཐུན་པའི་ཧེང་བཀལ་འགྲོ་ཡུལ་ཨའི་པི་ཡོད་མི་བཙག་གཏང།"

#: ../firestarter.schemas.in.h:20
msgid "Filter out redundant hits"
msgstr "ཧེང་བཀལ་མང་དྲགས་ཡོད་མི་ཚུ་བཙག་གཏང་།"

#: ../firestarter.schemas.in.h:21
msgid "Filter out sequences of identical hits."
msgstr "འབྱུང་རིམ་དབྱེར་མེད་པ་ཧེང་བཀལ་ཚུ་བཙག་གཏང་།"

#: ../firestarter.schemas.in.h:22
msgid "Highest IP in the range to serve to DHCP clients"
msgstr "ཌི་ཨེཆ་སི་པི་ཞབས་ཏོག་སྤྱོད་མི་ལུ་ཞབས་ཏོག་ཞུ་མི་གི་ཁྱབ་ཚད་ནང་གི་ཨའི་པི་མཐོ་ཤོས།"

#: ../firestarter.schemas.in.h:23
msgid "Is it the first time the user is running the program"
msgstr "ལག་ལེན་པ་གིས་རིམ་ལུགས་འདི་འགོ་དང་པ་གཡོག་བཀོལཝ་ཨིན་ནང་།"

#: ../firestarter.schemas.in.h:24
msgid "Lowest IP in the range to serve to DHCP clients"
msgstr "ཌི་ཨེཆ་སི་པི་ཞབས་ཏོག་སྤྱོད་མི་ལུ་ཞབས་ཏོག་ཞུ་མི་གི་ཁྱབ་ཚད་ནང་གི་ཨའི་པི་དམའོ་ཤོས།"

#: ../firestarter.schemas.in.h:25
msgid "Minimize to tray"
msgstr "ཤོག་སྣོད་ལུ་ཆུང་ཀུ་བཟོ།"

#: ../firestarter.schemas.in.h:26
msgid "Nameserver DHCP clients should use"
msgstr "ཌི་ཨེཆ་སི་པི་ཞབས་ཏོག་སྤྱོད་མི་ཟེར་བའི་མིང་ཅན་སར་བར་ལག་ལེན་འཐབ་དགོ།"

#: ../firestarter.schemas.in.h:27
msgid "Policy auto apply"
msgstr "སྲིད་བྱུས་རང་བཞིན་གྱྀས་འཇུག་སྤྱོད་འབད་ནི།"

#: ../firestarter.schemas.in.h:28
msgid "Restrict outbound traffic by default, only allowing whitelisted outgoing connections."
msgstr "ཝའིཊ་ལིས་ཊེཌ་ཕྱིར་འགྲོའི་མཐུད་ལམ་རྐྱངམ་ཅིག་འགྱོ་བཅུག་སྟེ་ སྔོན་སྒྲིག་གི་ཐོག་ལས་ཨའུཊ་བོནཌ་འགྲུལ་ལམ་བཀག་དམ་འབད་ནི།"

#: ../firestarter.schemas.in.h:29
msgid "Restrictive outbound policy mode"
msgstr "བཀག་དམ་ཅན་གྱི་ཨའུཊ་བཱོནཌ་སྲིད་བྱུས་ཐབས་ལམ།"

#: ../firestarter.schemas.in.h:30
msgid "Run the system dhcpd server in conjunction with Firestarter Internet connection sharing."
msgstr "རིམ་ལུགས་ཌི་ཨེཆ་སི་པི་ཌི་སར་བར་འདི་ཕ་ཡར་ཨིས་ཊ་ཊར་ཨིན་ཊར་ནེཊ་ མཐུད་ལམ་རུབ་སྤྱོད་དང་གཅིག་ཁར་འབྲེལ་མཐུད་དང་མཉམ་གཡོག་བཀོལ།"

#: ../firestarter.schemas.in.h:31
msgid "Show the Firestarter icon in the notification area."
msgstr "ཕ་ཡར་ཨིས་ཊ་ཊར་གྱི་ངོས་དཔར་འདི་བརྡ་བསྐུལ་གྱི་མངའ་ཁོངས་ནང་ལུ་སྟོན།"

#: ../firestarter.schemas.in.h:32
msgid "Show the Type of Service column in the Hit View."
msgstr "ཧེང་བཀལ་བཀྲམ་ནིའི་མཐོང་སྣང་ནང་ལུ་ཞབས་ཏོག་ཀེར་ཐིག་གི་དབྱེ་བ་འདི་སྟོན།"

#: ../firestarter.schemas.in.h:33
msgid "Show the destination column in the Hit View"
msgstr "འགྲོ་ཡུལ་གྱི་ཀེར་ཐིག་འདི་ཧེང་བཀལ་བཀྲམ་ནིའི་མཐོང་སྣང་ནང་ལུ་སྟོན"

#: ../firestarter.schemas.in.h:34
msgid "Show the destination column in the Hit View."
msgstr "འགྲོ་ཡུལ་གྱི་ཀེར་ཐིག་འདི་ཧེང་བཀལ་བཀྲམ་ནིའི་མཐོང་སྣང་ནང་ལུ་སྟོན།"

#: ../firestarter.schemas.in.h:35
msgid "Show the device the hit was received from in the Hit View."
msgstr "ཧེང་བཀལ་བཀྲམ་ནིའི་མཐོང་སྣང་ནང་ལུ་ ཧེང་བཀལ་བཀྲམ་ནི་ཐོབ་ཡོད་པའི་ ཐབས་འཕྲུལ་སྟོན།"

#: ../firestarter.schemas.in.h:36
msgid "Show the device the hit was routed to in the Hit View."
msgstr "ཧེང་བཀལ་བཀྲམ་ནིའི་མཐོང་སྣང་ནང་ལུ་ ཧེང་བཀལ་བཀྲམ་ནི་འདི་འགྲུལ་ལམ་བཟོ་ཡོད་པའི་ ཐབས་འཕྲུལ་སྟོན།"

#: ../firestarter.schemas.in.h:37
msgid "Show the direction column in the Hit View"
msgstr "ཧེང་བཀལ་བཀྲམ་ནིའི་མཐོང་སྣང་ནང་ལུ་ ཁ་ཕྱོགས་ཀྱི་ཀེར་ཐིག་སྟོན"

#: ../firestarter.schemas.in.h:38
msgid "Show the direction column in the Hit View."
msgstr "ཧེང་བཀལ་བཀྲམ་ནིའི་མཐོང་སྣང་ནང་ལུ་ ཁ་ཕྱོགས་ཀྱི་ཀེར་ཐིག་སྟོན།"

#: ../firestarter.schemas.in.h:39
msgid "Show the in column in the Hit View"
msgstr "ཧེང་བཀལ་བཀྲམ་ནིའི་མཐོང་སྣང་ནང་ལུ་ ནང་གི་ཀེར་ཐིག་སྟོན"

#: ../firestarter.schemas.in.h:40
msgid "Show the length column in the Hit View"
msgstr "ཧེང་བཀལ་བཀྲམ་ནིའི་མཐོང་སྣང་ནང་ལུ་ རིང་ཚད་ཀེར་ཐིག་སྟོན།"

#: ../firestarter.schemas.in.h:41
msgid "Show the out column in the Hit View"
msgstr "ཧེང་བཀལ་བཀྲམ་ནིའི་མཐོང་སྣང་ནང་ལུ་ ཕྱིའི་ཀེར་ཐིག་སྟོན"

#: ../firestarter.schemas.in.h:42
msgid "Show the packet length column in the Hit View."
msgstr "ཧེང་བཀལ་བཀྲམ་ནིའི་མཐོང་སྣང་ནང་ལུ་ འཕྲིན་ཚན་རིང་ཚད་ཀྱི་ཀེར་ཐིག་སྟོན།"

#: ../firestarter.schemas.in.h:43
msgid "Show the port column in the Hit View"
msgstr "ཧེང་བཀལ་བཀྲམ་ནིའི་མཐོང་སྣང་ནང་ལུ་ འདྲེན་ལམ་ཀེར་ཐིག་སྟོན"

#: ../firestarter.schemas.in.h:44
msgid "Show the port column in the Hit View."
