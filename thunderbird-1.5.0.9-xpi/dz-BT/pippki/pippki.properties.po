msgid "You should click OK only if you trust \"%S\" to protect your encryption private key."
msgstr " \"%S\"གིས་ཁྱོད་རའི་ གསང་བཟོའི་སྒེར་གྱི་ལྡེ་མིག་ ཉེན་སྐྱོབ་འབད་ནི་ལུ་ བློ་གཏད་ཡོད་པ་ཅིན་རྐྱངམ་གཅིག་ བཏུབ་ གུ་ཨེབ་གཏང་འབད།"

#Token Manager
#: password_not_set
msgid "(not set)"
msgstr "(གཞི་སྒྲིག་མ་འབད་བས་)"

#: failed_pw_change
msgid "Unable to change Master Password."
msgstr "ཆོག་ཡིག་ཨམ་སོར་མ་ཚུགས་"

#: incorrect_pw
msgid "You did not enter the correct current Master Password. Please try again."
msgstr "ཁྱོད་ཀྱིས་ད་ལྟོའི་ཆོག་ཡིག་ཨམ་ ངེས་བདེན་དེ་མ་བཙུགས་པས། ལོག་སྟེ་འབད་རྩོལབསྐྱེད་གནང་།"

#: pw_change_ok
msgid "Master Password successfully changed."
msgstr "ཆོག་ཡིག་ཨམ་ མཐར་འཁྱོལ་སྦེ་སོར་ཡི།"

#: pw_erased_ok
msgid "Warning! You have deleted your Master Password."
msgstr "ཉེན་བརྡ་! ཁྱོད་ཀྱིས་ཁྱོད་རའི་ཆོག་ཡིག་ཨམ་ བཏོན་བཏང་ནུག།"

#: pw_not_wanted
msgid "Warning! You have decided not to use a Master Password."
msgstr "ཉེན་བརྡ་: ཁྱོད་ཀྱིས་ཆོག་ཡིག་ཨམ་ ལག་ལེན་མི་འཐབ་ནི་སྦེ་ ཐག་བཅད་ནུག།"

#: pw_empty_warning
msgid "Your stored web and email passwords, form data, and private keys will not be protected."
msgstr "ཁྱོད་ཀྱིས་གསོག་འཇོག་འབད་ཡོད་པའི་ ཝེབ་དང་གློག་འཕྲིན་གྱི་ཆོག་ཡིག་ཚུ་དང་ འབྲི་ཤོག་གི་གནས་སྡུད་ སྒེར་གྱི་ལྡེ་མིག་ཚུ་ ཉེན་སྐྱོབ་འབད་མི་ཚུགས།"

#: pw_change2empty_in_fips_mode
msgid "You are currently in FIPS mode. FIPS requires a non-empty Master Password."
msgstr "ད་ལྟོ་ཁྱོད་ FIPS ཐབས་ལམ་ནང་འདུག། FIPS འདི་ལུ་ སྟོངམ་མེན་པའི་ ཆོག་ཡིག་ཅིག་དགོཔ་ཨིན། "

#: login_failed
msgid "Failed to Login"
msgstr "ནང་བསྐྱོད་འབད་མ་ཚུགས།"

#: loadPK11TokenDialog
msgid "Choose a PKCS#11 device to load"
msgstr "མངོན་གསལ་འབད་ནི་ལུ་ PKCS#11 ཐབས་འཕྲུལ་ཅིག་ གདམ་ཁ་རྐྱབས།"

#: devinfo_modname
msgid "Module"
msgstr "མོ་ཌུལ་"

