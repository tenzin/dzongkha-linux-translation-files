# Values for changepassword.xul
#: setPassword.title
msgid "Change Master Password"
msgstr "ཆོག་ཡིག་ཨམ་སོར་"

#: setPassword.tokenName.label
#: getPassword.tokenName.label
msgid "Security Device"
msgstr "སྲུང་སྐྱོབ་ཐབས་འཕྲུལ་"

#: setPassword.oldPassword.label
msgid "Current password:"
msgstr "ད་ལྟོའི་ཆོག་ཡིག་:"

#: setPassword.newPassword.label
msgid "New password:"
msgstr "ཆོག་ཡིག་གསརཔ་:"

#: setPassword.reenterPassword.label
msgid "New password (again):"
msgstr "ཆོག་ཡིག་གསརཔ་ (ལོག་སྟེ་):"

#: setPassword.meter.label
msgid "Password quality meter"
msgstr "ཆོག་ཡིག་གི་སྤུས་ཚད་མི་ཊར་"

#: setPassword.meter.loading
msgid "Loading"
msgstr "མངོན་གསལ་འབད་དོ་"

# Values for resetpassword.xul
#: resetPasswordButtonLabel
msgid "Reset"
msgstr "སླར་སྒྲིག་"

#: resetPassword.title
msgid "Reset Master Password"
msgstr "ཆོག་ཡིག་ཨམ་སླར་སྒྲིག་འབད་"

#: resetPassword.text
msgid "If you reset your master password, all your stored web and e-mail passwords, form data, personal certificates, and private keys will be forgotten. Are you sure you want to reset your master password?"
msgstr "ཁྱོད་ཀྱིས་ཁྱོད་རའི་ཆོག་ཡིག་ཨམ་ སླར་སྒྲིག་འབད་བ་ཅིན་ ཁྱོད་རའི་གསོག་འཇོག་འབད་ཡོད་པའི་ ཝེབ་དང་གློག་འཕྲིན་གྱི་ཆོག་ཡིག་ འབྲི་ཤོག་གནས་སྡུད་ རང་དོན་ལག་ཁྱེར་ སྒེར་གྱི་ལྡེ་མིག་ཚུ་ ཆ་མཉམ་ར་ བརྗེད་འགྱོ་འོང་། ཁྱོད་ཀྱིས་ཁྱོད་རའི་ཆོག་ཡིག་ཨམ་ སླར་སྒྲིག་འབད་ནི་ ངེས་གཏན་ཨིན་ན?"

# Values for getpassword.xul
#: getPassword.title
msgid "Enter Master Password"
msgstr "ཆོག་ཡིག་ཨམ་བཙུགས་"

#: getPassword.password.label
msgid "Password:"
msgstr "ཆོག་ཡིག་:"

# These should be common
#: piperror.title
msgid "Error"
msgstr "འཛོལ་བ་"

#: pipalert.title
msgid "Error - cannot recover"
msgstr "འཛོལ་བ་ - སླར་གསོ་འབད་མི་ཚུགས་པས་"

# Prefs panel strings
#: pref.security.general.lHeader
msgid "Security"
msgstr "སྲུང་སྐྱོབ་"

#: pref.security.general.rHeader
#: pref.security.general.header
msgid "Settings for general security"
msgstr "ཡོངས་ཁྱབ་སྲུང་སྐྱོབ་ཀྱི་་སྒྲིག་སྟངས་"

#: pref.security.general.level.title
msgid "Security Level"
msgstr "སྲུང་སྐྱོབ་གནས་རིམ་"

#: pref.security.general.paranoid.label
msgid "Use paranoid security settings"
msgstr "པེ་ར་ནོ་ཡིཌི་ སྲུང་སྐྱོབ་སྒྲིག་སྟངས་ ལག་ལེན་འཐབ་"

#: pref.security.general.certificates.title
msgid "Certificates"
msgstr "ལག་ཁྱེར་ཚུ་"

#: pref.security.general.manageCert.label
msgid "Manage Certificates"
msgstr "ལག་ཁྱེར་ཚུ་འཛིན་སྐྱོང་འཐབ་"

# Downloading a cert
#: downloadCert.title
msgid "Downloading Certificate"
msgstr "ལག་ཁྱེར་ཕབ་ལེན་འབད་དོ་"

#: downloadCert.message1
msgid "You have been asked to trust a new Certificate Authority (CA)."
msgstr "ཁྱོད་ལུ་ལག་ཁྱེར་དབང་འཛིན་གསརཔ་ཅིག་ལུ་ བློ་གཏད་དགོཔ་སྦེ་སླབ་ནུག།"

#: downloadCert.trustSSL
msgid "Trust this CA to identify web sites."
msgstr "ཝེབ་ས་ཁོངས་ངོས་འཛིན་འབད་ནི་ལུ་ ལག་ཁྱེར་དབང་འཛིན་འདི་ལུ་བློ་གཏད།"

#: downloadCert.trustEmail
msgid "Trust this CA to identify email users."
msgstr "གློག་འཕྲིན་ལག་ལེན་པ་ ་ངོས་འཛིན་འབད་ནི་ལུ་ ལག་ཁྱེར་དབང་འཛིན་འདི་ལུ་བློ་གཏད།"

#: downloadCert.trustObjSign
msgid "Trust this CA to identify software developers."
msgstr "མཉེན་ཆས་བཟོ་མི་ ངོས་འཛིན་འབད་ནི་ལུ་ ལག་ཁྱེར་དབང་འཛིན་འདི་ལུ་བློ་གཏད།"

#: downloadCert.message3
msgid "Before trusting this CA for any purpose, you should examine its certificate and its policy and procedures (if available)."
msgstr "དགོས་དོན་ག་ཅི་གི་དོན་ལུ་ཡང་ ལག་ཁྱེར་དབང་འཛིན་འདི་ལུ་ བློ་གཏད་མ་འབད་བའི་ཧེ་མ་ འདི་གི་ལག་ཁྱེར་དང་ འདི་གི་སྲིད་བྱུས་ བྱ་སྒོའི་ལམལུགས་ཚུ་ལུ་ (ཡོད་པ་ཅིན་)བརྟག་དཔྱད་འབད་དགོ།"

#: downloadCert.viewCert.label
msgid "View"
msgstr "སྟོན་"

#: downloadCert.viewPolicy.label
msgid "Policy"
msgstr "སྲིད་བྱུས་"

#: downloadCert.viewCert.text
msgid "Examine CA certificate"
msgstr "ལག་ཁྱེར་དབང་འཛིན་གྱི་ ལག་ཁྱེར་ལུ་ བརྟག་དཔྱད་འབད་"

#: downloadCert.viewPolicy.text
msgid "Examine CA policies and procedures"
msgstr "ལག་ཁྱེར་དབང་འཛིན་གྱི་ སྲིད་བྱུས་དང་བྱ་སྒོའི་ལམ་ལུགས་ཚུ་ལུ་ བརྟག་དཔྱད་འབད།"

# Certificate Exists in database
#: caCertExists.title
msgid "Certificate Exists"
msgstr "ལག་ཁྱེར་འདུག་"

#: caCertExists.message
msgid "The Certificate already exists."
msgstr "ལག་ཁྱེར་ཧེ་མ་ལས་འདུག།"

# Strings for the MismatchDomain dialog
#: domainMismatch.title
msgid "Security Error: Domain Name Mismatch"
msgstr "སྲུང་སྐྱོབ་ཀྱི་འཛོལ་བ་: ཌོ་མེན་གྱི་མིང་མི་མཐུན་པ།"

#: examineCert.label
#: examineCert.accesskey
msgid "&View Certificate"
msgstr "ལག་ཁྱེར་སྟོན་ (&V)"

# Strings for the Server cert expired dialog
#: serverCertExpired.continue
msgid "Would you like to continue anyway?"
msgstr "ཁྱོད་ཀྱིས་གང་རུང་སྦེ་ འཕྲོ་མཐུད་ནིཨིན་ན?"

#: serverCertExpired.accept.label
msgid "Continue"
msgstr "འཕྲོ་མཐུད་"

# Strings for the SSL client auth ask dialog
#: clientAuthAsk.title
msgid "User Identification Request"
msgstr "ལག་ལེན་པ་ངོས་འཛིན་གྱི་ཞུ་བ་"

#: clientAuthAsk.message1
msgid "This site has requested that you identify yourself with a certificate:"
msgstr "ས་ཁོངས་འདི་གིས་ ཁྱོད་ལུ་ལག་ཁྱེར་ཅིག་གི་ཐོག་ལས་ ཁྱོད་ར་ངོས་འཛིན་འབད་དགོ་པའི་ ཞུ་བ་འབད་ནུག། "

#: clientAuthAsk.message2
msgid "Choose a certificate to present as identification:"
msgstr "ངོ་རྟགས་སྦེ་སྤྲོད་ནིའི་ ལག་ཁྱེར་ཅིག་ གདམ་ཁ་རྐྱབས།"

#: clientAuthAsk.message3
#: certPicker.detailsLabel
msgid "Details of selected certificate:"
msgstr "སེལ་འཐུ་འབད་མི་ ལག་ཁྱེར་གྱི་རྒྱས་བཤད་:"

# Strings for the cert picker dialog
#: certPicker.title
msgid "Select Certificate"
msgstr "ལག་ཁྱེར་སེལ་འཐུ་འབད་"

#: certPicker.info
msgid "Certificate:"
msgstr "ལག་ཁྱེར་:"

#: pkcs12.setpassword.title
msgid "Choose a Certificate Backup Password"
msgstr "ལག་ཁྱེར་རྒྱབ་ཐག་ཆོག་ཡིག་ཅིག་ གདམ་ཁ་རྐྱབས།"

#: pkcs12.setpassword.message
msgid "The certificate backup password you set here protects the backup file that you are about to create.  You must set this password to proceed with the backup."
msgstr "ཁྱོད་ཀྱིས་ནཱ་ལུ་ གཞི་སྒྲིག་འབད་མི་ རྒྱབ་ཐག་ཆོག་ཡིག་དེ་གིས་ ཁྱོད་ཀྱིས་གསར་བསྐྲུན་འབད་ནི་འབད་མི་ རྒྱབ་ཐག་ཡིག་སྣོད་དེ་ ཉེན་སྐྱོབ་འབད་འོང་། རྒྱབ་ཐག་བཟོ་ནི་ལུ་ ཁྱོད་ཀྱིས་ཆོག་ཡིག་འདི་ གཞི་སྒྲིག་འབད་དགོ།"

#: pkcs12.setpassword.label1
msgid "Certificate backup password:"
msgstr "ལག་ཁྱེར་རྒྱབ་ཐག་ཆོག་ཡིག་:"

#: pkcs12.setpassword.label2
msgid "Certificate backup password (again):"
msgstr "ལག་ཁྱེར་རྒྱབ་ཐག་ཆོག་ཡིག་ (ལོག་སྟེ་):"

#: pkcs12.setpassword.reminder
msgid "Important: If you forget your certificate backup password, you will not be able to restore this backup later.  Please record it in a safe location."
msgstr "གལ་ཅན་: ཁྱོད་ཀྱིས་ཁྱོད་རའི་ ལག་ཁྱེར་གྱི་རྒྱབ་ཐག་ཆོག་ཡིག་དེ་ བརྗེད་སོང་པ་ཅིན་ ཤུལ་ལས་རྒྱབ་ཐག་འདི་ སོར་ཆུད་འབད་མི་ཚུགས། དེ་གིས་སྦེ་ ཉེན་སྲུང་ཅན་གྱི་གནས་ཁོངས་ཅིག་ནང་ བཀོད་བཞག་དགོ།"

#: pkcs12.getpassword.title
msgid "Password Entry Dialog"
msgstr "ཆོག་ཡིག་བཙུགས་སའི་ཌའི་ལོག"

#: pkcs12.getpassword.message
msgid "Please enter the password that was used to encrypt this certificate backup."
msgstr "ལག་ཁྱེར་རྒྱབ་ཐག་འདི་ གསང་བ་བཟོ་ནི་ལུ་ ལག་ལེན་འཐབ་མི་ ཆོག་ཡིག་དེ་བཙུགས་གནང་།"

#: chooseToken.title
msgid "Choose Token Dialog"
msgstr "ངོ་རྟགས་ཌའོ་ལོག་ གདམ་ཁ་རྐྱབས་"

#: chooseToken.message1
msgid "Please choose a token."
msgstr "ངོ་རྟགས་ཅིག་ གདམ་ཁ་བརྐྱབ་གནང་།"

#: escrowWarn.title
msgid "Encryption Key Copy"
msgstr "གསང་བཟོའི་ལྡེ་མིག་འདྲ་"

#: escrowWarn.message1
msgid "Important: This certificate authority has asked to make a backup of your encryption private key."
msgstr "གལ་ཅན་: ལག་ཁྱེར་དབང་འཛིན་འདི་གིས་ ཁྱོད་རའི་གསང་བཟོའི་སྒེར་གྱི་ལྡེ་མིག་གི་ རྒྱབ་ཐག་ཅིག་ བཟོ་ཟེར་སླབ་ནུག།"

#: escrowWarn.benefit1
msgid "The benefit is that if you lose access to your encryption private key, you can request a copy from this certificate authority."
msgstr "འདི་གི་ཕན་ཐོགས་དེ་ ཁྱོད་ཀྱིས་ཁྱོད་རའི་གསང་བཟོའི་སྒེར་གྱི་ལྡེ་མིག་ལུ་ འཛུལ་སྤྱོད་འབད་མ་ཚུགས་པ་ཅིན་ ཁྱོད་ཀྱིས་ལག་ཁྱེར་དབང་འཛིན་འདི་ལས་ འདྲ་ཅིག་ལེན་ཚུགས།"

#: escrowWarn.message2
msgid "However, your encryption private key will be stored by the certificate authority, and could be used to read your encrypted email or documents without your permission."
msgstr "ཨིན་རུང་ ཁྱོད་ཀྱི་གསང་བཟོའི་སྒེར་གྱི་ལྡེ་མིག་དེ་ ལག་ཁྱེར་དབང་འཛིན་གྱིས་ གསོག་འཇོག་སྦེ་བཞག་སྟེ་ ཁྱོད་ཀྱི་གནང་བ་མེད་པར་ ཁྱོད་རའི་གསང་བཟོས་གློག་འཕྲིན་དང་ ཡིག་ཆ་ཚུ་ ལྷག་ནི་ལུ་ལག་ལེན་འཐབ་ཚུགས།"

#: serverCrlNextupdate.message
msgid "Please ask your system administrator for assistance"
msgstr "གྲོགས་རམ་གྱི་དོན་ལུ་ ཁྱོད་རའི་རིམ་ལུགས་བདག་སྐྱོང་པ་ལུ་སླབ་གནང་།"

# Strings for the CreateCertInfo dialog
#: createCertInfo.title
msgid "Generating A Private Key"
msgstr "སྒེར་གྱི་ལྡེ་མིག་ཅིག་བཟོ་བསྐྲུན་འབད་དོ་"

#: createCertInfo.msg1
msgid "Key Generation in progress... This may take a few minutes...."
msgstr "ལྡེ་མིག་བཟོ་བསྐྲུན་འབད་དོ་་་་་་ འདི་གི་དོན་ལུ་ སྐར་མ་དག་པ་ཅིག་འགོར་འོང་།"

#: createCertInfo.msg2
msgid "Please wait..."
msgstr "བསྒུག་གནང་་་་་"

# Form Signing confirmation prompt
#: formSigning.title
msgid "Text Signing Request"
msgstr "ཚིག་ཡིག་ལུ་མིང་རྟགས་བཀོད་དགོ་པའི་ཞུ་བ་"

#: formSigning.cert
msgid "Signing Certificate"
msgstr "མིང་རྟགས་བཀོད་ནིའི་ལག་ཁྱེར་"

#: formSigning.confirmPassword
msgid "To confirm you agree to sign this text message using your selected certificate, please confirm by entering the master password:"
msgstr "ཁྱོད་རའི་སེལ་འཐུ་འབད་མི་ལག་ཁྱེར་ ལག་ལེན་འཐབ་པའི་ཐོག་ལས་ ཚིག་ཡིག་འཕྲིན་དོན་འདི་ལུ་ མིང་རྟགས་བཀོད་ནི་ལུ་ ཐད་རི་འབའ་རི་ ཁས་ལེན་འབད་ནིའི་དོན་ལུ་ ཆོག་ཡིག་ཨམ་བཙུགས་པའི་ཐོག་ལས་ ངེས་དཔྱད་འབད་གནང་།"

