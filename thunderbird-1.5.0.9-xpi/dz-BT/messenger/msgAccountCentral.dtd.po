#: emailSectionHdr.label
msgid "Email"
msgstr "གློག་འཕྲིན་"

#: readMsgsLink.label
msgid "Read messages"
msgstr "འཕྲིན་དོན་ཚུ་ལྷག་"

#: composeMsgLink.label
msgid "Write a new message"
msgstr "འཕྲིན་དོན་གསརཔ་ཅིག་བྲིས་"

#: newsSectionHdr.label
msgid "Newsgroups"
msgstr "གནས་ཚུལ་སྡེ་ཚན་ཚུ་"

#: subscribeNewsLink.label
msgid "Manage newsgroup subscriptions"
msgstr "གནས་ཚུལ་སྡེ་ཚན་གྱི་མཁོ་མངགས་ཚུ་འཛིན་སྐྱོང་འཐབ་"

#: accountsSectionHdr.label
msgid "Accounts"
msgstr "རྩིས་ཐོ་ཚུ་"

#: subscribeImapFolders.label
msgid "Manage folder subscriptions"
msgstr "སྣོད་འཛིན་གྱི་མཁོ་མངགས་ཚུ་འཛིན་སྐྱོང་འཐབ།"

#: settingsLink.label
msgid "View settings for this account"
msgstr "རྩིས་ཐོ་འདི་གི་དོན་ལུ་ སྒྲིག་སྟངས་ཚུ་སྟོན་"

#: newAcctLink.label
msgid "Create a new account"
msgstr "རྩིས་ཐོ་ཅིག་གསར་བསྐྲུན་འབད་"

#: advFeaturesSectionHdr.label
msgid "Advanced Features"
msgstr "མཐོ་རིམ་ཅན་གྱི་ཁྱད་རྣམ་ཚུ་"

#: searchMsgsLink.label
msgid "Search messages"
msgstr "འཕྲིན་དོན་ཚུ་འཚོལ་ཞིབ་འབད་"

#: filtersLink.label
msgid "Manage message filters"
msgstr "འཕྲིན་དོན་གྱི་ཚགས་མ་ཚུ་འཛིན་སྐཡོང་འཐབ་"

#: junkSettings.label
msgid "Junk mail settings"
msgstr "མཁོ་མེད་ཡིག་འཕྲིན་གྱི་སྒྲིག་སྟངས་ཚུ་"

#: offlineLink.label
msgid "Offline settings"
msgstr "གློག་ཐོག་མེན་པའི་སྒྲིག་སྟངས་ཚུ་"

#: rssSectionHdr.label
msgid "RSS News &amp; Blogs"
msgstr "ཨར་ཨེསི་ཨེསི་ གནས་ཚུལ་&amp; ཝེབ་དྲན་དེབ་"

#: subscibeRSS.label
msgid "Manage subscriptions"
msgstr "མཁོ་མངགས་ཚུ་འཛིན་སྐྱོང་འཐབ་"

