#: dialog.title
msgid "Send Options"
msgstr "གདམ་ཁ་ཚུ་གཏང་"

#: sendMail.title
msgid "Text Format"
msgstr "ཚིག་ཡིག་རྩ་སྒྲིག་"

#: sendMaildesc.label
msgid "When sending messages in HTML format and one or more recipients are not listed as being able to receive HTML:"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་རྩ་སྒྲིག་ནང་ འཕྲིན་དོན་ཚུ་གཏངམ་ད་ ལེན་མཁན་གཅིག་ ཡང་ན་ དེ་ལས་མངམ་ ཨེཆ་ཊི་ཨེམ་ཨེལ་ འཐོབ་ཚུགས་པའི་ཐོ་ཡིག་ནང་ མ་འཁོད་པའི་སྐབས་ལུ་:"

#: askMe.label
#: askMe.accesskey
msgid "As&k me what to do"
msgstr "ག་ཅི་འབདན་ན་ ང་ལུ་དྲིས་(&k)"

#: convertPlain.label
#: convertPlain.accesskey
msgid "&Convert the message to plain text"
msgstr "འཕྲིན་དོན་དེ་ ཡིག་རྐྱང་ལུ་གཞི་བསྒྱུར་འབད་(&C)"

#: sendHTML.label
#: sendHTML.accesskey
msgid "&Send the message in HTML anyway"
msgstr "འཕྲིན་དོན་དེ་ ཨེཆ་ཊི་ཨེམ་ཨེལ་ནང་ གང་རུང་སྦེ་གཏང་(&S)"

#: sendBoth.label
#: sendBoth.accesskey
msgid "S&end the message in both plain text and HTML"
msgstr "འཕྲིན་དོན་དེ་ ཡིག་རྐྱང་དང་ཨེཆ་ཊི་ཨེམ་ཨེལ་ གཉིས་ཆ་ར་ནང་གཏང་།(&e)"

#: receiveHTML.label
msgid "If you know a particular recipient can receive HTML mail, uncheck the 'Send email as plain text (no HTML)' checkbox on the recipient's Address Book card."
msgstr "ཁྱོད་ཀྱིསཐོབ་མཁན་ཅིག་གིས་ ཨེཆ་ཊི་ཨེམ་ཨེལ་ འཐོབ་ཚུགསཔ་ཤེས་པ་ཅིན་ ཐོབ་མཁན་དེ་གི་ཁ་བྱང་ཀི་དེབ་ཤོག་བྱང་གུ་ 'གློག་འཕྲིན་ཡིག་རྐྱང་སྦེ་གཏང་(ཨེཆ་ཊི་ཨེམ་ཨེལ་མེན)' ཟེར་མི་དཔྱད་སྒྲོམ་དེ་ལུ་ ཨིན་རྟགས་མེདཔ་བཟོ།"

#: override.label
msgid "Note: Use the Address Book to specify preferred text formats for recipients."
msgstr "དྲན་འཛིན་: ཐོབ་མཁན་གྱི་དོན་ལུ་ དགའ་གདམ་ཅན་གྱི་ཚིག་ཡིག་རྩ་སྒྲིག་ གསལ་བཀོད་འབད་ནི་ལུ་ ཁ་བྱང་ཀི་དེབ་ལག་ལེན་འཐབ།"

#: domaindesc.label
msgid "When you send a message to an address with one of the domain names listed below, &brandShortName; automatically sends the message in the correct format."
msgstr "འོག་ལུ་བཀོད་དེ་ཡོད་པའི་ ཌོ་མེན་གཅིག་གི་མིང་ཡོད་པའི་ ཁ་བྱང་ཅིག་ལུ་ འཕྲིན་དོན་གཏང་བའི་སྐབས་ &brandShortName; གིས་ རང་བཞིན་གྱིས་འཕྲིན་དོན་དེ་ ངེས་བདེན་རྩ་སྒྲིག་ནང་གཏངམ་ཨིན།"

#: HTMLTab.label
#: HTMLTab.accesskey
msgid "&HTML Domains"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ཌོ་མེན་ཚུ་(&H)"

#: PlainTextTab.label
#: PlainTextTab.accesskey
msgid "&Plain Text Domains"
msgstr "ཡིག་རྐྱང་གི་ཌོ་མེན་ཚུ་(&P)"

#: AddButton.label
msgid "Add..."
msgstr "ཁ་སྐོང་རྐྱབས་་་་"

#: AddHtmlDomain.accesskey
msgid "a"
msgstr "a"

#: AddPlainText.accesskey
#: add.htmldomain.accesskey
#: add.plaintext.accesskey
msgid "d"
msgstr "d"

#: DeleteButton.label
msgid "Delete"
msgstr "བཏོན་གཏང་"

#: DeleteHtmlDomain.accesskey
msgid "l"
msgstr "l"

#: DeletePlainText.accesskey
msgid "t"
msgstr "t"

#: add.htmltitle
msgid "Add HTML Domain Name"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ཌོ་མེན་མིང་ ཁ་སྐོང་རྐྱབས།"

#: add.htmldomain
msgid "HTML Domain Name:"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་ཌོ་མེན་མིང་:"

#: add.plaintexttitle
msgid "Add Plain Text Domain Name"
msgstr "ཡིག་རྐྱང་ཌོ་མེན་མིང་ ཁ་སྐོང་རྐྱབས་"

#: add.plaintextdomain
msgid "Plain Text Domain Name:"
msgstr "ཡིག་རྐྱང་ཌོ་མེན་མིང་ :"

# LOCALIZATION NOTE: do not translate @string@
#: domainnameError.title
msgid "Domain Name Error"
msgstr "ཌོ་མེན་མིང་གི་འཛོལ་བ་"

#: duplicationError.label
msgid "The domain name @string@ already exists in this list."
msgstr "ཌོ་མེན་གྱི་མིང་ @string@ དེ་ ཧེ་མ་ལས་ར་ ཐོ་ཡིག་འདི་ནང་འདུག"

# LOCALIZATION NOTE: do not translate @string@
#: dualEntryError.label
msgid "A domain name cannot appear in both the HTML and Plain Text lists. The domain name @string@ already exists."
msgstr "ཌོ་མེན་མིང་ཅིག་ ཨེཆ་ཊི་ཨེམ་ཨེལ་དང་ ཡིག་རྐྱང་ཐོ་ཡིག་གཉིས་ཆ་ར་ནང་ འབྱུང་མི་ཚུགས། ཌོམེན་མིང་ @string@ དེ་ ཧེ་མ་ལས་ར་འདུག།"

