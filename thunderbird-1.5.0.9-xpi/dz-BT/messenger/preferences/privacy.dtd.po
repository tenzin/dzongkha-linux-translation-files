msgid "Security"
msgstr "སྲུང་སྐྱོབ་"

#: disableImageInMailNews.label
#: disableImageInMailNews.accesskey
msgid "&Block loading of remote images in mail messages"
msgstr "ཡིག་འཕྲིན་གྱི་འཕྲིན་དོན་ཚུ་ནང་ ཐག་རིང་གི་གཟུགས་བརྙན་ཚུ་ མངོན་གསལ་འབད་ནི་ལུ་ བཀག་ཆ་འབད་(&B)"

#: allowRemoteImagesForFriends.label
#: allowRemoteImagesForFriends.accesskey
msgid "&Allow remote images if the sender is in my:"
msgstr "གཏང་མི་དེ་ ངེད་ཀྱི་ནང་ཡོད་པ་ཅིན་ ཐག་རིང་གཟུགས་བརྙན་ཚུ་ མངོན་གསལ་འབད་བཅུག:(&A)"

#: enbJsCheckMailNews.label
#: enbJsCheckMailNews.accesskey
msgid "Block &JavaScript in mail messages"
msgstr "ཡིག་འཕྲིན་གྱི་འཕྲིན་དོན་ཚུ་ནང་ ཇ་བ་ཡིག་ཚུགས་ བཀག་ཆ་འབད།(&J)"

# Phishing Detector
#: phishingDetector.intro
msgid "&brandShortName; can analyze messages for possible email scams by looking for common techniques used to deceive you."
msgstr "&brandShortName; གིས་ ཁྱོད་ལུ་གཡོ་སྒྱུ་གཏང་མི་ སྤྱིར་བཏང་ཐབས་རིག་ཚུ་ལུ་ བལྟ་བའི་ཐོག་ལས་ གློག་འཕྲིན་གྱི་གཡོ་སྒྱུ་ཡོད་མེད་ཚུ་གི་དོན་ལུ་ འཕྲིན་དོན་ཚུ་དཔྱད་ཞིབ་འབད་ཚུགས།"

#: enablePhishingDetector.label
#: enablePhishingDetector.accesskey
msgid "&Check mail messages for email scams"
msgstr "གློག་འཕྲིན་གྱི་གཡོ་སྒྱུ་ཡོད་མེད་བལྟ་ནིའི་དོན་ལུ་ འཕྲིན་དོན་ཚུ་ཞིབ་དཔྱད་འབད་(&C)"

# Passwords
#: savedPasswords.intro
msgid "&brandShortName; can remember password information for all of your accounts so you do not need to re-enter your login details."
msgstr "&brandShortName; གིས་ཁྱོད་ཀྱི་རྩིས་ཐོ་ཆམཉམ་གྱི་དོན་ལུ་ ཆོག་ཡིག་གི་བརྡ་དོན་ཚུ་ སེམས་ཁར་བཞག་ཚུགསཔ་ལས་ ཁྱོད་ཀྱིས་ཁྱོད་རའི་ནང་བསྐྱོད་ཀྱི་གནད་དོན་ཚུ་ ལོག་བཙུགས་མི་དགོ།"

#: encryptEnabled.label
#: encryptEnabled.accesskey
msgid "U&se a master password to encrypt stored passwords"
msgstr "གསོག་འཇོག་འབད་དེ་ཡོད་པའི་ཆོག་ཡིག་ཚུ་ གསང་བ་བཟོ་ནི་ལུ་ ཆོག་ཡིག་ཨམ་ཅིག་ ལག་ལེན་འཐབ།(&s)"

#: masterPassword.intro
msgid "When set, the Master Password protects all your passwords - but you must enter it once per session."
msgstr "ཆོག་ཡིག་ཨམ་ གཞི་སྒྲིག་འབད་ཞིནམ་ལས་ ཁྱོད་ཀྱི་ཆོག་ཡིག་ཆ་མཉམ་ར་ ཉེན་སྐྱོབ་འབདཝ་ཨིན། དེ་འབདཝ་ད་ ཁྱོད་ཀྱིས་ལཱ་ཡུན་རེ་ལུ་ ཆོག་ཡིག་ཨམ་དེ་ ཚར་རེ་བཙུགས་དགོ།"

#: setMasterPassword.label
#: setMasterPassword.accesskey
msgid "&Master Password"
msgstr "ཆོག་ཡིག་ཨམ་(&M)"

#: removeMasterPassword.label
#: removeMasterPassword.accesskey
msgid "&Remove Master Password..."
msgstr "ཆོག་ཡིག་ཨམ་རྩ་བསྐྲད་གཏང་་་་(&R)"

#: viewPasswords.label
#: viewPasswords.accesskey
msgid "&View Saved Passwords"
msgstr "བསྲུངས་ཏེ་ཡོད་པའི་ཆོག་ཡིག་ཚུ་སྟོན་(&V)"

# Security
#: certificates.label
msgid "Certificates"
msgstr "ལག་ཁྱེར་ཚུ་"

#: certificatesInfo.label
msgid "Manage certificates, revocation lists, certificate verification and security devices."
msgstr "ལག་ཁྱེར་དང་ ཆ་མེད་བཏང་བའི་ཐོ་ཡིག་ ལག་ཁྱེར་བདེན་སྦྱོར་ སྲུང་སྐྱོབ་ཐབས་འཕྲུལ་ཚུ་ འཛིན་སྐྱོང་འཐབ།"

#: viewCertificates.label
#: viewCertificates.accesskey
msgid "V&iew Certificates"
msgstr "ལག་ཁྱེར་ཚུ་སྟོན་(&i)"

#: viewCRLs.label
#: viewCRLs.accesskey
msgid "R&evocation Lists"
msgstr "ཆ་མེད་བཏང་བའི་ཐོ་ཡིག་ཚུ་(&e)"

#: viewOCSP.label
#: viewOCSP.accesskey
msgid "Veri&fication"
msgstr "བདེན་སྦྱོར་(&f)"

#: viewSecurityDevices.label
#: viewSecurityDevices.accesskey
msgid "Sec&urity Devices"
msgstr "སྲུང་སྐྱོབ་ཐབས་འཕྲུལ་ཚུ་(&u)"

# Anti Virus
#: antiVirus.intro
msgid "&brandShortName; can make it easy for anti-virus software to analyze incoming mail messages for viruses before they are stored locally."
msgstr "&brandShortName; གིས་ གནོད་འབུཔ་ཚུ་ གསོག་འཇོག་མ་འབདཝ་ལས་ འབུཔ་གནོན་མཉེན་ཆས་ཀྱིས་ ནང་འབྱོར་ཡིག་འཕྲིན་གྱི་འཕྲིན་དོན་ཚུ་ དཔྱད་ཞིབ་འབད་ཚུགསཔ་བཟོ་ཨིན།"

#: antiVirus.label
#: antiVirus.accesskey
msgid "A&llow anti-virus clients to quarantine individual incoming messages"
msgstr "འབུཔ་གནོན་གྱི་ཞབས་ཏོག་སྤྱོད་མི་ཚུ་ལུ་ ནང་འབྱོར་འཕྲིན་དོན་ རེ་རེ་བཞིན་དུ་ ཟུར་བཀག་འབད་བཅུག།(&l)"

