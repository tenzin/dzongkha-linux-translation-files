#: generalSettings.caption
msgid "Default Settings"
msgstr "སྔོན་སྒྲིག་སྒྲིག་སྟངས་"

#: generalSettings.label
msgid "Make &brandShortName; the default application for:"
msgstr "&brandShortName; འདི་སྔོན་སྒྲིག་གློག་རིམ་སྦེ་བཟོ་:"

#: setDefaultMailClient.label
#: setDefaultMailClient.accesskey
msgid "&Mail"
msgstr "ཡིག་འཕྲིན་གྱི་དོན་ལུ་(&M)"

#: setDefaultNewsClient.label
#: setDefaultNewsClient.accesskey
msgid "&News"
msgstr "གནས་ཚུལ་དོན་ལུ་(&N)"

#: setDefaultFeedClient.label
#: setDefaultFeedClient.accesskey
msgid "RSS &Feeds"
msgstr "ཨར་ཨེསི་ཨེསི་ མཁོ་སྤྲོད་དོན་ལུ་( &F)"

#: checkDefaultMailClient.label
#: checkDefaultMailClient.accesskey
msgid "&brandShortName; should check to see if it is the default mail clie&nt when starting"
msgstr "&brandShortName; གིས་འགོ་བཙུགས་སྐབས་ལུ་ འདི་སྔོན་སྒྲིག་ཡིག་འཕྲིན་ཞབས་ཏོག་སྤྱོད་མི་ ཨིན་ན་མེན་བལྟ་ནི་ལུ་ ཞིབ་དཔྱད་འབད་དགོ།"

#: defaultClientAdvanced.label
#: defaultClientAdvanced.accesskey
msgid "A&dvanced..."
msgstr "མཐོ་རིམ་ཅན་་་་(&d)"

#: windowSettings.label
msgid "Window Configuration"
msgstr "སྒོ་སྒྲིག་རིམ་སྒྲིག་"

#: selectWindowLayout.label
#: selectWindowLayout.accesskey
msgid "&Select the window layout you prefer for Mail."
msgstr "ཁྱོད་ཀྱིས་ཡིག་འཕྲིན་གྱི་དོན་ལུ་དགའ་མི་ སྒོ་སྒྲིག་སྒྲིག་བཀོད་དེ་ སེལ་འཐུ་འབད།(&S)"

#: messengerStartPage.label
msgid "&brandShortName; Start Page"
msgstr "&brandShortName; ཤོག་བཙུགས་ཤོག་ལེབ་"

#: enableStartPage.label
#: enableStartPage.accesskey
msgid "&When &brandShortName; launches, show the Start Page in the message area"
msgstr "&brandShortName; གསར་བཙུགས་འབདཝ་ད་ འཕྲིན་དོན་མངའ་ཁོངས་ནང་ ཤོག་ལེབ་འགོ་བཙུགས་སྟོན།(&W)"

#: location.label
#: location.accesskey
msgid "&Location:"
msgstr "མངའ་ཁོངས་:(&L)"

#: useDefault.label
#: useDefault.accesskey
msgid "&Restore Default"
msgstr "སྔོན་སྒྲིག་སོར་ཆུད་འབད་(&R)"

#: bounceSystemDockIcon.label
#: bounceSystemDockIcon.accesskey
msgid "An&imate the dock icon"
msgstr "ཌོཀ་གི་ངོས་དཔར་བསྒུལ་ཅན་བཟོ་(&i)"

#: newMessagesArrive.label
msgid "When new messages arrive:"
msgstr "འཕྲིན་དོན་གསརཔ་ཚུ་ལྷོད་པའི་སྐབས་:"

#: playSound.label
#: playSound.accesskey
msgid "&Play a sound"
msgstr "སྒྲ་སྐད་ཅིག་གཏང་(&P)"

#: showAnimatedAlert.label
#: showAnimatedAlert.accesskey
msgid "&Show an alert"
msgstr "དྲན་བརྡ་ཅིག་སྟོན་(&S)"

#: soundbutton.label
#: soundbutton.accesskey
msgid "&Advanced..."
msgstr "མཐོ་རིམ་ཅན་་་་(&A)"

#: showConnections.label
#: showConnections.accesskey
msgid "C&onnection Settings..."
msgstr "མཐུད་ལམ་སྒྲིག་སྟངས་་་་(&o)"

#: proxiesInfo.label
msgid "Determine how &brandShortName; connects to the Internet."
msgstr "&brandShortName; གིས་ཨིན་ཊར་ནེཊི་ལུ་ ག་དེ་སྦེ་མཐུདཔ་ཨིན་ན་ ཐག་བཅདཔ་ཨིན།"

#: connectionsInfo.caption
msgid "Connection"
msgstr "མཐུད་ལམ་"

