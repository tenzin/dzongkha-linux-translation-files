#: dialog.title
#: receiptSettings.label
msgid "Return Receipts"
msgstr "སླར་ལོག་འབྱོར་རྟགས་"

#: requestReceipt.label
#: requestReceipt.accesskey
msgid "&When sending messages, always request a return receipt"
msgstr "འཕྲིན་དོན་ཚུ་གཏང་བའི་སྐབས་ ཨ་རྟག་ར་ སླར་ལོག་འབྱོར་རྟགས་ཅིག་གི་ཞུ་བ་འབད་(&W)"

#: receiptArrive.label
msgid "When a receipt arrives:"
msgstr "འབྱོར་རྟགས་ཅིག་ལྷོད་པའི་སྐབས་:"

#: leaveIt.label
#: leaveIt.accesskey
msgid "Leave it in my &Inbox"
msgstr "ངེད་ཀྱི་ནང་སྒྲོམ་ནང་བཞག་(&I)"

#: moveToSent.label
#: moveToSent.accesskey
msgid "&Move it to my &quot;Sent&quot; folder"
msgstr "ངེད་ཀྱི་&quot;Sent&quot;སྣོད་འཛིན་ནང་སྤོ་(&M)"

#: requestMDN.label
msgid "When I receive a request for a return receipt:"
msgstr "ང་གིས་སླར་ལོག་འབྱོར་རྟགས་ཅིག་ཅིག་གི་ ཞུ་བ་ཅིག་ཐོབ་པའི་སྐབས་:"

#: never.label
#: never.accesskey
msgid "&Never send a return receipt"
msgstr "ནམ་ཡང་སླར་ལོག་འབྱོར་རྟགས་ཅིག་མ་བཏང་།(&N)"

#: returnSome.label
#: returnSome.accesskey
msgid "Allow &return receipts for some messages"
msgstr "འཕྲིན་དོན་ལ་ལོ་ཅིག་གི་དོན་ལུ་ སླར་ལོག་འབྱོར་རྟགས་གཏང་བཅུག་(&r)"

#: notInToCc.label
#: notInToCc.accesskey
msgid "I&f I'm not in the To or Cc of the message:"
msgstr "ང་ འཕྲིན་དོན་གཏང་ཡུལ་ ཡང་ན་ ཤོག་གནག་འདྲ་བཤུས་ནང་མེད་པ་ཅིན་:(&f)"

#: outsideDomain.label
#: outsideDomain.accesskey
msgid "If &the sender is outside my domain:"
msgstr "གཏང་མི་དེ་ ངེད་ཀྱི་ཌོ་མེན་གྱི་ཕྱི་ཁར་ཨིན་པ་ཅིན་:(&t)"

#: otherCases.label
#: otherCases.accesskey
msgid "In &all other cases:"
msgstr "གནད་དོན་ཆ་མཉམ་ནང་:(&a)"

#: askMe.label
msgid "Ask me"
msgstr "ང་ལུ་དྲིས་"

#: alwaysSend.label
msgid "Always send"
msgstr "ཨ་རྟག་ར་གཏང་"

#: neverSend.label
msgid "Never send"
msgstr "ནམ་ཡང་མ་གཏང་"

