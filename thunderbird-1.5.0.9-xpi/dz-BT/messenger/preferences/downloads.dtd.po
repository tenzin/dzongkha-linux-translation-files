#: askOnSave.label
msgid "Attachments Folder"
msgstr "མཉམ་སྦྲགས་སྣོད་འཛིན་"

#: alwaysAsk.label
#: alwaysAsk.accesskey
msgid "&Ask me where to save every file"
msgstr "ཡིག་སྣོད་རེ་རེ་བཞིན་དུ་་ག་ཏེ་བསྲུང་ནི་ཨིན་ན་ ང་ལུ་དྲིས།(&A)"

#: neverAsk.label
#: neverAsk.accesskey
msgid "&Save all attachments to this folder:"
msgstr "མཉམ་སྦྲགས་ཆ་མཉམ་ སྣོད་འཛིན་འདི་ནང་སྲུངས།(&S)"

#: desktop.label
msgid "Desktop"
msgstr "ཌེཀསི་ཊོཔ་"

#: downloads.label
msgid "My Downloads"
msgstr "ངེད་ཀྱི་ཕབ་ལེན་ཚུ་"

#: chooseFolderWin.label
#: chooseFolderWin.accesskey
msgid "&Browse..."
msgstr "བརྡ་འཚོལ་་་་(&B)"

#: chooseFolderMac.label
#: chooseFolderMac.accesskey
msgid "&Choose..."
msgstr "གདམ་ཁ་རྐྱབས་་་(&C)"

#: fileTypes.label
msgid "Download Actions"
msgstr "ཕབ་ལེན་བྱ་བ་ཚུ་"

#: configureActions.label
#: configureActions.accesskey
msgid "&View &amp; Edit Actions..."
msgstr "&amp;སྟོན། ཞུན་དག་བྱ་བ་ཚུ་་་(&V)"

#: fileTypesDescription.label
msgid "&brandShortName; can automatically save or open attachments of certain types."
msgstr "&brandShortName; གིས་ མཉམ་སྦྲགས་ཀྱི་དབྱེ་བ་ལ་ལོ་ཅིག་ རང་བཞིན་གྱིས་བསྲུང་ནིའམ་ ཁ་ཕྱེ་ནི་ཚུ་འབད་ཚུགསཔ་ཨིན།"

#: fileTypeColName.label
msgid "File Type"
msgstr "ཡིག་སྣོད་དབྱེ་བ་"

#: fileHandlerColName.label
msgid "Application"
msgstr "གློག་རིམ་"

#: editFileHandler.label
#: editFileHandler.accesskey
msgid "Change &Action..."
msgstr "བྱ་བ་སོར་་་་(&A)"

#: removeFileHandler.label
#: removeFileHandler.accesskey
msgid "&Remove"
msgstr "རྩ་བསྐྲད་གཏང་(&R)"

