#: doNotDownloadPop3Movemail.label
msgid "To save disk space, do not download:"
msgstr "ཌིཀསི་གི་བར་སྟོང་བསྲུང་ནི་ལུ་ ཕབལེན་མ་འབད་:"

#: doNotDownload.label
msgid "To save disk space, do not download for offline use:"
msgstr "ཌིཀསི་གི་བར་སྟོང་བསྲུང་ནི་ལུ་ གློག་ཐོག་མེན་པའི་ ལག་ལེན་གྱི་དོན་ལུ་ ཕབ་ལེཔ་མ་འབད་:"

#: offlineNewFolder.label
#: offlineNewFolder.accesskey
msgid "When I &create new folders, select them for offline use"
msgstr "ང་གིས་སྣོད་འཛིན་གསརཔ་ གསར་བསྐྲུན་འབདཝ་ད་ དེ་ཚུ་གློག་ཐོག་མེན་པའི་ ལག་ལེན་གྱི་དོན་ལུ་ སེལ་འཐུ་འབད་(&c)"

#: offlineNotDownload.label
#: offlineNotDownload.accesskey
msgid "&Messages larger than"
msgstr "ལས་སྦོམ་མི་འཕྲིན་དོན་ཚུ་(&M)"

#: kb.label
msgid "KB"
msgstr "ཀེ་བི་"

#: days.label
msgid "days"
msgstr "ཉིན་གྲངས་"

#: daysOld.label
msgid "days old"
msgstr "ཉིན་གྲངས་ལངས་མི་"

#: message.label
msgid "messages"
msgstr "འཕྲིན་དོན་ཚུ་"

#: nntpNotDownloadRead.label
#: nntpNotDownloadRead.accesskey
msgid "&Read messages"
msgstr "འཕྲིན་དོན་ཚུ་ལྷག་(&R)"

#: nntpDownloadMsg.label
#: nntpDownloadMsg.accesskey
msgid "M&essages more than"
msgstr "ལས་ལྷག་མི་འཕྲིན་དོན་ཚུ་(&e)"

#: retentionCleanup.label
msgid "To recover disk space, old messages can be permanently deleted"
msgstr "ཌིཀསི་ནང་བར་སྟོང་སླར་གསོ་འབད་ནིའི་དོན་ལུ་ འཕྲིན་དོན་རྙིངམ་དེ་ཚུ་ རྟག་བརྟན་སྦེ་ བཏོན་གཏང་བཏུབ།"

#: retentionKeepMsg.label
#: retentionKeepMsg.accesskey
msgid "Delete &messages more than"
msgstr "ལས་ལྷག་མི་འཕྲིན་དོན་ཚུ་ བཏོན་གཏང་(&m)"

#: retentionKeepAll.label
#: retentionKeepAll.accesskey
msgid "Don't delete &any messages"
msgstr "འཕྲིན་དོན་གཅིག་ཡང་བཏོན་མ་གཏང་(&a)"

#: retentionKeepNew.label
#: retentionKeepNew.accesskey
msgid "De&lete all but the last"
msgstr "མཐའ་མཇུག་དེ་མ་གཏོགས་ འཕྲིན་དོན་ཆ་མཉམ་བཏོན་གཏང་།(&l)"

#: retentionKeepUnread.label
#: retentionKeepUnread.accesskey
msgid "Always delete &read messages"
msgstr "ཨ་རྟག་ར་ ལྷག་ཡོད་པའི་འཕྲིན་དོན་ཚུ་ བཏོན་གཏང་(&r)"

#: nntpRemoveBody.label
#: nntpRemoveBody.accesskey
msgid "&Only message bodies less than"
msgstr "ལས་ཉུང་མི་ འཕྲིན་དོན་མ་གཟུགས་ཚུ་རྐྱངམ་གཅིག་(&O)"

#: offlineSelectNntp.label
msgid "Select newsgroups for offline use..."
msgstr "གློག་ཐོག་མེན་པའི་ལག་ལེན་གྱི་དོན་ལུ་ གནས་ཚུལ་སྡེ་ཚན་ཚུ་ སེལ་འཐུ་འབད་་་"

#: offlineSelectImap.label
msgid "Select folders for offline use..."
msgstr "གློག་ཐོག་མེན་པའི་ ལག་ལེན་གྱི་དོན་ལས་ སྣོད་འཛིན་ཚུ་སེལ་འཐུ་འབད་་་་"

#: offlineSelect.accesskey
msgid "S"
msgstr "S"

#: offlineDesc.label
msgid "You can download your messages locally so that they are available when you are working offline."
msgstr "ཁྱོད་ཀྱིས་ཁྱོད་རའི་འཕྲིན་དོན་ཚུ་ ཉེ་གནས་ལུ་ཕབ་ལེན་འབད་བཞག་པ་ཅིན་ ཁྱོད་གློག་ཐོག་མེནམ་སྦེ་ ལཱ་འབད་བའི་སྐབས་ལུ་འཐོབ་ཚུགས།"

#: makeInboxMsgsAvailable.label
#: makeInboxMsgsAvailable.accesskey
msgid "Make the messages in my In&box available when I am working offline"
msgstr "ང་ གློག་ཐོག་ལུ་མེནམ་སྦེ་ ལཱ་འབད་བའི་སྐབས་ལུ་ ངེད་ཀྱི་ནང་སྒྲོམ་ནང་གི་ འཕྲིན་དོན་ཚུ་ འཐོབ་ཚུགསཔ་བཟོ།(&b)"

#: offlineGroupTitle.label
msgid "Offline"
msgstr "གློག་ཐོག་ལུ་མེནམ་"

#: diskspaceGroupTitle.label
msgid "Disk Space"
msgstr "ཌིཀསི་གི་བར་སྟོང་"

