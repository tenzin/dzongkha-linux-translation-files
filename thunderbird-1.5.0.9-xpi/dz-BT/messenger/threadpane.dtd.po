# Tooltips
#: columnChooser.tooltip
msgid "Click to select columns to display"
msgstr "བཀྲམ་སྟོན་འབད་ནིའི་ཀེར་ཐིག་ སེལ་འཐུ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད།"

#: threadColumn.tooltip
msgid "Click to display message threads"
msgstr "འཕྲིན་དོན་ཐེརེཌི་ བཀྲམ་སྟོན་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད།"

#: senderColumn.tooltip
msgid "Click to sort by sender"
msgstr "གཏང་མིའི་ཐོག་ལས་ དབྱེ་སེལ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད་"

#: recipientColumn.tooltip
msgid "Click to sort by recipient"
msgstr " ཐོབ་མིའི་ཐོག་ལས་ དབྱེ་སེལ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད་"

#: subjectColumn.tooltip
msgid "Click to sort by subject"
msgstr "དོན་ཚད་ཐོག་ལས་  དབྱེ་སེལ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད་"

#: dateColumn.tooltip
msgid "Click to sort by date"
msgstr "ཚེས་གྲངས་ཐོག་ལས་  དབྱེ་སེལ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད་"

#: priorityColumn.tooltip
msgid "Click to sort by priority"
msgstr "གཙོ་རིམ་ཐོག་ལས་  དབྱེ་སེལ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད་"

#: labelColumn.tooltip
msgid "Click to sort by label"
msgstr "ཁ་ཡིག་གི་ཐོག་ལས་  དབྱེ་སེལ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད་"

#: accountColumn.tooltip
msgid "Click to sort by account"
msgstr "རྩིས་ཐོའི་ཐོག་ལས་  དབྱེ་སེལ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད་"

#: statusColumn.tooltip
msgid "Click to sort by status"
msgstr "གནས་ཚད་ཐོག་ལས་  དབྱེ་སེལ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད་"

#: sizeColumn.tooltip
msgid "Click to sort by size"
msgstr "ཚད་ཀྱི་ཐོག་ལས་  དབྱེ་སེལ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད་"

#: junkStatusColumn.tooltip
msgid "Click to sort by junk status"
msgstr "མཁོ་མེད་གནས་ཚད་ཐོག་ལས་  དབྱེ་སེལ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད་"

#: unreadColumn.tooltip
msgid "Number of unread messages in thread"
msgstr "ཐེརེཌི་ནང་ མ་ལྷག་པར་ཡོད་པའི་ འཕྲིན་དོན་གྱི་གྱངས་ཁ་"

#: totalColumn.tooltip
msgid "Total number of messages in thread"
msgstr "ཐེརེཌི་ནང་གི་ འཕྲིན་དོན་གྱི་བསྡོམས་"

#: readColumn.tooltip
msgid "Click to sort by read"
msgstr "ལྷག་ཡོད་པའི་ཐོག་ལས་ དབྱེ་སེལ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད་"

#: flagColumn.tooltip
msgid "Click to sort by flag"
msgstr "ཟུར་རྟགས་ཐོག་ལས་  དབྱེ་སེལ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད་"

#: locationColumn.tooltip
msgid "Click to sort by location"
msgstr "གནས་ཁོངས་ཐོག་ལས་  དབྱེ་སེལ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད་"

#: idColumn.tooltip
msgid "Click to sort by order received"
msgstr "ཐོབ་ཡོད་པའི་བཀོད་རྒྱའི་ཐོག་ལས་  དབྱེ་སེལ་འབད་ནི་ལུ་ ཨེབ་གཏང་འབད་"

