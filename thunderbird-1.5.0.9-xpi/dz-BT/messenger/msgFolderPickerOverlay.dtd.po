# Labels
#: subscribechoosethis.label
msgid "choose this news server"
msgstr "གནས་ཚུལ་སར་བར་འདི་གདམ་ཁ་རྐྱབས་"

#: subscribeclickhere.label
msgid "click here to choose a news server"
msgstr "གནས་ཚུལ་སར་བར་ཅིག་་གདམ་ཁ་བརྐྱབ་ནི་ལུ་ ནཱ་ལུ་ཨེབ་གཏང་འབད་"

#: newfolderchoosethis.label
msgid "choose this for the parent"
msgstr "རྩ་ལག་གི་དོན་ལུ་་འདི་གདམ་ཁ་རྐྱབས་"

#: newfolderclickhere.label
msgid "Click here to select"
msgstr "སེལ་འཐུ་འབད་ནི་ལུ་ ནཱ་ལུ་ཨེབ་གཏང་འབད་"

#: renamefolderchoosethis.label
#: filemessageschoosethis.label
msgid "choose this folder"
msgstr "སྣོད་འཛིན་འདི་གདམ་ཁ་རྐྱབས་"

#: renamefolderclickhere.label
#: filemessagesclickhere.label
msgid "click here to choose a folder"
msgstr "སྣོད་འཛིན་ཅིག་ གདམ་ཁ་བརྐྱབ་ནི་ལུ་ ནཱ་ལུ་ཨེབ་གཏང་འབད་"

#: chooseaccountclickhere.label
msgid "Click here to select an account"
msgstr "རྩིས་ཐོ་ཅིག་སེལ་འཐུ་འབད་ནི་ལུ་ ནཱ་ལུ་ཨེབ་གཏང་འབད་"

#: choosefolderclickhere.label
msgid "Click here to select a folder"
msgstr "སྣོད་འཛིན་ཅིག་སེལ་འཐུ་འབད་ནི་ལུ་ ནཱ་ལུ་ཨེབ་གཏང་འབད་"

#: chooseserverToDeferTo.label
msgid "click here to select an account to use for storing messages"
msgstr "འཕྲིན་དོན་ཚུ་ གསོག་འཇོག་གི་དོན་ལུ་ ལག་ལེན་འཐབ་ནིའི་ རྩིས་ཐོ་ཅིག་སེལ་འཐུ་འབད་ནི་ལུ་ ནཱ་ལུ་ཨེབ་གཏང་འབད།"

