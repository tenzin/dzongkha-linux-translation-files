#: window.title
msgid "Junk Mail Controls"
msgstr "མཁོ་མེད་ཡིག་འཕྲིན་ཚད་འཛིན་"

#: mainDescription.label
msgid "&brandShortName; has several ways to detect junk mail. These controls evaluate incoming messages and identify those that are most likely to be junk mail."
msgstr "&brandShortName; ལུ་ མཁོ་མེད་ཡིག་འཕྲིན་ ངོས་འཛིན་འབད་ཐངས་ལེ་ཤ་ཅིག་ཡོད། ཚད་འཛིན་འདི་ཚུ་གིས་ ནང་འབྱོར་འཕྲིན་དོན་ཚུ་ བརྟག་ཞིབ་འབད་དེ་ མཁོ་མེད་ཡིག་འཕྲིན་ཨིནམ་བཟུམ་ཚུ་ ངོས་འཛིན་འབདཝ་ཨིན།"

# Tab titles
#: settingsTab.label
msgid "Settings"
msgstr "སྒྲིག་སྟངས་"

# Adaptive Filter tab
#: adaptiveTab.label
#: adaptiveCaption.label
msgid "Adaptive Filter"
msgstr "མཐུན་འགྱུར་ཅན་གྱི་ཚགས་མ་"

# Settings tab
#: move.label
msgid "Move incoming messages determined to be junk mail to:"
msgstr "ནང་འབྱོར་འཕྲིན་དོན་ཚུ་ནང་ལས་ མཁོ་མེད་ཡིག་འཕྲིན་སྦེ་ ངོས་འཛིན་བྱུང་མི་ཚུ་ སྤོ་བཤུད་འབད་:"

#: otherFolder.label
msgid "Other:"
msgstr "གཞན་:"

#: junkFolderOn.label
msgid "&quot;Junk&quot; folder on:"
msgstr "&quot;Junk&quot; སྣོད་འཛིན་ཨཱོན་"

#: purge1.label
#: purge1.accesskey
msgid "A&utomatically delete junk messages older than"
msgstr "འཕྲིན་དོན་ཚུ་ སྣོད་འཛིན་འདི་ནང་ལས་ རང་བཞིན་གྱིས་བཏོན་གཏང་(&u)"

#: purge2.label
msgid "days from this folder"
msgstr "ཉིན་གྲངས་ ལས་ལྷག་མི་"

#: manualMark.label
msgid "When I manually mark messages as Junk:"
msgstr "ང་གིས་ལག་ཐོག་ལས་ འཕྲིན་དོན་ཚུ་ མཁོ་མེད་སྦེ་ རྟགས་བཀལ་བའི་སྐབས་:"

#: manualMarkModeMove.label
msgid "Move them to the &quot;Junk&quot; folder"
msgstr "དེ་ཚུ་ &quot;Junk&quot; སྣོད་འཛིན་ལུ་ སྤོ་བཤུད་འབད་"

#: manualMarkModeDelete.label
msgid "Delete them"
msgstr "དེ་ཚུ་བཏོན་གཏང་"

#: renderWithSafeHtml.label
msgid "When displaying HTML messages marked as junk, sanitize the HTML"
msgstr "ཨེཆ་ཊི་ཨེམ་ཨེལ་་འཕྲིན་དོན་ཚུ་ མཁོ་མེད་སྦེ་རྟགས་བཀལ་ཏེ་ བཀྲམ་སྟོན་འབད་བའི་སྐབས་ ཨེཆ་ཊི་ཨེམ་ཨེལ་ དང་ལེན་ཅན་བཟོ།"

#: log.label
msgid "Logging"
msgstr "དྲན་དེབ་ནང་བཀོད་པ་"

#: handling.label
msgid "Handling"
msgstr "ལེགས་སྐྱོང་འཐབ་པ་"

#: account.label
#: account.accesskey
msgid "&Configure Junk Settings for:"
msgstr "མཁོ་མེད་སྒྲིག་སྟངས་རིམ་སྒྲིག་འབད་:(&C)"

#: viewLog.label
#: viewLog.accesskey
msgid "&Junk Mail Log"
msgstr "མཁོ་མེད་ཡིག་འཕྲིན་དྲན་དེབ་དོན་ལུ་(&J)"

#: loggingDescription.label
msgid "View and configure junk mail logging."
msgstr "མཁོ་མེད་ཡིག་འཕྲིན་ དྲན་དེབ་བཀོད་པ་ སྟོན་ཞིནམ་ལས་ རིམ་སྒྲིག་འབད། "

#: trainingWarning.label
msgid "If enabled, you must first train &brandShortName; to identify junk mail by using the Junk toolbar button to mark messages as junk or not. You need to identify both junk and non junk messages."
msgstr "གལ་སྲིད་ལྕོགས་ཅན་ཨིན་པ་ཅིན་ ཁྱོད་ཀྱིས་འཕྲིན་དོན་ཚུ་ མཁོ་མེད་ཨིནམ་མེནམ་ རྟགས་བཀལ་ནི་ལུ་ མཁོ་མེད་ལག་ཆས་ཕྲ་རིང་ཨེབ་རྟ་ ལག་ལེན་འཐབ་ཐོག་ལས་ མཁོ་མེད་ཡིག་འཕྲིན་ངོས་འཛིན་འབད་ནི་ལུ་ དང་པ་ར་ &brandShortName; ལུ་སྦྱོང་བརྡར་བྱིན་དགོ། ཁྱོད་ཀྱིས་མཁོ་མེད་འཕྲིན་དོན་དང་ མཁོ་ཅན་འཕྲིན་དོན་གཉིས་ཆ་ར་ ངོས་འཛིན་འབད་དགོ།"

#: level.label
#: level.accesskey
msgid "&Enable adaptive junk mail detection"
msgstr "མཐུན་བསྒྱུར་ཅན་གྱི་ མཁོ་མེད་ཡིག་འཕྲིན་ ངོས་འཛིན་འབད་ནི་ ལྕོགས་ཅན་བཟོ།(&E)"

#: resetTrainingDescription.label
msgid "Resetting the training data used by the adaptive filter requires you to retrain the filter:"
msgstr "མཐུན་འགྱུར་ཅན་གྱི་ཚགས་མ་ཚུ་གིས་ ལག་ལེན་འཐབ་མི་ སྦྱོང་བརྡར་གནས་སྡུད་ཚུ་ སླར་སྒྲིག་འབད་ནི་ལུ་ ཚགས་མ་ལུ་སྦྱོང་བརཨདར་བྱིན་དགོ།"

#: resetTrainingData.label
#: resetTrainingData.accesskey
msgid "&Reset Training Data"
msgstr "སྦྱོན་བརྡར་གནས་སྡུད་སླར་སྒྲིག་འབད་(&R)"

#: whitelist.label
#: whitelist.accesskey
msgid "&Do not mark messages as junk if the sender is in: "
msgstr "འཕྲིན་དོན་ཚུ་ མཁོ་མེད་སྦེ་ རྟགས་མ་བཀལ་ གལ་སྲིད་གཏང་མི་དེ་:(&D)"

#: ispHeaders.label
#: ispHeaders.accesskey
msgid "&Trust junk mail headers set by: "
msgstr "གིས་ གཞི་སྒྲིག་འབད་མི་ མཁོ་མེད་ཡིག་འཕྲིན་མགོ་ཡིག་ཚུ་ལུ་ བློ་གཏད་འབད།(&T)"

