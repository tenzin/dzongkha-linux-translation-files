# Entities for AccountWizard
#: windowTitle.label
msgid "Account Wizard"
msgstr "རྩིས་ཐོའི་ཝི་ཛརཌི།"

#: accountWizard.size
msgid ""
"_: Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger.\n"
"width: 40em; height: 38em;"
msgstr "རྒྱ་ཚད་: ཨི་ཨེམ་༤༠། མཐོ་ཚད་: ཨི་ཨེམ་༣༨།"

# Entities for Account Type page
#: accountSetupInfo.label
msgid "In order to receive messages, you first need to set up a Mail or Newsgroup account."
msgstr "འཕྲིན་དོན་འཐོབ་ཚུགས་ནི་ལུ་ དང་པ་ར་ ཁྱོད་ཀྱིས་ཡིག་འཕྲིན་ ཡང་ན་ གནས་ཚུལ་སྡེ་ཚན་གྱི་རྩིས་ཐོ་ཅིག་ གཞི་སྒྲིག་འབད་དགོ།"

#: accountTypeTitle.label
msgid "New Account Setup"
msgstr "རྩིས་ཐོ་གསར་པའི་གཞི་སྒྲིག་"

#: accountTypeDesc.label
msgid "This Wizard will collect the information necessary to set up a Mail or Newsgroup account. If you do not know the information requested, please contact your System Administrator or Internet Service Provider."
msgstr "ཝི་ཛརཌི་འདི་གིས་ ཡིག་འཕྲིན་ ཡང་ན་ གནས་ཚུལ་སྡེ་ཚན་གྱི་རྩིས་ཐོ་ གཞི་སྒྲིག་འབད་ནི་ལུ་མཁོ་བའི་བརྡ་དོན་ཚུ་ བསྡུ་ལེན་འབད་འོང་། གལ་སྲིད་ཞུ་བ་འབད་མི བརྡ་དོན་དེ་ ཁྱོད་ཀྱིས་མ་ཤེས་པ་ཅིན་ ཁྱོད་རའི་རིམ་ལུགས་བདག་སྐྱོང་པ་ ཡང་ན་ ཨིན་ཊར་ནེཊི་གི་ཞབས་ཏོག་བྱིན་མི་ལུ་ འབྲེལ་བ་འཐབ་གནང་།"

#: accountTypeDirections.label
msgid "Select the type of account you would like to set up:"
msgstr "ཁྱོད་ཀྱིས་གཞི་སྒྲིག་འབད་དགོ་མནོ་མི་ རྩིས་ཐོའི་དབྱེ་བ་དེ་ སེལ་འཐུ་འབད་:"

#: accountTypeMail.label
#: accountTypeMail.accesskey
msgid "E&mail account"
msgstr "གློག་འཕྲིན་རྩིས་ཐོ་(&m)"

#: accountTypeNews.label
#: accountTypeNews.accesskey
msgid "Ne&wsgroup account"
msgstr "གནས་ཚུལ་སྡེ་ཚན་གྱི་རྩིས་ཐོ་(&w)"

# Entities for Identity page
#: identityTitle.label
msgid "Identity"
msgstr "ངོ་རྟགས་"

#: identityDesc.label
msgid "Each account has an identity, which is the information that identifies you to others when they receive your messages."
msgstr "རྩིས་ཐོ་རེ་ལུ་ངོ་རྟགས་རེ་ཡོད། འདི་གིས་གཞན་ཚུ་ལུ་ ཁྱོད་ཀྱིས་འཕྲིན་དོན་ཐོབ་པའི་སྐབས་ ཁྱོད་ངོས་འཛིན་འབདཝ་ཨིན།"

#: fullnameDesc.label
msgid ""
"_: do not translate two of \"&quot;\" in below line\n"
"Enter the name you would like to appear in the &quot;From&quot; field of your outgoing messages"
msgstr "ཁྱོད་ཀྱི་ཕྱིར་བསྐྱོད་འཕྲིན་དོན་ཚུ་གི་ &quot;From&quot; ས་སྒོའི་ནང་ འབྱུང་དགོ་པའི་མིང་དེ་བཙུགས།"

# 1, do not translate two of "&quot;"
# 2, Use localized full name instead of "John Smith"
#
#: fullnameExample.label
msgid ""
"_: use following directions for below line\n"
"(for example, &quot;John Smith&quot;)."
msgstr "(དཔེར་ན་  &quot;ཇཱོན་ སིམིཐ་&quot;)"

#: fullnameLabel.label
#: fullnameLabel.accesskey
msgid "&Your Name:"
msgstr "ཁྱོད་རའི་མིང་:(&Y)"

#: emailLabel.label
#: emailLabel.accesskey
msgid "&Email Address:"
msgstr "གློག་འཕྲིན་ཁ་བྱང་:(&E)"

#: emailDesc.label
msgid "Enter your email address; this is the address others will use to send email to you"
msgstr "ཁྱོད་རའི་གློག་འཕྲིན་ཁ་བྱང་བཙུགས། དེ་ཡང་གཞན་ཚུ་གིས་ཁྱོད་ལུ་ གློག་འཕྲིན་གཏངམ་ད་ ལག་ལེན་འཐབ་ནིའི་ ཁ་བྱང་དེ་ཨིན།"

#: emailExample.label
msgid ""
"_: do not translate \"user@example.net\" in below line\n"
"(for example: user@example.net)."
msgstr "(དཔེར་ན་: user@example.net)."

# Entities for Server page
#: serverTitle.label
msgid "Server Information"
msgstr "སར་བར་གྱི་བརྡ་དོན་"

#: incomingServerTitle.label
msgid "Incoming Server"
msgstr "ནང་འབྱོར་སར་བར་"

#: incomingServerTypeDesc.label
msgid "Select the type of incoming server you are using."
msgstr "ཁྱོད་ཀྱིས་ལག་ལེན་འཐབ་བཞིན་ཡོད་མི་ སར་བར་གྱི་དབྱེ་བ་ སེལ་འཐུ་འབད།"

#: incomingServerTypeLabel.label
msgid "Server Type:"
msgstr "སར་བར་གྱི་དབྱེ་བ་:"

#: incomingServerNameDesc.label
msgid ""
"_: Do not translate \"&quot;pop.example.net&quot;\" in below line\n"
"Enter the name of your incoming server (for example, &quot;mail.example.net&quot;)."
msgstr "ཁྱོད་རའི་ནང་འབྱོར་སར་བར་གྱི་མིང་བཙུགས།(དཔེཡ་ན་: &quot;mail.example.net&quot;)"

#: incomingServerLabel.label
#: incomingServerLabel.accesskey
msgid "Incoming &Server:"
msgstr "ནང་འབྱོར་སར་བར་:(&S)"

#: newsServerNameDesc.label
msgid ""
"_: Do not translate \"NNTP\" or the \"&quot;\" entities in below line\n"
"Enter the name of your news server (NNTP) (for example, &quot;news.example.net&quot;)."
msgstr "ཁྱོད་རའི་གནས་ཚུལ་སར་བར་(NNTP)གྱི་མིང་བཙུགས་  (དཔེར་ན་ &quot;news.example.net&quot;)."

#: smtpServerTitle.label
msgid ""
"_: Do not translate \"SMTP\" in below line\n"
"Outgoing Server (SMTP)"
msgstr "ཕྱིར་བསྐྱོད་སར་བར་ (SMTP)"

#: smtpServerDesc.label
msgid ""
"_: Do not translate \"SMTP\" and \"&quot;smtp.example.net&quot;\" in below line\n"
"Enter the name of your outgoing server (SMTP) (for example, &quot;smtp.example.net&quot;)."
msgstr "ཁྱོེད་རའི་ཕྱིར་བསྐྱོད་སར་བར་གྱི་མིང་བཙུགས་ (SMTP) (དཔེར་ན་ &quot;smtp.example.net&quot;)."

#: smtpServerLabel.label
#: smtpServerLabel.accesskey
msgid "&Outgoing Server:"
msgstr "ཕྱིར་བསྐྱོད་སར་བར་'(&O)"

#: newsServerLabel.label
#: newsServerLabel.accesskey
msgid "&Newsgroup Server:"
msgstr "གནས་ཚུལ་སྡེ་ཚན་སརབར་:(&N)"

#: imapType.label
#: imapType.accesskey
msgid ""
"_: Do not translate \"IMAP\" in below line\n"
"&IMAP"
msgstr "&IMAP)"

#: popType.label
#: popType.accesskey
msgid ""
"_: Do not translate \"POP\" in below line\n"
"&POP"
msgstr "&POP"

#: nntpType.label
msgid "News Server"
msgstr "གནས་ཚུལ་སར་བར་"

# for when the user has an SMTP server pre-configured
# LOCALIZATION NOTE (currentSmtpBefore.label) : Do not translate "SMTP" and "&quot;" in
# this variable. Also, translate currentSmtpBefore.label and currentSmtpAfter.label as a single sentence,
# inserting text after the "&quot;" entity in currentSmtpAfter.label, if required grammatically
#
# LOCALIZATION NOTE (haveSmtp1.prefix and haveSmtp1.suffix) : Do not translate "SMTP" and "&quot;" in
# these variables. Also, translate haveSmtp1.prefix and haveSmtp1.suffix as a single sentence, inserting
# text after the "&quot;" entity in haveSmtp1.suffix, if required grammatically.
#
#: haveSmtp1.prefix
msgid "Your existing outgoing server (SMTP), &quot;"
msgstr "ཁྱོད་ཀྱི་ཡོད་བཞིན་པའི་ ཕྱིར་བསྐྱོད་སར་བར་(ཨེསི་ཨེམ་ཊི་པི)&quot;"

#: haveSmtp1.suffix
#: haveSmtp2.suffix
msgid "&quot;, will be used. You can modify outgoing server settings by choosing Account Settings from the Tools menu."
msgstr "&quot; ལག་ལེན་འཐབ་ནི་ཨིན། ཁྱོད་ཀྱིས་ལག་ཆས་དཀར་ཆག་ནང་ལས་ རྩིས་ཐོ་སྒྲིག་སྟངས་ གདམ་ཁ་བརྐྱབས་པའི་ཐོག་ལས་ ཕྱིར་བསྐྱོད་སར་བར་གྱི་སྒྲིག་སྟངས་ ལེགས་བཅོས་འབད་བཏུབ།"

# Entities for Login page
#: loginTitle.label
msgid "User Names"
msgstr "ལག་ལེན་པའི་མིང་ཚུ་"

#: usernameDesc.label
msgid "Enter the incoming user name given to you by your email provider "
msgstr "ཁྱོད་ལུ་གློག་འཕྲིན་གཏང་མི་དེ་གིས་བྱིན་མི་ ནང་འབྱོར་ལག་ལེན་པའི་མིང་དེ་བཙུགས།"

#: usernameExample.label
msgid ""
"_: do not translate \"&quot;jsmith&quot;\" in below line\n"
"(for example, &quot;jsmith&quot;)."
msgstr "(དཔེར་ན་ &quot;jsmith&quot;)"

#: usernameLabel.label
#: usernameLabel.accesskey
msgid "&Incoming User Name:"
msgstr "ནང་འབྱོར་ལག་ལེན་པའི་མིང་:(&I)"

#: usernameSmtpDesc.label
msgid "Enter the outgoing user name given to you by your email provider (this is typically the same as your incoming user name)."
msgstr "ཁྱོད་ལུ་གློག་འཕྲིན་གཏང་མི་དེ་གིས་བྱིན་མི་ ཕྱིར་བསྐྱོད་ལག་ལེན་པའི་མིང་དེ་བཙུགས།(དེ་ཡང་ཁྱོད་རའི་ནང་འབྱོར་ལག་ལེན་པའི་མིང་དང་ཅོག་གཅིགཔ་ཨིན)"

#: usernameSmtpLabel.label
#: usernameSmtpLabel.accesskey
msgid "&Outgoing User Name:"
msgstr "ཕྱིར་བསྐྱོད་ལག་ལེན་པའི་མིང་:(&O)"

# LOCALIZATION NOTE (haveSmtp2.prefix and haveSmtp2.suffix) : Do not translate "SMTP" and "&quot;" in
# these variables. Also, translate haveSmtp2.prefix and haveSmtp2.suffix as a single sentence, inserting
# text after the "&quot;" entity in haveSmtp2.suffix, if required grammatically.
#
#: haveSmtp2.prefix
msgid "Your existing outgoing (SMTP) username, &quot;"
msgstr "ཁྱོད་ཀྱི་ཡོད་བཞིན་པའི་ ཕྱིར་བསྐྱོད་(ཨེསི་ཨེམ་ཊི་པི) ལག་ལེན་པའི་མིང་། &quot;"

# LOCALIZATION NOTE (haveSmtp3.prefix and haveSmtp3.suffix) : Do not translate "SMTP" and "&quot;" in
# these variables. Also, translate haveSmtp3.prefix and haveSmtp3.suffix as a single sentence, inserting
# text after the "&quot;" entity in haveSmtp3.suffix, if required grammatically.
#
#: haveSmtp3.prefix
msgid "Your outgoing (SMTP) server, &quot;"
msgstr "ཁྱོད་ཀྱི་ཡོད་བཞིན་པའི་ ཕྱིར་བསྐྱོད་(ཨེསི་ཨེམ་ཊི་པི) སརབར། &quot;"

#: haveSmtp3.suffix
msgid "&quot;, is identical to your incoming server, your incoming user name will be used to access it. You can modify outgoing server settings by choosing Account Settings from the Tools menu."
msgstr "&quot; དེ་ཁྱོད་ཀྱི་ནང་འབྱོར་སར་བར་དང་ཅོག་གཅིགཔ་ཨིན། འདི་ལུ་འཛུལ་སྤྱོད་འབད་ནི་ལུ་ ཁྱོད་རའི་ནང་འབྱོར་ལག་ལེན་པའི་མིང་དེ་ ལག་ལེན་འཐབ་འོང་།  ཁྱོད་ཀྱིས་ལག་ཆས་དཀར་ཆག་ནང་ལས་ རྩིས་ཐོའི་སྒྲིག་སྟངས་ གདམ་ཁ་བརྐྱབས་པའི་ཐོག་ལས་ ཕྱིར་བསྐྱེད་སར་བར་གྱི་སྒྲིག་སྟངས་ ལེགས་བཅོས་འབད་བཏུབ།"

# Entities for Account name page
#: accnameTitle.label
msgid "Account Name"
msgstr "རྩིས་ཐོའི་མིང་"

#: accnameDesc.label
msgid ""
"_: do not translate any \"&quot;\" in below line\n"
"Enter the name by which you would like to refer to this account (for example, &quot;Work Account&quot;, &quot;Home Account&quot; or &quot;News Account&quot;)."
msgstr "རྩིས་ཐོ་འདི་ལུ་ གཞི་བསྟུན་འབད་དགོ་མནོ་མི་ མིང་དེ་བཙུགས། (དཔེར་ན་ &quot;ལཱ་སའི་རྩིས་ཐོ་&quot;, &quot;ཁྱིམ་གྱི་རྩིས་ཐོ་&quot; or &quot;གནས་ཚུལ་གྱི་རྩིས་ཐོ་&quot;)"

#: accnameExample.label
msgid "For example, Work Account, Home Account or News Account"
msgstr "དཔེར་ན་ ལཱ་སའི་རྩིས་ཐོ། ཁྱིམ་གྱི་རྩིས་ཐོ། གནས་ཚུལ་རྩིས་ཐོ།"

#: accnameLabel.label
#: accnameLabel.accesskey
msgid "&Account Name:"
msgstr "རྩིས་ཐོའི་མིང་:(&A)"

# Entities for Done (Congratulations) page
#: completionTitle.label
msgid "Congratulations!"
msgstr "ལེགས་སོ་!"

#: completionText.label
msgid "Please verify that the information below is correct."
msgstr "འོག་གི་བརྡ་དོན་དེ་ ངེས་གཏན་ཨིན་པའི་ བདེན་སྦྱོར་འབད།"

#: serverTypePrefix.label
msgid "Incoming Server Type:"
msgstr "ནང་འབྱོར་སར་བར་གྱི་དབྱེ་བ་:"

#: serverNamePrefix.label
msgid "Incoming Server Name:"
msgstr "ནང་འབྱོར་སར་བར་གྱི་མིང་:"

#: smtpServerNamePrefix.label
msgid "Outgoing Server Name (SMTP):"
msgstr "ཕྱིར་བསྐྱོད་སར་བར་གྱི་མིང་(ཨེསི་ཨེམ་ཊི་པི་):"

#: newsServerNamePrefix.label
msgid "News Server Name (NNTP):"
msgstr "གནས་ཚུལ་སར་བར་གྱི་མིང་(ཨེན་ཨེན་ཊི་པི་):"

#: downloadOnLogin.label
msgid "Download messages now"
msgstr "འོག་གི་འཕྲིན་དོན་ཚུ་ ཕབལེན་འབད་"

#: deferStorageDesc.label
msgid "Uncheck this checkbox to store mail for this account in its own directory. That will make this account appear as a top-level account. Otherwise, it will be part of the Local Folders Global Inbox account."
msgstr "རྩིས་ཐོ་འདི་གི་འཕྲིན་དོན་ཚུ་ དེ་རང་གི་སྣོད་ཐོ་ནང་ གསོག་འཇོག་འབད་ནི་ལུ་ དཔྱད་སྒྲོམ་འདི་ལུ་ ཨིན་རྟགས་མེདཔ་བཟོ། འདི་གིས་རྩིས་ཐོ་འདི་ གནས་རིམ་མཐོ་བའི་རྩིས་ཐོ་ཅིག་སྦེ་འབྱུང་འོང་། དེ་མེན་པ་ཅིན་ ཉེ་གནས་ཀྱི་སྣོད་འཛིན་གྱི་ སྤྱི་ཁྱབ་ནང་སྒྲོམ་རྩིས་ཐོའི་ཡན་ལག་ཅིག་སྦེ་སྡོད་འོང་།"

#: deferStorage.label
msgid "Use Global Inbox (store mail in Local Folders)"
msgstr "སྤྱི་ཁྱབ་ནང་སྒྲོམ་ ལང་ལེན་འཐབ་(ཡིག་འཕྲིན་ཉེ་གནས་ཀྱི་སྣོད་འཛིན་ཚུ་ནང་གསོག་འཇོག་འབད་ནི)"

#: clickFinish.label
msgid "Click Finish to save these settings and exit the Account Wizard."
msgstr "སྒྲིག་སྟངས་ཚུ་ བསྲུང་ནིའི་དོན་ལུ་ རྫོགས་སོ་ གུ་ཨེབ་གཏང་འབད་ཞིནམ་ལས་ རྩིས་ཐོའི་ཝི་ཛརཌི་ ཕྱིར་འཐོན་འབད།"

