#
# The following are used by the outlook express import code to display status/error
# and informational messages
#
# Short name of import module
## @name TEXTIMPORT_NAME
## @loc None
#: 2000
msgid "Text file (LDIF, .tab, .csv, .txt)"
msgstr "ཚིག་ཡིག་ཡིག་སྣོད་(LDIF, .tab, .csv, .txt)"

# Description of import module
## @name TEXTIMPORT_DESCRIPTION
## @loc None
#: 2001
msgid "Import an address book from a text file, including:  LDIF (.ldif, .ldi), tab-delimited (.tab, .txt) or comma-separated (.csv) formats."
msgstr "ཚིག་ཡིག་ཡིག་སྣོད་ཅིག་ནང་ལས་ ཨེལ་ཌི་ཨའི་ཨེཕ་(.ldif, .ldi) མཆོངས་ལྡེ་གི་བཅད་མཚམས་བཟོ་ཡོདཔ་(.tab, .txt) ཡང་ན་ ལྷོད་རྟགས་ཀྱིས་སོ་སོ་འཕྱལ་ཡོད་པའི་(.csv) རྩ་སྒྲིག་བརྩིས་ཏེ་ ཁ་བྱང་ཀི་དེབ་ཅིག་ནང་འདྲེན་འབད།"

# Description of import module
## @name TEXTIMPORT_ADDRESS_NAME
## @loc None
#: 2002
msgid "Text Address Book"
msgstr "ཚིག་ཡིག་གི་ཁ་བྱང་ཀི་དེབ་"

# Description
## @name TEXTIMPORT_ADDRESS_SUCCESS
## @loc None
#: 2003
msgid "Imported address book %S"
msgstr "ནང་འདྲེན་འབད་ཡོད་པའི་ཁ་བྱང་ཀི་དེབ་ %S་"

# Error message
## @name TEXTIMPORT_ADDRESS_BADPARAM
## @loc None
#: 2004
msgid "Bad parameter passed to import address book."
msgstr "ཁ་བྱང་ཀི་དེབ་ནང་འདྲེན་འབད་ནི་ལུ་ ཚད་བཟུང་བྱང་ཉེས་སྤྲོད་ཡོདཔ།"

# Error message
## @name TEXTIMPORT_ADDRESS_BADSOURCEFILE
## @loc None
#: 2005
msgid "Error accessing file for address book %S."
msgstr "ཁ་བྱང་ཀི་དེབ་%Sཀྱི་དོན་ལུ་ ཡིག་སྣོད་ལུ་འཛུལ་སྤྱོད་འབད་ནི་ལུ་འཛོལ་བ།"

# Error message
## @name TEXTIMPORT_ADDRESS_CONVERTERROR
## @loc None
#: 2006
msgid "Error importing address book %S, all addresses may not have been imported."
msgstr "ཁ་བྱང་ཀི་དེབ་ ནང་འདྲེན་འབད་ནི་ལུ་འཛོལ་བ་བྱུངམ་ལས་ ཁ་བྱང་ཆ་མཉམ་ར་ ནང་འདྲེན་མ་འབདཝ་འོང་།"

