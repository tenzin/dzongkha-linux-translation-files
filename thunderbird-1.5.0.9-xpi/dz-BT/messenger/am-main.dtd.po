# extracted from am-main.xul
#: accountTitle.label
msgid "Account Settings"
msgstr "རྩིས་ཐོའི་སྒྲིག་སྟངས་"

#: accountName.label
#: accountName.accesskey
msgid "Account &Name:"
msgstr "རྩིས་ཐོའི་མིང་:"

#: accountNameDesc.label
msgid "The account name is used to identify each account"
msgstr "རྩིས་ཐོའི་མིང་དེ་ རྩིས་ཐོ་རེ་རེ་བཞིན་དུ་ ངོས་འཛིན་འབད་ནི་ལུ་ ལག་ལེན་འཐབ་ཨིན།"

#: identityTitle.label
msgid "Default Identity"
msgstr "སྔོན་སྒྲིག་ངོ་རྟགས་"

#: identityDesc.label
msgid "Each account has an identity, which is the information that other people see when they read your messages."
msgstr "རྩིས་ཐོ་རེ་ལུ་ངོ་རྟགས་རེ་ཡོད་མི་དེ་ མི་གཞན་གྱིས་ཁྱོད་ཀྱི་འཕྲིན་དོན་ཚུ་ལྷག་པའི་སྐབས་ མཐོང་ཚུགས་པའི་བརྡ་དོན་དེ་ཨིན།"

#: name.label
#: name.accesskey
msgid "&Your Name:"
msgstr "ཁྱོད་རའི་མིང་:(&Y)"

#: email.label
#: email.accesskey
msgid "&Email Address:"
msgstr "གློག་འཕྲིན་ཁ་བྱང་:(&E)"

#: replyTo.label
#: replyTo.accesskey
msgid "Reply-to Addre&ss:"
msgstr "ཁ་བྱང་ལུ་ལན་གསལ་གཏང་:(&s)"

#: organization.label
#: organization.accesskey
msgid "&Organization:"
msgstr "ལས་སྡེ་:(&O)"

#: signature.label
#: signature.accesskey
msgid "A&ttach this signature:"
msgstr "མིང་རྟགས་འདི་མཉམ་སྦརགས་འབད་:(&t)"

#: edit.label
msgid "Edit..."
msgstr "ཞུན་དག་རྐྱབས་་་"

#: choose.label
#: choose.accesskey
msgid "&Choose..."
msgstr "གདམ་ཁ་རྐྱབས་་་(&C)"

#: editVCard.label
#: editVCard.accesskey
msgid "E&dit Card..."
msgstr "ཤོག་བྱང་ཞུན་དག་རྐྱབས་་་་(&d)"

#: attachVCard.label
#: attachVCard.accesskey
msgid ""
"_: do not translate \"vCard\" in below line\n"
"Attach my &vCard to messages"
msgstr "ངེད་ཀྱི་འཕྲིན་དོན་ཚུ་ལུ་ &vCard མཉམ་སྦྲགས་འབད།"

#: manageIdentities.label
#: manageIdentities.accesskey
msgid "&Manage Identities..."
msgstr "ངོ་རྟགས་ཚུ་འཛིན་སྐྱོང་འཐབ་་་་(&M)"

#: smtpName.label
#: smtpName.accesskey
msgid ""
"_: do not translate \"SMTP\" in below line\n"
"O&utgoing Server (SMTP):"
msgstr "ཕྱིར་བསྐྱོད་སར་བར་(SMTP):"

#: smtpDefaultServer.label
msgid "Use Default Server"
msgstr "སྔོན་སྒྲིག་སར་བར་ལག་ལེན་འཐབ་"

