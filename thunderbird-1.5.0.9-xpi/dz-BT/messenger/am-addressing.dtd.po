# extracted from am-addressing.xul
#: addressing.label
msgid "Composition &amp; Addressing"
msgstr "&amp;རྩོམ་འབྲི། ཁ་བྱང་བཀོད་པ་"

#: addressingGroupTitle.label
msgid "Addressing"
msgstr "ཁ་བྱང་བཀོད་པ་"

#: addressingText.label
msgid "When looking up addresses:"
msgstr "ཁ་བྱང་ཚུ་ལུ་བལྟ་བའི་སྐབས་:"

# LOCALIZATION NOTE (override.label) : do not translate "LDAP" in below line
#: useGlobal.label
#: useGlobal.accesskey
msgid "&Use my global LDAP server preferences for this account"
msgstr "རྩིས་ཐོ་འདི་གི་དོན་ལུ་ ངེད་ཀྱི་སྤྱི་ཁྱབ་ ཨེལ་ཌི་ཨེ་པི་སར་བར་ དགད་གདམ་ཚུ་ ལག་ལེན་འཐབ།(&U)"

#: editDirectories.label
#: editDirectories.accesskey
msgid "&Edit Directories..."
msgstr "སྣོད་ཐོ་ཚུ་ཞིན་དག་རྐྱབས་་་་(&E)"

#: directories.label
#: directories.accesskey
msgid ""
"_: do not translate \"LDAP\" in below line\n"
"Use a different L&DAP server:"
msgstr "L&DAP སོ་སོ་ཅིག་ལག་ལེན་འཐབ་:"

# am-addressing.xul
#: compositionGroupTitle.label
msgid "Composition"
msgstr "རྩོམ་འབྲི་"

#: useHtml.label
#: useHtml.accesskey
msgid ""
"_: do not translate \"html\" in below line\n"
"&Compose messages in HTML format"
msgstr " འཕྲིན་དོན་ཚུ་ HTML རྩ་སྒྲིག་ནང་བརྩམས།(&C)"

#: autoQuote.label
#: autoQuote.accesskey
msgid "Automatically &quote the original message when replying"
msgstr "ལན་གསལ་གཏང་པའི་སྐབས་ རང་བཞིན་གྱིས་ འཕྲིན་དོན་ངོ་མ་དེ་ ལུང་འདྲེན་འབད།(&q)"

#: then.label
#: then.accesskey
msgid ""
"_: This will concatenate with the 4 strings that follow.\n"
"&Then,"
msgstr "དེ་ལས་(&T)"

#: aboveQuote.label
msgid "start my reply above the quote"
msgstr "ངེད་ཀྱི་ལན་གསལ་ ལུང་འདྲེན་གྱི་ལྟག་ལུ་ འགོ་བཙུགས།"

#: belowQuote.label
msgid "start my reply below the quote"
msgstr "ངེད་ཀྱི་ལན་གསལ་ ལུང་འདྲེན་གྱི་འོག་ལུ་ འགོ་བཙུགས།"

#: selectAndQuote.label
msgid "select the quote"
msgstr "ལུང་འདྲེན་དེ་ སེལ་འཐུ་འབད་"

#: place.label
#: place.accesskey
msgid "and &place my signature"
msgstr "དེ་ལས་ངེད་ཀྱི་མིང་རྟགས་བཀལ་(&p)"

#: belowText.label
msgid "below the quote (recommended)"
msgstr "ལུང་འདྲེན་གྱི་འོག་ལུ་(འོས་སྦྱོར་འབད་ཡོདཔ་)"

#: aboveText.label
msgid "below my reply (above the quote)"
msgstr "ངེད་ཀྱི་ལན་གསལ་གྱི་འོག་ལུ་(ལུང་འདྲེན་གྱི་ལྟག་ལུ་)"

