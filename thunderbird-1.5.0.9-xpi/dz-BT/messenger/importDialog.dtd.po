#: importDialog.windowTitle
msgid "Import"
msgstr "ནང་འདྲེན་"

#: importDialog.mailTitle
msgid "Mail"
msgstr "ཡིག་འཕྲིན་"

#: importDialog.addressbookTitle
msgid "Address Books"
msgstr "ཁ་བྱང་ཀི་དེབ་"

#: importDialog.settingsTitle
msgid "Settings"
msgstr "སྒྲིག་སྟངས་"

#: importTitle.label
msgid "&brandShortName; Mail Import Wizard"
msgstr "&brandShortName; ཡིག་འཕྲིན་ནང་འདྲེན་ཝི་ཛརཌི་"

#: importDesc.label
msgid "Import Mail, Address Books and Settings from other programs"
msgstr "ཡིག་འཕྲིན་དང་ ཁ་བྱང་ཀི་དེབ་ སྒྲིག་སྟངས་ཚུ་ ལས་རིམ་གཞན་ནང་ལས་ ནང་འདྲེན་འབད།"

#: importDescription.label
msgid "This wizard will import mail messages, address book entries, and/or preferences from other mail programs and common address book formats into &brandShortName; Mail."
msgstr "ཝི་ཛརཌི་འདི་གིས་ ཡིག་འཕྲིན་གྱི་འཕྲིན་དོན་དང་ ཁ་བྱང་ཀི་དེབ་ཐོ་བཀོད་དང་/ཡང་ན་ དགའ་གདམ་ཚུ་ ཡིག་འཕྲིན་ལས་རིམ་དང་ སྤྱི་མཐུན་ཁ་བྱང་ཀི་དེབ་རྩ་སྒྲིག་ཚུ་ནང་ལས་ &brandShortName; གི་ཡིག་འཕྲིན་ནང་ལུ་ ནང་འདྲེན་འབད་འོང་།"

#: importDescription2.label
msgid "Once they have been imported, you will be able to access them from within &brandShortName; Mail and/or Address Book."
msgstr "ཁྱོད་ཀྱིས་དེ་ཚུ་ནང་འདྲེན་འབད་ཚར་ཞིནམ་ལས་ &brandShortName; གི་ཡིག་འཕྲིན་དང་/ཡང་ན་ ཁ་བྱང་ཀི་དེབ་ནང་འཁོད་ལུ་ འཛུལ་སྤྱོད་འབད་ཚུགས།"

#: selectDescription.label
msgid "Please select the program from which you would like to import:"
msgstr "ཁྱོད་ཀྱིས་ག་ཏེ་ལས་ ནང་འདྲེན་འབད་ནི་ཨིན་ན་ ལས་རིམ་དེ་སེལ་འཐུ་འབད་གནང་།"

#: back.label
msgid "&lt; Back"
msgstr "&lt; རྒྱབ་བསྐྱོད་"

#: forward.label
msgid "Next &gt;"
msgstr "ཤུལ་མམ་&gt;"

#: finish.label
msgid "Finish"
msgstr "རྫོགས་"

#: cancel.label
msgid "Cancel"
msgstr "ཆ་མེད་"

#: select.label
msgid "Select the type of material to import:"
msgstr "ནང་འདྲེན་འབད་ནིའི་ ཅ་དངོས་ཀྱི་དབྱེ་བ་དེ་ སེལ་འཐུ་འབད་:"

#: title.label
msgid "Title"
msgstr "མགོ་མིང་"

#: processing.label
msgid "Importing..."
msgstr "ནང་འདྲེན་འབད་དོ་་་་"

#: FieldDiscInputText1.label
msgid "Netscape Communicator 4.x has one mailing address for each card, while &brandShortName; has two (Home and Work)."
msgstr "ནེཊི་སི་ཀེཔ་ ཀ་མུས་ནི་ཀེ་ཊར་ 4.x ལུ་ ཤོག་བྱང་རེ་ལུ་ ཡིག་འཕྲིན་ཁ་བྱང་རེ་ཡོདཔ་ད་ &brandShortName; ལུ་ གཉིས་རེ་ཡོད།(ཁྱིམ་གྱི་དང་ལཱ་སའི)"

#: FieldDiscInputText2.label
msgid "Select the category in which you want to store the imported mailing addresses:"
msgstr "ཁྱོད་ཀྱིས་ནང་འདྲེན་འབད་མི་ ཡིག་འཕྲིན་ཁ་བྱང་ཚུ་ གསོག་འཇོག་འབད་སའི་ དབྱེ་བ་དེ་སེལ་འཐུ་འབད་:"

#: FieldDiscWarning.label
msgid "This category will be used for all the entries of the selected address book: "
msgstr "སེལ་འཐུ་འབད་མི་ཁ་བྱང་ཀི་དེབ་དེ་གི་ ཐོ་བཀོད་ཆ་མཉམ་གྱི་དོན་ལུ་ དབྱེ་བ་འདི་ལག་ལེན་འཐབ་ནི་ཨིན།"

#: importDialog.homeTitle
msgid "Home"
msgstr "ཁྱིམ་གྱི་"

#: importDialog.workTitle
msgid "Work"
msgstr "ལཱ་སའི་"

