# Title
#: selectAddressWindow.title
msgid "Select Addresses"
msgstr "ཁ་བྱང་སེལ་འཐུ་འབད་"

# Buttons
#: toButton.label
#: toButton.accesskey
msgid "&To->"
msgstr "གཏང་ཡུལ་->(&T)"

#: ccButton.label
#: ccButton.accesskey
msgid "&Cc->"
msgstr "ཤོག་གནག་འདྲ་བཤུས་->(&C)"

#: bccButton.label
#: bccButton.accesskey
msgid "&Bcc->"
msgstr "མཐོང་མེད་ཤོག་གནག་འདྲ་བཤུས་->(&B)"

#: newButton.label
#: newButton.accesskey
msgid "&New..."
msgstr "གསརཔ་་་་(&N)"

#: editButton.label
#: editButton.accesskey
msgid "&Edit..."
msgstr "ཞུན་དག་་་་(&E)"

#: removeButton.label
#: removeButton.accesskey
msgid "&Remove"
msgstr "རྩ་བསྐྲད་(&R)"

#: lookIn.label
#: lookIn.accesskey
msgid "&Look in:"
msgstr "ནང་ལུ་བལྟ་:(&L)"

#: for.label
#: for.accesskey
msgid "&for:"
msgstr "དོན་ལས་:(&f)"

#: clearButton.label
#: clearButton.accesskey
msgid "Cle&ar"
msgstr "བསལ་(&a)"

#: stopButton.label
msgid "Stop"
msgstr "བཀག་"

#: addressMessageTo.label
msgid "Address message to:"
msgstr "འཕྲིན་དོན་གསང་སའི་ཁ་བྱང་:"

# Tooltips items
#: addressPickerSearchButton.tooltip
msgid "Search the selected address book"
msgstr "སེལ་འཐུ་འབད་མི་ཁ་བྱང་ཀི་དེབ་དེ་འཚོལ་ཞིབ་འབད།"

#: addressPickerNewButton.tooltip
msgid "Create a new address book card"
msgstr "ཁ་བྱང་ཀི་དེབ་ཀྱི་ཤོག་བྱང་གསརཔ་ཅིག་ གསར་བསྐྲུན་འབད།"

#: addressPickerEditButton.tooltip
msgid "Edit the selected card"
msgstr "སེལ་འཐུ་འབད་མི་ཤོག་བྱང་དེ་ ཞུན་དག་རྐྱབས།"

