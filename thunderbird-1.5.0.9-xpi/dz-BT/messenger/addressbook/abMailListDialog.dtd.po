# Title
#: mailListWindow.title
msgid "Mailing List"
msgstr "ཡིག་གཏོང་ཐོ་ཡིག"

# Labels
# Access Keys
#: addToAddressBook.label
#: addToAddressBook.accesskey
msgid "&Add to: "
msgstr "ཁ་སྐོང་རྐྱབ་ས་:(&A)"

#: ListName.label
#: ListName.accesskey
msgid "&List Name: "
msgstr "མིང་ཐོ་བཀོད་འབད་:(&L)"

#: ListNickName.label
#: ListNickName.accesskey
msgid "List &Nickname: "
msgstr "སྐྱོན་མིང་ཐོ་བཀོད་འབད་:(&N)"

#: ListDescription.label
#: ListDescription.accesskey
msgid "D&escription: "
msgstr "འགྲེལ་བཤད་:(&e)"

# See bug 58485, when we implement drag and drop, add 'or drag addresses' back in
#: AddressTitle.label
msgid "Type email addresses to add them to the mailing list:"
msgstr "ཡིག་འཕྲིན་ཐོ་ཡིག་ནང་ ཁ་སྐོང་བརྐྱབ་ནིའི་དོན་ལུ་ གློག་འཕྲིན་ཁ་བྱང་ཚུ་ ཡིག་དཔར་རྐྱབས།"

#: RemoveButton.label
msgid "Remove"
msgstr "རྩ་བསྐྲད་གཏང་"

#: UpButton.label
msgid "Move Up"
msgstr "ཡར་སྤོ་"

#: DownButton.label
msgid "Move Down"
msgstr "མར་སྤོ་"

