msgstr "ཁ་མ་བསྡམས་པའི་ཧེ་མ་"

#: BeforePreview
msgid "before viewing in Navigator"
msgstr "འགྲུལ་བསྐྱོད་ནང་མ་སྟོན་པའི་ཧེ་མ་"

#: BeforeValidate
msgid "before validating the document"
msgstr "ཡིག་ཆ་ནུས་ཅན་མ་བཟོ་བའི་ཧེ་མ་"

# LOCALIZATION NOTE (SaveFilePrompt, PublishPrompt): Don't translate %title% and %reason% (this is the reason for asking user to close, such as "before closing")
#: SaveFilePrompt PublishPrompt
msgid "Save changes to \"%title%\" %reason%?"
msgstr "\"%title%\" %reason%ལུ་བསྒྱུར་བཅོས་འབད་མི་ཚུ་ བསྲུང་ནི་ཨིན་ན?"

#: SaveFileFailed
msgid "Saving file failed!"
msgstr "ཡིག་སྣོད་བསྲུང་ནི་མ་བཏུབ་"

# Publishing error strings:
# LOCALIZATION NOTE Don't translate %dir% or %file% in the Publishing error strings:
#: FileNotFound
msgid "%file% not found."
msgstr "%file%མ་ཐོབ་"

#: SubdirDoesNotExist
msgid "The subdirectory \"%dir%\" doesn't exist on this site or the filename \"%file%\" is already in use by another subdirectory."
msgstr "ཡན་ལག་སྣོད་ཐོ་  \"%dir%\" དེ་ ས་ཁོངས་འདི་ཁར་མིན་འདུག་ ཡང་ན་ ཡིག་སྣོད་མིང་ \"%file%\" དེ་ ཧེ་མ་ལས་ར་ ཡན་ལག་སྣོད་ཐོ་གཞན་ཅིག་གིས་ ལག་ལེན་འཐབ་དོ་ཡོདཔ་འོང་ནི་མས།"

#: FilenameIsSubdir
msgid "The filename \"%file%\" is already in use by another subdirectory."
msgstr "ཡིག་སྣོད་མིང་  \"%file%\" དེ་ ཧེ་མ་ལས་ར་ ཡན་ལག་སྣོད་ཐོ་གཞན་ཅིག་གིས་ ལག་ལེན་འཐབ་སྟེ་འདུག།"

#: ServerNotAvailable
msgid "The server is not available. Check your connection and try again later."
msgstr "སར་བར་དེ་མི་འཐོབ་པས། ཁྱོད་རའི་མཐུད་ལམ་ལུ་ཞིབ་དཔྱད་འབད་ཞིནམ་ལས་ ལོག་ཤུལལས་འབད་རྩོལ་བསྐྱེད།"

#: Offline
msgid "You are currently offline. Click the icon near the lower-right corner of any window to go online."
msgstr "ད་ལྟོ་ཁྱོད་གློག་ཐོག་ལུ་མེན་པས། གློག་ཐོག་ལུ་འགྱོ་ནིའི་དོན་ལུ་ འོག་གི་གཡས་ཟུར་གྱི་སྦོ་ལོགས་ཁར་ཡོད་མི་ ངོས་དཔར་དེ་ལུ་ ཨེབ་གཏང་འབད།"

#: DiskFull
msgid "There is not enough disk space available to save the file \"%file%.\""
msgstr "ཡིག་སྣོད་ \"%file%.\"བསྲུང་ནི་ལུ་ ཌིཀསི་ནང་བར་སྟོང་ལངམ་མིན་འདུག།"

#: NameTooLong
msgid "The filename or subdirectory name is too long."
msgstr "ཡིག་སྣོད་མིང་ ཡང་ན་ ཡན་ལག་སྣོད་ཐོའི་མིང་དེ་ རིང་དྲགས་པས།"

#: AccessDenied
msgid "You do not have permission to publish to this location."
msgstr "གནས་ཁོངས་འདི་ནང་ དཔེ་བསྐྲུན་འབད་ནི་ལུ་ ཁྱོད་ལུ་གནང་བ་མིན་འདུག།"

#: UnknownPublishError
msgid "Unknown publishing error occurred."
msgstr "མ་ཤེས་པའི་དཔེ་བསྐྲུན་གྱི་འཛོལ་བ་བྱུང་ཡི།"

#: PublishFailed
msgid "Publishing failed."
msgstr "དཔེ་བསྐྲུན་མ་བཏུབ་"

#: PublishCompleted
msgid "Publishing completed."
msgstr "དཔེ་བསྐྲུན་འབད་ཚར་"

#: AllFilesPublished
msgid "All files published"
msgstr "ཡིག་སྣོད་ཆ་མཉམ་དཔེ་བསྐྲུན་འབད་ཡི།"

# LOCALIZATION NOTE Don't translate %x% or %total%
#: FailedFileMsg
msgid "%x% of %total% files failed to publish."
msgstr "ཡིག་སྣོད་བསྡོམས་་%total% ལས་%x% དཔེ་བསྐྲུན་འབད་མ་ཚུགས།"

# End-Publishing error strings
#: Prompt
msgid "Prompt"
msgstr "ནུས་སྤེལ་"

# LOCALIZATION NOTE (PromptFTPUsernamePassword): Don't translate %host%
