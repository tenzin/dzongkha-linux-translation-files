#: windowtitle.label
msgid "Password Manager"
msgstr "ཆོག་ཡིག་འཛིན་སྐྱོང་པ་"

#: tab.signonsstored.label
msgid "Passwords Saved"
msgstr "ཆོག་ཡིག་ཚུ་བསྲུངས་ཡོད་"

#: tab.signonsnotstored.label
msgid "Passwords Never Saved"
msgstr "ཆོག་ཡིག་ནམ་ཡང་མ་བསྲུངས་པས་"

#: spiel.signonsstored.label
msgid "Password Manager has saved login information for the following sites:"
msgstr "ཆོག་ཡིག་འཛིན་སྐྱོང་པ་གིས་ འོག་གི་ས་ཁོངས་ཚུ་གི་དོན་ལུ་ ནང་བསྐྱོད་བརྡ་དོན་ཚུ་ བསྲུངས་བཞག་ནུག་:"

#: spiel.signonsnotstored.label
msgid "Password Manager will never save login information for the following sites:"
msgstr "ཆོག་ཡིག་འཛིན་སྐྱོང་པ་གིས་ འོག་གི་ས་ཁོངས་ཚུ་གི་དོན་ལུ་ ནམ་ཡང་ནང་བསྐྱོད་བརྡ་དོན་ བསྲུང་མི་བཞག་:"

#: treehead.site.label
msgid "Site"
msgstr "ས་ཁོངས་"

#: treehead.username.label
msgid "Username"
msgstr "ལག་ལེན་པའི་མིང་"

#: treehead.password.label
msgid "Password"
msgstr "ཆོག་ཡིག་"

#: remove.label
#: remove.accesskey
msgid "&Remove"
msgstr "རྩ་བསྐྲད་གཏང་(&R)"

#: removeall.label
#: removeall.accesskey
msgid "Remove &All"
msgstr "ཆ་མཉམ་རྩ་བསྐྲད་གཏང་(&A)"

