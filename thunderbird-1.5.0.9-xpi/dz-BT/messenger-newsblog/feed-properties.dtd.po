# Feed Properties Dialog
#: window.title
msgid "Feed Properties"
msgstr "རྒྱུ་དངོས་མཁོ་སྤྲོད་འབད།"

#: feedFolder.label
#: feedFolder.accesskey
msgid "&Store Articles in: "
msgstr "ནང་ལུ་རྩིས་བྲིས་ཚུ་གསོག་འཇོག་འབད།(&S)"

#: feedLocation.label
#: feedLocation.accesskey
msgid "&Feed URL: "
msgstr "ཡུ་ཨར་ཨེལ་ མཁོ་སྤྲོད་འབད་:(&F)"

#: choosethisfolder.label
msgid "choose this folder"
msgstr "སྣོད་འཛིན་འདི་སེལ་འཐུ་འབད་"

#: quickMode.label
#: quickMode.accesskey
msgid "S&how the article summary instead of loading the web page"
msgstr "ཝེབ་ཤོག་ལེབ་ཀྱི་ཚབ་ལུ་ རྩིས་བྲིས་ཀྱི་བཅུད་དོན་སྟོན།(&h)"

#: copyLinkCmd.label
#: copyLinkCmd.accesskey
msgid "&Copy Link Location"
msgstr "འབྲེལ་ལམ་གནས་ཁོངས་འདྲ་བཤུས་རྐྱབས།(&C)"

