#: dialog.title
msgid "Customize Toolbar"
msgstr "ལག་ཆས་ཕྲ་རིང་ སྲོལ་སྒྲིག་འབད་"

#: instructions.description
msgid "You can add or remove items by dragging to or from the toolbars."
msgstr "ཁྱོད་ཀྱིས་རྣམ་གྲངས་ཚུ་ ལག་ཆས་ཕྲ་རིང་ལུ་འདྲུད་པའམ་ ལག་ཆས་ཕྲ་རིང་ལས་འདྲུད་པའི་ཐོག་ལས་ ཁ་སྐོང་རྐྱབ་པའམ་ རྩ་བསྐྲད་གཏང་ནི་ཚུ་འབད་ཚུགས།"

#: show.label
msgid "Show:"
msgstr "སྟོན་:"

#: iconsAndText.label
msgid "Icons and Text"
msgstr "ངོས་དཔར་དང་ཚིག་ཡིག་"

#: icons.label
msgid "Icons"
msgstr "ངོས་དཔར་ཚུ་"

#: text.label
msgid "Text"
msgstr "ཚིག་ཡིག་"

#: useSmallIcons.label
msgid "Use Small Icons"
msgstr "ངོས་དཔར་ཆུང་ཀུ་ཚུ་ ལག་ལེན་འཐབ་"

#: restoreDefaultSet.label
msgid "Restore Default Set"
msgstr "སྔོན་སྒྲིག་གཞི་སྒྲིག་ཚུ་ སོར་་ཆུད་འབད་"

#: addNewToolbar.label
msgid "Add New Toolbar"
msgstr "ལག་ཆས་ཕྲ་རིང་གསརཔ་ ཁ་སྐོང་རྐྱབས་"

#: saveChanges.label
msgid "Done"
msgstr "འབད་ཚར་"

#: undoChanges.label
msgid "Undo Changes"
msgstr "བསྒྱུར་བཅོས་ཚུ་ འབད་ཤོལ་འབད་"

