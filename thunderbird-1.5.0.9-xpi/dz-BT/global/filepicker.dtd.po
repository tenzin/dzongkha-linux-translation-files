#: lookInMenuList.label
#: lookInMenuList.accesskey
msgid "&Look in:"
msgstr "ནང་ལུ་བལྟ་:(&L)"

#: textInput.label
#: textInput.accesskey
msgid "File &name:"
msgstr "ཡིག་སྣོད་མིང་:(&n)"

#: filterMenuList.label
#: filterMenuList.accesskey
msgid "Files of &type:"
msgstr "ཡིག་སྣོད་ཀྱི་དབྱེ་བ་:"

#: name.label
msgid "Name"
msgstr "མིང་"

#: size.label
msgid "Size"
msgstr "ཚད་"

#: lastModified.label
msgid "Last Modified"
msgstr "མཐའ་མཇུག་ལུ་ལེགས་བཅོས་འབད་ཡོདཔ་"

#: permissions.label
msgid "Permissions"
msgstr "གནང་བ་ཚུ་"

#: showHiddenFiles.label
#: showHiddenFiles.accesskey
msgid "&Show hidden files and directories"
msgstr "སྦ་སྟེ་ཡོད་པའི་ཡིག་སྣོད་དང་ སྣོད་ཐོ་ཚུ་སྟོན། (&S)"

#: noPermissionError.label
msgid "You do not have the permissions necessary to view this directory."
msgstr "ཁྱོད་ལུ་སྣོད་ཐོ་འདི་ བལྟ་ནི་ལུ་མཁོ་བའི་ གནང་བ་མིན་འདུག།"

#: folderUp.tooltiptext
msgid "Go up a level"
msgstr "གནས་རིམ་ཅིག་ཡར་འགྱོ་"

#: folderHome.tooltiptext
msgid "Go to home"
msgstr "ཁྱིམ་ལུ་འགྱོ་"

#: folderNew.tooltiptext
msgid "Create new directory"
msgstr "སྣོད་ཐོ་གསར་བསྐྲུན་འབད་"

