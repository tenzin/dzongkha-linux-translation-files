#: description_label
msgid "Description"
msgstr "འགྲེལ་བཤད་"

#: suffixes_label
msgid "Suffixes"
msgstr "རྗེས་ཚིག་ཚུ་"

#: enabled_label
msgid "Enabled"
msgstr "ལྕོགས་ཅན་"

#: yes_label
msgid "Yes"
msgstr "ཨིན་"

#: no_label
msgid "No"
msgstr "མེན་"

