#: 1
msgid "Parsing an XSLT stylesheet failed."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ སི་ཊ་ཡེལ་ཤིཊི་ཅིག་ མིང་དཔྱད་འབད་ནི་མ་བཏུབ།"

#: 2
msgid "Parsing an XPath expression failed."
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་ གསལ་བརྗོད་ཅིག་ མིང་དཔྱད་འབད་མ་བཏུབ།"

#: 4
msgid "XSLT transformation failed."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་བཟོ་བསྒྱུར་མ་བཏུབ།"

#: 5
msgid "Invalid XSLT/XPath function."
msgstr "ནུས་མེད་ཨེགསི་ཨེསི་ཨེལ་ཊི་/ཨེགསི་འགྲུལ་ལམ་གྱི་ལས་འགན།"

#: 6
msgid "XSLT Stylesheet (possibly) contains a recursion."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ སི་ཊ་ཡེལ་ཤིཊི་ནང་ལུ་ རིམ་ལོག་ཅིག་ ཡོད་ཚོད་འདུག།"

#: 7
msgid "Attribute value illegal in XSLT 1.0."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ ༡.༠ ནང་ལུ་ ཁྱད་ཆོས་ཀྱི་བེ་ལུ་ ཁྲིམས་འགལ་ཨིན་མས།"

#: 8
msgid "An XPath expression was expected to return a NodeSet."
msgstr "ཨེགསི་འགྲུལ་ལམ་གསལ་བརྗོད་ཅིག་གིས་ ནོཌི་སེཊི་ཅིག་ སླར་ལོག་གཏང་བའི་རེ་བ་བསྐྱེད་ནུག།"

#: 9
msgid "XSLT transformation was terminated by <xsl:message>."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་བཟོ་བསྒྱུར་དེ་ <xsl:message> གིས་ རྩ་འགྲོལ་བཏང་ནུག།"

#: 10
msgid "A network error occured loading an XSLT stylesheet:"
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ སི་ཊ་ཡེལ་ཤིཊི་ མངོན་གསལ་འབད་བའི་སྐབས་ ཡོངས་འབྲེལ་གྱི་འཛོལ་བ་ཅིག་བྱུང་ནུག་:"

#: 11
msgid "An XSLT stylesheet does not have an XML mimetype:"
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ སི་ཊ་ཡེལ་ཤིཊི་ཅིག་ལུ་ ཨེགསི་ཨེམ་ཨེལ་ མ་ཡིམ་དབྱེ་བ་མིན་འདུག་:"

#: 12
msgid "An XSLT stylesheet directly or indirectly imports or includes itself:"
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ སི་ཊ་ཡེལ་ཤིཊི་གིས་ ཐད་ཀར་ ཡང་ན་ ཐད་སྐོར་ཐོག་ལུ་ དེ་རང་ནང་འདྲེན་ ཡང་ན་ གྲངས་བཙུགས་འབདཝ་ཨིན།"

#: 13
msgid "An XPath function was called with the wrong number of arguments."
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་ལས་འགན་ཅིག་ སྒྲུབ་རྟགས་ཀྱི་ཨང་གྲངས་ཕྱི་འགྱུར་གྱི་ཐོག་ལས་འབོ་ནུག།"

#: 14
msgid "An unknown XPath extension function was called."
msgstr "མ་ཤེས་པའི་ཨེགསི་འགྲུལ་ལམ་ རྒྱ་བསྐྱེད་ཀྱི་ལས་འགན་ཅིག་འབོ་ནུག།"

#: 15
msgid "XPath parse failure: ')' expected:"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་ མིང་དཔྱད་འཐུས་ཤོར་: ')' རེ་བ་བསྐྱེད་ཅི་:"

#: 16
msgid "XPath parse failure: invalid axis:"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་མིང་དཔྱད་འཐུས་ཤོར་: ནུས་མེད་ཚད་ཐིག་:"

#: 17
msgid "XPath parse failure: Name or Nodetype test expected:"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་མིང་དཔྱད་འཐུས་ཤོར་: མིང་ ཡང་ན་ ནོཌི་དབྱེ་བའི་བརྟག་ཞིབ་རེ་བ་བསྐྱེད་ཅི།"

#: 18
msgid "XPath parse failure: ']' expected:"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་མིང་དཔྱད་འཐུས་ཤོར་:']' རེ་བ་བསྐྱེད་ཅི་:"

#: 19
msgid "XPath parse failure: invalid variable name:"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་མིང་དཔྱད་འཐུས་ཤོར་: ནུས་མེད་འགྱུར་ཅན་མིང་:"

#: 20
msgid "XPath parse failure: unexpected end of expression:"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་མིང་དཔྱད་འཐུས་ཤོར་:རེ་བ་མེད་པར་གསལ་བརྗོད་མཇུག་བསྡུ་ཡོདཔ་:"

#: 21
msgid "XPath parse failure: operator expected:"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་མིང་དཔྱད་འཐུས་ཤོར་: བཀོལ་སྤྱོད་པ་རེ་བ་བསྐྱེད་ཅི།"

#: 22
msgid "XPath parse failure: unclosed literal:"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་མིང་དཔྱད་འཐུས་ཤོར་: མ་རྫོགས་པའི་ཡིག་དོན་:"

#: 23
msgid "XPath parse failure: ':' unexpected:"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་མིང་དཔྱད་འཐུས་ཤོར་:རེ་བ་མ་བསྐྱེདཔ་:"

#: 24
msgid "XPath parse failure: '!' unexpected, negation is not():"
msgstr "ཨེགསི་འགྲུལ་ལམ་གྱི་འཐུས་ཤོར་: '!'  རེ་བ་མ་བསྐྱེད། དགག་ཆ་():མེན་པས།"

#: 25
msgid "XPath parse failure: illegal character found:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་མ་བཏུབ།་: ཁྲིམས་འགལ་གྱི་ཡིག་འབྲུ་ཐོབ་ཅི་:"

#: 26
msgid "XPath parse failure: binary operator expected:"
msgstr "ཨེགསི་འགྲུལ་ལམ་མིང་དཔྱད་མ་བཏུབ་: བའི་ན་རི་བཀོལ་སྤྱོད་པ་རེ་བ་བསྐྱེད་ཅི་:"

#: 27
msgid "An XSLT stylesheet load was blocked for security reasons."
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ སི་ཊ་ཡེལ་ཤིཊི་གི་མངོན་གསལ་ཅིག་ སྲུང་སྐྱོབ་ཀྱི་རྒྱུ་མཚན་ལུ་བརྟེན་ཏེ་ བཀག་བཞག་ནུག།"

#: LoadingError
msgid "Error loading stylesheet: %S"
msgstr "སི་ཊ་ཡེལ་ཤིཊི་: %Sམངོན་གསལ་འབད་ནི་ལུ་འཛོལ་བ་"

#: TransformError
msgid "Error during XSLT transformation: %S"
msgstr "ཨེགསི་ཨེསི་ཨེལ་ཊི་ བཟོ་བསྒྱུར་:%Sགི་སྐབས་ལུ་འཛོལ་བ་"

