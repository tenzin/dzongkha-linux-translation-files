#: caption.label
msgid "Opening #1"
msgstr "#1 ཁ་ཕྱེ་དོ་"

# Localization note: The default intro text has 3 "inserts":
# #1 - replaced with the descriptive text that describes the mime type
# (e.g., "MSWord Document")
# #2 - replaced with the mime type (e.g., "application/ms-word")
# #3 - replaced with the filename (e.g., "manual.doc")
# #4 - will be replaced by the brand name (e.g., "Mozilla")
# If there is no descriptive text, then the alternative text is used which
# has only insert #2.
#
#: intro.label
msgid "The file &quot;#3&quot; is of type #2 (#1), and #4 does not know how to handle this file type. This file is located at:"
msgstr "ཡིག་སྣོད་&quot;#3&quot;འདི་ དབྱེ་བ་#2 (#1),ཨིནམ་ལས་ #4 གིས་ ཡིག་སྣོད་ཀྱི་དབྱེ་བ་འདི་ ལེགས་སྐྱོང་འཐབ་མི་ཤེས་པས། ཡིག་སྣོད་འདི་ གནས་ཏེ་ཡོད་ས་:"

#: intro.noDesc.label
msgid "The file &quot;#3&quot; is of type #2, and #4 does not know how to handle this file type. This file is located at:"
msgstr "ཡིག་སྣོད་&quot;#3&quot;འདི་ དབྱེ་བ་#2 ཨིནམ་ལས་ #4 གིས་ ཡིག་སྣོད་ཀྱི་དབྱེ་བ་འདི་ ལེགས་སྐྱོང་འཐབ་མི་ཤེས་པས། ཡིག་སྣོད་འདི་ གནས་ཏེ་ཡོད་ས་:"

#: intro.attachment.label
msgid "The site has suggested that &quot;#3&quot; be handled as an attachment. It is of type #2 (#1) and located at:"
msgstr "ས་ཁོངས་འདི་གིས་ &quot;#3&quot;དེ་ མཉམ་སྦྲགས་ཅིག་སྦེ་ ལེགས་སྐྱོང་འཐབ་དགོ་པའི་ བསམ་འཆར་བཀོདཔ་ཨིན་མས། འདི་གི་དབྱེ་བ་ #2 (#1) ཨིནམ་ད་ འདི་གནས་ཏེ་ཡོད་ས་:"

#: intro.attachment.noDesc.label
msgid "The site has suggested that &quot;#3&quot; be handled as an attachment. It is of type #2 and located at:"
msgstr "ས་ཁོངས་འདི་གིས་ &quot;#3&quot;དེ་ མཉམ་སྦྲགས་ཅིག་སྦེ་ ལེགས་སྐྱོང་འཐབ་དགོ་པའི་ བསམ་འཆར་བཀོདཔ་ཨིན་མས། འདི་གི་དབྱེ་བ་ #2ཨིནམ་ད་ འདི་གནས་ཏེ་ཡོད་ས་:"

#: intro.sniffed.label
msgid "#4 has detected that the file &quot;#3&quot; is likely to be of type #2 (#1). The file is located at:"
msgstr "#4 གིས་ ཡིག་སྣོད་&quot;#3&quot;དེ་ དབྱེ་བ་#2 (#1)ཨིནམ་བཟུམ་ཅིག་གི་ ངོས་འཛིན་བྱུང་ནུག། ཡིག་སྣོད་དེ་གནས་ཏེ་ཡོད་ས་:"

#: intro.sniffed.noDesc.label
msgid "#4 has detected that the file &quot;#3&quot; is likely to be of type #2. The file is located at:"
msgstr "#4 གིས་ ཡིག་སྣོད་&quot;#3&quot;དེ་ དབྱེ་བ་#2 ཨིནམ་བཟུམ་ཅིག་གི་ ངོས་འཛིན་བྱུང་ནུག། ཡིག་སྣོད་དེ་གནས་ཏེ་ཡོད་ས་:"

#: prompt.label
msgid "What should #1 do with this file?"
msgstr "#1གིས་ ཡིག་སྣོད་འདི་ལུ་ ག་ཅི་འབད་དགོཔ་ཨིན་ན?"

#: alwaysHandle.label
#: alwaysHandle.accesskey
msgid "&Always perform this action when handling files of this type"
msgstr "དབྱེ་བ་འདི་བཟུམ་གྱི་ ཡིག་སྣོད་ཚུ་ ལེགས་སྐྱོང་འཐབ་པའི་སྐབས་ ཨ་རྟག་ར་ བྱ་བ་འདི་ལག་ལེན་འཐབ།(&A)"

#: saveToDisk.label
#: saveToDisk.accesskey
msgid "&Save it to disk"
msgstr "འདི་ཌིཀསི་གུ་སྲུངསཝ(&S)"

#: useSystemDefault.label
#: useSystemDefault.accesskey
msgid "&Open it with the default application (#1)"
msgstr "འདི་སྔོན་སྒྲིག་གློག་རིམ་(#1)ཐོག་ལས་ཁ་ཕྱེ།(&O)"

#: useSystemDefault.noDesc.label
msgid "Open it with the default application"
msgstr "འདི་སྔོན་སྒྲིག་གློག་རིམ་ཐོག་ལས་ཁ་ཕྱེ།"

# LOCALIZATION NOTE:  do not localize "#1"
#: openUsing.label
#: openUsing.accesskey
msgid "Open it &with"
msgstr "གི་ཐོག་ལས་ཁ་ཕྱེ།(&w)"

#: chooseApp.label
#: chooseApp.accesskey
msgid "&Choose..."
msgstr "གདམ་ཁ་རྐྱབས་་་་(&C)"

#: chooseAppFilePickerTitle
msgid "Choose Helper Application"
msgstr "ལས་རོགས་པའི་གློག་རིམ་ གདམ་ཁ་རྐྱབས།"

#: badApp
msgid "The application you specified (&quot;#1&quot;) could not be found.  Check the file name or select another application."
msgstr "ཁྱོད་ཀྱིས་གསལ་བཀོད་འབད་མི་གློག་རིམ་ (&quot;#1&quot;)དེ་ འཚོལ་མ་ཐོབ། ཡིག་སྣོད་ཀྱི་མིང་ ཞིབ་དཔྱད་བརྐྱབས་ཏེ་ གློག་རིམ་གཞན་ཅིག་ སེལ་འཐུ་འབད།"

#: badApp.title
msgid "Application not found"
msgstr "གློག་རིམ་འཚོལ་མ་ཐོབ།"

