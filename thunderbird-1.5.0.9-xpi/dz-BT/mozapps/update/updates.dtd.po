#: updateWizard.title
msgid "Software Update"
msgstr "མཉེན་ཆས་དུས་མཐུན་བཟོ་བ་"

#: window.width
msgid ""
"_: Do not translate this.  Only change the numeric values if you need this dialogue box to appear bigger.\n"
"36em"
msgstr "ཨི་ཨེམ་༣༦།"

#: window.macWidth
msgid "39em"
msgstr "ཨི་ཨེམ་༣༩།"

#: checking.title
msgid "Checking for Updates"
msgstr "དུས་མཐུན་ཡོད་མེད་ཞིབ་དཔྱད་འབད་དོ་"

#: checking.label
msgid "&brandShortName; is now checking for updates..."
msgstr "&brandShortName;གིས་ ད་ལྟོ་དུས་མཐུན་ཡོད་མེད་ཞིབ་དཔྱད་འབད་དོ་་་་"

#: cancel.label
#: cancel.accesskey
msgid "&Cancel"
msgstr "ཆ་མེད་(&C)"

#: noupdatesfound.title
msgid "No Updates Found"
msgstr "དུས་མཐུན་ཚུ་མ་ཐོབ་"

#: noupdatesfound.intro
msgid "There are no new updates available. &brandShortName; may check periodically for new updates."
msgstr "དུས་མཐུན་གསརཔ་མིན་འདུག། &brandShortName;གིས་ དུས་མཚམས་ལུ་ དུས་མཐུན་གསརཔ་ཡོད་མེད་ཞིབ་དཔྱད་འབད་འོང་།"

#: updatesfound.title
msgid "Update Available"
msgstr "དུས་མཐུན་བཟོ་དགོཔ་འདུག་"

#: moreDetails.label
#: moreDetails.accesskey
msgid "&More Details &#0187;"
msgstr "གནད་དོན་ཁ་གསལ་&#0187;(&M)"

#: clickHere.label
msgid "View more information about this update"
msgstr "དུས་མཐུན་འདི་གི་སྐོར་ལས་ བརྡ་དོན་མངམ་སྟོན།"

#: incompatible.warning
msgid "This update will cause some of your extensions and/or themes to stop working until they are updated."
msgstr "དུས་མཐུན་བཟོ་མི་འདི་གིས་ ཁྱོད་ཀྱི་རྒྱ་བསྐྱེད་དང་/ཡང་ན་ བརྗོད་དོན་ལ་ལོ་ཅིག་ དུས་མཐུན་མ་བཟོ་ཚུན་ཚོད་ ལཱ་འབད་ནི་མཚམས་འཇོག་འབད་འོང་།"

#: listIncompatible.label
#: listIncompatible.accesskey
msgid "Show &List"
msgstr "ཐོ་ཡིག་སྟོན་(&L)"

#: later.label
#: later.accesskey
msgid "L&ater"
msgstr "ཤུལ་ལས་(&a)"

#: download.label
#: download.accesskey
msgid "&Download &amp; Install Now &#0187;"
msgstr "&Download &amp;གིས་ད་ལྟོ་ར་ &#0187;གཞི་བཙུགས་འབད་"

#: upgrade.evangelism
msgid "It is strongly recommended that you upgrade &brandShortName; as soon as possible."
msgstr "ཁྱོད་ཀྱིས་ག་དེ་མགྱོགས་མགྱོགས་ &brandShortName;དུས་མཐུན་བཟོ་དགོ།"

#: license.title
msgid "License Agreement"
msgstr "ཆོག་ཐམ་གན་ཡིག།"

#: license.intro
msgid "To install this update you must agree to this license agreement. Please read it carefully:"
msgstr "དུས་མཐུན་འདི་ གཞི་བཙུགས་འབད་ནི་ལུ་ ཁྱོད་ཀྱིས་ཆོག་ཐམ་གན་ཡིག་འདི་ལུ་ ཁས་ལེན་འབད་དགོ།"

#: license.instructions
msgid "If you agree to the terms of this agreement, click I Agree below to continue installing this update."
msgstr "ཁྱོད་ཀྱིས་གན་ཡིག་འདི་གི་དོན་ཚིག་ཚུ་ ཁས་ལེན་འབད་བ་ཅིན་ དུས་མཐུན་འདི་གཞི་བཙུགས་འབད་ནི་ལུ་ འོག་ལུ་ ང་གིས་ཁས་ལེན་འབདཝ་ཨིན་ ཟེར་མི་ལུ་ ཨེབ་གཏང་འབད།"

#: downloading.title
msgid "Downloading Update"
msgstr "དུས་མཐུན་ཕབལེན་འབད་དོ་"

#: downloading.intro
msgid "Downloading the update..."
msgstr "དུས་མཐུན་དེ་ ཕབ་ལེན་འབད་དོ་་་"

#: connecting.label
msgid "Connecting to the update server..."
msgstr "དུས་མཐུན་སར་བར་ལུ་མཐུན་དོ་་་་"

#: verificationFailedText.label
msgid "&brandShortName; was unable to verify the integrity of the incremental update it downloaded, so it is now downloading the complete update package."
msgstr "&brandShortName;གིས་ ཕབ་ལེན་འབད་མི་ ཡར་འཕེལ་ཅན་གྱི་དུས་མཐུན་དེ་གི་ གཅིག་སྒྲིལ་དེ་ བདེན་སྦྱོར་འབད་མ་ཚུགསཔ་ལས་ དུསམཐུན་ཐུམ་སྒྲིལ་ཆ་ཚང་ ཕབ་ལེན་འབད་དོ།"

#: details.link
#: update.details.label
msgid "Details"
msgstr "རྒྱས་བཤད་"

#: pause.label
#: pause.accesskey
msgid "Paus&e"
msgstr "ཐེམ་(&e)"

#: close.label
#: close.accesskey
msgid "&Close"
msgstr "ཁ་བསྡམས་(&C)"

#: error.label
msgid "There were problems checking for, downloading or installing this update. &brandShortName; could not be updated because:"
msgstr "དུས་མཐུན་འདི་ ཕབ་ལེན་འབད་ནི་ ཡང་ན་ གཞི་བཙུགས་འབད་ནིའི་དོན་ལུ་ ཞིབ་དཔྱད་འབད་ནི་ལུ་དཀའ་ངལ་འདུག། &brandShortName;འདི་དུས་མཐུན་བཟོ་མ་ཚུགས། ག་ཅི་སྦེ་ཟེར་བ་ཅིན་:"

#: errorManual.label
msgid "You can update &brandShortName; manually by visiting this link and downloading the latest version:"
msgstr "ཁྱོད་ཀྱིས་འབྲེལ་ལམ་འདི་ལུ་ ལྟ་སྤྱོད་འབད་དེ་ མཐའ་མཇུག་གི་ཐོན་རིམ་ ཕབ་ལེན་འབད་བའི་ཐོག་ལས་ ལག་ཐོག་ལས་ &brandShortName; དུས་མཐུན་བཟོ་ཚུགས། "

#: errorpatching.title
msgid "Update Failed"
msgstr "དུས་མཐུན་བཟོ་མ་བཏུབ་"

#: errorpatching.intro
msgid "The partial Update could not be applied. &brandShortName; will try again by downloading a complete Update."
msgstr "ཆ་ཤས་ཀྱི་དུས་མཐུན་ འཇུག་སྤྱོད་འབད་མ་ཚུགས། &brandShortName;གིས་ དུས་མཐུན་ཆ་ཚང་ཅིག་ ཕབལེན་འབད་དེ་ འབད་རྩོལ་བསྐྱེད་ནི་ཨིན།"

#: finished.title
msgid "Update Downloaded"
msgstr "དུས་མཐུན་ཕབ་ལེན་འབད་ཡི་"

#: finished.intro
msgid "The update was successfully downloaded and verified. You must now restart &brandShortName; so that the update can be installed."
msgstr "དུས་མཐུན་དེ་མཐར་འཁྱོལ་སྦེ་ ཕབ་ལེན་འབད་དེ་ བདེན་སྦྱོར་འབད་ཡི། ད་ཁྱོད་ཀྱིས་ &brandShortName; ལོག་སྟེ་འགོ་བཙུགས་དགོ། དེ་ལས་དུས་མཐུན་དེ་ གཞི་བཙུགས་འབད་ཚུགས།"

#: finishedBackground.title
msgid "Update Ready to Install"
msgstr "དུས་མཐུན་དེ་ གཞི་བཙུགས་འབད་ནི་ལུ་གྲ་སྒྲིག་ཡོད།"

#: finishedBackground.intro
msgid "&brandShortName; has just completed downloading an important update and must now be restarted so that the update can be installed."
msgstr "&brandShortName;གིས་ གལ་ཅན་གྱི་དུས་མཐུན་ཅིག་ ཕབ་ལེན་འབད་ཚར་ཡོདཔ་ལས་ དུས་མཐུན་དེ་ གཞི་བཙུགས་འབད་ནི་ལུ་ གློག་རིག་ལོག་སྟེ་འོག་བཙུགས་དགོ།"

#: finishedBackground.name
msgid "Update:"
msgstr "དུས་མཐུད་བཟོ་"

#: finishedBackground.instruction1
msgid "Click Restart &brandShortName; Now to close all &brandShortName; windows and install the update."
msgstr "&brandShortName; ད་ལྟོ་ར་ལོག་སྟེ་འགོ་བཙུགས་ལུ་ ཨེབ་གཏང་འབདཝ་ད་ &brandShortName;གི་སྒོ་སྒྲིག་ཚུ་ག་ར་ ཁ་བསྡམས་ཞིནམ་ལས་ དུས་མཐུན་གཞི་བཙུགས་འབད་ནི་ལུ་ &brandShortName; ད་ལྟོ་ར་ལོག་སྟེ་འགོ་བཙུགས་ལུ་ ཨེབ་གཏང་འབད།"

#: finishedBackground.instruction2
msgid "Click Later to continue without restarting. The update will be installed the next time you start &brandShortName;."
msgstr "ལོག་སྟེ་འགོ་མ་བཙུགས་པར་ འཕྲོ་མཐུད་ནི་ལུ་ ཤུལ་ལས་གུ་ཨེབ་གཏང་འབད། ཤུལ་ལས་ཁྱོད་ཀྱིས་ &brandShortName;འགོ་བཙུགསཔ་ད་ དུས་མཐུན་དེ་ གཞི་བཙུགས་འབད་འོང་།"

#: installed.title
msgid "Update Installed"
msgstr "དུས་མཐུན་གཞི་བཙུགས་འབད་ཡི་"

#: installed.intro
msgid "The update was successfully installed."
msgstr "དུས་མཐུན་དེ་ མཐར་འཁྱོལ་ཅན་སྦེ་ གཞི་བཙུགས་འབད་ཡི།"

#: whatsnew.label
msgid "Read more about what's new..."
msgstr "གསརཔ་ག་ཅིག་ཨིན་ན་གི་སྐོར་ལས་ མངམ་ལྷག།"

#: update.installedOn.label
msgid "Installed on:"
msgstr "གཞི་བཙུགས་འབད་ཚེས་:"

#: update.status.label
msgid "Status:"
msgstr "གནས་ཚད་:"

