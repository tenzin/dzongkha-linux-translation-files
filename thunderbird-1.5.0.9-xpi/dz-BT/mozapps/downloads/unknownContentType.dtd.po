#: intro.label
msgid "You have chosen to open"
msgstr "ཁྱོད་ཀྱིས་ ཁ་ཕྱེ་ནི་སྦེ་ གདམ་ཁ་བརྐྱབས་ནུག་"

#: from.label
msgid "from:"
msgstr "ནང་ལས་:"

#: actionQuestion.label
msgid "What should &brandShortName; do with this file?"
msgstr "&brandShortName; གིས་ ཡིག་སྣོད་འདི་ལུ་ ག་ཅི་འབད་དགོཔ་སྨོ?"

#: openWith.label
#: openWith.accesskey
msgid "&Open with"
msgstr "ཐོག་ལས་ཁ་ཕྱེ་(&O)"

#: other.label
msgid "Other..."
msgstr "གཞན་་་་"

#: save.label
#: save.accesskey
msgid "&Save to Disk"
msgstr "ཌིཀསི་ལུ་སྲུངས་(&S)"

#: rememberChoice.label
#: rememberChoice.accesskey
msgid "Do this &automatically for files like this from now on."
msgstr "ད་ལས་ཕར་ འདི་བཟུམ་གྱི་ཡིག་སྣོད་ཚུ་གི་དོན་ལུ་ རང་བཞིན་གྱིས་འབད།(&a)"

#: settingsChange.label
msgid "Settings can be changed in the Downloads section of Tools, Options."
msgstr "སྒྲིག་སྟངས་འདི་ཚུ་ ལག་ཆས་ཀྱི་གདམ་ཁ་ནང་གི་ ཕབ་ལེན་སྡེ་ཚན་ནང་ལས་ བསྒྱུར་བཅོས་འབད་བཏུབ།"

#: settingsChangeMac.label
msgid "Settings can be changed in the Downloads section of &brandShortName;, Preferences."
msgstr "སྒྲིག་སྟངས་འདི་ཚུ་ &brandShortName;,གི་དགའ་གདམ་ཚུ་གི་  ཕབ་ལེན་སྡེ་ཚན་ནང་ལས་ བསྒྱུར་བཅོས་འབད་བཏུབ།"

#: settingsChangeUnix.label
msgid "Settings can be changed in the Downloads section of Edit, Preferences."
msgstr "སྒྲིག་སྟངས་འདི་ཚུ་ ཞུན་དག་གི་དགའ་གདམ་ཚུ་གི་  ཕབ་ལེན་སྡེ་ཚན་ནང་ལས་ བསྒྱུར་བཅོས་འབད་བཏུབ།"

#: whichIsA.label
msgid "which is a:"
msgstr "ཅིག་ཨིན་མི་:"

#: chooseHandlerMac.label
#: chooseHandlerMac.accesskey
msgid "&Choose..."
msgstr "གདམ་ཁ་རྐྱབས་་་(&C)"

#: chooseHandler.label
#: chooseHandler.accesskey
msgid "&Browse..."
msgstr "བརྡ་འཚོལ་འབད་་་་(&B)"

