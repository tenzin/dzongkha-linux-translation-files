#: changeAction.title
msgid "Change Action"
msgstr "བྱ་བ་སོར་"

#: whenDownloading.label
msgid "When downloading files of type:"
msgstr "དབྱེ་བ་འདི་བཟུམ་གྱི་ཡིག་སྣོད་ཚུ་ ཕབ་ལེན་འབད་བའི་སྐབས་:"

#: always.label
msgid "automatically:"
msgstr "རང་བཞིན་གྱིས་:"

#: openDefault.label
msgid "Open them in the Default application:"
msgstr "དེ་ཚུ་སྔོན་སྒྲིག་གློག་རིམ་ནང་ལུ་ཁ་ཕྱེ་:"

#: openApplication.label
msgid "Open them in this application:"
msgstr "དེ་ཚུ་གློག་རིམ་འདི་ནང་ཁ་ཕྱེ།"

#: changeApp.label
#: changeApp.accesskey
msgid "&Change..."
msgstr "བསྒྱུར་བཅོས་འབད་་་་(&C)"

#: customAppPath.label
msgid "Click 'Change ...' to choose an application."
msgstr "གློག་རིམ་ཅིག་ གདམ་ཁ་བརྐྱབ་ནི་ལུ་ 'བསྒྱུར་བཅོས་འབད་་་་་་' གུ་ ཨེབ་གཏང་འབད།"

#: saveToDisk.label
msgid "Save to Disk"
msgstr "ཌིཀསི་ལུ་སྲུངས།"

#: changeAppWindowTitle.label
msgid "Choose Application..."
msgstr "གློག་རིམ་གདམ་ཁ་རྐྱབས་་་་"

