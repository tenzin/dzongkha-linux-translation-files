#: lHeader
msgid "Attachments"
msgstr "མཉམ་སྦྲགས་ཚུ་"

#: askOnSave.label
msgid "Attachment Folder"
msgstr "མཉམསྦྲགས་སྣོད་འཛིན་"

#: alwaysAsk.label
msgid "Ask me where to save every attachment"
msgstr "མཉམ་ཚུ་རེོ་རེ་བཞིན་དུ་ ག་ཏེ་བསྲུང་ནི་ཨིན་ན་ ང་ལུ་དྲིས།"

#: neverAsk.label
msgid "Save all attachments to this folder:"
msgstr "མཉམ་སྦྲགས་ཚུ་ཆ་མཉམ་ སྣོད་འཛིན་འདི་ལུ་སྲུངས།"

#: desktop.label
msgid "Desktop"
msgstr "ཌེཀསི་ཊོཔ་"

#: downloads.label
msgid "My Downloads"
msgstr "ངེད་ཀྱི་ཕབ་ལེན་ཚུ་"

#: choose.label
msgid "Other..."
msgstr "གཞན་་་་"

#: showFolder.label
msgid "Show Folder"
msgstr "སྣོད་འཛིན་སྟོན་"

#: downloadManagerWindow.label
msgid "Attachment Manager"
msgstr "མཉམ་སྦྲགས་འཛིན་སྐྱོང་པ།"

#: showWhenStarting.label
msgid "Show Attachment Manager window when a download begins."
msgstr "ཕབ་ལེན་ཅིག་འོག་བཙུགསཔ་ད་ མཉམ་སྦྲགས་འཛིན་སྐྱོང་པའི་སྒོ་སྒྲིག་སྟོན།"

#: closeWhenDone.label
msgid "Close the Attachment Manager when all attachments are complete."
msgstr "མཉམ་སྦྲགས་ཆ་མཉམ་མཇུག་བསྡུ་ཚརཝ་ད་ མཉམ་སྦྲགས་འཛིན་སྐྱོང་པ་དེ་ཁ་བསྡམས།"

#: fileTypes.label
msgid "File Types"
msgstr "ཡིག་སྣོད་དབྱེ་བ་"

# XXX this text string could improve
#: fileTypesDescription.label
msgid "Automatically perform the associated action with each of the following file types:"
msgstr "འོག་གི་དབྱེ་བ་ཚུ་དང་གཅིག་ཁར་འབྲེལ་བའི་བྱ་བ་དེ་ རང་བཞིན་གྱིས་འབདཝ་ཨིན་:"

#: fileTypeColName.label
msgid "File Type"
msgstr "ཡིག་སྣོད་དབྱེ་བ་"

#: fileHandlerColName.label
msgid "Application"
msgstr "གློག་རིམ་"

#: editFileHandler.label
#: editFileHandler.accesskey
msgid "Change &Action..."
msgstr "བྱ་བ་སོར་་་་(&A)"

#: removeFileHandler.label
#: removeFileHandler.accesskey
msgid "&Remove"
msgstr "རྩ་བསྐྲད་གཏང་(&R)"

#: plugins.label
#: plugins.accesskey
msgid "&Plug-Ins..."
msgstr "པ་ལག་ཨིནསི་་་་(&P)"

