#: title
msgid "Opening %S"
msgstr " %Sཁ་ཕྱེ་དོ་"

#: saveDialogTitle
msgid "Enter name of file to save to..."
msgstr "བསྲུང་ནིའི་ཡིག་སྣོད་ཀྱི་མིང་བཙུགས་་་་་"

#: defaultApp
msgid "%S (default)"
msgstr "%S (སྔོན་སྒྲིག་)"

#: chooseAppFilePickerTitle
msgid "Choose Helper Application"
msgstr "ལས་རོགས་པའི་གློག་རིམ་གདམ་ཁ་རྐྱབས་"

#: badApp
msgid "The application you chose (\"%S\") could not be found.  Check the file name or choose another application."
msgstr "ཁྱོད་ཀྱིས་གདམ་ཁ་བརྐྱབས་མི་གློགརིམ་(\"%S\") དེ་འཚོལ་མ་ཐོབ། ཡིག་སྣོད་ཀྱི་མིང་ ཞིབ་དཔྱད་རྐྱབས་ ཡང་ན་ གློགརིམ་གཞན་ཅིག་ གདམ་ཁ་རྐྱབས།"

#: badApp.title
msgid "Application not found"
msgstr "གློག་རིམ་འཚོལ་མ་ཐོབ་"

#: selectDownloadDir
msgid "Select Download Folder"
msgstr "ཕབ་ལེན་སྣོད་འཛིན་སེལ་འཐུ་འབད།"

#: myDownloads
msgid "My Downloads"
msgstr "ངེད་ཀྱི་ཕབ་ལེན་ཚུ་"

