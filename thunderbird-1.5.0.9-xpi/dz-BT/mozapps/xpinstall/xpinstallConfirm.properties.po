#: Unsigned
msgid "Unsigned"
msgstr "མིང་རྟགས་མ་བཀོདཔ་"

#: itemWarningIntroMultiple
msgid "A web site is requesting permission to install the following %S items:"
msgstr "ཝེབ་ས་ཁོངས་ཅིག་གིས་ འོག་གི་རྣམ་གྲངས་%S ཚུ་ གཞི་བཙུགས་འབད་ནིའི་གནང་བ་དགོ་པའི་ ཞུ་བ་འབད་དོ་:"

#: itemWarningIntroSingle
msgid "A web site is requesting permission to install the following item:"
msgstr "ཝེབ་ས་ཁོངས་ཅིག་གིས་ འོག་གི་རྣམ་གྲངས་ཚུ་གཞི་བཙུགས་འབད་ནིའི་གནང་བ་དགོ་པའི་ ཞུ་བ་འབད་དོ་"

#: installButtonDisabledLabel
msgid "Install (%S)"
msgstr "(%S)གཞི་བཙུགས་འབད་"

#: installButtonLabel
msgid "Install Now"
msgstr "ད་ལྟོ་ར་གཞི་བཙུགས་འབད་"

#: installComplete
msgid "Software Installation is complete. You will have to restart %S for changes to take effect."
msgstr "མཉེན་ཆས་གཞི་བཙུགས་འབད་ཚར་ཡི། བསྒྱུར་བཅོས་ཚུ་ལུ་ ནུས་པ་ཐོབ་ནི་ལུ་ ཁྱོད་ཀྱིས་%Sལོག་སྟེ་འགོ་བཙུགས་དགོ།"

#: installCompleteTitle
msgid "Installation Complete"
msgstr "གཞི་བཙུགས་མཇུག་བསྡུ་ཡི།"

#: error-203
msgid "Error Installing Item"
msgstr "རྣམ་གྲངས་གཞི་བཙུགས་འབད་ནི་ལུ་འཛོལ་བ་"

