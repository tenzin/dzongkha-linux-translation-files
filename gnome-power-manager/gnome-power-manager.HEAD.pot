#. common descriptions of this program
#: ../data/gnome-power-manager.desktop.in.h:1
#: ../src/gpm-common.h:30
msgid "Power Manager"
msgstr "ནུས་ཤུགས་ཀྱི་འཛིན་སྐྱོང་པ།"

#: ../data/gnome-power-manager.desktop.in.h:2
msgid "Power management daemon"
msgstr "ནུས་ཤུགས་འཛིན་སྐྱོངས་པའི་ ཌེ་མཱོན།"

#: ../data/gnome-power-manager.schemas.in.h:1
msgid "After resume, gnome-power-manager will suppress policy actions for a number of seconds to allow messages to settle and HAL to refresh. Commonly five seconds is enough while not being so long that the user gets confused."
msgstr "བསྐྱར་ལོག་གི་ཤུལ་ལས་  སྐར་ཆ་གི་གྲངས་ལུ་འཕྲིན་དོན་གཞིས་ཆགས་བཅུག་ནི་དང་ ཨེཆ་ཨེ་ཨེལ་ཡང་སེལ་འབད་བཅུག་ནའི་དོན་ལུ་ ཇི་ནོམ་ - ནུས་ཤུགས་ - འཛིན་སྐྱོང་པ་གིས་ སྲིད་བྱུས་ཀྱི་བྱ་བ་མར་མནོན་འབད་འོང་། མཐུན་མོང་དུ་འབད་བ་ཅིན་སྣ་མེད་ས་མེད་ཡུན་རིངམ་འབད་ལག་ལེན་པ་མགོ་འཐོམ་པ་ལས་ སྐར་ཆ་ལྔ་གི་ལངམ་ཨིན།"

#: ../data/gnome-power-manager.schemas.in.h:2
msgid "Battery critical low action"
msgstr "འཕྲུལ་ལྟོའི་ཚབས་ཆེན་གྱི་བྱ་བ་དམའ་ཤོས།"

#: ../data/gnome-power-manager.schemas.in.h:3
msgid "Check CPU load before sleeping"
msgstr "གཉིད་མ་ཁུག་པའི་ཧེ་མ་ལས་སི་པི་ཡུ་གི་མངོན་གསལ་འབད་བཞིནམ་དང་ཞིབ་དཔྱད་འབད།"

#: ../data/gnome-power-manager.schemas.in.h:4
msgid "Check network utilization before sleeping"
msgstr "གཉིད་མ་ཁུག་པའི་ཧེ་མ་ལས་ཡོངས་འབྲེལ་གྱི་བེད་སྤྱོད་འདི་ཞིབ་དཔྱད་འབད།"

#: ../data/gnome-power-manager.schemas.in.h:5
msgid "Dim the screen after a period of inactivity"
msgstr "ལཱ་འབད་ཚར་བའི་དུས་ཡུན་ལུ་གསལ་གཞི་མདངས་གྲིབ་འབད།"

#: ../data/gnome-power-manager.schemas.in.h:6
msgid "Hibernate button action"
msgstr "ཨེབ་རྟའི་བྱ་བ་འདི་གཉིད་ནང་བཙུགས་བཞག"

#: ../data/gnome-power-manager.schemas.in.h:7
msgid "Hibernate enabled"
msgstr "གཉིད་ནང་བཙུགས་བཞག་ནི་འདི་ལྕོགས་ཅན་བཟོ་ཡོདཔ།"

#: ../data/gnome-power-manager.schemas.in.h:8
msgid "LCD brightness when on AC"
msgstr "ཨེ་སི་གི་སྐབས་ལུ་ཨེལ་སི་ཌི་དཀར་མདངས།"

#: ../data/gnome-power-manager.schemas.in.h:9
msgid "LCD brightness when on battery"
msgstr "འཕྲུལ་ལྟོའི་སྐབས་ལུ་ཨེལ་སི་ཌི་གི་དཀར་མདངས།"

#: ../data/gnome-power-manager.schemas.in.h:10
msgid "Laptop lid close action on battery"
msgstr "འཕྲུལ་ལྟོ་གུ་ལེཔ་ཊོཔ་གི་ལིཌི་བྱ་བ་ཁ་བསྡམ་མི།"

#: ../data/gnome-power-manager.schemas.in.h:11
msgid "Laptop lid close action when on AC"
msgstr "ཨེ་སི་གི་སྐབས་ལུ་ལེབ་ཊོབ་ལིཌི་བྱ་བ་ཁ་བསྡམ་མི།"

#: ../data/gnome-power-manager.schemas.in.h:12
msgid "Lock screen on hibernate"
msgstr "གཉིད་ནང་བཙུགས་བཞག་མི་གུ་གསལ་གཞི་བསྡམ་བཞག།"

#: ../data/gnome-power-manager.schemas.in.h:13
msgid "Lock screen on suspend"
msgstr "འཕྲལ་བཀག་གུ་གསལ་གཞི་བསྡམ་བཞག"

#: ../data/gnome-power-manager.schemas.in.h:14
msgid "Lock screen when it is blanked"
msgstr "སྟོངམ་འབད་ཡོད་པའི་སྐབས་སུ་གསལ་གཞི་བསྡམ་བཞག"

#: ../data/gnome-power-manager.schemas.in.h:15
msgid "Notify on a HAL suspend or hibernate error."
msgstr "ཨེཆ་ཨེ་ཨེལ་འཕྲལ་བཀག་ཡང་ན་འཛོལ་བ་གཉིད་ནང་བཙུགས་བཞག་གུ་བརྡ་བསྐུལ་འབད།"

#: ../data/gnome-power-manager.schemas.in.h:16
msgid "Notify on fully charged"
msgstr "ཤུགས་ཚད་ཆ་ཚང་གུ་བརྡ་བསྐུལ་འབད།"

#: ../data/gnome-power-manager.schemas.in.h:17
msgid "Notify when AC adapter is removed"
msgstr "ཨེ་སི་མཐུན་བྱེད་འདི་རྩ་བསྐྲད་གཏང་བཞིནམ་ལས་བརྡ་བསྐུལ་འབད།"

#: ../data/gnome-power-manager.schemas.in.h:18
msgid "Number of seconds to suppress policy after resume"
msgstr "བསྐྱར་ལོག་གི་ཤུལ་མ་ སྲིད་བྱུས་མར་མནོན་གྱི་དོན་ལུ་ སྐར་ཆ་གི་གྲངས།"

#: ../data/gnome-power-manager.schemas.in.h:19
msgid "Option when to show the notification icon. Valid options are \"never\", \"critical\", \"charge\", \"present\" and \"always\""
msgstr "བརྡ་བསྐུལ་གྱི་ངོས་དཔར་ནམ་སྟོན་ནི་ཨིན་ན་གི་གདམ་ཁ། ནུས་ཅན་གྱི་གདམ་ཁ་ཚུ་  \"ནམ་ཡང་།\", \"ཚབས་ཆེན།\", \"ཤུགས་ཚད།\", \" ངོ་འཛོམས་ཡོདཔ\" དང་ \"ཨ་རྟག་རང་།\""

#: ../data/gnome-power-manager.schemas.in.h:20
msgid "Percentage action is taken"
msgstr "བརྒྱ་ཆའི་བྱ་བ་འདི་འབག་ཡར་སོང་ནུག"

#: ../data/gnome-power-manager.schemas.in.h:21
msgid "Percentage is considered critical"
msgstr "བརྒྱ་ཆ་འདི་ཚབས་ཆེན་སྦེ་ཆ་འཇོག་འབད་ནུག"

#: ../data/gnome-power-manager.schemas.in.h:22
msgid "Percentage is considered low"
msgstr "བརྒྱ་ཆ་འདི་དམའ་ཤོས་འབད་ཆ་འཇོག་འབད་ནུག"

#: ../data/gnome-power-manager.schemas.in.h:23
msgid "Percentage is considered very low"
msgstr "བརྒྱ་ཆ་འདི་ཧ་ཅང་གི་དམའ་ཤོས་འབད་ཆ་འཇོག་འབད་ནུག"

#: ../data/gnome-power-manager.schemas.in.h:24
msgid "Power button action"
msgstr "ཨེབ་རྟ་ནུས་ཤུགས་ཀྱི་བྱ་བ།"

#: ../data/gnome-power-manager.schemas.in.h:25
msgid "Sleep timeout computer when on AC"
msgstr "ཨེ་སི་གི་སྐབས་ལུ་གློག་རིག་ངལ་མཚམས་བསྒུག་ནི།"

#: ../data/gnome-power-manager.schemas.in.h:26
msgid "Sleep timeout computer when on battery"
msgstr "འཕྲུལ་ལྟོའི་སྐབས་སུ་གློག་རིག་ངལ་མཚམས་བསྒུག་ནི།"

#: ../data/gnome-power-manager.schemas.in.h:27
msgid "Sleep timeout display when on AC"
msgstr "ཨེ་སི་གི་སྐབས་ལུ་ངལ་མཚམས་བསྒུག་ནི་འདི་བཀྲམ་སྟོན་འབདཝ་ཨིན།"

#: ../data/gnome-power-manager.schemas.in.h:28
msgid "Sleep timeout display when on battery"
msgstr "འཕྲུལ་ལྟོའི་སྐབས་ལུ་ངལ་མཚམས་ཀྱི་བསྒུག་ནི་བཀྲམ་སྟོན་འབདཝ་ཨིན།"

#: ../data/gnome-power-manager.schemas.in.h:29
msgid "Suspend button action"
msgstr "འཕྲལ་བཀག་གི་ཨེབ་རྟའི་བྱ་བ།"

#: ../data/gnome-power-manager.schemas.in.h:30
msgid "Suspend enabled"
msgstr "འཕྲལ་བཀག་ལྕོགས་ཅན་བཟོ་ཡོདཔ།"

#: ../data/gnome-power-manager.schemas.in.h:31
msgid "The action to take when the UPS is critically low. Valid values are \"hibernate\", \"suspend\", \"shutdown\" and \"nothing\""
msgstr "བྱ་བ་འདི་ ཡུ་པི་ཨེསི་འདི་ཚབས་ཆེན་པོ་སྦེ་དམའ་བ་ཡོདཔ་ད་ལེན་ནི། ནུས་ཅན་གྱི་གནས་གོང་ཚུ་  \"གཉིད་ནང་བཙུགས་ནི\", \"འཕྲལ་བཀག\", \"སྒོ་བསྡམ་ནི།།\" དང་ \"ག་ནི་ཡང་མེདཔ\""

#: ../data/gnome-power-manager.schemas.in.h:32
msgid "The action to take when the battery is critically low. Valid values are \"hibernate\", \"suspend\", \"shutdown\" and \"nothing\""
msgstr "འཕྲུལ་ལྟོ་འདི་ཚབས་ཆེན་སྦེ་དམའ་བ་འགྱོཝ་ད་གི་ལག་ལེན་འཐབ་མིའི་བྱ་བ། ནུས་ཅན་གྱི་གནས་གོང་ཚུ་  \"གཉིད་ནང་བཙུགས་ནི\", \"འཕྲལ་བཀག\", \"སྒོ་བསྡམ་ནི།།\" དང་ \"ག་ནི་ཡང་མེདཔ\""

#: ../data/gnome-power-manager.schemas.in.h:33
msgid "The action to take when the laptop lid is closed and the laptop is on AC power. Valid values are \"suspend\", \"hibernate\" and \"blank\"."
msgstr "ལེཔ་ཊོཔ་གི་ལིཌི་འདི་ཁ་བསྡམ་ཡོད་པའི་སྐབས་སུ་དང་  ལེབ་ཊོཔ་འདི་ཨེ་སི་ནུས་ཤུགས་གུ་ཡོདཔ་ད་ལག་ལན་འཐབ་མིའི་བྱ་བ། ནུས་ཅན་གྱི་གནས་གོང་ཚུ་  \"འཕྲལ་བཀག\", \"གཉིད་ནང་བཙུགས་ནི\"དང་ \"སྟོངམ\"."

#: ../data/gnome-power-manager.schemas.in.h:34
msgid "The action to take when the laptop lid is closed and the laptop is on battery power. Valid values are \"suspend\", \"hibernate\" and \"blank\"."
msgstr "ལེབ་ཊོཔ་གི་ལིཌི་འདི་ཁ་བསྡམ་ཡོད་པའི་སྐབས་སུ་དང་  ལེབཊོཔ་འདི་འཕྲུལ་ལྟོ་ནུས་ཤུགས་གུ་ཡོདཔ་ད་ ལག་ལེན་འཐབ་པའི་བྱ་བ།ནུས་ཅན་གྱི་གནས་གོང་ཚུ་  \"འཕརལ་བཀག\", \"གཉིད་ནང་བཙུགས་ནི\" དང་ \"སྟོངམ\"."

#: ../data/gnome-power-manager.schemas.in.h:35
msgid "The action to take when the system hibernate button is pressed. Valid values are \"suspend\", \"hibernate\", \"interactive\", \"shutdown\" and \"nothing\"."
msgstr "རིམ་ལུགས་གཉིད་ནང་བཙུགས་མི་གི་ཨེབ་རྟ་འདི་ཨེབ་ཚརཝ་ད་ལུ་ ལག་ལེན་འཐབ་པའི་བྱ་བ། ནུས་ཅན་གྱི་གནས་གོང་ཚུ་  \"འཕྲལ་བཀག\", \"གཉིད་ནང་བཙུགས་མི\", \"ཕན་ཚུན་འབྲེལ་ལྡན།\", \"སྒོ་བསྡམ་ནི།།\" དང་\"ག་ནི་ཡང་མེདཔ\"."

#: ../data/gnome-power-manager.schemas.in.h:36
msgid "The action to take when the system power button is pressed. Valid values are \"suspend\", \"hibernate\", \"interactive\", \"shutdown\" and \"nothing\"."
msgstr "རིམ་ལུགས་ནུས་ཤུགས་ཀྱི་ཨེབ་རྟ་འདི་ཨེབ་པའི་སྐབས་སུ་ལག་ལེན་འཐབ་པའི་བྱ་བ། ནུས་ཅན་གྱི་གནས་གོང་ཚུ་ \"འཕྲལ་བཀག\", \"གཉིད་ནང་བཙུགས་ནི\", \"ཕན་ཚུན་འབྲེལ་ལྡན།\", \"སྒོ་བསྡམ\" དང་ \"ག་ནི་ཡང་མེདཔ\"."

#: ../data/gnome-power-manager.schemas.in.h:37
msgid "The action to take when the system suspend button is pressed. Valid values are \"suspend\", \"hibernate\", \"interactive\", \"shutdown\" and \"nothing\"."
msgstr "རིམ་ལུགས་ནུས་ཤུགས་ཀྱི་ཨེབ་རྟ་འཕྲལ་བཀག་འདི་ཨེབ་པའི་སྐབས་སུ་ ལག་ལེན་འཐབ་མི་གི་བྱ་བ། ནུས་ཅན་གྱི་གནས་གོང་ཚུ་  \"འཕྲལ་བཀག\", \"གཉིད་ནང་བཙུགས་ནི\", \"ཤུགས་མེད\", \"སྒོ་བསྡམ\" དང་ \"ག་ནི་ཡང་མེདཔ\"."

#: ../data/gnome-power-manager.schemas.in.h:38
msgid "The brightness of the display when on AC power. Valid values are between 0 and 100."
msgstr "ཨེ་སི་ནུས་ཤུགས་གུ་ཡོད་པའི་སྐབས་སུ་བཀྲམ་སྟོན་གྱི་དཀར་མདངས།  ནུས་ཤུགས་ཀྱི་གནས་གོང་ཚུ་  ༠ དང་ ༡༠༠གྱི་བར་ན།"

#: ../data/gnome-power-manager.schemas.in.h:39
msgid "The brightness of the display when on battery power. Valid values are between 0 and 100."
msgstr "འཕྲུལ་ལྟོའི་ནུས་ཤུགས་གུ་ཡོདཔ་ད་ བཀྲམ་སྟོན་གི་དཀར་མདངས། ནུས་ཅན་གྱི་གནས་གོང་ཚུ་  ༠ དང་ ༡༠༠་གྱི་བར་ན།"

#: ../data/gnome-power-manager.schemas.in.h:40
msgid "The brightness of the screen when session idle"
msgstr "ལཱ་ཡུན་གྱི་ལས་མེད་ཀྱི་སྐབས་ལུ་གསལ་གཞི་གི་དཀར་མདངས།"

#: ../data/gnome-power-manager.schemas.in.h:41
msgid "The damping factor used when calculating the rate"
msgstr "མགྱོགས་ཚད་རྩིས་སྟོན་པའི་སྐབས་ལུ་ཆ་རྐྱེན་གྱི་ནུས་པ་ཉམས་ནི་འདི་ལག་ལེན་འཐབ་ཨིན།"

#: ../data/gnome-power-manager.schemas.in.h:42
msgid "The number of seconds before the display goes to sleep when the computer is on AC power."
msgstr "གློག་རིག་འདི་ཨེ་སི་གུ་ཡོདཔ་ད་ བཀྲམ་སྟོན་མ་བསྒུག་པའི་ཧེ་མ་ལས་སྐར་ཆ་གི་གྲངས།"

#: ../data/gnome-power-manager.schemas.in.h:43
msgid "The number of seconds the computer on AC power needs to be inactive before it goes to sleep."
msgstr "ཨེ་སི་ནུས་ཤུགས་གུ་ཡོད་པའི་ གློག་རིགས་སྐར་ཆ་གི་གྲངས་འདི་ ཨེ་སི་གི་ནུས་ཤུགས་ མ་བསྒུག་པའི་ཧེ་མ་ལས་ཤུགས་མེད་འོང་དགོ།"

#: ../data/gnome-power-manager.schemas.in.h:44
msgid "The number of seconds the computer on battery power needs to be inactive before it goes to sleep."
msgstr "གློག་རིག་ འཕྲུལ་ལྟོའི་ནུས་ཤུགས་གུ་ཡོད་པའི་སྐར་ཆ་གི་གྲངས་འདི་  མ་བསྒུགས་པའི་ཧེ་མ་ལས་ཤུགས་མེད་དགོ།"

#: ../data/gnome-power-manager.schemas.in.h:45
msgid "The number of seconds the computer on battery power needs to be inactive before the display goes to sleep."
msgstr "བཀྲམ་སྟོན་འདི་མ་བསྒུག་པའི་ཧེ་མ་ལས་ གློག་རིག་འཕྲུལ་ལྟོའི་ནུས་ཤུགས་གུ་ཡོད་མི་ སྐར་ཆ་གི་གྲངས་འདི་ཤུགས་མེད་དགོ། "

#: ../data/gnome-power-manager.schemas.in.h:46
msgid "The percentage of the battery when it is considered critical. Only valid when use_time_for_policy false."
msgstr "འཕྲུལ་ལྟོ་གི་བརྒྱ་ཆ་འདི་ ཚབས་ཆེན་སྦེ་ཆ་འཇོག་འབད་བའི་བསྒང་། སྲིད་བྱུས་རྫུན་མ་གི་དོན་ལུ་དུས་ཡུན་ _ལག་ལེན་འཐབ་ད_་རྐྱངམ་ཅིག་ _ནུས་ཅན།"

#: ../data/gnome-power-manager.schemas.in.h:47
msgid "The percentage of the battery when it is considered low. Only valid when use_time_for_policy false."
msgstr "འཕྲུལ་ལྟོ་གི་བརྒྱ་ཆ་དམའ་ཤོས་སྦེ་ཆ་འཇོག་འབད་བའི་བསྒང་། སྲིད་བྱུས་རྫུན་མ་གི་དོན་ལུ་དུས་ཡུན་ _ལག་ལེན་འཐབ་ད_་རྐྱངམ་ཅིག་ _ནུས་ཅན།"

#: ../data/gnome-power-manager.schemas.in.h:48
msgid "The percentage of the battery when it is considered very low. Only valid when use_time_for_policy false."
msgstr "འཕྲུལ་ལྟོ་གི་བརྒྱ་ཆ་ཧ་ཅང་གི་དམའ་ཤོས་སྦེ་ཆ་འཇོག་འབད་བའི་བསྒང་།སྲིད་བྱུས་རྫུན་མ་གི་དོན་ལུ་དུས་ཡུན་ _ལག་ལེན་འཐབ་ད_་རྐྱངམ་ཅིག་ _ནུས་ཅན།"

#: ../data/gnome-power-manager.schemas.in.h:49
msgid "The percentage of the battery when the critical action is performed. Only valid when use_time_for_policy false."
msgstr "འཕྲུལ་ལྟོ་གི་བརྒྱ་ཆ་འདི་བྱ་བ་ ཚབས་ཆེན་སྦེ་ལཱ་འགན་འགྲུབ་པའི་བསྒང་། སྲིད་བྱུས་རྫུན་མ་གི་དོན་ལུ་དུས་ཡུན་ _ལག་ལེན་འཐབ་ད_་རྐྱངམ་ཅིག་ _ནུས་ཅན།"

#: ../data/gnome-power-manager.schemas.in.h:50
msgid "The time remaining in seconds of the battery when critical action is taken. Only valid when use_time_for_policy true."
msgstr "ཚབ་ཆེན་གྱི་བྱ་བ་ལག་ལེན་འཐབ་པའི་བསྒང་ འཕྲུལ་ལྟོ་གི་སྐར་ཆ་ལྷག་ལུས། སྲིད་བྱུས་བདེན་མ་གི་ _ཆུ་ཚོད་དོན་ལུ་_ རྐྱངམ་ཅིག་_ནུས་ཅན།"

#: ../data/gnome-power-manager.schemas.in.h:51
msgid "The time remaining in seconds of the battery when it is considered critical. Only valid when use_time_for_policy true."
msgstr "ཚབ་ཆེན་གྱི་བྱ་བ་ལག་ལེན་འཐབ་པའི་བསྒང་ འཕྲུལ་ལྟོ་གི་སྐར་ཆ་ལྷག་ལུས། སྲིད་བྱུས་བདེན་མ་གི་ _ཆུ་ཚོད་དོན་ལུ་_ རྐྱངམ་ཅིག་_ནུས་ཅན།"

#: ../data/gnome-power-manager.schemas.in.h:52
msgid "The time remaining in seconds of the battery when it is considered low. Only valid when use_time_for_policy true."
msgstr "དམའ་ཤོས་སྦེ་ཆ་འཇོག་འབད་བའི་བསྒང་ འཕྲུལ་ལྟོ་གི་དུས་ཡུན་དཀར་ཆ་ནང་ལྷག་ལུས། སྲིད་བྱུས་བདེན་མ་གི་ _ཆུ་ཚོད་དོན་ལུ་_ རྐྱངམ་ཅིག་_ནུས་ཅན།"

#: ../data/gnome-power-manager.schemas.in.h:53
msgid "The time remaining in seconds of the battery when it is considered very low. Only valid when use_time_for_policy true."
msgstr "དམའ་ཤོས་སྦེ་ཆ་འཇོག་འབད་བའི་བསྒང་འཕྲུལ་ལྟོ་གི་དུས་ཡུན་སྐར་ཆ་ནང་ལྷག་ལུས། སྲིད་བྱུས་བདེན་མ་གི་ _དུས་ཡུན་དོན་ལུ་_ རྐྱངམ་ཅིག་_ནུས་ཅན།"

#: ../data/gnome-power-manager.schemas.in.h:54
msgid "The time remaining when action is taken"
msgstr "བྱ་བ་འདི་ལག་ལེན་འཐབ་པའི་བསྒང་ དུས་ཡུན་ལྷག་ལུས།"

#: ../data/gnome-power-manager.schemas.in.h:55
msgid "The time remaining when critical"
msgstr "ཚབ་ཆེན་གྱི་བསྒང་ དུས་ཡུན་ལྷག་ལུས།"

#: ../data/gnome-power-manager.schemas.in.h:56
msgid "The time remaining when low"
msgstr "དམའ་ཤོས་ཀྱི་སྐབས་ལུ་ དུས་ཡུན་ལྷག་ལུས།"

#: ../data/gnome-power-manager.schemas.in.h:57
msgid "The time remaining when very low"
msgstr "ཧ་ཅང་གི་དམའ་བའི་བསྒང་ དུས་ཡུན་ལྷག་ལུས།"

#: ../data/gnome-power-manager.schemas.in.h:58
msgid "The type of sleeping that should be performed when the computer is inactive. Valid values are \"hibernate\", \"suspend\" and \"nothing\"."
msgstr "གློག་རིག་འདི་ཤུགས་མེད་པའི་བསྒང་ བསྒུག་ནི་གི་དབྱེ་བ་ ལཱ་འགན་འགྲུབ་དགོཔ། ནུས་ཅན་གཡི་གནས་གོང་ཚུ་   \"གཉིད་ནང་བཙུགས་ནི\", \"འཕྲལ་བཀབ\" དང་ \"ག་ནི་ཡང་མེདཔ\"."

#: ../data/gnome-power-manager.schemas.in.h:59
msgid "The value of the damping factor used when calculating the exponentially weighted average of the rate. Increasing this value will increase the damping effect of the rate calculations, which makes the time remaining more accurate, but the graphs less responsive."
msgstr "མགྱོགས་ཚད་བསྒྱུར་གྲངས་ཀྱི་ལྗིད་ཚད་ཆ་སྙོམས་འདི་ རྩིས་སྟོན་པའི་བསྒང་ ཆ་རྐྱེན་ནུས་པ་ཉམས་མི་གི་གནས་གོང་འདི་ ལག་ལེན་འཐབ་ཨིན། འ་ནི་གནས་གོང་ཡར་འཕར་འགྱོ་མི་འདི་གིས་ དུས་ཡུན་ལྷག་ལུས་ཧེང་བཀལ་ཀྲིག་ཀྲི་བཟོ་ནི་དང་ དེ་འབདཝ་ད་ཚད་ཁྲམ་འདི་ལན་ཉུང་བཟོ་མི་ལུ་ མགྱོགས་ཚད་རྩིས་ཀྱིས་ ཆ་རྐྱེན་ཉམས་མི་འདི་གོང་ཡར་འཕར་བཏངམ་ཨིན།"

#: ../data/gnome-power-manager.schemas.in.h:60
msgid "This is the laptop panel screen brightness used when the session is idle. Only valid when use_time_for_policy true."
msgstr "ལཱ་ཡུན་འདི་ལས་མེད་ཀྱི་སྐབས་ལུ་ འ་ནི་ལེབ་ཊོཔ་པེ་ནཱལ་གྱི་གསལ་གཞི་འདི་ལག་ལེན་འཐབ་ཨིན། སྲིད་བྱུས་བདེན་མའི་དོན་ལུ་_དུས་ཡུན་ལག་ལེན་འཐབ་ད་_རྐྱངམ་ཅིག་_ནུས་ཅན།"

#: ../data/gnome-power-manager.schemas.in.h:61
msgid "UPS critical low action"
msgstr "ཡུ་པི་ཨེསི་ཚབས་ཆེན་གྱི་ བྱ་བ་དམའ་ཤོས།"

#: ../data/gnome-power-manager.schemas.in.h:62
msgid "Use gnome-screensaver lock setting"
msgstr "ཇི་ནོམ་གསལ་གཞི་ཉན་སྲུང་གི་ལྡེ་མིག་མའི་གཞི་སྒྲིག་ལག་ལེན་འཐབ།"

#: ../data/gnome-power-manager.schemas.in.h:63
msgid "When to show the notification icon"
msgstr "བརྡ་བསྐུལ་གྱི་ངོས་དཔར་ནམ་སྟོན་ནི་ཨིན་ན།"

#: ../data/gnome-power-manager.schemas.in.h:64
msgid "Whether or not a notification message is displayed if the HAL action (suspend or hibernate) fails"
msgstr "ཨེཆ་ཨེ་ཨེལ་གྱི་བྱ་བ་འདི་འཐུས་ཤོར་བྱུང་པ་ཅིན་(འཕྲལ་བཀག་ཡང་ན་གཉིད་ནང་བཙུགས་ནི) བརྡ་བསྐུལ་གྱི་འཕྲིན་དོན་འདི་ བཀྲམ་སྟོན་འབད་ཚུགས་རུང་མ་ཚུགས་རུང་།"

#: ../data/gnome-power-manager.schemas.in.h:65
msgid "Whether or not a notification message is displayed when the battery is fully charged"
msgstr "འཕྲུལ་ལྟོ་འདི་ཆ་ཚང་སྦེ་ཤུགས་ཚད་འབད་ཚར་བའི་བསྒང་  བརྡ་བསྐུལ་གྱི་འཕྲིན་དོན་འདི་བཀྲམ་སྟོན་འབད་ཚུགས་རུང་མ་ཚུགས་རུང་།"

#: ../data/gnome-power-manager.schemas.in.h:66
msgid "Whether or not the screen is locked when the screen is turned off. NOTE this option is only used if lock_use_screensaver_settings is set to false."
msgstr "གསལ་གཞི་འདི་ཊཱན་ཨོཕ་འབད་བའི་བསྒང་ལས་ གསལ་གཞི་འདི་བསྡམ་བཞག་ཚུགས་རུང་མ་ཚུགས་རུང་། དྲན་འཛིན་འབད་དགོཔ་ ལྡེ་མིག་མ་གིས་_གསལ་གཞི་ཉན་སྲུང་ལག་ལེན་པའི་  གཞི་སྒྲིག་འདི་_རྫུན་མ་ལུ་ཆ་ཚན་འབད་འདི་ཡོད་པ་ཅིན་_རྐྱངམ་ཅིག་ འ་ནི་གདམ་ཁ་འདི་ལག་ལེན་འཐབ་དགོ།"

#: ../data/gnome-power-manager.schemas.in.h:67
msgid "Whether or not the user can hibernate the computer"
msgstr "ལག་ལེན་པ་གིས་གློག་རིག་འདི་གཉིད་ནང་བཙུགས་ཚུགས་རུང་མ་ཚུགས་རུང་།"

#: ../data/gnome-power-manager.schemas.in.h:68
msgid "Whether or not the user can suspend the computer"
msgstr "ལག་ལེན་པ་གིས་གློག་རིག་འདི་འཕྲལ་བཀག་འབད་ཚུགས་རུང་མ་ཚུགས་རུང་།"

#: ../data/gnome-power-manager.schemas.in.h:69
msgid "Whether or not the user is notified when the AC adapter is removed"
msgstr "ཨེ་སི་མཐུན་བྱེད་འདི་རྩ་བསྐྲད་གཏང་པའི་བསྒང་ལག་ལེན་པ་འདི་བརྡ་བསྐུལ་འབད་ཚུགས་རུང་མ་ཚུགས་རུང་།"

#: ../data/gnome-power-manager.schemas.in.h:70
msgid "Whether or not to allow the Suspend and Hibernate options in the notification area drop down menu."
msgstr "བརྡ་བསྐུལ་མངའ་ཁོངས་ མར་བཀོག་བཞག་གི་དཀར་ཆག་ནང་ལུ་ཡོད་མི་ འཕྲལ་བཀག་དང་གཉིད་ནང་བཙུགས་ནིའི་གདམ་ཁ་ཚུ་ འབད་བཅུག་ཚུགས་རུང་མ་ཚུགས་རུང་།"

#: ../data/gnome-power-manager.schemas.in.h:71
msgid "Whether or not to dim the screen to save power when the computer is idle."
msgstr "གློག་རིག་འདི་ལས་མེད་འབདཝ་ད་ ནུས་ཤུགས་སྲུང་ནའི་ལུ་གསལ་གཞི་མདངས་གྲིབ་འབད་ཚུགས་རུང་མ་ཚུགས་རུང་།"

#: ../data/gnome-power-manager.schemas.in.h:72
msgid "Whether or not to lock the screen when the computer wakes up from a hibernate. NOTE this option is only used if lock_use_screensaver_settings is set to false."
msgstr "གློག་རིག་འདི་གཉིད་ནང་བཙུགས་མི་ལས་ཝེགསི་ཨཔ་འབདཝ་ད་གསལ་གཞི་འདི་བསྡམ་བཞག་ཚུགས་རུང་མ་ཚུགས་རུང་། འདྲན་འཛིན་འབད་ ལྡེ་མིག་མ་གི་_གསལ་གཞི་ཉན་སྲུང་ལག་ལེན་པའི་ _གཞི་སྒྲིག་འདི་རྫུན་མ་ལུ་ཆ་ཚན་འབད་ཡོད་པ་ཅིན་_རྐྱངམ་ཅིག་ གདམ་ཁ་འདི་ལག་ལེན་འཐབ་ཨིན།"

#: ../data/gnome-power-manager.schemas.in.h:73
msgid "Whether or not to lock the screen when the computer wakes up from a suspend. NOTE this option is only used if lock_use_screensaver_settings is set to false."
msgstr "གློག་རིག་འདི་འཕྲལ་བཀག་ལས་ཝེགས་ཨཔ་འབད་བའི་བསྒང་གསལ་གཞི་བསྡམ་བཞག་ཚུགས་རུང་མ་ཚུགས་རུང་།  འདྲན་འཛིན་འབད་ ལྡེ་མིག་མ་གི་_གསལ་གཞི་ཉན་སྲུང་ལག་ལེན་པའི་ _གཞི་སྒྲིག་འདི་རྫུན་མ་ལུ་ཆ་ཚན་འབད་ཡོད་པ་ཅིན་_རྐྱངམ་ཅིག་ གདམ་ཁ་འདི་ལག་ལེན་འཐབ་ཨིན།"

#: ../data/gnome-power-manager.schemas.in.h:74
msgid "Whether or not to use the screen lock setting of gnome-screensaver to decide to lock the screen after a hibernate, suspend or blank screen."
msgstr "གསལ་གཞི་སྟོངམ་ཡང་ན་ འཕྲལ་བཀག་འབད་རུང་ གཉིད་ནང་བཙུགས་བཞིནམ་ལས་ གསལ་གཞི་འདི་བསྡམ་བཞག་ནི་ལུ་ གྲོས་ཐག་བཅད་ནའི་དོན་ལུ་ ཇི་ནོམ་གྱི་གསལ་གཞི་ཉན་སྲིང་གི་གསལ་གཞི་ལྡེ་མིག་མའི་གཞི་སྒྲིག་ ལག་ལེན་འཐབ་ཚུགས་རུང་མ་ཚུགས་རུང་།"

#: ../data/gnome-power-manager.schemas.in.h:75
msgid "Whether or not to use time based notifications. If set to false, then the percentage change is used instead, which may fix some broken ACPI BIOS's."
msgstr "བརྡ་བསྐུལ་དུས་ཡུན་གཞི་བཞག་ལག་ལེན་འཐབ་ཚུགས་རུང་མ་ཚུགས་རུང་། རྫུན་མ་ལུ་ཆ་ཚན་འབད་ཡོད་པ་ཅིན་ དེ་ལས་ བརྒྱ་ཆའི་བསྒྱུར་བཅོས་ཀྱི་ཚབ་ལུ་ལག་ལེན་འཐབ་ཨིན། དེ་ཡང་ ཆད་ཡོད་མི་ ཨེ་སི་པི་ཨའེ་ བི་ཨའེ་ཨོ་ཨེསི་ལ་ལོ་ཅིག་དཀའ་ངལ་སེལ་ཚུགས་ནི།"

#: ../data/gnome-power-manager.schemas.in.h:76
msgid "Whether to allow the Suspend and Hibernate options in the menu"
msgstr "དཀར་ཆག་གི་ནང་ལུ་འཕྲལ་བཀག་དང་གཉིད་ནང་བཙུགས་ནི་གི་གདམ་ཁ་ཚུ་འབད་བཅུག་ཚུགས་རུང་མ་ཚུགས་རུང་།"

#: ../data/gnome-power-manager.schemas.in.h:77
msgid "Whether to hibernate, suspend or do nothing when inactive"
msgstr "ཤུགས་མེད་པའི་བསྒང་ གཉིད་ནང་བཙུགས་ནི་ལུ་ འཕྲལ་བཀག་ཡང་ན་ག་ནི་ཡང་མེདཔ་སྦེ་རུང་།"

#: ../data/gnome-power-manager.schemas.in.h:78
msgid "Whether to use time-based notifications"
msgstr "ཆུ་ཚོད་གཞི་རྟེན་གྱི་བརྡ་བསྐུལ་ ལག་ལེན་འཐབ་རུང་མ་འཐབ་རུང་།"

#: ../data/gnome-power-manager.schemas.in.h:79
msgid "Whether we should check network utilization before doing the idle action."
msgstr "ལས་མེད་ཀྱི་བྱ་བ་འདི་མ་འབད་བའི་ཧེ་མ་ལས་ ང་བཅས་ཚུ་གིས་ཡོངས་འབྲེལ་བེད་སྤྱོད་འབད་དགོ་ག་མི་དགོ་ག་ཞིབ་དཔྱད་འབད་དགོ།"

#: ../data/gnome-power-manager.schemas.in.h:80
msgid "Whether we should check the CPU load before doing the idle action."
msgstr "ལས་མེད་ཀྱི་བྱ་བ་འདི་མ་འབད་བའི་ཧེ་མ་ལས་ ང་བཅས་ཚུ་གིས་ སི་པི་ཡུ་མངོན་གསལ་འབད་དགོ་ག་མི་དགོ་ག་ཞིབ་དཔྱད་འབད་དགོ།"

