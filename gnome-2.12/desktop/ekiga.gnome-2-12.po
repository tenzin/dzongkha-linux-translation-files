#. Add the window icon and title
#: ../gnomemeeting.desktop.in.h:1
#: ../src/main_window.cpp:3719
#: ../src/main_window.cpp:3911
msgid "GnomeMeeting"
msgstr "ཇི་ནོམ་མི་ཊིང།"

#: ../gnomemeeting.desktop.in.h:2
msgid "Talk to people over the Internet"
msgstr "ཨིན་ཊར་ནེཊི་གི་ཐོག་ལས་ མི་མང་ལུ་ཁ་སླབ།"

#: ../gnomemeeting.desktop.in.h:3
msgid "Videoconferencing"
msgstr "གློག་ཐོག་ཞལ་འཛོམས།"

#: ../gnomemeeting.schemas.in.in.h:1
msgid "1: 56Kbps, 2: ISDN, 3:DSL, 4: LAN, 5: Custom"
msgstr "༡: ཀེ་བི་པི་ཨེསི་༥༦། ༢: ཨའི་ཨེསི་ཌི་ཨེན། ༣: ཌི་ཨེསི་ཨེལ། ༤: ཨེལ་ཨེ་ཨེན། ༥: སྲོལ་སྒྲིག"

#: ../gnomemeeting.schemas.in.in.h:2
msgid "Address of public IP detector"
msgstr "མི་མང་གི་ ཨའི་པི་སྐྱོན་འཛིན་པའི་ཁ་བྱང་།"

#: ../gnomemeeting.schemas.in.in.h:3
msgid "Always forward calls to the given host"
msgstr "ཨ་རྟག་ར་ འབོད་བརྡ་ཚུ་ གྲ་བསྒྲིགས་ཧོསིཊི་ལུ་ གདོང་བསྐྱོད་འབད།"

#: ../gnomemeeting.schemas.in.in.h:4
msgid "Audio input device"
msgstr "རྣར་ཉན་ཨིན་པུཊི་ཐབས་ལམ།"

#: ../gnomemeeting.schemas.in.in.h:5
msgid "Audio output device"
msgstr "རྣར་ཉན་ཨའུཊི་པུཊི་ཐབས་ལམ།"

#: ../gnomemeeting.schemas.in.in.h:6
msgid "Audio plugin"
msgstr "རྣར་ཉན་ པ་ལག་ཨིན།"

#: ../gnomemeeting.schemas.in.in.h:7
msgid "Automatic echo cancellation"
msgstr "རང་བཞིན་གྱི་ བྲག་ཅ་ཆ་མེད།"

#: ../gnomemeeting.schemas.in.in.h:8
#: ../src/pref_window.cpp:716
msgid "Automatically clear the text chat at the end of calls"
msgstr "བོད་བརྡ་ཚུ་ ་མཇུག་བསྡུཝ་ད་ ཁ་སླབ་ཚིག་ཡིག་ཚུ་ རང་བཞིན་གྱིས་བསལ།"

#: ../gnomemeeting.schemas.in.in.h:9
#: ../src/pref_window.cpp:784
msgid "Automatically reject or forward incoming calls if no answer is given after the specified amount of time (in seconds)"
msgstr "ནང་འབྱོར་འབོད་བརྡ་ཚུ་ལུ་ གསལ་བཀོད་འབད་མི་དུས་ཚོད་ནང་འཁོད་(སྐར་ཆའི་ནང་) ལན་གསལ་མེད་པ་ཅིན་ རང་བཞིན་གྱིས་ དང་ལེན་མེད་པའམ་ གདོང་བསྐྱོད་འབད།"

#: ../gnomemeeting.schemas.in.in.h:10
msgid "Automatically switch to fullscreen for images displayed in the main GUI at the beginning of the call"
msgstr "འབོད་བརྡ་འགོ་འཛུགས་སྐབས་ ཇི་ཡུ་ཨའི་གཙོ་བོའི་ནང་ བཀྲམ་སྟོན་འབད་ནིའི་ གཟུགས་བརྙན་ཚུ་གི་དོན་ལུ་ རང་བཞིན་གྱིས་ གསལ་གཞི་གངམ་ལུ་སོར་བསྒྱུར་འབད།"

#: ../gnomemeeting.schemas.in.in.h:11
msgid "Change the control panel section"
msgstr "ཚད་འཛིན་པེ་ནཱལ་གྱི་དབྱེ་ཚན་ བསྒྱུར་བཅོས་འབད།"

#: ../gnomemeeting.schemas.in.in.h:12
msgid "Change the control panel section (4 means that the control panel is off)"
msgstr "ཚད་འཛིན་པེ་ནཱལ་གྱི་དབྱེ་ཚན་ བསྒྱུར་བཅོས་འབད། (༤ གི་དོན་དག་ ཚད་འཛིན་པེ་ནཱལ་དེ་ཨོཕ་ཨིན་)"

#: ../gnomemeeting.schemas.in.in.h:13
msgid "Choose the number of blocks (that have not changed) transmitted with each frame. These blocks fill in the background"
msgstr "གཞི་ཁྲམ་རེ་རེ་དང་གཅིག་ཁར་ སྤེལ་བཀྲམ་འབད་མི་ སྡེབ་ཚན་ཚུ་ གདམ་ཁ་རྐྱབས། (བསྒྱུར་བཅོས་མ་འགྱོ་མི་ཚུ) སྡེབ་ཚན་འདི་ཚུ་གིས་ རྒྱབ་གཞི་བཀངམ་ཨིན་།"

#: ../gnomemeeting.schemas.in.in.h:14
msgid "Clear inactive calls"
msgstr "ནུས་མེད་ཀྱི་ འབོད་བརྡ་ཚུ་བསལ་གཏང་།"

#: ../gnomemeeting.schemas.in.in.h:15
#: ../src/addressbook_window.cpp:974
msgid "Comment"
msgstr "བསམ་བཀོད།"

#: ../gnomemeeting.schemas.in.in.h:16
msgid "Connection will be established in Fast Start mode. Fast Start is a new way to start calls faster that was introduced in H.323v2. It is not supported by Netmeeting and using both Fast Start and H.245 Tunnelling can crash some versions of Netmeeting"
msgstr "མགྱོགས་པར་འགོ་བཙུགས་ཐབས་ལམ་ནང་ མཐུད་ལམ་གཞི་བཙུགས་འབད་ནི་ཨིན། མགྱོགས་པར་འགོ་བཙུགས་ཐབས་ལམ་དེ་ འབོད་བརྡ་ཚུ་ H.323v2 ནང་འགོ་བཙུགས་ཏེ་ཡོད་མི་དེ་ལས་ མགྱོགས་དྲགས་སྦེ་ འགོ་བཙུགས་ནིའི་ཐབས་ཤེས་གསརཔ་ཅིག་ཨིན། དེ་ལུ་ནེཊི་མི་ཊིང་གིས་ རྒྱབ་སྐྱོར་མེདཔ་ལས་ མགྱོགས་པར་འགོ་བཙུགས་དང་ H.245 ཊ་ན་ལིང་གཉིས་ཆ་རང་ ལག་ལེན་འཐབ་པ་ཅིན་ ནེཊི་མི་ཊིང་གི་་ ཐོན་རིམ་ལ་ལོ་ཅིག་ལུ་ བརྡབ་འཁྲུག་འོང་།"

#: ../gnomemeeting.schemas.in.in.h:17
msgid "Country code"
msgstr "རྒྱལ་ཁབ་ཀྱི་ཀོཌི།"

#: ../gnomemeeting.schemas.in.in.h:18
msgid "DTMF sending"
msgstr "ཌི་ཊི་ཨེམ་ཨེཕ་ གཏང་དོ།"

#: ../gnomemeeting.schemas.in.in.h:19
msgid "Display a popup window when receiving a call"
msgstr "འབོད་བརྡ་ཅིག་འབྱོར་བའི་སྐབས་ པོཔ་ཨཔ་སྒོ་སྒྲིག་ཅིག་ བཀྲམ་སྟོན་འབད།"

#: ../gnomemeeting.schemas.in.in.h:20
msgid "E-mail address"
msgstr "གློག་འཕྲིན་ཁ་བྱང་།"

#: ../gnomemeeting.schemas.in.in.h:21
msgid "Enable 'Fast Start'"
msgstr "མགྱོགས་པར་འགོ་བཙུགས་ ལྕོགས་ཅན་བཟོ།"

#: ../gnomemeeting.schemas.in.in.h:22
msgid "Enable H.245 tunnelling"
msgstr "H.245 ཊ་ན་ལིང་ ལྕོགས་ཅན་བཟོ།"

#: ../gnomemeeting.schemas.in.in.h:23
msgid "Enable IP checking"
msgstr "ཨའི་པི་ཞིབ་དཔྱད་ ལྕོགས་ཅན་བཟོ།"

#: ../gnomemeeting.schemas.in.in.h:24
msgid "Enable IP translation"
msgstr "ཨའི་པི་སྐད་བསྒྱུར་ ལྕོགས་ཅན་བཟོ།"

#: ../gnomemeeting.schemas.in.in.h:25
msgid "Enable STUN Support"
msgstr "ཨེསི་ཊི་ཡུ་ཨེན་ རྒྱབ་སྐྱོར་ལྕོགས་ཅན་བཟོ།"

#: ../gnomemeeting.schemas.in.in.h:26
msgid "Enable bilinear filtering on displayed video"
msgstr "བཀྲམ་སྟོན་འབད་མི་ ཝི་ཌིའོ་ལུ་ བའི་ལི་ནི་ཡར་ འཚག་ནི་ལྕོགས་ཅན་བཟོ།"

#: ../gnomemeeting.schemas.in.in.h:27
msgid "Enable early H.245"
msgstr "སྔ་ཆའི་H.245 ལྕོགས་ཅན་བཟོ།"

#: ../gnomemeeting.schemas.in.in.h:28
msgid "Enable or disable bilinear interpolation when rendering video images (this has no effect in fullscreen mode)"
msgstr "ཝི་ཌོའི་གཟུགས་བརྙན་ཚུ་ལྷག་སྟོན་འབད་བའི་སྐབས་ བའི་ལི་ནི་ཡར་བསྐྱར་བསྣོན་ ལྕོགས་ཅན་ནམ་ ལྕོགས་མིན་བཟོ། (འདི་གིས་གསལ་གཞི་གང་བའི་ཐབས་ལམ་ལུ་ནུས་པ་མེད་)"

#: ../gnomemeeting.schemas.in.in.h:29
msgid "Enable registering"
msgstr "ཐོ་འགོད་འབད་ནི་ ལྕོགས་ཅན་བཟོ།"

#: ../gnomemeeting.schemas.in.in.h:30
msgid "Enable silence detection"
msgstr "ཁུ་སིམ་སིམ་སྐྱོན་འཛིན་ ལྕོགས་ཅན་བཟོ།"

#: ../gnomemeeting.schemas.in.in.h:31
msgid "Enable video reception"
msgstr "ཝི་ཌིའོ་སྒྲ་འབྱོར་ ལྕོགས་ཅན་བཟོ།"

#: ../gnomemeeting.schemas.in.in.h:32
msgid "Enable video transmission"
msgstr "ཝི་ཌིའོ་སྤེལ་བཀྲམ་ ལྕོགས་ཅན་བཟོ།"

#: ../gnomemeeting.schemas.in.in.h:33
msgid "Enter a comment about yourself for the user directory"
msgstr "ལག་ལེན་པའི་སྣོད་ཐོའི་དོན་ལུ་ ཁྱོད་རའི་སྐོར་ལས་ བསམ་བཀོད་ཅིག་བཙུགས།"

#: ../gnomemeeting.schemas.in.in.h:34
msgid "Enter a valid URL to connect to a web application that outputs the public IP of your host"
msgstr "ཁྱོད་རའི་ཧོསིཊི་གི་ མི་མང་ཨའི་པི་བྱིན་མི་ ཝེབ་ཀྱི་གློག་རིམ་ཅིག་ལུ་ མཐུད་ནིའི་དོན་ལས་ ནུས་ཅན་གྱི་ཡུ་ཨར་ཨེལ་ཅིག་བཙུགས།"

#: ../gnomemeeting.schemas.in.in.h:35
msgid "Enter the public IP of your NAT/PAT router if you want to use IP translation. If you are registered to ils.seconix.com, GnomeMeeting will automatically fetch the public IP using the ILS service"
msgstr "ཁྱོད་ཀྱིས་ཨའི་པི་སྐད་བསྒྱུར་ ལག་ལེན་འཐབ་འདོད་ཡོད་པ་ཅིན་ ཁྱོད་རའི་ཨེན་ཨེ་ཊི་/པི་ཨེ་ཊི་ རོ་ཊར་གྱི་ མི་མང་ཨའི་པི་དེ་བཙུགས། ཁྱོད་རང་ ils.seconix.com ལུ་ཐོ་འགོད་འབད་དེ་ཡོད་པ་ཅིན་ ཇི་ནོམ་མི་ཊིང་གིས་ རང་བཞིན་གྱིས་ ཨའི་ཨེལ་ཨེསི་ཞབས་ཏོག་ ལག་ལེན་འཐབ་པའི་ཐོག་ལས་ མི་མང་གི་ཨའི་པི་ལེན་འོང་།"

#: ../gnomemeeting.schemas.in.in.h:36
#: ../src/pref_window.cpp:663
msgid "Enter your e-mail address"
msgstr "ཁྲོད་རའི་གློག་འཕྲིན་ཁ་བྱང་བཙུགས།"

#: ../gnomemeeting.schemas.in.in.h:37
#: ../src/pref_window.cpp:649
msgid "Enter your first name"
msgstr "ཁྱོད་རའི་འགོ་མིང་བཙུགས།"

#: ../gnomemeeting.schemas.in.in.h:38
msgid "Enter your last name"
msgstr "ཁྱོར་རའི་མཇུག་མིང་བཙུགས།"

#: ../gnomemeeting.schemas.in.in.h:39
msgid "Enter your location (country or city) for the user directory"
msgstr "ལག་ལེན་པའི་སྣོད་ཐོའི་དོན་ལུ་ ཁྱོད་རའི་ཉེ་གནས་བཙུགས། (རྒྱལ་ཁབ་བམ་ཁྲོམ་སྡེ་)"

#: ../gnomemeeting.schemas.in.in.h:40
msgid "First name"
msgstr "འགོ་མིང་།"

#: ../gnomemeeting.schemas.in.in.h:41
msgid "Forward calls to host"
msgstr "འབོད་བརྡ་ཚུ་ ཧོསིཊི་ལུ་ གདོང་བསྐྱོད་འབད།"

#: ../gnomemeeting.schemas.in.in.h:42
msgid "Forward calls to the given host if busy"
msgstr "ཁོམ་མེད་ཨིན་པ་ཅིན་ འབོད་བརྡ་ཚུ་ གྲ་བསྒྲིགས་ཧོསིཊི་ལུ་ གདོང་བསྐྱོད་འབད།"

#: ../gnomemeeting.schemas.in.in.h:43
