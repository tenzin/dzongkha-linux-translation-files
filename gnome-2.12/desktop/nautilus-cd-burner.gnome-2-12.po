#: ../nautilus-cd-burner.c:1805
msgid "It appears that the disc, when created, will contain only disc image files.  Do you want to continue and write them to the disc as files?"
msgstr "དེ་སྦེ་བལྟཝ་དང་ ཌིཀསི་དེ་གསར་བསྐྲུན་འབདཝ་ད་ དེ་གི་ནང་ན་ ཌིཀསི་གཟུགས་བརྙན་ཡིག་སྣོད་རྐྱངམ་གཅིག་ འོང་ནི་བཟུམ་ཅིག་འདུག ཁྱོད་ཀྱིས་དེ་ཚུ་འཕྲོ་མཐུད་དེ་ ཌིཀསི་གུ་གཟུགས་བརྙན་བཟུམ་ཅིག་སྦེ་ འབྲི་ནི་ཨིན་ན?"

#: ../nautilus-cd-burner.c:1808
msgid "Create disc containing only disc image files?"
msgstr "ཌིཀསི་གཟུགས་བརྙན་ཡིག་སྣོད་ཡོད་པའི་ ཌིཀསི་རྐྱངམ་གཅིག་ གསར་བསྐྲུན་འབད།"

#: ../nautilus-cd-burner.c:1812
msgid "Create With Files"
msgstr "བཅས་པའི་ཡིག་སྣོད་ཚུ་ གསར་བསྐྲུན་འབད།"

#: ../nautilus-cd-burner.c:1843
#, c-format
msgid "The file '%s' is unreadable.  Do you wish to skip this file and continue?"
msgstr "ཡིག་སྣོད་ '%s'  དེ་ལྷག་མི་བཏུབ་པས། ཁྱོད་ཀྱིས་ཡིག་སྣོད་འདི་ བཀོག་བཞག་ཞིནམ་ལས་ འཕྲོ་མཐུད་ནི་ཨིན་ན?"

#: ../nautilus-cd-burner.c:1846
msgid "Skip unreadable file?"
msgstr "ལྷག་མ་བཏུབ་པའི་ཡིག་སྣོད་ གོམ་འགྱོ་ནི་ཨིན་ན?"

#: ../nautilus-cd-burner.c:1851
msgid "Skip"
msgstr "གོམ་འགྱོ།"

#: ../nautilus-cd-burner.c:1851
msgid "Skip All"
msgstr "ཆ་མཉམ་གོམ་འགྱོ།"

#: ../nautilus-cd-burner.c:1875
msgid "No files selected"
msgstr "ཡིག་སྣོད་གཅིག་ཡང་ སེལ་འཐུ་མ་འབད་བས།"

#: ../nautilus-cd-burner.c:1876
msgid "You need to copy the files you want to write to disc to the CD/DVD Creator window."
msgstr "ཁྱོད་ཀྱིས་ཌིཀསི་ལུ་ འབྲི་དགོ་མི་ཡིག་སྣོད་ཚུ་ སི་ཌི་/ཌི་བི་ཌི་ གསར་བསྐྲུན་པའི་སྒོ་སྒྲིག་ལུ་ འདྲ་བཤུས་བརྐྱབ་དགོ"

#: ../nautilus-cd-burner.c:1879
msgid "Open CD/DVD Creator"
msgstr "སི་ཌི་/ཌི་བི་ཌི་ གསར་བསྐྲུན་པ་ཁ་ཕྱེ།"

#: ../nautilus-cd-burner.c:1916
#, c-format
msgid "The file '%s' is not a valid disc image."
msgstr "ཡིག་སྣོད་%s' དེ་ ནུས་ལྡན་སི་ཌི་གི་ གཟུགས་བརྙན་ཅིག་མིན་པས།"

#. FIXME: get type of media in drive
#: ../nautilus-cd-burner.c:1988
msgid "CD or DVD"
msgstr "སི་ཌི་ ཡང་ན་ ཌི་བི་ཌི།"

#: ../nautilus-cd-burner.c:2050
#, c-format
msgid "The specified device '%s' is not a valid CD/DVD drive."
msgstr "གསལ་བཀོད་འབད་མི་ཐབས་འཕྲུལ་%s' དེ་ ནུས་ལྡན་སི་ཌི་/ཌི་བི་ཌི་ འདྲེན་འཕྲུལ་ཅིག་མིན་པས།"

#: ../nautilus-cd-burner.c:2052
msgid "Unable to read specified location"
msgstr "གསལ་བཀོད་འབད་མི་ གནས་ཁོངས་ལྷག་མ་ཚུགས།"

#: ../nautilus-cd-burner.c:2064
msgid "No media available"
msgstr "བརྡ་ལམ་མི་འཐོབ་པས།"

#: ../nautilus-cd-burner.c:2065
msgid "There doesn't seem to be any media in the selected drive."
msgstr "སེལ་འཐུ་འབད་མི་ འདྲེན་འཕྲུལ་དེ་ནང་ བརྡ་ལམ་ག་ནི་ཡང་ མེདཔ་བཟུམ་ཅིག་འདུག"

#: ../nautilus-cd-burner.c:2169
msgid "Use CD/DVD device as source instead of burn:///"
msgstr "སི་ཌི་/ཌི་བི་ཌི་ཐབས་འཕྲུལ་དེ་ བཙུགས་སྤྱོད་ཚབ་ལུ་ འབྱུང་ཁུངས་སྦེ་ ལག་ལེན་འཐབ།///"

#: ../nautilus-cd-burner.c:2171
msgid "Use ISO image as source instead of burn:///"
msgstr "ཨའི་ཨེསི་ཨོ་གཟུགས་བརྙན་ བཙུགས་སྤྱོད་ཚབ་ལུ་ འབྱུང་ཁུངས་སྦེ་ ལག་ལེན་འཐབ།///"

#: ../nautilus-cd-burner.c:2173
msgid "Use CUE/TOC file as source instead of burn:///"
msgstr "སི་ཡུ་ཨི་/ཊི་ཨོ་སི་ཡིག་སྣོད་ བཙུགས་སྤྱོད་ཚབ་ལུ་ འབྱུང་ཁུངས་སྦེ་ ལག་ལེན་འཐབ།"

#: ../nautilus-cd-burner.c:2194
msgid "Too many parameters"
msgstr "ཚད་བཟུང་མང་དྲགས་པས།"

#: ../nautilus-cd-burner.c:2195
msgid "Too many parameters were passed to the application."
msgstr "གློག་རིམ་ལུ་ ཚད་བཟུང་གཏང་ནི་ མང་དྲགས་ནུག"

#: ../ncb.schemas.in.h:1
msgid "Default burn speed"
msgstr "བཙུགས་སྤྱོད་མགྱོགས་ཚད་ སྔོན་སྒྲིག་འབད།"

#: ../ncb.schemas.in.h:2
msgid "Default speed at which the media will be written."
msgstr "བརྡ་ལམ་འབྲི་ནིའི་མགྱོགས་ཚད་གུ་ སྔོན་སྒྲིག་འབད།"

#: ../ncb.schemas.in.h:3
msgid "Directory for the temporary ISO images"
msgstr "འཕྲལ་སྐབས་ཀྱི་ ཨའི་ཨེསི་ཨོ་ གཟུགས་བརྙན་ཚུ་གི་དོན་ལུ་སྣོད་ཐོ།"

#: ../ncb.schemas.in.h:4
msgid "Directory used for the temporary ISO images, leave blank to use the default system temporary directory."
msgstr "འཕྲལ་སྐབས་ཀྱི་ ཨའི་ཨེསི་ཨོ་ གཟུགས་བརྙན་ཚུ་གི་དོན་ལུ་ ལག་ལེན་འཐབ་མི་ ཁ་བྱང་ཐོ་ཡིག འཕྲལ་སྐབས་ཀྱི་སྣོད་ཐོ་ སྔོན་སྒྲིག་རིམ་ལུགས་ཀྱི་དོན་ལུ་ ལག་ལེན་འཐབ་ནིའི་ བར་སྟོང་བཞག"

#: ../ncb.schemas.in.h:5
msgid "Enable Burnproof option"
msgstr "བཙུགས་སྤྱོད་ཉེན་སྲུང་གི་གདམ་ཁ་ ལྕོགས་ཅན་བཟོ།"

#: ../ncb.schemas.in.h:6
msgid "Enable debug"
msgstr "རྐྱེན་སེལ་ལྕོགས་ཅན་བཟོ།"

#: ../ncb.schemas.in.h:7
msgid "Enable overburn"
msgstr "ཚད་ལས་བརྒལ་བའི་བཙུགས་སྤྱོད་ ལྕོགས་ཅན་བཟོ།"

#: ../ncb.schemas.in.h:8
msgid "Whether to allow the data to exceed the size of the physical medium."
msgstr "གནད་སྡུད་དེ་ དངོས་སུ་ཡོད་པའི་བརྡ་ལམ་གྱི་ཚད་ལས་ ལྷག་བཅུག་ནི་ཨིན་ན་མེན།"

#: ../ncb.schemas.in.h:9
msgid "Whether to enable the debugging for CD Recording."
msgstr "སི་ཌི་སྒྲ་བཟུང་གི་དོན་ལུ་ རྐྱེན་སེལ་ལྕོགས་ཅན་ བཟོ་ནི་ཨིན་ན་མེན།"

#: ../ncb.schemas.in.h:10
msgid "Whether to use the Burnproof option available on some recorders."
msgstr "བཟུང་འཕྲུལ་གཞན་ཚུ་ལུ་ཡོད་མི་ བཙུགས་སྤྱོད་ཉེན་སྲུང་ ལག་ལེན་འཐབ་ནི་ཨིན་ན་མེན།"

