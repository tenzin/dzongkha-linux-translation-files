#: ../gnome-jabber.desktop.in.h:1
#: ../src/gnome-jabber.glade.h:132
msgid "Gnome Jabber"
msgstr "ཇི་ནོམ་ཇབ་བར་"

#: ../gnome-jabber.desktop.in.h:2
msgid "Instant Messenger"
msgstr "འཕྲལ་མྱུར་འཕྲིན་དོན་"

#: ../gnome-jabber.schemas.in.h:1
msgid "Agents are the services which provide MSN, ICQ, etc..."
msgstr "ལས་ཚབ་ཚུ་ ཨེམ་ཨེསི་ཨེན་ ཨའི་སི་ཀིའུ་ལ་སོགས་པ་བྱིན་མི་ཞབས་ཏོག་ཚུ་ཨིན་"

#: ../gnome-jabber.schemas.in.h:2
msgid "Automatically Away status message."
msgstr "རང་བཞིན་ ཕྱི་ཁའི་གནས་ཚད་འཕྲིན་དོན།"

#: ../gnome-jabber.schemas.in.h:3
msgid "Automatically Extended Away status message."
msgstr "རང་བཞིན་རྒྱ་བསྐྱེད་ཅན་གྱི་ཕྱིའི་གནས་ཚད་འཕྲིན་དོན།"

#: ../gnome-jabber.schemas.in.h:4
msgid "Automatically connect when the application starts."
msgstr "གློག་རིམ་འགོ་བཙུགསཔ་ད་ རང་བཞིན་གྱིས་མཐུདཔ་ཨིན།"

#: ../gnome-jabber.schemas.in.h:5
msgid "Automatically re-connect when disconnected from the server."
msgstr "སར་བར་ལས་མཐུད་བཏོག་ཡོད་པའི་སྐབས་ རང་བཞིན་གྱིས་སླར་མཐུད་འབདཝ་ཨིན། "

#: ../gnome-jabber.schemas.in.h:6
msgid "Automatically set presence to away after 'n' minutes."
msgstr "'n'དཀར་ཆའི་ཤུལ་མ་ རང་བཞིན་གྱིས་ངོ་འཛོམས་འདི་ཕྱི་ཁ་སྦེ་གཞི་སྒྲིག་འབདཝ་ཨིན།"

#: ../gnome-jabber.schemas.in.h:7
msgid "Automatically set presence to extended away after 'n' minutes."
msgstr "'n'དཀར་ཆའི་ཤུལ་མ་ ངོ་འཛོམས་འདི་རྒྱ་བསྐྱེད་ཅན་གྱི་ཕྱི་ཁ་སྦེ་གཞི་སྒྲིག་འབདཝ་ཨིན།"

#: ../gnome-jabber.schemas.in.h:8
msgid "Do not show additional presence information on the roster."
msgstr "ལས་རིམ་རེའུ་མིག་གུ་ ཁ་སྐོང་ཅན་གྱི་ངོ་འཛོམས་བརྡ་དོན་མ་སྟོན།"

#: ../gnome-jabber.schemas.in.h:9
msgid "Do not show agents on the roster."
msgstr "ལས་རིམ་རེའུ་མིག་གུ་ ལས་ཚབ་མ་སྟོན།"

#: ../gnome-jabber.schemas.in.h:10
msgid "Do not show offline contacts on the roster."
msgstr "ལས་རིམ་རེའུ་མིག་གུ་ ཨཱོཕ་ལ་ཡིན་གྱི་འབྲེལ་ས་མ་སྟོན།"

#: ../gnome-jabber.schemas.in.h:11
msgid "Do not show the offline contacts group on the roster."
msgstr "ལས་རིམ་རེའུ་མིག་གུ་ ཨཱོཕ་ལ་ཡིན་འབྲེལ་སའི་སྡེ་ཚན་མ་སྟོན།"

#: ../gnome-jabber.schemas.in.h:12
msgid "For example, if your account is joebloggs@jabber.org, the username would be joebloggs."
msgstr "དཔེར་ན་ ཁྱོད་ཀྱི་རྩིས་ཐོ་འདི་ joebloggs@jabber.org ཨིན་པ་ཅིན་ ལག་ལེན་པའི་མིང་འདི་ ཇོ་ཡེ་བུ་ལོགསི་ཨིན།"

#: ../gnome-jabber.schemas.in.h:13
msgid "Hide the timestamp in the chat window."
msgstr "ཁ་སླབ་སྒོ་སྒྲིག་ནང་ དུས་བཀོད་སྦ།"

#: ../gnome-jabber.schemas.in.h:14
msgid "If not set, you can auto connect on start up and re-connect on disconnections. If set, you will be prompted for a password when connecting."
msgstr "གཞི་སྒྲིག་མ་འབད་བ་ཡོད་པ་ཅིན་ འགོ་བཙུག་གུ་རང་བཞིན་གྱིས་མཐུད་བཏུབ་དང་མཐུད་བཏོག་གུ་ སླར་མཐུད་འབད་བཏུབ་ཨིན། གཞི་སྒྲིག་འབད་དེ་ཡོད་པ་ཅིན་ མཐུད་པའི་སྐབས་ལུ་ ཆོག་ཡིག་བཙུག་དགོ་པའི་ནུས་པ་སྤེལ་འོང་།"

#: ../gnome-jabber.schemas.in.h:15
msgid "If this is unchecked, then each users' presence status is shown under their name."
msgstr "ཞིབ་དཔྱད་འབད་དེ་མེད་པ་ཅིན་ ལག་ལེན་པ་རེ་རེའི་ངོ་འཛོམས་གནས་ཚད་འདི་ ཁོང་རའི་མིང་གི་འོག་ལུ་ སྟོནམ་ཨིན།"

#: ../gnome-jabber.schemas.in.h:16
msgid "If you use SHA1 (Secure Hashing Algorithm), then the password is not obvious to people if they read the data stream."
msgstr "ཨེས་ཨེཆ་ཨའི་༡ (མཐའ་བཙན་ཨང་རྟགས་ཨཱལ་གོ་རི་དམ་)ལག་ལེན་འཐབ་པ་ཅིན་ དོརུང་གནད་སྡུད་རྒྱུན་རིམ་དེ་ལྷག་པ་ཅིན་ ཆོག་ཡིག་འདི་མི་གིས་མི་མཐོང་།"

#: ../gnome-jabber.schemas.in.h:17
msgid "Importance for message delivery."
msgstr "འཕྲིན་དོན་འགྲེམ་སྤེལ་གྱི་དོན་ལུ་ དགོས་མཁོ་ཡོདཔ་ཨིན།"

#: ../gnome-jabber.schemas.in.h:18
msgid "N/A Yet."
msgstr "ད་ཚུན་ ཨེན་/ཨེ་"

#: ../gnome-jabber.schemas.in.h:19
msgid "Password to use for authentication."
msgstr "བདེན་བཤད་ལགལེན་འཐབ་ནི་གི་དོན་ལུ་ཆོག་ཡིག"

#: ../gnome-jabber.schemas.in.h:20
msgid "Port to connect to."
msgstr "མཐུད་ནིའི་འདྲེན་ལམ།"

#: ../gnome-jabber.schemas.in.h:21
msgid "Priorities are only relevant when you use resources. If you have two active resources, and someone sends a message to you, the resource with the lowest priority number will be delivered the message."
msgstr "འབྱུང་ཁུངས་ཚུ་ལག་ལེན་འཐབ་པ་ཅིན་ གཙོ་རིམ་ཚུ་གཞི་བསྟུན་རྐྱངམ་ཅིག་ཨིན། ཁྱོད་ལུ་ཤུགས་ལྡན་འབྱུང་ཁུངས་གཉིས་ཡོད་པའི་ཁར་ གཞན་ཅིག་གིས་འཕྲིན་དོན་ཅིག་གཏང་པ་ཅིན་ གཙོ་རིམ་ཨང་དམའ་ཤོས་ཀྱི་འབྱུང་ཁུངས་ཀྱིས་ འཕྲིན་དོན་འགྲེམ་སྤེལ་འབད་འོང་།"

#: ../gnome-jabber.schemas.in.h:22
msgid "Prompt for the password."
msgstr "ཆོག་ཡིག་གི་དོན་ལུ་ ནུས་པ་སྤེལ།"

#: ../gnome-jabber.schemas.in.h:23
msgid "Raises the chat window when messages arrive."
msgstr "འཕྲིན་དོན་ལྷོད་པའི་ཚེ་ ཁ་སླབ་སྒོ་སྒྲིག་འདི་ཆེར་བསྐྱེད་འབདཝ་ཨིན། "

#: ../gnome-jabber.schemas.in.h:24
msgid "Resource to identify this connection."
msgstr "མཐུད་ལམ་ངོས་འཛིན་འབད་ནིའི་འབྱུང་ཁུངས།"

#: ../gnome-jabber.schemas.in.h:25
msgid "Resources are really handy if you log on to the same account with DIFFERENT resources (note: they can not be the same or the originating account is forced to log out and is disconnected). You can use these resources to identify different locations (for example, home and work)."
msgstr "འབྱུང་ཁུངས་ཚུ་ འབྱུང་ཁུངས་སོ་སོར་དང་བཅས་རྩིས་ཐོ་ཅོག་ཐདཔ་གུ་ནང་བསྐྱོད་འབ་བ་ཅིན་ སྟབས་བདེ་ཏོག་ཏོག་ཨིན་(དྲན་དགོཔ་: དེ་ཚུ་སོར་སོ་ཨིན་ ཡངན་ འགོ་ཐོག་འབྱུང་སའི་རྩིས་ཐོ་འདི་ སྦང་ཅན་སྦེ་ཕྱིར་བསྐྱོད་དང་མཐུད་བཏོགཔ་ཨིན)། འབྱུང་་ཁུངས་ཚུ་ གནས་ཁོངས་མ་འདྲཝ་ཚུ་ངོས་འཛིན་འབད་ནི་གི་དོན་ལས་ དེ་ལག་ལེན་འཐབ་བཏུབ་ཨིན་(དཔེར་ན་ ཁྱིམ་དང་ལཱ་)།"

#: ../gnome-jabber.schemas.in.h:26
msgid "Reversing the text direction means the newest message appears at the top."
msgstr "་"

#: ../gnome-jabber.schemas.in.h:27
msgid "Server to connect to."
msgstr "མཐུད་ནིའི་སར་བར།"

#: ../gnome-jabber.schemas.in.h:28
msgid "The default port for Jabber is 5222, and 5223 for SSL connections."
msgstr "ཇབ་བར་གྱི་དོན་ལུ་ སྔོན་སྒྲིག་འདྲེན་ལམ་འདི་ ༥༢༢༣ དང་ ཨེསི་ཨེསི་ཨེལ་མཐུད་ལམ་གྱི་དོན་ལུ་ ༥༢༢༣ ཨིན། "

#: ../gnome-jabber.schemas.in.h:29
msgid "The path where history files can be found."
msgstr "ལོ་རྒྱུས་ཡིག་སྣོད་ཐོབ་ཚུགས་པའི་འགྲུལ་ལམ།"

#: ../gnome-jabber.schemas.in.h:30
msgid "The path where the log can be found."
msgstr "དྲན་དེབ་ཐོབ་ཚུགས་པའི་འགྲུལ་ལམ།"

#: ../gnome-jabber.schemas.in.h:31
msgid "This is even MORE secure than SHA1. All transmissions are encrypted."
msgstr "འ་ནི་འདི་ ཨེསི་ཨེཆ་ཨེ་༡ ལས་ལྷག་སྟེ་མཐའ་བཙན་དང་ལྡནམ་ཨིན། སྤེལ་བཀྲམ་ཚུ་ཆ་མཉམ་ གསང་བཟོ་འབདཡོདཔ།"

#: ../gnome-jabber.schemas.in.h:32
msgid "This is usually the host name of the server your Jabber account is associated with. For example, if your account is joebloggs@jabber.org, the server would be jabber.org."
msgstr "འ་ནི་འདི་ ཁྱོད་རའི་ཇབ་བར་རྩིས་ཐོ་དང་འབྲེལ་བ་ཡོད་པའི་སར་བར་གྱི་ཧོསཊི་མིང་ཨིན། དཔེར་ན་ ཁྱོད་ཀྱི་རྩིས་ཐོ་འདི་ joebloggs@jabber.org ཨིན་པ་ཅིན་ སར་བར་འདི་ jabber.org ཨིན་དགོ"

#: ../gnome-jabber.schemas.in.h:33
msgid "Use automatic away notifications."
msgstr "རང་བཞིན་ཕྱི་ཁའི་བརྡ་བསྐུལ་ ལག་ལེན་འཐབ།"

#: ../gnome-jabber.schemas.in.h:34
msgid "Use chat or messages to be the double click action."
msgstr "ཨེབ་གཏང་གཉིས་ལྡན་གྱི་བྱ་བའི་དོན་ལུ་ ཁ་སླབ་ ཡང་ན་ འཕྲིན་དོན་ལག་ལེན་འཐབ།"

#: ../gnome-jabber.schemas.in.h:35
msgid "Use smiley icons where text matches. E.g. :)"
msgstr "ཚིག་ཡིག་མཐུན་སྒྲིག་འབདཝ་ཨིན་པའི་སི་མའི་ལི་ ལག་ལེན་འཐབ། དཔེར་ན་ :)"

#: ../gnome-jabber.schemas.in.h:36
msgid "Username to authenticate with."
msgstr "བདེན་པ་བཤད་ནིའི་དོན་ལུ་ ལག་ལེན་པའི་མིང་།"

#: ../gnome-jabber.schemas.in.h:37
msgid "Uses SSL (encryption)."
msgstr "ཨེསི་ཨེསི་ཨེལ་ ལག་ལེན་འཐབ་ཨིན་(གསང་བཟོ་)།"

#: ../gnome-jabber.schemas.in.h:38
msgid "Uses readable text instead of SHA1."
msgstr "ཨེསི་ཨེཅ་ཨེ་༡ གི་ཚབ་ལུ་ ལྷག་བཏུབ་པའི་ཚིག་ཡིག་ལག་ལེན་འཐབ་ཨིན།"

#: ../gnome-jabber.schemas.in.h:39
msgid "When a contact goes offline, they are moved into an OFFLINE group. This option allows you to hide that group."
