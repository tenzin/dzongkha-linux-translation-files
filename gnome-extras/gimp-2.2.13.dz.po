#: ../app/main.c:501
msgid "GIMP version"
msgstr "ཇི་ཨའི་ཨེམ་པི་ ཐོན་རིམ"

#: ../app/main.c:509
#, c-format
msgid ""
"\n"
"Usage: %s [option ... ] [file ... ]\n"
"\n"
msgstr ""
"\n"
"བེད་སྤྱོད: %s [དགའ་གདམ ... ] [ཡིག་སྣོད ... ]\n"
"\n"

#: ../app/main.c:511
msgid "Options:\n"
msgstr "འདམ་ཁ:\n"

#: ../app/main.c:512
msgid "  -h, --help               Output this help.\n"
msgstr "  -h, --གྲོགས་རམ               ཨའུཊི་པུཊི་གྲོགས་རམ་འདི.\n"

#: ../app/main.c:513
msgid "  -v, --version            Output version information.\n"
msgstr "  -v, --ཐོན་རིམ          ཐོན་རིམ་ཨའུཊི་པུཊི་བརྡ་དོན.\n"

#: ../app/main.c:514
msgid "  --verbose                Show startup messages.\n"
msgstr "  --ཚིག་མང                འགོ་བཙུགས་འཕྲིན་དོན་སྟོན.\n"

#: ../app/main.c:515
msgid "  --no-shm                 Do not use shared memory between GIMP and plugins.\n"
msgstr " --no-shm ཇི་ཨའི་ཨེམ་པི་དང་ པ་ལག་ཨིནསི་ གྱི་བར་ན་ རུབ་སྤྱོད་ཀྱི་དྲན་ཚད་ལག་ལེན་མ་འཐབ།\n"

#: ../app/main.c:516
msgid "  --no-cpu-accel           Do not use special CPU accelerations.\n"
msgstr "  --no-cpu-accel    དམིགས་བསལ་གྱི་་་སི་པི་ཡུ་་་མྱུར་བསྐྱོད་ལས་འགན་ ལག་ལེན་མ་འཐབ།\n"

#: ../app/main.c:517
msgid "  -d, --no-data            Do not load brushes, gradients, palettes, patterns.\n"
msgstr "-d, --no-data དྭངས་བཟོ་་ སྟེགས་རིས་་་ དཔེ་གཞི་་ཚུ་ མངོན་གསལ་འབད་མི་གོ\n"

#: ../app/main.c:518
msgid "  -f, --no-fonts           Do not load any fonts.\n"
msgstr "  -f, --མེད-ཡིག་ཚུགས་ཚུ           ཡིག་ཚུགས་གང་རུང་མངོན་གསལ་མ་འབད.\n"

#: ../app/main.c:519
msgid "  -i, --no-interface       Run without a user interface.\n"
msgstr " -i, --no-ངོས་འདྲ་བའི   ངོས་འདྲ་བའི་ལག་ལེན་པ་མེདཔར་ གཡོག་བཀོལ།\n"

#: ../app/main.c:520
msgid "  --display <display>      Use the designated X display.\n"
msgstr "  --བཀྲམ་སྟོན <display>     གོ་གནས་བཀོད་ཡོད་པའིX བཀྲམ་སྟོན་ལག་ལེན་འཐབ.\n"

#: ../app/main.c:521
msgid "  -s, --no-splash          Do not show the startup window.\n"
msgstr "-s, --no-splash  འགོ་བཙུགས་ཀྱི་སྒོ་སྒྲིག་ སྟོན་མི་གོ།\n"

#: ../app/main.c:522
msgid "  --session <name>         Use an alternate sessionrc file.\n"
msgstr "--ཡུན་ལཱ <name> ཐབས་གཞན་ལཱ་ཡུན་་ཨར་སི་ གི་་ཡིག་སྣོད་ལག་ལེན་འཐབ།\n"

#: ../app/main.c:523
msgid "  -g, --gimprc <gimprc>    Use an alternate gimprc file.\n"
msgstr " -g, --ཇི་ཨའི་ཨེམ་པི་ཨཱར་སི <gimprc>  ཐབས་གཞན་ལག་ལེན་པ་་ ཇི་ཨའི་ཨེམ་པི་ཨཱར་སི་ཡིག་སྣོད་དེ་ ལག་ལེན་འཐབ།\n"

#: ../app/main.c:524
msgid "  --system-gimprc <gimprc> Use an alternate system gimprc file.\n"
msgstr "--རིམ་ལུགས-ཇི་ཨའི་ཨེམ་པི་ཨཱར་སི <gimprc>  ཐབས་གཞན་རིམ་ལུགས་ ཇི་ཨཡི་ཨེམ་པི་ཨཱར་སི་ དེ་ལག་ལེན་འཐབ།\n"

#: ../app/main.c:525
msgid "  --dump-gimprc            Output a gimprc file with default settings.\n"
msgstr "--བཀོག་བཞག་-ཇིམ་ཨའི་ཨེམ་པི་ཨཱར་་སི     ཇི་ཨའི་ཨེམ་པི་ཨཱར་སི་ ཡིག་སྣོད་་དེ་ སྔོན་སྒྲིག་་སྒྲིག་སྟངས་དང་ ཅིག་ཁར་ ཨའུཊི་པུཊི་འབད།\n"

#: ../app/main.c:526
msgid "  -c, --console-messages   Display warnings to console instead of a dialog box.\n"
msgstr "-c, --མ་སྒྲོམ-འཕྲིན་དོན ཌའི་ལོག་ལག་ལེན་ མ་འཐབ་པར་དེ་གི་ཚབ་ལུ་ འཕྲིན་དོན་ཚུ་མ་སྒྲོམ་ནང་བཏང་།\n"

#: ../app/main.c:527
msgid "  --debug-handlers         Enable non-fatal debugging signal handlers.\n"
msgstr "--རྐྱེན་སེལ-ལེགས་སྐྱོང་པ་ཚུ  ཚབས་ཆེན་མ་ཡིན་པའི་ རྐྱེན་སེལ་བརྡ་རྟགས་ ལེགས་སྐྱོང་པ་་ལྕོགས་ཅན་བཟོ།\n"

#: ../app/main.c:528
msgid ""
"  --stack-trace-mode <never | query | always>\n"
"                           Debugging mode for fatal signals.\n"
msgstr ""
"  --བརྩེགས་བཤུལ་ཐབས་ལམ<never | query | always>\n"
"                          འཚུབ་ཆེན་བརྡ་རྟགས་ཀྱི་དོན་ལས་རྐྱེན་སེལ་འབད་བའི་ཐབས་ལམ\n"

#: ../app/main.c:530
msgid ""
"  --pdb-compat-mode <off | on | warn>\n"
"                           Procedural Database compatibility mode.\n"
msgstr ""
"  --pdb-མཐུན-ཐབས་ལམ <off | on | warn>\n"
"                           བྱ་སྒོའི་ལམ་ལུགས་གནད་སྡུད་གཞི་རྟེན་མཐུན་འགྱུར་ཐབས་ལམ.\n"

#: ../app/main.c:532
msgid ""
"  --batch-interpreter <procedure>\n"
"                           The procedure to process batch commands with.\n"
msgstr ""
"  --རུ་ཚན-ཁ་སྒྱུར་མི <procedure>\n"
"                          རུ་ཚན་བརྡ་བཀོད་ལས་སྦྱོར་དང་ཅིག་ཁར་གྱི་ བྱ་སྒོའི་ལམ་ལུགས.\n"

#: ../app/main.c:534
msgid "  -b, --batch <commands>   Process commands in batch mode.\n"
msgstr "  -b, --རུ་ཚན <commands>   རུ་ཚན་ཐབས་ལམ་ནང་ལས་སྦྱོར་བརྡ་བཀོད.\n"

#: ../app/sanity.c:194
#, c-format
msgid ""
"The configured filename encoding cannot be converted to UTF-8: %s\n"
"\n"
"Please check the value of the environment variable G_FILENAME_ENCODING."
msgstr ""
"རིམ་སྒྲིག་་འབད་ཚར་མི་ཡིག་སྣོད་ཀྱི་མིང་ཨིན་ཀོ་ཌིང་དེ་ ཡུ་ཊི་ཨེཕི་-༨:%s\n"
"\n"
"ལུ་ངཞི་བསྒྱུར་འབད་མི་བཏུབ།མཐའ་འཁོར་འགྱུར་ཅན་གྱི་གནས་གོང་དེ་ཞིབ་དཔྱད་འབད་གནང་ ཇི_ཡིག་སྣོད་_ཨིན་ཀོ་ཌིང་།"

#: ../app/sanity.c:213
#, c-format
msgid ""
"The name of the directory holding the GIMP user configuration cannot be converted to UTF-8: %s\n"
"\n"
"Most probably your filesystem stores files in an encoding different from UTF-8 and you didn't tell GLib about this. Please set the environment variable G_FILENAME_ENCODING."
msgstr ""
"ཇི་ཨའི་ཨེམ་པི་ ལག་ལེན་འཐབ་པ རིམ་སྒྲིག་འབད་མི་གི་ མིང་འཆང་མི་ སྣོད་ཐོ་དེ་ ཡུ་ཊི་ཨེཕི་-8: %s\n"
"\n"
"ལུ་གཞི་བསྒྱུར་འབད་མི་བཏུབ ཁྱོད་ཀྱི་ཡིག་སྣོད་རིམ་ལུགས་ནང་ལུ་ ཡུ་ཊི་ཨེཕི་-8 མ་ཡིན་པའི་ ཨེན་ཀོ་ཌིང་ཡིག་སྣོད་ཚུ་ གསོག་འཇོག་འབད་དེ་ཡོདཔ་བཟུམ་ཅིག་འདུག འདི་མ་ཚད་ཁྱོད་ཀྱི་དེ་གི་སྐོར་ལས་ ཇི་ལིབ་ལུ་ མ་སླབ་པས།མཐའ་འཁོར་འགྱུར་ཅན་གཞི་སྒྲིག་འབད་གནང་། ཇི་_ཡིག་སྣོད་མིང་_ཨེན་ཀོ་ཌིང་།"

