#: ../gperfmeter.desktop.in.h:1
#: ../src/perfmeter.c:739
msgid "Monitor disk, memory, CPU and network usage"
msgstr "གསལ་འཕྲུལ་ཌིཀསི་ དྲན་ཚད་ སི་པི་ཡུ་དང་ཡོངས་འབྲེལ་ལག་ལེན་"

#: ../gperfmeter.desktop.in.h:2
#: ../src/perfmeter.c:2550
msgid "Performance Meter"
msgstr "ལཱ་ཤུགས་མི་ཊར་"

#: ../gperfmeter.schemas.in.h:1
msgid "Autoload host resources when changed"
msgstr "སོར་ཏེ་ཡོད་པའི་སྐབས་ལུ་ ཧོསཊི་འབྱུང་ཁུངས་འདི་ རང་བཞིན་མངོན་གསལ་འབད་"

#: ../gperfmeter.schemas.in.h:2
msgid "Automatically load host specific resources when the monitor host is changed."
msgstr "གསལ་འཕྲུལ་ཧོསཊི་འདི་སོར་ཡོད་པའི་སྐབས་ལུ་ ཧོསཊི་འབྱུང་ཁུངས་ཚུ་རང་བཞིན་གྱིས་ མངོན་གསལ་འབདཝ་ཨིན།"

#: ../gperfmeter.schemas.in.h:3
msgid "Current value of max threshold"
msgstr "ཐེརེཤ་ཧཱོལ་མང་མཐའི་བེ་ལུ་"

#: ../gperfmeter.schemas.in.h:4
msgid "Display action bar"
msgstr "བྱ་བའི་ཕྲ་རིང་བཀྲམ་སྟོན་འབད་"

#: ../gperfmeter.schemas.in.h:5
msgid "Display menubar"
msgstr "དཀར་ཆག་ཕྲ་རིང་ བཀྲམ་སྟོན་འབད་"

#: ../gperfmeter.schemas.in.h:6
msgid "Display strip chart"
msgstr "བཤུབ་ཀྱི་དཔེ་རིས་ བཀྲམ་སྟོན་འབད་"

#: ../gperfmeter.schemas.in.h:7
msgid "Display threshold line"
msgstr "ཐེརེཤ་ཧཱོལ་གྲལ་ཐིག་ བཀྲམ་སྟོན་འབད་"

#: ../gperfmeter.schemas.in.h:8
msgid "Enable log"
msgstr "དྲན་དེབ་ ལྕོགས་ཅན་བཟོ་"

#: ../gperfmeter.schemas.in.h:9
msgid "Graph type"
msgstr "ཚད་ཁྲམ་དབྱེ་བ་"

#: ../gperfmeter.schemas.in.h:10
msgid "If false, try to migrate previous CDE settings to GConf."
msgstr "བརྫུན་པ་ཅིན་ ཧེ་མའི་སི་ཌི་ཨི་སྒྲིག་སྟངས་ཚུ་ ཇི་ཀཱོནཕ་ལུ་སྤོ་བཤུད་འབད།"

#: ../gperfmeter.schemas.in.h:11
msgid "If true, display a moving rectangle called Action Bar."
msgstr "བདེན་པ་ཅིན་ བྱ་བའི་ཕྲ་རིང་ཟེར་མི་བསྒུལ་ཅན་གྲུ་གསུམ་ཅིག་ བཀྲམ་སྟོན་འབད་"

#: ../gperfmeter.schemas.in.h:12
msgid "If true, display the menubar."
msgstr "བདེན་པ་ཅིན་ དཀར་ཆག་ཕྲ་རིང་ བཀྲམ་སྟོན་འབད་"

#: ../gperfmeter.schemas.in.h:13
msgid "If true, display the threshold line in each graph."
msgstr "བདེན་པ་ཅིན་ ཚད་ཁྲམ་རེ་ནང་ ཐེརེཤ་ཧཱོལ་བཀྲམ་སྟོན་འབད་"

#: ../gperfmeter.schemas.in.h:14
msgid "If true, load the resources by hostname."
msgstr "བདེན་པ་ཅིན་ ཧོསཊི་མིང་གི་ཐོག་ལས་ འབྱུང་ཁུངས་ཚུ་མངོན་གསལ་འབད་"

#: ../gperfmeter.schemas.in.h:15
msgid "If true, look at the HostName key to load the appropriate value. Otherwise perfmeter monitors the local host."
msgstr "བདེན་པ་ཅིན་ འོས་ལྡན་བེ་ལུ་ཅིག་མངོན་གསལ་འབད་ནི་གི་དོན་ལུ་ ཧོསཊི་མིང་གི་ལྡེ་མིང་ལུ་བལྟ། དེ་མེན་པ་ཅིན་ པརཕི་མི་ཊར་གྱིས་ ཉེ་གནས་ཧོསཊི་བལྟ་རྟོག་འབདཝ་ཨིན། "

#: ../gperfmeter.schemas.in.h:16
msgid "If true, save the resources by hostname."
msgstr "བདེན་པ་ཅིན་ ཧོསཊི་མིང་གི་ཐོག་ལས་ འབྱུང་ཁུངས་ཚུ་སྲུངས།"

#: ../gperfmeter.schemas.in.h:17
msgid "If true, the application will continue to sample when iconized."
msgstr "བདེན་པ་ཅིན་ ངོས་པར་བཟོ་ཡོད་པའི་སྐབས་ལུ་ གློག་རིམ་འདི་དཔེ་ཚད་ལུ་འཕྲོ་མཐུད་འོང་།"

#: ../gperfmeter.schemas.in.h:18
msgid "If true, turn on logging to the file specified in LogFilename."
msgstr "བདེན་པ་ཅིན་ དྲན་དེབ་ཡིག་སྣོད་ནང་གསལ་བཀོད་འབད་ཡོད་མི་ཡིག་སྣོད་ལུ་ དྲན་དེབ་བཀོད།"

#: ../gperfmeter.schemas.in.h:19
msgid "Limit color index"
msgstr "ཚོས་གཞིའི་ཟུར་ཐོའི་ཚད་"

#: ../gperfmeter.schemas.in.h:20
msgid "Load resources by hostname"
msgstr "ཧོསཊི་མིང་གི་ཐོག་ལས་ འབྱུང་ཁུངས་ཚུ་མངོན་གསལ་འབད་"

#: ../gperfmeter.schemas.in.h:21
msgid "Log page length"
msgstr "དྲན་དེབ་ཤོག་ལེབ་རིང་ཚད་ "

#: ../gperfmeter.schemas.in.h:22
msgid "Logfile"
msgstr "དྲན་དེབ་ཡིག་སྣོད་"

#: ../gperfmeter.schemas.in.h:23
msgid "Maximum limit"
msgstr "ཚད་མང་མཐའ་"

#: ../gperfmeter.schemas.in.h:24
msgid "Maximum value of max threshold"
msgstr "ཐེརེཤ་ཧཱོལ་མང་མཐའི་བེ་ལུ་མང་མཐའ་"

#: ../gperfmeter.schemas.in.h:25
msgid "Meter color index"
msgstr "མི་ཊར་ཚོས་གཞིའི་ཟུར་ཐོ་"

#: ../gperfmeter.schemas.in.h:26
msgid "Meters to display"
msgstr "བཀྲམ་སྟོན་འབད་ནིའི་ མི་ཊར་ཚུ་"

#: ../gperfmeter.schemas.in.h:27
msgid "Migrate CDE settings"
msgstr "སི་ཌི་ཨི་སྒྲིག་སྟངས་ཚུ་ སྤོ་བཤུད་འབད་"

#: ../gperfmeter.schemas.in.h:28
msgid "Minimum value of max threshold"
msgstr "ཐེརེཤ་ཧཱོལ་མང་མཐའི་བེ་ལུ་ཉུང་མཐའ་"

#: ../gperfmeter.schemas.in.h:29
msgid "Monitor remote host"
msgstr "གསལ་འཕྲུལ་ཐག་རིང་ཧོསཊི་"

#: ../gperfmeter.schemas.in.h:30
msgid "Orientation of meters"
msgstr "མི་ཊར་ཚུ་གི་ཁ་ཕྱོགས་"

#: ../gperfmeter.schemas.in.h:31
msgid "Remote hostname"
msgstr "ཐག་རིང་ཧོསཊི་མིང་"

#: ../gperfmeter.schemas.in.h:32
msgid "Sample interval"
msgstr "དཔེ་ཚད་བར་མཚམས་"

#: ../gperfmeter.schemas.in.h:33
msgid "Sample when iconized"
msgstr "ངོས་པར་བཟོ་ཡོད་པའི་སྐབས་ཀྱི་དཔེ་ཚད་"

#: ../gperfmeter.schemas.in.h:34
msgid "Save resources by hostname"
msgstr "ཧོསཊི་མིང་གི་ཐོག་ལས་ འབྱུང་ཁུངས་སྲུངས་"

#: ../gperfmeter.schemas.in.h:35
msgid "Sets the log output page length."
msgstr "དྲན་དེབ་ཨའུཊི་པུཊི་གི་ཤོག་ལེབ་རིང་ཚད་འདི་ གཞི་སྒྲིག་འབདཝ་ཨིན།"

#: ../gperfmeter.schemas.in.h:36
msgid "Sets the orientation of the visible meters. The supported values are \"horizontal\" and \"vertical\"."
msgstr "མཐོང་ཚུགས་མི་མི་ཊར་ཚུ་གི་ཁ་ཕྱོགས་ གཞི་སྒྲིག་འབདཝ་ཨིན། རྒྱབ་སྐྱོར་འབད་ཡོད་པའི་བེ་ལུ་ཚུ་ \"horizontal\" དང་ \"vertical\"གཉིས་ཨིན།"

#: ../gperfmeter.schemas.in.h:37
msgid "Specifies the file to log statistics to (pathname + filename)."
msgstr "ཡིག་སྣོད་འདི་ དྲན་དེབ་ཚད་རྩིས་ལུ་གསལ་བཀོད་འབདཝ་ཨིན་(འགྲུལ་ལམ་+ཡིག་སྣོད་མིང་)།"

#: ../gperfmeter.schemas.in.h:38
msgid "Specifies the list of meters to display. The supported values are: cpu (0), load(1), disk(2), page(3), cntx(4), swap(5), intr(6), pkts(7), coll(8), errs(9)."
msgstr "བཀྲམ་སྟོན་འབད་ནིའི་མི་ཊར་ཐོ་ཡིག་གསལ་བཀོད་འབདཝ་ཨིན། རྒྱབ་སྐྱོར་འབད་ཡོད་པའི་བེ་ལུ་ཚུ་: སི་པི་ཡུ་ (༠) མངོན་གསལ་(༡) ཌིཀསི་(༢) ཤོག་ལེབ་(༣) སྐབས་དོན་(༤) བརྗེ་སོར་(༥) intr(༦) འཕྲིན་ཚན་(༧) ཀེལ་ཐིག་(༨)དང་ འཛོལ་བ་(༩)ཚུ་ཨིན།"

#: ../gperfmeter.schemas.in.h:39
msgid "The interval between each sample (depends on the time unit)."
msgstr "དཔེ་ཚད་རེ་རེའི་བར་མཚམས་(ཆུ་ཚོད་ཆ་ཕྲན་ལུ་བརྟེནམ་ཨིན་)།"

#: ../gperfmeter.schemas.in.h:40
msgid "The name of the remote host."
msgstr "ཐག་རིང་ཧོསཊི་གི་མིང་།"

#: ../gperfmeter.schemas.in.h:41
msgid "The unit of the time interval (either second(0), minute(1), hour(2))."
msgstr "ཆུ་ཚོད་བར་མཚམས་ཀྱི་ཆ་ཕྲན་(དཀར་ཆག་(༠) དང་དཀར་མ་(༡) ཡངན་ ཆུ་ཚོད་(༢))ཨིན།"

#: ../gperfmeter.schemas.in.h:42
msgid "Time interval unit"
msgstr "ཆུ་ཚོད་བར་མཚམས་ཆ་ཕྲན་"

#: ../gperfmeter.schemas.in.h:43
msgid "Type of graph displayed; either a bar chart (key \"solid\") or a strip chart (key \"line\")."
msgstr "བཀྲམ་སྟོན་འབད་ཡོད་པའི་ཚད་ཁྲམ་ ཕྲ་རིང་དཔེ་རིས་(ལྡེ་མིག་\"solid\") ཡངན་ བཤུབ་པའི་དཔེ་རིས་\"line\")ཨིན།"

#: ../gperfmeter.schemas.in.h:44
msgid "if true, display the Strip Chart"
msgstr "བདེན་པ་ཅིན་ བཤུབ་པའི་དཔེ་རིས་འདི་བཀྲམ་སྟོན་འབད་"

