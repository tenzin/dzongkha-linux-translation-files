msgstr "སི་ཌི་འབྲི་དོ།"

#: ../plugins/marlin-burn.c:401
msgid "Finishing up."
msgstr "རྫོགས་དོ།"

#: ../plugins/marlin-burn.c:426
msgid "Please make sure another application is not using the drive."
msgstr "གློག་རིམ་གཞན་མི་ཅིག་གིས་འདྲེན་འཕྲུལ་མི་འཐབ་དོཡོདཔ་ཨིནམ་ལས་ ངེས་གཏན་བཟོ་གནང་།"

#: ../plugins/marlin-burn.c:427
msgid "Drive is busy"
msgstr "འདྲེན་འཕྲུལ་འདི་ཁོམ་མེད་ཨིན་པས་"

#: ../plugins/marlin-burn.c:429
msgid "Please put a rewritable or blank disc into the drive."
msgstr "བསྐྱར་འབྲི་འབད་བཏུབ་མི་དང་ ཡང་ན་ ཌིཀསི་སྟོངམ་སྟདེ་འདྲེན་འཕྲུལ་དེ་ནང་ལུ་བཙུགས་གནང་།"

#: ../plugins/marlin-burn.c:430
msgid "Insert rewritable or blank disc"
msgstr "བསྐྱར་འབྲི་འབད་བཏུབ་མི་ ཡང་ན་ ཌིཀསི་སྟོངམ་བཙུགས་"

#: ../plugins/marlin-burn.c:432
msgid "Please insert a blank disc into the drive."
msgstr "ཌིཀསི་སྟོངམ་ཅིག་འདྲེན་འཕྲུལ་དེ་ནང་ལུ་བཙུགས་གནང་།"

#: ../plugins/marlin-burn.c:433
msgid "Insert blank disc"
msgstr "ཌིཀསི་སྟོངམ་བཙུགས་"

#: ../plugins/marlin-burn.c:435
msgid "Please replace the disc in the drive with a rewritable or blank disc."
msgstr "འདྲ་འཕྲུར་དེ་ནང་ལུ་ ཌིཀསི་གི་ཚབ་སྦེ་བསྐྱར་འབྲི་འབད་བཏུབ་མི་ ཡང་ན་ ཌིཀསི་སྟོངམ་ཅིག་བཙུགས།"

#: ../plugins/marlin-burn.c:436
msgid "Reload rewritable or blank disc"
msgstr "བསྐྱར་འབྲི་འབད་བཏུབ་མི་ ཡང་ན་ ཌིཀསི་སྟོངམ་ཡང་བསྐྱར་མངོན་གསལ་འབད།"

#: ../plugins/marlin-burn.c:438
msgid "Please replace the disc in the drive with a blank disc."
msgstr "འདྲེན་འཕྲུལ་དེ་ནང་ལུ་ཡོད་མི་ཌིཀསི་གི་ཚབ་སྦེ་ ཌིཀསི་སྟོངམ་ཅིག་བཙུགས།"

#: ../plugins/marlin-burn.c:439
msgid "Reload blank disc"
msgstr "ཌིཀསི་སྟོངམ་ཅིག་ཡང་བསྐྱར་མངོན་གསལ་འབད་"

#: ../plugins/marlin-burn.c:460
msgid "Retry disc"
msgstr "ཌིཀསི་སླར་རྩོལ་འབད་"

#: ../plugins/marlin-burn.c:544
msgid "Writing sample to CD"
msgstr "དཔེ་ཚད་དེ་སི་ཌི་ལུ་འབྲི་དོ་"

#: ../plugins/marlin-burn.c:545
msgid "The sample is being written to the disc. This operation may take a long time, depending on the length of the sample and write speed."
msgstr "དཔེ་ཚད་དེ་ ཌིཀསི་དེ་ལུ་བྲིས་ཡོདཔ་ཨིན། དཔེ་ཚད་ཀྱི་རིང་ཚད་དང་ འབྲི་ནིའི་མགྱོགས་ཚད་དང་བསྟུན་ཏེ་ བཀོལ་སྤྱོད་འདི་གིས་དུས་ཡུན་རིངམོ་འགོར་འོང་།"

#: ../plugins/marlin-burn.c:549
msgid "Preparing temporary files."
msgstr "གནས་སྐབས་ཀྱི་ཡིག་སྣོད་ཚུ་གྲ་སྒྲིག་འབད་དོ།"

