msgid "Allow popups"
msgstr "པོཔ་ཨཔས་འབད་བཅུག།"

#: ../galeon.schemas.in.h:8
msgid "Allow sites to change the statusbar message using JavaScript (if JavaScript is enabled)."
msgstr "ཇ་བ་ཡིག་ཚུགས་ལག་ལེན་འཐབ་ཐོག་ལས་ས་ཁོངས་ཚུ་གིས་གནད་ཚད་ཕྲ་རིང་འཕྲིན་དོན་འབད་བཅུག།(ཇ་བ་ཡིག་ཚུགས་འདི་ལྕོགས་ཅན་བཟོ་བ་ཅིན)."

#: ../galeon.schemas.in.h:9
msgid "Allow sites to open new windows using JavaScript (if JavaScript is enabled)."
msgstr "ཇ་བ་ཡིག་ཚུགས་ལག་ལེན་འཐབ་ཐོག་ལས་ས་ཁོངས་ཚུ་གིསཝིན་ཌོ་གསརཔ་ཁ་ཕྱེ་བཅུག།(ཇ་བ་ཡིག་ཚུགས་འདི་ལྕོགས་ཅན་བཟོ་བ་ཅིན)."

#: ../galeon.schemas.in.h:10
msgid "Allow statusbar updates"
msgstr "གནས་ཚད་ཕྲ་རིང་དུས་མཐུན་བཟོ་བཅུག"

