#: ../data/libgnetwork.schemas.in.h:1
msgid "Authenticate HTTP Proxy Connections"
msgstr "ཨེཆ་ཊི་ཊི་པི་པོརོ་སི་མཐུད་ལམ་ཚུ་ བདེན་བཤད་འབད་"

#: ../data/libgnetwork.schemas.in.h:2
msgid "Enables proxy settings when accessing HTTP over the internet."
msgstr "ཨེཆ་ཊི་ཊི་པི་འདི་ ཨིན་ཊར་ནེཊི་གུ་ལས་འཛུལ་སྤྱོད་འབད་བའི་སྐབས་ པོརོ་སི་སྒྲིག་སྟངས་ལྕོགས་ཅན་བཟོཝ་ཨིན།"

#: ../data/libgnetwork.schemas.in.h:3
msgid "FTP Proxy Port"
msgstr "ཨེཕ་ཊི་པི་ པོརོ་སི་འདྲེན་ལམ་"

#: ../data/libgnetwork.schemas.in.h:4
msgid "HTTP Proxy Location"
msgstr "ཨེཆ་ཊི་ཊི་པི་པོརོ་སི་གནས་ཁོངས་"

#: ../data/libgnetwork.schemas.in.h:5
msgid "HTTP Proxy Port"
msgstr "ཨེཆ་ཊི་ཊི་པི་པོ་རོ་སི་འདྲེན་ལམ་"

#: ../data/libgnetwork.schemas.in.h:6
msgid "HTTP Proxy User Name"
msgstr "ཨེཆ་ཊི་ཊི་པི་པོ་རོ་སི་ ལག་ལེན་པའི་མིང་"

#: ../data/libgnetwork.schemas.in.h:7
msgid "If true, then connections to the proxy server require authentication. The username/password defined by /system/http_proxy/authentication_user and /system/http_proxy/authentication_password."
msgstr "བདེན་པ་ཨིནཔ་ཅིན་ པོརོ་སི་སར་བར་འདི་ལུའི་མཐུད་ལས་ཚུ་ལུ་ བདེན་བཤད་དགོཔ་ཨིན། ལག་ལེན་པའི་མིང་/ཆོག་ཡིག་  /system/http_proxy/authentication_user དང་ /system/http_proxy/authentication_password གིས་ངེས་འཛིན་འབད་ཡོད་མི་འདི།"

#: ../data/libgnetwork.schemas.in.h:8
msgid "Proxy host name"
msgstr "པོརོ་སི་ཧོསིཊི་ནེམ་"

#: ../data/libgnetwork.schemas.in.h:9
msgid "Proxy port"
msgstr "པོརོ་སི་འདྲེན་ལམ་"

#: ../data/libgnetwork.schemas.in.h:10
msgid "SOCKS Proxy Host Name"
msgstr "སོཀསི་པོརོ་སི་ཧོསིཊི་ནེམ་"

#: ../data/libgnetwork.schemas.in.h:11
msgid "SOCKS Version"
msgstr "སོཀསི་ཐོན་རིམ་"

#: ../data/libgnetwork.schemas.in.h:12
msgid "SSL Proxy Host Name"
msgstr "ཨེསི་ཨེསི་ཨེལ་པོརོ་སི་ཧོསིཊི་ནེམ་"

#: ../data/libgnetwork.schemas.in.h:13
msgid "SSL Proxy Port"
msgstr "ཨེསི་ཨེསི་ཨེལ་པོརོ་སི་འདྲེན་ལམ་"

#: ../data/libgnetwork.schemas.in.h:14
msgid "The SOCKS proxy version used by /system/proxy/socks_host."
msgstr "སོཀསི་པོརོ་སི་ཐོན་རིམ་  /system/proxy/socks_host གིས་ལག་ལེན་འཐབ་ཡོད་མི།"

#: ../data/libgnetwork.schemas.in.h:15
msgid "The location of the FTP proxy server."
msgstr "ཨེཕ་ཊི་པི་པོརོ་སི་སར་བར་འདི་གི་གནས་ཁོངས།"

#: ../data/libgnetwork.schemas.in.h:16
msgid "The location of the proxy server to use for HTTP connections."
msgstr "ཨེཆ་ཊི་ཊི་པི་མཐུད་ལམ་ཚུ་གི་དོན་ལུ་ལག་ལེན་འཐབ་ནིའི་དོན་ལས་ པོརོ་སི་སར་བར་འདི་གི་གནས་ཁོངས་"

#: ../data/libgnetwork.schemas.in.h:17
msgid "The location of the proxy server which handles encrypted connections."
msgstr "གསང་བཟོ་མཐུད་ལམ་ཚུ་ལེགས་སྐྱོང་འཐབ་མི་ པོརོ་སི་སར་བར་འདི་གི་གནས་ཁོངས།"

#: ../data/libgnetwork.schemas.in.h:18
msgid "The location of the proxy server which handles non-HTTP and non-SSL connections."
msgstr "ཨེཆ་ཊི་ཊི་པི་མེན་མི་དང་ ཨེསི་ཨེསི་ཨེལ་མེན་མི་ མཐུད་ལམ་ཚུ་ལེགས་སྐྱོང་འཐབ་མི་ པོརོ་སི་སར་བར་འདི་གི་གནས་ཁོངས།"

#: ../data/libgnetwork.schemas.in.h:19
msgid "The password to use when authenticating with the HTTP proxy server."
msgstr "ཨེཆ་ཊི་ཊི་པི་པོརོ་སི་སར་བར་འདི་དང་གཅིག་ཁར་བདེན་བཤད་འབད་བའི་སྐབས་ ལག་ལེན་འཐབ་ནི་གི་ཆོག་ཡིག"

#: ../data/libgnetwork.schemas.in.h:20
msgid "The port on the proxy server defined by /system/http_proxy/host."
msgstr "པོརོ་སི་སར་བར་འདི་གུའི་འདྲེན་ལམ་  /system/http_proxy/host གིས་ངེས་འཛིན་འབད་ཡོད་མི།"

#: ../data/libgnetwork.schemas.in.h:21
msgid "The port on the proxy server defined by /system/proxy/ftp_host."
msgstr "པོརོ་སི་སར་བར་འདི་གུའི་འདྲེན་ལམ་  /system/proxy/ftp_hostགིས་ངེས་འཛིན་འབད་ཡོད་མི།"

#: ../data/libgnetwork.schemas.in.h:22
msgid "The port on the proxy server defined by /system/proxy/socks_host."
msgstr "པོརོ་སི་སར་བར་འདི་གུའི་འདྲེན་ལམ་ /system/proxy/socks_host གིས་ངེས་འཛིན་འབད་ཡོད་མི།"

#: ../data/libgnetwork.schemas.in.h:23
msgid "The port on the proxy server defined by /system/proxy/ssl_host."
msgstr "པོརོ་སི་སར་བར་འདི་གུའི་འདྲེན་ལམ་ /system/proxy/ssl_host གིས་ངེས་འཛིན་འབད་ཡོད་མི།"

#: ../data/libgnetwork.schemas.in.h:24
msgid "The username to use when authenticating with the HTTP proxy server."
msgstr "ཨེཆ་ཊི་ཊི་པི་པོརོ་སི་སར་བར་འདི་དང་གཅིག་ཁར་བདེན་བཤད་འབདབའི་སྐབས་ ལག་ལེན་འཐབ་ནི་གི་ ལག་ལེན་པའི་མིང་།"

#: ../data/libgnetwork.schemas.in.h:25
msgid "Use a proxy when accessing HTTP"
msgstr "ཨེཆ་ཊི་ཊི་པི་འཛུལ་སྤྱོད་འབད་བའི་སྐབས་ པོརོ་སི་ཅིག་ལག་ལེན་འཐབ་"

#: ../data/libgnetwork.schemas.in.h:26
msgid "http proxy password"
msgstr "ཨེཆ་ཊི་ཊི་པི་པོརོ་སི་ཆོག་ཡིག་"

#: ../libgnetwork/gnetwork-connection.c:94
msgid "Connection Type"
msgstr "མཐུད་ལམ་འབྱེ་བ་"

#: ../libgnetwork/gnetwork-connection.c:95
msgid "The type of connection represented by the implementing object."
msgstr "བསྟར་སྤྱོད་འབད་ནིའི་དངོས་པོ་འདི་གིས་ བརྡ་མཚོན་འབད་མི་མཐུད་ལམ་འདི་གི་དབྱེ་བ།"

#: ../libgnetwork/gnetwork-connection.c:103
msgid "Connection Status"
msgstr "མཐུད་ལམ་གནས་ཚད་"

#: ../libgnetwork/gnetwork-connection.c:104
msgid "The status of this connection."
msgstr "འ་ནི་མཐུད་ལམ་འདི་གི་གནས་ཚད།"

