#. Tell user that it's already open:
#: ../bakery/App/App_WithDoc.cc:105
msgid "Document already open"
msgstr "ཡིག་ཆ་ཧེ་མ་ལས་རང་ཁ་ཕྱེ་སྟེ་ཡོད་"

#: ../bakery/App/App_WithDoc.cc:105
msgid "This document is already open."
msgstr "ཡིག་ཆ་འདི་ཧེ་མ་ལས་རང་ཁ་ཕྱེ་སྟེ་ཡོདཔ་ཨིན།"

#: ../bakery/App/App_WithDoc.cc:155
msgid "Open failed."
msgstr "ཁ་ཕྱེ་ནི་ལུ་འཐུས་ཤོར་བྱུང་ཡོདཔ།"

