msgid "Print all key/value pairs in a directory."
msgstr "སྣོད་ཐོ་ཅིག་ནང་གི་ ལྡེ་མིག / བེ་ལུ་གི་ ཟུང་ཆ་མཉམ་ དཔར་བསྐྲུན་འབད།"

#: gconf/gconftool.c:123
msgid "Print all subdirectories in a directory."
msgstr "སྣོད་ཐོ་ཅིག་ནང་ཡོད་མི་ ཡན་ལག་སྣོད་ཐོ་ཆ་མཉམ་ དཔར་བསྐྲུན་འབད"

#: gconf/gconftool.c:132
msgid "Print all subdirectories and entries under a dir, recursively."
msgstr "ཌི་ཨའི་ཨར་གྱི་འོག་ལུ་ཡོད་མི་ ཡན་ལག་སྣོད་ཐོ་དང་ ཐོ་བཀོད་ཆ་མཉམ་ ལོག་རིམ་གྱི་ཐོག་ལས་དཔར་བསྐྲུན་འབད།"

#: gconf/gconftool.c:141
msgid "Return 0 if the directory exists, 2 if it does not."
msgstr "སྣོད་ཐོ་ཡོད་པ་ཅིན་ ༠ སླར་ལོག མེད་པ་ཅིན་ ༢ སླར་ལོག"

#: gconf/gconftool.c:150
msgid "Shut down gconfd. DON'T USE THIS OPTION WITHOUT GOOD REASON."
msgstr "ཇི་ཀཱོནཕི་ཌི་སྒོ་བསྡམས། རྒྱུ་མཚན་ལེགས་ཤོམ་མེད་པར་ གདམ་ཁ་འདི་ ལག་ལེན་མ་འཐབ།"

#: gconf/gconftool.c:159
msgid "Return 0 if gconfd is running, 2 if not."
msgstr "ཇི་ཀཱོནཕི་ཌི་ གཡོག་བཀོལ་བཞིན་ཡོད་བ་ཅིན་ ༠ སླར་ལོག དེ་མིན་པ་ཅིན་ ༢ སླར་ལོག"

#: gconf/gconftool.c:168
msgid "Launch the config server (gconfd). (Normally happens automatically when needed.)"
msgstr "རིམ་སྒྲིག་སར་བར་ (ཇི་ཀཱོནཕི་ཌི་) གསར་བཙུགས་འབད། (སྤྱིར་བཏང་ལུ་ མཁོ་བའི་སྐབས་ རང་བཞིན་གྱིས་འབྱུངམ་ཨིན།)"

#: gconf/gconftool.c:177
msgid "Specify the type of the value being set, or the type of the value a schema describes. Unique abbreviations OK."
msgstr "གཞི་སྒྲིག་འབད་བའི་བསྒང་གི་ བེ་ལུ་གི་དབྱེ་བ་ གསལ་བཀོད་འབད། ཡང་ཅིན་ ལས་འཆར་གྱིས་ འགྲེལ་བཤད་འབད་མི་ བེ་ལུ་གི་དབྱེ་བ་ གསལ་བཀོད་འབད། མཐུན་མོང་མ་ཡིན་པའི་སྡུད་ཚིག་བཏུབ་པས།"

#: gconf/gconftool.c:178
msgid "int|bool|float|string|list|pair"
msgstr "ཨིནཊི|བཱུལ|ཕོལོཊི|ཡིག་རྒྱུན|ཐོ་ཡིག|ཟུང་"

#: gconf/gconftool.c:186
msgid "Specify the type of the list value being set, or the type of the value a schema describes. Unique abbreviations OK."
msgstr "གཞི་སྒྲིག་འབད་བའི་བསྒང་གི་ ཐོ་ཡིག་བེ་ལུ་གི་དབྱེ་བ་ ཡང་ཅིན་ ལས་འཆར་གྱིས་ འགྲེལ་བཤད་རྐྱབ་མི་ བེ་ལུའི་དབྱེ་བ་ གསལ་བཀོད་འབད། མཐུན་མོང་མ་ཡིན་པའི་སྡུད་ཚིག་ཚུ་བཏུབ་པས།"

#: gconf/gconftool.c:187
#: gconf/gconftool.c:196
#: gconf/gconftool.c:205
msgid "int|bool|float|string"
msgstr "ཨིནཊི|བཱུལ|ཕོལོཊི| ཡིག་རྒྱུན༌།"

#: gconf/gconftool.c:195
msgid "Specify the type of the car pair value being set, or the type of the value a schema describes. Unique abbreviations OK."
msgstr "གཞི་སྒྲིག་འབད་བའི་བསྒང་གི་ ཀཱར་ཟུང་ཆའི་བེ་ལུ་གི་དབྱེ་བ་ ཡང་ཅིན་ ལས་འཆར་གྱིས་ འགྲེལ་བཤད་རྐྱབ་མི་ ལེ་བུའི་དབྱེ་བ་ གསལ་བཀོད་འབད། མཐུན་མོང་མ་ཡིན་པའི་སྡུད་ཚིག་ཚུ་བཏུབ་པས"

#: gconf/gconftool.c:204
msgid "Specify the type of the cdr pair value being set, or the type of the value a schema describes. Unique abbreviations OK."
msgstr "གཞི་སྒྲིག་འབད་བའི་བསྒང་གི་ སི་ཌི་ཨར་ཟུང་ཆའི་བེ་ལུའི་དབྱེ་བ་ ཡང་ཅིན་ ལས་འཆར་ཅིག་གིས་འགྲེལ་བཤད་རྐྱབ་མི་ བེ་ལུའི་དབྱེ་བ་ གསལ་བཀོད་འབད། མཐུན་མོང་མ་ཡིན་པའི་སྡུད་ཚིག་ཚུ་བཏུབ་པས"

#: gconf/gconftool.c:213
msgid "Specify a short half-line description to go in a schema."
msgstr "ལས་འཆར་ཅིག་ནང་འགྱོ་ནིའི་དོན་ལུ་ གྱལ་ཕྱེད་ཀ་འབད་མི་ འགྲེལ་བཤད་ཐུང་ཀུ་ཅིག་ གསལ་བཀོད་འབད།"

#: gconf/gconftool.c:214
#: gconf/gconftool.c:223
msgid "DESCRIPTION"
msgstr "འགྲེལ་བཤད།"

#: gconf/gconftool.c:222
msgid "Specify a several-line description to go in a schema."
msgstr "ལས་འཆར་ཅིག་ནང་འགྱོ་ནིའི་དོན་ལུ་ གྱལ་དག་པ་ཅིག་འབད་མི་ འགྲེལ་བཤད་ཅིག་ གསལ་བཀོད་འབད།"

#: gconf/gconftool.c:231
msgid "Specify the owner of a schema"
msgstr "ལས་འཆར་ཅིག་གི་བདག་པོ་དེ་ གསལ་བཀོད་འབད།"

#: gconf/gconftool.c:232
msgid "OWNER"
msgstr "བདག་པོ།"

#: gconf/gconftool.c:240
msgid "Specify a schema file to be installed"
msgstr "གཞི་བཙུགས་འབད་དགོ་མི་ ལས་འཆར་ཡིག་སྣོད་ཅིག་ གསལ་བཀོད་འབད།"

#: gconf/gconftool.c:241
msgid "FILENAME"
msgstr "ཡིག་སྣོད་མིང་།"

#: gconf/gconftool.c:249
msgid "Specify a configuration source to use rather than the default path"
msgstr "ལག་ལེན་འཐབ་ནིའི་དོན་ལུ་ སྔོན་སྒྲིག་འགྲུལ་ལམ་མིན་པར་ རིམ་སྒྲིག་འབྱུང་ཁུངས་ཅིག་ གསལ་བཀོད་འབད།"

#: gconf/gconftool.c:250
msgid "SOURCE"
msgstr "འབྱུང་ཁུངས།"

#: gconf/gconftool.c:258
msgid "Access the config database directly, bypassing server. Requires that gconfd is not running."
msgstr "སར་བར་ལས་ཟུར་ཏེ་ ཐད་ཀར་དུ་ རིམ་སྒྲིག་གི་ གནད་སྡུད་གཞི་རྟེན་ནང་ འཛུལ་སྤྱོད་འབད། ཇི་ཀཱོནཕི་དེ་ གཡོག་མ་བཀོལ་བར་དགོ"

#: gconf/gconftool.c:267
msgid "Properly installs schema files on the command line into the database. GCONF_CONFIG_SOURCE environment variable should be set to a non-default config source or set to the empty string to use the default."
msgstr "ལས་འཆར་ཡིག་སྣོད་ཚུ་ བཀོད་ལམ་གུ་ གནད་སྡུད་གཞི་རྟེན་ནང་ ཚུལ་ལྡན་སྦེ་ གཞི་བཙུགས་འབད། ཇི་ཀཱོནཕ་-རིམ་སྒྲིག་-འབྱུང་ཁུངས་ ཀྱི་མཐའ་འཁོར་འགྱུར་བ་ སྔོན་སྒྲིག་མེན་པའི་ རིམ་སྒྲིག་འབྱུང་ཁུངས་ཅིག་ལུ་ གཞི་སྒྲིག་འབད་དགོ ཡང་ཅིན་ སྔོན་སྒྲིག་ལག་ལེན་འཐབ་ནིའི་དོན་ལུ་ ཡིག་རྒྱུན་སྟོངམ་ལུ་ གཞི་སྒྲིག་འབད་དགོ"

#: gconf/gconftool.c:276
msgid "Torture-test an application by setting and unsetting a bunch of values of different types for keys on the command line."
msgstr "བཀོད་ལམ་གུ་ ལྡེ་མིག་གི་དབྱེ་བ་མ་འདྲ་བའི་ བེ་ལུསི་ཆག་པ་ཅིག་ གཞི་སྒྲིག་དང་ སྒྲིག་བཤོལ་འབད་བའི་ཐོག་ལས་ གློག་རིམ་ཅིག་ལུ་ དཀའ་ངལ་བརྟག་དཔྱད་འབད།"

#: gconf/gconftool.c:285
msgid "Torture-test an application by setting and unsetting a bunch of keys inside the directories on the command line."
msgstr "བཀོད་ལམ་གུ་ སྣོད་ཐོའི་ནང་ན་ ལྡེ་མིག་གི་ཆག་པ་ཅིག་ གཞི་སྒྲིག་དང་ སྒྲིག་བཤོལ་འབད་བའི་ཐོག་ལས་ གློག་རིམ་ཅིག་ལུ་ དཀའ་ངལ་བརྟག་དཔྱད་འབད།"

#: gconf/gconftool.c:294
msgid "Get the short doc string for a key"
msgstr "ལྡེ་མིག་ཅིག་གི་དོན་ལུ་ ཡིག་ཆའི་ཡིག་རྒྱུན་ ཐུང་ཀུ་ཅིག་ལེན།"

#: gconf/gconftool.c:303
msgid "Get the long doc string for a key"
msgstr "ལྡེ་མིག་ཅིག་གི་དོན་ལུ་ ཡིག་ཆའི་ཡིག་རྒྱུན་ རིངམ་ཅིག་ལེན།"

#: gconf/gconftool.c:312
msgid "Get the name of the schema applied to this key"
msgstr "ལྡེ་མིག་འདི་གི་དོན་ལུ་ འཇུག་སྤྱོད་འབད་མི་ ལས་འཆར་གྱི་མིང་དེ་ལེན།"

#: gconf/gconftool.c:321
msgid "Specify the schema name followed by the key to apply the schema name to"
msgstr "ལས་འཆར་གྱི་མིང་ འཇུག་སྤྱོད་ནིའི་དོན་ལུ་ ལས་འཆར་གྱི་མིང་དང་ དེ་གི་ཤུལ་བདའ་སྟེ་ ལྡེ་མིག་དེ་གསལ་བཀོད་འབད།"

#: gconf/gconftool.c:330
msgid "Get the name of the default source"
msgstr "སྔོན་སྒྲིག་འབྱུང་ཁུངས་ཀྱི་མིང་དེ་ལེན།"

#: gconf/gconftool.c:382
#, c-format
msgid ""
"Error on option %s: %s.\n"
"Run '%s --help' to see a full list of available command line options.\n"
msgstr ""
"གདམ་ཁ་ %s་:%s ལུ་འཛོལ་བ། \n"
"བཀོད་ལམ་གདམ་ཁའི་ ཐོ་ཡིག་ཆ་ཚང་ཅིག་ བལྟ་ནིའི་དོན་ལུ་ %s--གྲོགས་རམ་ གཡོག་བཀོལ། \n"

#: gconf/gconftool.c:394
msgid "Can't get and set/unset simultaneously\n"
msgstr "དུས་མཉམ་ལུ་ ལེན་ནི་དང་ གཞི་སྒྲིག་/སྒྲིག་བཤོལ་ འབད་མི་ཚུགས།\n"

#: gconf/gconftool.c:401
msgid "Can't set and get/unset simultaneously\n"
msgstr "དུས་མཉམ་ལུ་ གཞི་སྒྲིག་དང་ ལེན་ནི་/སྒྲིག་བཤོལ་ འབད་མི་ཚུགས།\n"

#: gconf/gconftool.c:409
msgid "Can't use --all-entries with --get or --set\n"
msgstr "ཐོ་བཀོད་-ཆ་མཉམ་--ལེན་ནིའམ་ --གཞི་སྒྲིག་དང་གཅིག་ཁར་ -- ལག་ལེན་ འཐབ་མི་ཚུགས།\n"

#: gconf/gconftool.c:417
msgid "Can't use --all-dirs with --get or --set\n"
msgstr "སྣོད་ཐོ་-ཆ་མཉམ་-- ལེན་ནིའམ་ --གཞི་སྒྲིག་དང་གཅིག་ཁར་  --ལག་ལེན་འཐབ་མི་ཚུགས།\n"

#: gconf/gconftool.c:427
msgid "--recursive-list should not be used with --get, --set, --unset, --all-entries, or --all-dirs\n"
msgstr "--རི་ཀར་སིབ་-ཐོ་ཡིག་དེ་ --ལེན་ནི་ --གཞི་སྒྲིག་ --སྒྲིག་བཤོལ་ --ཐོ་བཀོད་-ཆ་མཉམ་ ཡང་ཅིན་ --སྣོད་ཐོ་-ཆ་མཉམ་དང་གཅིག་ཁར་ ལག་ལེན་འཐབ་ནི་མི་འོང་།\n"

#: gconf/gconftool.c:437
msgid "--set_schema should not be used with --get, --set, --unset, --all-entries, --all-dirs\n"
msgstr "--གཞི་སྒྲིག་ལས་འཆར་དེ་ --ལེན་ནི་ --གཞི་སྒྲིག་ --སྒྲིག་བཤོལ་ --ཐོ་བཀོད་-ཆ་མཉམ་ ཡང་ཅིན་ --སྣོད་ཐོ་-ཆ་མཉམ་དང་གཅིག་ཁར་ ལག་ལེན་འཐབ་ནི་མི་འོང་།\n"

#: gconf/gconftool.c:443
msgid "Value type is only relevant when setting a value\n"
msgstr "བེ་ལུའི་དབྱེ་བ་དེ་ བེ་ལུ་ཅིག་ གཞི་སྒྲིག་འབད་བའི་སྐབས་ལུ་རྐྱངམ་ཅིག་ འབྲེལ་བ་ཡོད།\n"

#: gconf/gconftool.c:449
msgid "Must specify a type when setting a value\n"
msgstr "བེ་ལུ་ཅིག་ གཞི་སྒྲིག་འབད་བའི་སྐབས་ལུ་ དབྱེ་བ་ཅིག་ གསལ་བཀོད་འབད་དགོ\n"

#: gconf/gconftool.c:459
msgid "Ping option must be used by itself.\n"
msgstr "པིང་སྒྲའི་གདམ་ཁ་འདི་ དེ་ཉིད་ཀྱིས་ལག་ལེན་འཐབ་དགོ།\n"

#: gconf/gconftool.c:469
msgid "--dir-exists option must be used by itself.\n"
msgstr "--ཌི་ཨའི་ཨར་-གནས་ཡོད་པའི་གདམ་ཁ་འདི་  དེ་ཉིད་ཀྱིས་ལག་ལེན་འཐབ་དགོ།\n"

#: gconf/gconftool.c:479
msgid "--install-schema-file must be used by itself.\n"
msgstr "--གཞི་བཙུགས་-འཆར་ལས་-ཡིག་སྣོད་འདི་དེ་ཉིད་ཀྱིས་ལག་ལེན་འཐབ་དགོ།\n"

#: gconf/gconftool.c:490
msgid "--makefile-install-rule must be used by itself.\n"
msgstr "--ཡིག་སྣོད་བཟོ་-གཞི་བཙུགས་འབད་བའི་-ལམ་ལུགས་འདི་དེ་ཉིད་ཀྱིས་ལག་ལེན་འཐབ་དགོ།\n"

#: gconf/gconftool.c:501
msgid "--break-key must be used by itself.\n"
msgstr "--ལྡེ་མིག་-མཚམས་འདི་ ཁོ་ར་གིས་རང་ལག་ལེན་འཐབ་དགོཔ་ཨིན།\n"

#: gconf/gconftool.c:512
msgid "--break-directory must be used by itself.\n"
msgstr "--སྣོད་ཐོ་-མཚམས་འདི་ ཁོ་ར་གིས་རང་ལག་ལེན་འཐབ་དགོཔ་ཨིན།\n"

#: gconf/gconftool.c:519
msgid "You must specify a config source with --config-source when using --direct\n"
msgstr "ཁྱོད་ཀྱིས་ --ཐད་ཀར་ ལག་ལེན་འཐབ་པའི་སྐབས་ རིམ་སྒྲིག་འབྱུང་ཁུངས་དང་གཅིག་ཁར་--རིམ་སྒྲིག་འབྱུང་ཁུངས་ ལག་ལེན་འཐབ་དགོ \n"

#: gconf/gconftool.c:525
msgid "You should use --direct when using a non-default configuration source\n"
msgstr "སྔོན་སྒྲིག་མེན་པའི་རིམ་སྒྲིག་འབྱུང་ཁུངས་གཅིག་ལག་ལེན་འཐབ་ད་  ཁྱོད་ཀྱིས་  --ཐད་ཀར་འདི་ལག་ལེན་འཐབ་དགོ།\n"

