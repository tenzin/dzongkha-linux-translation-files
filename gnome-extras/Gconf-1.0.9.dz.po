#: backends/bdb-backend.c:211
msgid "Unloading BerkeleyDB (BDB) backend module."
msgstr "བེར་ཀེ་ལེ་ཌི་བི་(བི་ཌི་བི་) རྒྱབ་མཐའི་ཚད་གཞི་ མངོན་གསལ་བཤོལ་དོ།"

#: backends/bdb-backend.c:234
#, c-format
msgid "Opened BerkeleyDB source at root %s"
msgstr "རྩ་བ་ %sལུ་  བར་ཀི་ལི་ཌི་བི་འབྱུང་ཁུངས་ཁ་ཕྱེ་ནུག"

#: backends/bdb-backend.c:574
msgid "Initializing BDB backend module"
msgstr "བི་ཌི་བི་ རྒྱབ་མཐའ་ཚད་གཞི་འགོ་འབྱེད་འབད་དོ།"

#: backends/dir-utils.c:108
#, c-format
msgid "Couldn't find the %s root directory in the address `%s'"
msgstr "ཁ་བྱང་ `%s' ནང་ རྩ་བའི་སྣོད་ཐོ་ %s འཚོལ་མ་ཐོབ།"

#: backends/dir-utils.c:124
#: backends/xml-backend.c:304
#: backends/xml-dir.c:1058
#, c-format
msgid "Could not make directory `%s': %s"
msgstr "སྣོད་ཐོ་`%s': %sབཟོ་མ་ཚུགས། "

#: backends/dir-utils.c:212
#, c-format
msgid "Can't read from or write to the %s root directory in the address `%s'"
msgstr "ཁ་བྱང་ `%s' ནང་ རྩ་བའི་སྣོད་ཐོ་ %s  ལུ་འབྲི་མ་ཚུགས་ཡང་ན་ ལས་ལྷག་མ་ཚུགས།"

#: backends/xml-backend.c:233
msgid "Unloading XML backend module."
msgstr "ཨེགསི་ཨེམ་ཨེལ་ རྒྱབ་མཐའི་ཚད་གཞི་ མངོན་གསལ་བཤོལ་བའི་བསྒང༌།"

#: backends/xml-backend.c:289
#, c-format
msgid "Couldn't find the XML root directory in the address `%s'"
msgstr "ཁ་བྱང་ `%s' ནང་ ཨེགསི་ཨེམ་ཨེལ་ རྩ་བའི་སྣོད་ཐོ་ འཚོལ་མ་ཐོབ།"

#: backends/xml-backend.c:414
#, c-format
msgid "Can't read from or write to the XML root directory in the address `%s'"
msgstr "ཁ་བྱང་ `%s' ནང་ རྩ་བའི་སྣོད་ཐོ་ ཨེགསི་ཨེམ་ཨེལ་ ལུ་འབྲི་མ་ཚུགས་ཡང་ན་ ལས་ལྷག་མ་ཚུགས།"

#: backends/xml-backend.c:424
#, c-format
msgid "Directory/file permissions for XML source at root %s are: %o/%o"
msgstr "རྩ་བ་ %s ལུ་ ཨེགསི་ཨེམ་ཨེལ་ འབྱུང་ཁུངས་ཀྱི་དོན་ལུ་ སྣོད་ཐོ་ / ཡིག་སྣོད་གནང་བ་ཚུ་%o/%o ཨིན།"

#: backends/xml-backend.c:714
msgid "Initializing XML backend module"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ རྒྱབ་མཐའི་ཚད་གཞི་ འགོ་འབྱེད་འབད་དོ།"

#: backends/xml-backend.c:779
#, c-format
msgid "Failed to give up lock on XML dir `%s': %s"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ སྣོད་ཐོ་ `%s': %s གུ་ལུ་ ལྡེ་མིག་མ་བློས་གཏང་ནི་འཐུས་ཤོར་བྱུང་ནུག"

#: backends/xml-cache.c:119
msgid "Unsynced directory deletions when shutting down XML backend"
msgstr "ཨེགསི་ཨེམ་ཨེལ་རྒྱབ་མཐའ་ཁ་བསྡམས་པའི་སྐབས་  མཉམ་འབྱུང་མེད་པའི་སྣོད་ཐོ་བཏོན་གཏང་ནི་ཚུ།"

#: backends/xml-cache.c:229
#, c-format
msgid "Unable to remove directory `%s' from the XML backend cache, because it has not been successfully synced to disk"
msgstr "སྣོད་ཐོ་  `%s'  དེ་ ཌིཀསི་ལུ་མཐར་འཁྱོལ་སྦེ་ མཉམ་འབྱུང་འབད་མ་ཚུགསཔ་ལས་ ཨེགསི་ཨེམ་ཨེལ་ རྒྱབ་མཐའི་འདྲ་མཛོད་ནང་ལས་ རྩ་བསྐྲད་གཏང་མ་ཚུགས།"

#: backends/xml-cache.c:256
#, c-format
msgid "%u items remain in the cache after cleaning already-synced items older than %u seconds"
msgstr "%uསྐར་ཆ་ལས་  ཧེ་མ་ལས་ཡོད་པའི་མཉམ་འབྱུང་རྣམ་གྲངས་བསལ་བའི་ཤུལ་ལས་ %u རྣམ་གྲངས་ཚུ་  འདྲ་མཛོད་ནང་ལུསཔ་ཨིན།"

#: backends/xml-dir.c:164
#, c-format
msgid "Could not stat `%s': %s"
msgstr "`%s': %s ཨེསི་ཊེཊི་ འབད་མ་ཚུགས"

#: backends/xml-dir.c:174
#, c-format
msgid "XML filename `%s' is a directory"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ ཡིག་སྣོད་མིང་ `%s' འབད་མི་དེ་ སྣོད་ཐོ་ཅིག་ཨིན།"

#: backends/xml-dir.c:298
#: backends/xml-dir.c:305
#, c-format
msgid "Failed to delete `%s': %s"
msgstr "`%s': %s བཏོན་གཏང་ནི་ འཐུས་ཤོར་བྱུང་ཡོདཔ།"

#: backends/xml-dir.c:346
#, c-format
msgid "Failed to write file `%s': %s"
msgstr "ཡིག་སྣོད་`%s': %s འབྲི་མ་ཚུགས། "

#: backends/xml-dir.c:359
#, c-format
msgid "Failed to set mode on `%s': %s"
msgstr "`%s': %s ལུ་ ཐབས་ལམ་གཞི་སྒྲིག་འབད་མ་ཚུགས། "

#: backends/xml-dir.c:373
#: backends/xml-dir.c:383
#, c-format
msgid "Failed to rename `%s' to `%s': %s"
msgstr " `%s' ལས་ `%s': %s ལུ་ མིང་ཚབ་བཏགས་མ་ཚུགས།"

#: backends/xml-dir.c:389
#, c-format
msgid "Failed to restore `%s' from `%s': %s"
msgstr "`%s': %s ལས་`%s' སོར་ཆུད་འབད་མ་ཚུགས། "

#: backends/xml-dir.c:401
#, c-format
msgid "Failed to delete old file `%s': %s"
msgstr "ཡིག་སྣོད་རྙིངམ་ `%s': %s བཏོན་གཏང་མ་ཚུགས། "

#. These are all fatal errors
#: backends/xml-dir.c:811
#, c-format
msgid "Failed to stat `%s': %s"
msgstr "`%s': %s ཨེསི་ཊེཊི་ འབད་མ་ཚུགས།"

#: backends/xml-dir.c:955
#, c-format
msgid "Duplicate entry `%s' in `%s', ignoring"
msgstr "%s ནང་ལུ་ %s གི་ངོ་བཤུས་ཐོ་བཀོད་ སྣང་མེད་བཞག་དོ།"

#: backends/xml-dir.c:977
#, c-format
msgid "Entry with no name in XML file `%s', ignoring"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ ཡིག་སྣོད་ `%s' ནང་ མིང་མེད་པའི་ཐོ་བཀོད་ སྣང་མེད་བཞག་དོ།"

#: backends/xml-dir.c:985
#, c-format
msgid "A toplevel node in XML file `%s' is <%s> rather than <entry>, ignoring"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ཡིག་སྣོད་ནང་གི་ གནས་རིམ་མཐོ་ཤོས་ཀྱི་ནོཌི་`%s' གི་ཚབ་ལུ་ <entry>སྣང་མེད་བཏག་ནི་མིན་པར་ <%s> ཨིན་པས།"

#: backends/xml-dir.c:1074
#, c-format
msgid "Failed to create file `%s': %s"
msgstr "ཡིག་སྣོད་ `%s': %s གསར་བསྐྲུན་འབད་མ་ཚུགས།"

#: backends/xml-dir.c:1082
#, c-format
msgid "Failed to close file `%s': %s"
msgstr "ཡིག་སྣོད་`%s': %s  ཁ་བསྡམ་མ་ཚུགས།"

#. There was an error
#: backends/xml-entry.c:149
#, c-format
msgid "Ignoring XML node with name `%s': %s"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ནོཌི་དེ་ མིང་ `%s': %s དང་ཅིག་ཁར་ སྣང་མེད་བཞག་དོ།"

#: backends/xml-entry.c:327
#, c-format
msgid "Ignoring schema name `%s', invalid: %s"
msgstr "ལས་འཆར་མིང་ `%s' སྣང་མེད་བཞག་དོ།  ནུས་མེད་:%s།"

#: backends/xml-entry.c:375
#, c-format
msgid "Ignoring XML node `%s': %s"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ ནོཌི་ `%s': %s སྣང་མེད་བཞག་དོ།"

#: backends/xml-entry.c:726
#, c-format
msgid "Failed reading default value for schema: %s"
msgstr "ལས་འཆར་:%sགི་དོན་ལུ་ སྔོན་སྒྲིག་བེ་ལུ་ ལྷག་མ་ཚུགས"

#: backends/xml-entry.c:940
#, c-format
msgid "No \"type\" attribute for <%s> node"
msgstr "ནོཌི་ <%s> གི་དོན་ལུ་ \"type\"གི་ཁྱད་ཆོས་མིན་འདུག"

#: backends/xml-entry.c:954
#, c-format
msgid "A node has unknown \"type\" attribute `%s', ignoring"
msgstr "ནོཌི་ཅིག་ལུ་ མ་ཤེས་པའི་ \"type\" གི་ཁྱད་ཆོས་ `%s' ཡོདཔ་ལས་་ སྣང་མེད་བཞག་དོ།"

#: backends/xml-entry.c:969
msgid "No \"value\" attribute for node"
msgstr "ནོཌི་གི་དོན་ལུ་ ཁྱད་ཆོས་ཀྱི་ \"value\" མིན་འདུག"

#: backends/xml-entry.c:1017
#: backends/xml-entry.c:1093
#, c-format
msgid "Didn't understand XML node <%s> inside an XML list node"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ ཐོ་ཡིག་ནོཌི་ཅིག་གི་ནང་ན་ ཨེགསི་ཨེམ་ཨེལ་ ནོཌི་ <%s> ཧ་གོ་མ་ཚུགས།"

#: backends/xml-entry.c:1051
msgid "Invalid type (list, pair, or unknown) in a list node"
msgstr "ཐོ་ཡིག་ནོཌི་ཅིག་ནང་ ནུས་མེད་དབྱེ་བ། (ཐོ་ཡིག་ ཟུང་ ཡང་ཅིན་ ཧ་གོ་མ་ཚུགསཔ་འདུག)"

#: backends/xml-entry.c:1074
#, c-format
msgid "Bad XML node: %s"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ནོཌི་ བྱང་ཉེས་ %s"

#: backends/xml-entry.c:1082
#, c-format
msgid "List contains a badly-typed node (%s, should be %s)"
msgstr "ཐོ་ཡིག་ནང་ བྱང་ཉེས་འབད་ཡིག་དཔར་བརྐྱབས་མི་ ནོཌི་ཅིག་འདུག (%s, འབད་མི་དེ་ %s ཨིན་དགོ)"

#: backends/xml-entry.c:1134
#, c-format
msgid "Ignoring bad car from XML pair: %s"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ཟུང་ :%s ནང་ལས་ ཀཱར་བྱང་ཉེས་ སྣང་མེད་བཞག་དོ།།"

#: backends/xml-entry.c:1143
#: backends/xml-entry.c:1166
msgid "parsing XML file: lists and pairs may not be placed inside a pair"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ཡིག་སྣོད་ མིང་དཔྱད་འབད་དོ: ཐོ་ཡིག་དང་ ཟུང་དེ་ཚུ་ ཟུང་ཅིག་གི་ནང་ན་བཙུགས་མ་བཞག"

#: backends/xml-entry.c:1156
#, c-format
msgid "Ignoring bad cdr from XML pair: %s"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ཟུང་ :%s ནང་ལས་ སི་ཌི་ཨར་་བྱང་ཉེས་ སྣང་མེད་བཞག་དོ།།"

#: backends/xml-entry.c:1175
#, c-format
msgid "Didn't understand XML node <%s> inside an XML pair node"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ ཟུང་གི་ནོཌི་ཅིག་ནང་གི་ ཨེགསི་ཨེམ་ཨེལ་ནོཌི་ <%s> ཧ་གོ་མ་ཚུགས།"

#: backends/xml-entry.c:1193
msgid "Didn't find car and cdr for XML pair node"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ ཟུང་གི་ནོཌི་གི་དོན་ལུ་ ཀཱར་དང་ སི་ཌི་ཨར་ འཚོལ་མ་ཐོབ།"

#: backends/xml-entry.c:1199
msgid "Missing cdr from pair of values in XML file"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ཡིག་སྣོད་ནང་གི་ བེ་ལུསི་ཟུང་ཅིག་ནང་ལས་ སི་ཌི་ཨར་ བྱང་སྟོར་ཞུགས་ནུག"

#: backends/xml-entry.c:1206
msgid "Missing car from pair of values in XML file"
msgstr "ཨེགསི་ཨེམ་ཨེལ་ ཡིག་སྣོད་ནང་གི་ ཟུང་གི་བེ་ལུསི་ནང་ལས་ ཀཱར་བྱང་སྟོར་ཞུགས་ནུག"

#: backends/xml-entry.c:1211
msgid "Missing both car and cdr values from pair in XML file"
