#: ../nact/nautilus-actions-config.glade.h:6
#, no-c-format
msgid "<b>%%</b>: a percent sign"
msgstr "<b>%%</b>:བརྒྱ་ཆའི་མིང་རྟགས།"

#: ../nact/nautilus-actions-config.glade.h:8
#, no-c-format
msgid "<b>%M</b>: space-separated list of the selected file(s)/folder(s) with their full paths"
msgstr "<b>%M</b>:ཡིག་སྣོད་(s)/སྣོད་འཛིན་སེལ་འཐུ་འབད་ཡོད་མི་ འགྲུལ་ལམ་ཆ་ཚང་ཚུ་དང་བཅས་ཡོད་མི་གི་  བར་སྟོང་ སོ་སོ་འཕྱལ་འཕྱལཝ་ཚུ།"

#: ../nact/nautilus-actions-config.glade.h:10
#, no-c-format
msgid "<b>%U</b>: username of the gnome-vfs URI"
msgstr "<b>%U</b>: ཇི་ནོམ་-ཝི་ཨེཕ་ཨེསི་ ཡུ་ཨར་ཨའི་གི་ལག་ལེན་པའི་མིང་།"

#: ../nact/nautilus-actions-config.glade.h:12
#, no-c-format
msgid "<b>%d</b>: base folder of the selected file(s)"
msgstr "<b>%d</b>:ཡིག་སྣོད་སེལ་འཐུ་འབད་ཡོད་མི་ཚུ་གི་ སྣོད་འཛིན་གཞི་རྟེན།(s)"

#: ../nact/nautilus-actions-config.glade.h:14
#, no-c-format
msgid "<b>%f</b>: the name of the selected file or the 1st one if many are selected"
msgstr "<b>%f</b>: ལེ་ཤ་སེལ་འཐུ་འབད་ཡོད་པ་ཅིན་ སེལ་འཐུ་འབད་བའི་ཡིག་སྣོད་འབད་རུང་ཡང་ཅིན་ དང་པ་འདི།"

#: ../nact/nautilus-actions-config.glade.h:16
#, no-c-format
msgid "<b>%h</b>: hostname of the gnome-vfs URI"
msgstr "<b>%h</b>: ཇི་ནོམ་ཝི་ཨེཕ་ཨེསི་ ཡུ་ཨར་ཨའི་ ཧོསཊི་གི་མིང་།"

#: ../nact/nautilus-actions-config.glade.h:18
#, no-c-format
msgid ""
"<b>%m</b>: space-separated list of the basenames of the selected\n"
"file(s)/folder(s)"
msgstr ""
"<b>%m</b>:སེལ་འཐུ་འབད་ཡོད་མི་ཡིག་སྣོད་(s)/སྣོད་འཛིན་(s)\n"
" གི་བར་སྟོང་ཁ་སོ་སོ་འཕྱལ་འཕྱལཝ་ཚུ།"

#: ../nact/nautilus-actions-config.glade.h:21
#, no-c-format
msgid "<b>%s</b>: scheme of the gnome-vfs URI"
msgstr "<b>%s</b>: ཇི་ནོམ་-ཝི་ཨེཕ་ཨེསི་ ཡུ་ཨར་ཨའི་གི་འཆར་ལས།"

#: ../nact/nautilus-actions-config.glade.h:23
#, no-c-format
msgid "<b>%u</b>: gnome-vfs URI"
msgstr "<b>%u</b>: ཇི་ནོམ་-ཝི་ཨེཕ་ཨེསི་ ཡུ་ཨར་ཨའི།"

#: ../nact/nautilus-actions-config.glade.h:24
msgid "<b>Action</b>"
msgstr "<b>བྱ་བ</b>"

#: ../nact/nautilus-actions-config.glade.h:25
msgid "<b>Appears if file matches</b>"
msgstr "<b>ཡིག་སྣོད་མཐུན་པ་ཅིན་འབྱུངམ་ཨིན</b>"

#: ../nact/nautilus-actions-config.glade.h:26
msgid "<b>Appears if scheme is in this list</b>"
msgstr "<b>ཡིག་སྣོད་འདི་གི་ནང་ལུ་འཆར་ལས་ཡོད་པ་ཅིན་འབྱུངམ་ཨིན</b>"

#: ../nact/nautilus-actions-config.glade.h:27
msgid "<b>Appears if selection contains</b>"
msgstr "<b>སེལ་འཐུ་ཡོད་པ་ཅིན་འབྱུངམ་ཨིན</b>"

#: ../nact/nautilus-actions-config.glade.h:28
msgid "<b>Nautilus Menu Item</b>"
msgstr "<b>ནའུ་ཊི་ལཱསི་གི་དཀར་ཆག་རྣམ་གྲངས</b>"

#: ../nact/nautilus-actions-config.glade.h:29
msgid "<b>Profiles</b>"
msgstr "<b>གསལ་སྡུད་ཚུ་</b>"

#: ../nact/nautilus-actions-config.glade.h:31
#, no-c-format
msgid "<i><b><span size=\"small\">e.g., %s</span></b></i>"
msgstr "<i><b><span size=\"small\">དཔེ་འབད་བ་ཅིན་%s</span></b></i>"

#: ../nact/nautilus-actions-config.glade.h:32
msgid "<span size=\"larger\"><b>Parameter Legend</b></span>"
msgstr "<span size=\"larger\"><b>ཚད་བཟུང་གི་གཏམ་རྒྱུད་</b></span>"

#: ../nact/nautilus-actions-config.glade.h:33
msgid ""
"<span weight=\"bold\" size=\"larger\">place primary text here</span>\n"
"\n"
"place secondary text here"
msgstr ""
"<span weight=\"bold\" size=\"larger\">གཞི་རིམ་ཚིག་ཡིག་བཞག</span>\n"
"\n"
"གལ་གནད་ཆུང་བའི་ཚིག་ཡིག་བཞག"

#: ../nact/nautilus-actions-config.glade.h:36
msgid "A string with wildcards (? or *) that will be used to match the filenames. You can set several filename patterns by separating them with a semi-colon (;)."
msgstr "ཝའིལ་ཀཌིསི་དང་བཅས་པའིཡིག་རྒྱུན་ཚུ་(? or *) ཡིག་སྣོད་ཀྱི་མིང་མཐུན་པ་བཟོ་ནི་གི་དོན་ལུ་ལག་ལེན་འཐབ་འོང་། སེ་མི་ཀོ་ལཱོན་ (;)གི་འབད་དེ་ཚུ་ སོ་སོ་འཕྱལ་བཞིནམ་ལས་ ཁྱོད་རང་ཡིག་སྣོད་ཀྱི་དཔེ་གཞི་ལེ་ཤ་ཅིག་ཆ་ཚན་འབད་ཚུགས།"

#: ../nact/nautilus-actions-config.glade.h:37
msgid "A string with wildcards (? or *) that will be used to match the mimetypes of files. You can set several mimetype patterns by separating them with a semi-colon (;)."
msgstr "ཡིག་སྣོད་ཚུ་གི་མ་ཡིམ་དབྱེ་བ་མཐུན་སྒྲིག་ལུ་ ལག་ལེན་འཐབ་ནི་གི་ཝའིལ་ཀཌིསི་  (? or *) ཡིག་རྒྱུན་དང་བཅས་ཡོད་མི།དེ་ཚུ་ སེ་མི་ཀོ་ལཱོན་ (;)གི་ཐོག་ལས་འབད་ སོ་སོ་འཕྱལ་བཞིན་ན་ཁྱོད་རང་  མ་ཡམ་དབྱེ་བའི་དཔེ་གཞི་ལེ་ཤ་ཅིག་ཆ་ཚན་འབད་ཚུགས།"

#: ../nact/nautilus-actions-config.glade.h:38
msgid "Action"
msgstr "བྱ་བ་"

#: ../nact/nautilus-actions-config.glade.h:39
msgid "Advanced Conditions"
msgstr "མཐོ་རིམ་གནས་སྟངས་ཚུ།"

#: ../nact/nautilus-actions-config.glade.h:40
msgid "Appears if selection has multiple files or folders"
msgstr "སེལ་འཐུ་འདི་ལུ་ཡིག་སྣོད་སྣ་མང་ཡོད་མི་ ཡང་ཅིན་སྣོད་འཛིན་ཡོད་པ་ཅིན་རྐྱངམ་ཅིག་འབྱུངམ་ཨིན།"

#: ../nact/nautilus-actions-config.glade.h:41
msgid "Automatic"
msgstr "རང་བཞིན་གྱི།"

#: ../nact/nautilus-actions-config.glade.h:42
msgid "Both"
msgstr "གཉིས་ཆ་རང་།"

#: ../nact/nautilus-actions-config.glade.h:43
msgid "Check this box if you want to get back all your configurations from the version of Nautilus-actions 0.7.1 or lesser."
msgstr "ནའུ་ཊི་ལཱསི་བྱ་བ་0.7.1 ཡང་ཅིན་ ཉུང་ཀུ་  གི་ཐོན་རིམ་ལས་  ཁྱོད་རང་གི་རིམ་སྒྲིག་ཚུ་ཆ་མཉམ་ལོག་ལེན་དགོ་པ་ཅིན་ སྒྲོམ་འདི་ཞིབ་དཔྱད་འབད།"

#: ../nact/nautilus-actions-config.glade.h:44
msgid "Click to add a new scheme."
msgstr "འཆར་ལས་གསརཔ་ཁ་སྐོང་རྐྱབས་ནའི་དོན་ལུ་ ཨེབ་གཏང་།"

#: ../nact/nautilus-actions-config.glade.h:45
msgid "Click to choose a command from the file chooser dialog."
msgstr "ཡིག་སྣོད་འདེམས་བྱེད་ཌའི་ལོག་འདི་ལས་ བརྡ་བཀོད་གདམ་ཁ་འབད་ནའི་དོན་ལུ་ཨེབ་གཏང་།"

#: ../nact/nautilus-actions-config.glade.h:46
msgid "Click to choose a custom icon from a file instead of a predefined icon from the drop-down list."
msgstr "སར-་འཇོག་ཐོ་བཀོད་ལས་སྔ་གོང་ངེས་འཛིན་འབད་ཡོད་པའི་ངོས་དཔར་གྱི་ཚབ་ལུ་ ཡིག་སྣོད་ལས་སྲོལ་སྒྲིག་ངོས་དཔར་གདམ་ཁ་འབད་ནི་ལུ་ཨེབ་གཏང་།"

#: ../nact/nautilus-actions-config.glade.h:47
msgid "Click to remove the selected scheme."
msgstr "འཆར་ལས་སེལ་འཐུ་འབད་ཡོད་མི་འདི་རྩ་བསྐྲད་གཏང་ནའི་དོན་ལུ་ཨེབ་གཏང་།"

#: ../nact/nautilus-actions-config.glade.h:48
msgid "Click to see the list of special tokens you can use in the parameter field."
msgstr "ཚད་བཟུང་ས་སྒོ་ནང་ལུ་ ལག་ལེན་འཐབ་བཏུབ་པའི་ངོ་རྟགས་དམིགས་བསལ་ཚུ་གི་ ཐོ་བཀོད་ལྟ་ནའི་དོན་ལུ་ཨེབ་གཏང་།"

#: ../nact/nautilus-actions-config.glade.h:49
msgid "Click to toggle the display of the list of special tokens you can use in the parameter field."
msgstr "ཚད་བཟུང་ས་སྒོ་ནང་ལུ་ ལག་ལེན་འཐབ་བཏུབ་པའི་་དམིགས་བསལ་ངོ་རྟགསཚུ་གི་ཡིག་ཐོ་བསྐྲམ་སྟོན་འདི་སོར་སྟོན་འབད་ནའི་དོན་ལུ་ཨེབ་གཏང་འབད།"

#: ../nact/nautilus-actions-config.glade.h:50
msgid "Conditions"
msgstr "གནས་སྟངས་ཚུ།"

#: ../nact/nautilus-actions-config.glade.h:51
msgid "Create a copy of the selected action."
msgstr "བྱ་བ་སེལ་འཐུ་གི་འདྲ་བཤུས་ཅིག་གསར་བསྐྲུན་འབད།"

#: ../nact/nautilus-actions-config.glade.h:52
msgid "Delete the action without any confirmation nor the possibility to restore it."
msgstr "བྱ་བ་འདི་ ངེས་དཔྱད་ག་ནི་ཡང་མེད་པ་དང་ དེ་མེན་པ་ཅིན་ སོར་ཆུད་འབད་ཚུགས་མི་ཅིག་གིས་ བཏོན་གཏང་།"

#: ../nact/nautilus-actions-config.glade.h:53
msgid "Export existing configs"
msgstr " ཡོད་བཞིན་པའི་རིམ་སྒྲིག་ཕྱིར་འདྲེན་འབད།"

#: ../nact/nautilus-actions-config.glade.h:54
msgid "File to Import:"
msgstr "ནང་འདྲན་འབད་ནའི་ཡིག་སྣོད:"

#: ../nact/nautilus-actions-config.glade.h:55
msgid "Filenames:"
msgstr "ཡིག་སྣོད་ཀྱི་མིང་ཚུ་ :"

#: ../nact/nautilus-actions-config.glade.h:56
msgid "GConf schema description file (Nautilus-actions v1.x and later)"
msgstr "ཇི་སི་ཨོ་ཨེན་ཨེཕ་ལས་འཆར་གྱི་འགྲེལ་བཤད། (ནའུ་ཊི་ལཱསི།-བྱ་བ་ཚུ་v1.x དང་ ཤུལ་མམ།)"

#: ../nact/nautilus-actions-config.glade.h:57
msgid "Icon:"
msgstr "ངོས་དཔར:"

