msgid "Only display when battery _present"
msgstr "འཕྲུལ་ལྟོ་ཡོདཔ་ད་རྐྱངམ་གཅིག་ བཀྲམ་སྟོན་འབད།(_p)"

#: ../data/gpm-prefs.glade.h:8
msgid "Only display when charging or _discharging"
msgstr "ནུས་ཤུགས་བླུག་པའི་བསྒང་དང་ནུས་ཤུགས་བཏོན་་པའི་བསྒང་རྐྱངམ་གཅིག་ བཀྲམ་སྟོན་འབད།(_d)"

#: ../data/gpm-prefs.glade.h:9
msgid "Power Management Preferences"
msgstr "ནུས་ཤུགས་འཛིན་སྐྱོང་གི་དགའ་གདམ་ཚུ།"

#: ../data/gpm-prefs.glade.h:10
msgid "Put _display to sleep when computer is inactive for:"
msgstr "གློག་རིག་འདི་ཤུགས་མེད་སྦེ་སྡོད་པའི་སྐབས་ བཀྲམ་སྟོན་འདི་གཉིད་ནང་ནང་བཙུགས:(_d)"

#: ../data/gpm-prefs.glade.h:11
msgid "Put computer to _sleep when it is inactive for:"
msgstr "ཤུགས་མེད་སྦེ་སྡོདཔ་ད་ གློག་རིག་འདི་གཉིད་ནང་བཙུགས།(_S)"

#: ../data/gpm-prefs.glade.h:12
msgid "Running on AC"
msgstr "ཨེ་སི་གུ་གཡོག་བཀོལ་བའི་བསྒང་།"

#: ../data/gpm-prefs.glade.h:13
msgid "Running on Battery"
msgstr "འཕྲུལ་ལྟོ་གུ་གཡོག་བཀོལ་བའི་བསྒང་།"

#: ../data/gpm-prefs.glade.h:14
msgid "Set display _brightness to:"
msgstr "བཀྲམ་སྟོན་ཆ་ཚན་འདི་ དཀར་མདངས་ལུ:(_b)"

#: ../data/gpm-prefs.glade.h:15
msgid "Sleep _type when inactive:"
msgstr "ཤུགས་མེད་ཀྱི་སྐབས་ལུ་ཉལ་ནི་གི་དབྱེ་བ།(_t)"

#: ../data/gpm-prefs.glade.h:16
